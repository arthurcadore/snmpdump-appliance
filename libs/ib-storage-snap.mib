-- Logic Volume Mirror Config Table
-- This table described how to config mirror for a logic volume.
ibMirrorConfigTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbMirrorConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table contains some entity to config mirror."
    ::= { ibSnapMibObjects 16 }

ibMirrorConfigEntry OBJECT-TYPE
    SYNTAX      IbMirrorConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "An entry containing some information applicable to mirror for
        the logic volume."
    INDEX
        {
            ibMirrorLvIndex
        }

    ::= { ibMirrorConfigTable 1 }

IbMirrorConfigEntry ::= SEQUENCE
    {
        ibMirrorLvIndex            IbLvIDType,
        ibMirrorType               INTEGER,
        ibMirrorStatus             IbStorageOnlineState,
        ibMirrorName               OCTET STRING,
        ibMirrorSyncPercentage     Integer32,
        ibMirrorSyncPerformance    Integer32,
        ibMirrorDelta              Integer32,
        ibMirrorRaidType           INTEGER,
        ibMirrorSelectPolicy       IbExtendSelectPolicy,
        ibMirrorSwitch             INTEGER,
        ibMirrorExtendRaidUuid     IbRaidIDType,
        ibMirrorRowStatus          RowStatus
    }

ibMirrorLvIndex OBJECT-TYPE
    SYNTAX      IbLvIDType
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This object identifies the source logic volume of the mirror.
        The object identified by this index is the same object as identified
        by the same value of ibLvindex.
        Note that an entry of ibMirrorConfigTable can be created only when
        the associated ibMirrorDistributeTable's objects is ready or the associated
        select policy is not equal to 'none'.
        If an entry is deleted, the associated ibMirrorDistributeTable
        objects should be deleted automatically."
    ::= { ibMirrorConfigEntry 1 }

ibMirrorType OBJECT-TYPE
    SYNTAX      INTEGER
        {
            sync(1),
            async(2),
            none(3)
        }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object identifies the type of the mirror."
    ::= { ibMirrorConfigEntry 2 }

ibMirrorStatus OBJECT-TYPE
    SYNTAX      IbStorageOnlineState
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object identifies the state of the mirror resource."
    ::= { ibMirrorConfigEntry 3 }

ibMirrorName OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object identifies the name of the mirror resource. it is used when 
        promote mirror resource."
    ::= { ibMirrorConfigEntry 4 }

ibMirrorSyncPercentage OBJECT-TYPE
    SYNTAX      Integer32(0..100)
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object identifies the percentage of the mirror sync."
    ::= { ibMirrorConfigEntry 5 }

ibMirrorSyncPerformance OBJECT-TYPE
    SYNTAX      Integer32(0..100)
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object identifies the performance of the mirror sync. The unit is MB/S"
    ::= { ibMirrorConfigEntry 6 }


ibMirrorDelta OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "MB"    
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object identifies size of delta data after the last
        mirror sync."
    ::= { ibMirrorConfigEntry 7 }


ibMirrorRaidType OBJECT-TYPE
    SYNTAX      INTEGER
        {
            virtual(1),
            serviceEnable(2),
            none(3)
        }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object identifies the raid type of the mirror resource."
    ::= { ibMirrorConfigEntry 8 }


ibMirrorSelectPolicy OBJECT-TYPE
    SYNTAX      IbExtendSelectPolicy
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object describes the policy about allocating the space 
        for mirror resource."
    DEFVAL { none }
    ::= { ibMirrorConfigEntry 9 }

ibMirrorSwitch OBJECT-TYPE
    SYNTAX      INTEGER
        {
            sync(1),
            swap(2),
            promote(3),
            none(4)
        }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object identifies the state of the mirror.
        The value of 'sync' means synchronize mannually.        
        The value of 'swap' means switch the roles of the primary resource and 
        mirror resource. 
        The value of 'promte' means break the mirrored pair and convert the mirror 
        resource into an independent logic volume. The new logic volume will have 
        all of the properties of a regular logic volume.
        Note that if the action of 'promte' is executed successfully, this mirror
        resource will become a primary logic volume, and this entry will be deleted
        automatically."
    ::= { ibMirrorConfigEntry 10 }
    
ibMirrorExtendRaidUuid OBJECT-TYPE
    SYNTAX      IbRaidIDType
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object identifies the raid of the mirror resource when the primary
        logic volume resource is extended."
    ::= { ibMirrorConfigEntry 11 }

ibMirrorRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object identifies the action to create or delete the mirror resource."
    ::= { ibMirrorConfigEntry 12 }


-- Logic Volume Mirror Resource Distributing Table
-- This table described how to config mirror resource for logic volume.
ibMirrorDistributeTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbMirrorDistributeEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table contains some entity to config the mirror resource."
    ::= { ibSnapMibObjects 17 }

ibMirrorDistributeEntry OBJECT-TYPE
    SYNTAX      IbMirrorDistributeEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "An entry containing some information applicable to mirror
        resource for the logic volume."
    INDEX
        {
            ibMirrorDistLvIndex,
            ibMirrorRaidUuid
        }

    ::= { ibMirrorDistributeTable 1 }

IbMirrorDistributeEntry ::= SEQUENCE
    {
        ibMirrorDistLvIndex     IbLvIDType,
        ibMirrorRaidUuid        IbRaidIDType,
        ibMirrorRaidSize        Integer32,
        ibMirrorExtRowStatus    RowStatus
    }


ibMirrorDistLvIndex OBJECT-TYPE
    SYNTAX      IbLvIDType
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This index is identical to ibLvIndex in IB-LOGIC-VOLUME-MIB."
    ::= { ibMirrorDistributeEntry 1 }

ibMirrorRaidUuid OBJECT-TYPE
    SYNTAX      IbRaidIDType
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This index is identical to ibRaidUuid in IB-RAID-MIB."
    ::= { ibMirrorDistributeEntry 2 }

ibMirrorRaidSize OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object identifies the size on the special raid distributed
        for the mirror resource."
    ::= { ibMirrorDistributeEntry 3 }

ibMirrorExtRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object identifies the action to create or delete the distribution
        for the mirror resource."
    ::= { ibMirrorDistributeEntry 4 }
END
