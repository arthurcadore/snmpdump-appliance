IB-ARP-TRAP-MIB DEFINITIONS ::= BEGIN

IMPORTS
        ibCommon
    FROM IB-OID-MIB
        DisplayString
    FROM SNMPv2-TC
        Unsigned32, IpAddress, OBJECT-TYPE, MODULE-IDENTITY, NOTIFICATION-TYPE
    FROM SNMPv2-SMI;
-- Node definitions
ibARPTrap MODULE-IDENTITY
    LAST-UPDATED "202304201710Z"      -- APR 20, 2023 at 17:10 GMT
    ORGANIZATION
        "Intelbras S.A. Industria de Telecomunicacao Eletronica Brasileria."
    CONTACT-INFO
        ""
    DESCRIPTION
        "This MIB file defines the ARP trap warning feature only."
    REVISION "202304201710Z"        -- APR 20, 2023 at 17:10 GMT
    DESCRIPTION
        "Add ibARPTrapPktQueue."
    REVISION "202205131800Z"        -- MAY 13, 2022 at 18:00 GMT
    DESCRIPTION
        "Added ibARPTrapPacketValidCheck, ibARPTrapHostMoveResume, ibARPTrapDtcDropVlanPkt, ibARPTrapDtcDropVsiPkt,
         ibARPTrapDtcDropIfPkt, ibARPTrapMacMismatch,
         ibARPTrapEntryCheck, and
         ibARPTrapGatewayCheck."
    REVISION "202112081800Z"        -- DEC 08, 2021 at 18:00 GMT
    DESCRIPTION
        "The initial version of this MIB file."
    ::= { ibCommon 217}

-- ibARPTrapEntry
ibARPTrapEntry OBJECT IDENTIFIER ::= { ibARPTrap 1 }

ibARPTrapsEntry OBJECT IDENTIFIER  ::= { ibARPTrapEntry 0 }

-- ibARPTrapsEntry
ibARPTrapThresholdAlarm NOTIFICATION-TYPE
    OBJECTS
        {
            ibARPTrapEntryThreshold,
            ibARPTrapDynamicEntryNum,
            ibARPTrapStaticEntryNum,
            ibARPTrapOtherEntryNum
        }
    STATUS      current
    DESCRIPTION
        "If the numbers of ARP entry on a device exceeds the threshold,
         a trap message is generated and sent to the remote monitoring device."
    ::= { ibARPTrapsEntry 1 }

ibARPTrapThresholdResume NOTIFICATION-TYPE
    OBJECTS
        {
            ibARPTrapEntryThreshold,
            ibARPTrapDynamicEntryNum,
            ibARPTrapStaticEntryNum,
            ibARPTrapOtherEntryNum
        }
    STATUS      current
    DESCRIPTION
        "If the numbers of ARP entry on a device drops below the threshold,
         a trap message is generated and sent to the remote monitoring device."
    ::= { ibARPTrapsEntry 2 }

ibARPTrapSlotThresholdAlarm NOTIFICATION-TYPE
    OBJECTS
        {
            ibARPTrapEntryChassis,
            ibARPTrapEntrySlot,
            ibARPTrapEntryThreshold,
            ibARPTrapDynamicEntryNum
        }
    STATUS      current
    DESCRIPTION
        "If the numbers of ARP entry of slot exceeds the threshold,
         a trap message is generated and sent to the remote monitoring device."
    ::= { ibARPTrapsEntry 3 }

ibARPTrapSlotThresholdResume NOTIFICATION-TYPE
    OBJECTS
        {
            ibARPTrapEntryChassis,
            ibARPTrapEntrySlot,
            ibARPTrapEntryThreshold,
            ibARPTrapDynamicEntryNum
        }
    STATUS      current
    DESCRIPTION
        "If the numbers of ARP entry of slot drops below the threshold,
         a trap message is generated and sent to the remote monitoring device."
    ::= { ibARPTrapsEntry 4 }

ibARPTrapIfThresholdAlarm NOTIFICATION-TYPE
    OBJECTS
        {
            ibARPTrapEntryIfName,
            ibARPTrapEntryThreshold,
            ibARPTrapDynamicEntryNum
        }
    STATUS      current
    DESCRIPTION
        "If the numbers of ARP entry exceeds the threshold of the interface,
         a trap message is generated and sent to the remote monitoring device."
    ::= { ibARPTrapsEntry 5 }

ibARPTrapIfThresholdResume NOTIFICATION-TYPE
    OBJECTS
        {
            ibARPTrapEntryIfName,
            ibARPTrapEntryThreshold,
            ibARPTrapDynamicEntryNum
        }
    STATUS      current
    DESCRIPTION
        "If the numbers of ARP entry drops below the threshold of the interface,
         a trap message is generated and sent to the remote monitoring device."
    ::= { ibARPTrapsEntry 6 }

ibARPTrapSuppThresholdAlarm NOTIFICATION-TYPE
    OBJECTS
        {
            ibARPTrapEntryThreshold,
            ibARPTrapSuppEntryNum
        }
    STATUS      current
    DESCRIPTION
        "If the numbers of ARP suppression entry on a device exceeds the
         threshold, a trap message is generated and sent to the remote
         monitoring device."
    ::= { ibARPTrapsEntry 7 }

ibARPTrapSuppThresholdResume NOTIFICATION-TYPE
    OBJECTS
        {
            ibARPTrapEntryThreshold,
            ibARPTrapSuppEntryNum
        }
    STATUS      current
    DESCRIPTION
        "If the numbers of ARP suppression entry on a device drops below the
        threshold, a trap message is generated and sent to the remote
        monitoring device."
    ::= { ibARPTrapsEntry 8 }

ibARPTrapEntryObjects OBJECT IDENTIFIER  ::= { ibARPTrapEntry 1 }

-- ibARPEntryTrapObjects

ibARPTrapEntryChassis OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "The information of chassis ID. If the value is 65535, the device is
        not a stacked device ."
    ::= { ibARPTrapEntryObjects 1 }

ibARPTrapEntrySlot OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
    "The information of slot."
    ::= { ibARPTrapEntryObjects 2 }

ibARPTrapEntryIfName OBJECT-TYPE
    SYNTAX      DisplayString(SIZE (0..255))
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "The information of interface name."
    ::= { ibARPTrapEntryObjects 3 }

ibARPTrapEntryThreshold OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "The information of Threshold."
    ::= { ibARPTrapEntryObjects 4 }

ibARPTrapDynamicEntryNum OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "The numbers of ARP dynamic entry."
    ::= { ibARPTrapEntryObjects 5 }

ibARPTrapStaticEntryNum OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "The numbers of ARP static entry."
    ::= { ibARPTrapEntryObjects 6 }

ibARPTrapOtherEntryNum OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "The numbers of ARP other entry."
    ::= { ibARPTrapEntryObjects 7 }

ibARPTrapSuppEntryNum OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "The numbers of ARP suppression entry."
    ::= { ibARPTrapEntryObjects 8 }

ibARPTrapPkt OBJECT IDENTIFIER  ::= { ibARPTrap 2 }

-- ibARPTrapLocalConflict
ibARPTrapLocalConflict OBJECT IDENTIFIER ::= { ibARPTrapPkt 1 }

ibARPTrapsLocal OBJECT IDENTIFIER  ::= { ibARPTrapLocalConflict 0 }

ibARPTrapMACConflictAlarm NOTIFICATION-TYPE
    OBJECTS
        {
            ibARPTrapLocalConflictIf,
            ibARPTrapLocalConflictMac,
            ibARPTrapLocalSrcIp,
            ibARPTrapLocalPeVlan,
            ibARPTrapLocalCeVlan,
            ibARPTrapLocalInboundIf
        }
    STATUS      current
    DESCRIPTION
        "If the MAC address conflict occurred, a trap message is generated
        and sent to the remote monitoring device."
    ::= { ibARPTrapsLocal 1 }

ibARPTrapMACConflictResume NOTIFICATION-TYPE
    OBJECTS
        {
            ibARPTrapLocalConflictIf,
            ibARPTrapLocalConflictMac,
            ibARPTrapLocalSrcIp,
            ibARPTrapLocalPeVlan,
            ibARPTrapLocalCeVlan,
            ibARPTrapLocalInboundIf
        }
    STATUS      current
    DESCRIPTION
        "If the MAC address conflict was removed, a trap message is generated
        and sent to the remote monitoring device."
    ::= { ibARPTrapsLocal 2 }

ibARPTrapMACIPConflictAlarm NOTIFICATION-TYPE
    OBJECTS
        {
            ibARPTrapLocalConflictIf,
            ibARPTrapLocalConflictMac,
            ibARPTrapLocalConflictIP,
            ibARPTrapLocalDstMac,
            ibARPTrapLocalDstIP,
            ibARPTrapLocalPeVlan,
            ibARPTrapLocalCeVlan,
            ibARPTrapLocalInboundIf
        }
    STATUS      current
    DESCRIPTION
        "If a MAC and IP address conflicts occurred, a trap message is generated
        and sent to the remote monitoring device."
    ::= { ibARPTrapsLocal 3 }

ibARPTrapMACIPConflictResume NOTIFICATION-TYPE
    OBJECTS
        {
            ibARPTrapLocalConflictIf,
            ibARPTrapLocalConflictMac,
            ibARPTrapLocalConflictIP,
            ibARPTrapLocalDstMac,
            ibARPTrapLocalDstIP,
            ibARPTrapLocalPeVlan,
            ibARPTrapLocalCeVlan,
            ibARPTrapLocalInboundIf
        }
    STATUS      current
    DESCRIPTION
        "If the MAC and IP address conflicts were resolved, a trap
          message is generated and sent
         to the remote monitoring device."
    ::= { ibARPTrapsLocal 4 }

ibARPTrapIPConflictAlarm NOTIFICATION-TYPE
    OBJECTS
        {
            ibARPTrapLocalConflictIf,
            ibARPTrapLocalConflictIP,
            ibARPTrapLocalSrcMac,
            ibARPTrapLocalPeVlan,
            ibARPTrapLocalCeVlan
        }
    STATUS      current
    DESCRIPTION
        "If the device received an ARP packet with a duplicate
         IP address, a trap message is generated and sent
         to the remote monitoring device."
    ::= { ibARPTrapsLocal 5 }

ibARPTrapIPConflictResume NOTIFICATION-TYPE
    OBJECTS
        {
            ibARPTrapLocalConflictIf,
            ibARPTrapLocalConflictIP,
            ibARPTrapLocalSrcMac,
            ibARPTrapLocalPeVlan,
            ibARPTrapLocalCeVlan
        }
    STATUS      current
    DESCRIPTION
        "If the IP address conflicts were resolved, a trap message is generated
        and sent to the remote monitoring device."
    ::= { ibARPTrapsLocal 6 }

ibARPTrapLocalObjects OBJECT IDENTIFIER  ::= { ibARPTrapLocalConflict 1 }

ibARPTrapLocalConflictIf OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(1..47))
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "Conflict Interface name."
    ::= { ibARPTrapLocalObjects 1 }

ibARPTrapLocalConflictMac OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(1..15))
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "The mac address of Conflict Interface."
    ::= { ibARPTrapLocalObjects 2 }

ibARPTrapLocalConflictIP OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "The ip address of Conflict Interface."
    ::= { ibARPTrapLocalObjects 3 }

ibARPTrapLocalSrcMac OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(1..15))
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "The source mac of Arp packet"
    ::= { ibARPTrapLocalObjects 4}

ibARPTrapLocalSrcIp OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "The source ip of Arp packet"
    ::= { ibARPTrapLocalObjects 5 }

ibARPTrapLocalDstMac OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(1..15))
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "The destination mac of Arp packet."
    ::= { ibARPTrapLocalObjects 6}

ibARPTrapLocalDstIP OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "The destination ip of Arp packet."
    ::= { ibARPTrapLocalObjects 7 }

ibARPTrapLocalPeVlan OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "The first vlan ip of Arp packet."
    ::= { ibARPTrapLocalObjects 8}

ibARPTrapLocalCeVlan OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "TThe second vlan ip of Arp packet."
    ::= { ibARPTrapLocalObjects 9 }

ibARPTrapLocalInboundIf OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(1..47))
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "TThe physical inbound inerface of Arp packet."
    ::= { ibARPTrapLocalObjects 10}

-- ibARPTrapHostMove
ibARPTrapHostMove OBJECT IDENTIFIER ::= { ibARPTrapPkt 2 }

ibARPTrapsHostMove OBJECT IDENTIFIER  ::= { ibARPTrapHostMove 0 }

ibARPTrapHostMoveAlarm NOTIFICATION-TYPE
    OBJECTS
        {
            ibARPTrapMoveUserIP,
            ibARPTrapMoveUserMAC,
            ibARPTrapMoveLocalIf,
            ibARPTrapMoveLocalPeVlan,
            ibARPTrapMoveLocalCeVlan,
            ibARPTrapMoveRemoteIF,
            ibARPTrapMoveRemotePeVlan,
            ibARPTrapMoveRemoteCeVlan
        }
    STATUS      current
    DESCRIPTION
        "If the device detected a user moved to another interface,
         a trap message is generated and sent
         to the remote monitoring device."
    ::= { ibARPTrapsHostMove 1 }

ibARPTrapHostMoveResume NOTIFICATION-TYPE
    OBJECTS
        {
            ibARPTrapMoveUserIP,
            ibARPTrapMoveUserMAC,
            ibARPTrapMoveLocalIf,
            ibARPTrapMoveLocalPeVlan,
            ibARPTrapMoveLocalCeVlan,
            ibARPTrapMoveRemoteIF,
            ibARPTrapMoveRemotePeVlan,
            ibARPTrapMoveRemoteCeVlan
        }
    STATUS      current
    DESCRIPTION
        "If the device detects that the event of the user moving to
        another interface is cleared, it sends a trap message to the remote
        monitoring device."
    ::= { ibARPTrapsHostMove 2 }

ibARPTrapHostMoveObjects OBJECT IDENTIFIER  ::= { ibARPTrapHostMove 1 }

ibARPTrapMoveUserIP OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "The ip address of user moved to another interface."
    ::= { ibARPTrapHostMoveObjects 1 }

ibARPTrapMoveUserMAC OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(1..15))
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "The mac address of user moved to another interface."
    ::= { ibARPTrapHostMoveObjects 2 }

ibARPTrapMoveLocalIf OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(1..47))
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "The interface before user move."
    ::= { ibARPTrapHostMoveObjects 3 }

ibARPTrapMoveLocalPeVlan OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "The first vlan before user move."
    ::= { ibARPTrapHostMoveObjects 4}

ibARPTrapMoveLocalCeVlan OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "The second vlan before user move."
    ::= { ibARPTrapHostMoveObjects 5 }

ibARPTrapMoveRemoteIF OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(1..47))
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "The interface after user move."
    ::= { ibARPTrapHostMoveObjects 6}

ibARPTrapMoveRemotePeVlan OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "The first vlan after user move."
    ::= { ibARPTrapHostMoveObjects 7 }

ibARPTrapMoveRemoteCeVlan OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "The second vlan after user move."
    ::= { ibARPTrapHostMoveObjects 8 }

-- ibARPTrapUserIpConflict
