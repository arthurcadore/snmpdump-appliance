-- ibARPTrapAckStrictCheck
ibARPTrapAckStrictCheck OBJECT IDENTIFIER ::= { ibARPTrapPkt 4 }

ibARPTrapsAckStrictCheck OBJECT IDENTIFIER  ::= { ibARPTrapAckStrictCheck 0 }

ibARPTrapAckStrictCheckAlarm NOTIFICATION-TYPE
    OBJECTS
        {
            ibARPTrapAckIfName,
            ibARPTrapAckSourceIP
        }
    STATUS      current
    DESCRIPTION
        "If the interface received an ARP reply that was not requested by the
         device, a trap message is generated and sent to the remote monitoring
         device."
    ::= { ibARPTrapsAckStrictCheck 1 }

ibARPTrapAckStrictObjs OBJECT IDENTIFIER  ::= { ibARPTrapAckStrictCheck 1 }

ibARPTrapAckIfName OBJECT-TYPE
    SYNTAX      DisplayString(SIZE (0..255))
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "The interface of receiving an ARP reply."
    ::= { ibARPTrapAckStrictObjs 1}

ibARPTrapAckSourceIP OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "The source ip of the ARP reply."
    ::= { ibARPTrapAckStrictObjs 2 }

-- ibARPTrapSpeedLimit
ibARPTrapSpeedLimit OBJECT IDENTIFIER ::= { ibARPTrapPkt 5 }

ibARPTrapsSpeedLimit OBJECT IDENTIFIER  ::= { ibARPTrapSpeedLimit 0 }

ibARPTrapSpeedLimitAlarm NOTIFICATION-TYPE
    OBJECTS
        {
            ibARPTrapSpeedLimitChassis,
            ibARPTrapSpeedLimitSlot,
            ibARPTrapSpeedLimitSupValue,
            ibARPTrapSpeedLimitCurValue,
            ibARPTrapSpeedLimitSupType
        }
    STATUS      current
    DESCRIPTION
        "If the rate of sending ARP packets or deliving ARP miss messages
        exceeded the rate limit, a trap message is generated and sent
        to the remote monitoring device."
    ::= { ibARPTrapsSpeedLimit 1 }

ibARPTrapSpeedLimitResume NOTIFICATION-TYPE
    OBJECTS
        {
            ibARPTrapSpeedLimitChassis,
            ibARPTrapSpeedLimitSlot,
            ibARPTrapSpeedLimitSupValue,
            ibARPTrapSpeedLimitCurValue,
            ibARPTrapSpeedLimitSupType
        }
    STATUS      current
    DESCRIPTION
        "If the rate of sending ARP packets or deliving ARP miss messages
        droped below the rate limit, a trap message is generated and sent
        to the remote monitoring device."
    ::= { ibARPTrapsSpeedLimit 2 }

ibARPTrapSpeedLimitObjs OBJECT IDENTIFIER  ::= { ibARPTrapSpeedLimit 1 }

ibARPTrapSpeedLimitChassis OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "The information of chassis ID. If the value is 65535, the device is
        not a stacked device ."
    ::= { ibARPTrapSpeedLimitObjs 1 }

ibARPTrapSpeedLimitSlot OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
    "The information of slot."
    ::= { ibARPTrapSpeedLimitObjs 2 }

ibARPTrapSpeedLimitSupValue OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "pps"
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "The threshold rate of speed limit."
    ::= { ibARPTrapSpeedLimitObjs 3 }

ibARPTrapSpeedLimitCurValue OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "pps"
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "The current rate of speed limit."
    ::= { ibARPTrapSpeedLimitObjs 4 }

ibARPTrapSpeedLimitSupType OBJECT-TYPE
    SYNTAX      INTEGER
        {
            packet(1),
            miss(2)
        }
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "The current rate of speed limit:
         'packet' means Arp packet Speed Limit.
         'miss' means Arp Miss Message Speed Limit."
    ::= { ibARPTrapSpeedLimitObjs 5}

-- ibARPTrapPacketValidCheck
ibARPTrapPacketValidCheck OBJECT IDENTIFIER ::= { ibARPTrapPkt 6 }

ibARPTrapsPacketValidCheck OBJECT IDENTIFIER  ::= { ibARPTrapPacketValidCheck 0 }

ibARPTrapPacketValidCheckAlarm NOTIFICATION-TYPE
    OBJECTS
        {
            ibARPTrapPktValidCheckIf,
            ibARPTrapPktValidCheckMac,
            ibARPTrapPktValidCheckIp,
            ibARPTrapPktValidCheckVlan,
            ibARPTrapPktValidtCheckSvlan
        }
    STATUS      current
    DESCRIPTION
        "If the device receives an invalid ARP packet,
        a trap message is generated and sent to
        the remote monitoring device."
