    STATUS      current
    DESCRIPTION
        "The table of the system information for each AP."
    ::= { ibDot11APConfigGroup 4 }

ibDot11APSysInfoSetEntry OBJECT-TYPE
    SYNTAX      IbDot11APSysInfoSetEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The system information configuration for each AP."
    INDEX
        {
            ibDot11SIDAPSerialID
        }
    ::= { ibDot11APSysInfoSetTable 1 }

IbDot11APSysInfoSetEntry ::= SEQUENCE
    {
        ibDot11APSysNetID            OCTET STRING,
        ibDot11APCpuUsageThreshold   Integer32,
        ibDot11APMemUsageThreshold   Integer32
    }

ibDot11APSysNetID OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(0..127))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents the system Net ID for AP."
    ::= { ibDot11APSysInfoSetEntry 1 }

ibDot11APCpuUsageThreshold OBJECT-TYPE
    SYNTAX      Integer32(0..100)
    UNITS       "onepercent"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The CPU usage threshold for AP."
    ::= { ibDot11APSysInfoSetEntry 2 }

ibDot11APMemUsageThreshold OBJECT-TYPE
    SYNTAX      Integer32(0..100)
    UNITS       "onepercent"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The memory usage threshold for AP."
    ::= { ibDot11APSysInfoSetEntry 3 }

