    DESCRIPTION
        "If radio type is 802.11an or 802.11gn, permit only clients of 11n mode.
        If radio type is 802.11ac, permit clients of both 11n and 11ac mode."
    DEFVAL { dot11false }
    ::= { ibDot11nRadioCfg2Entry 12 }

ibDot11nRadioCfg2SupportMaxMcs OBJECT-TYPE
    SYNTAX Integer32 (0..76)
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "Specify the maximum Modulation and Coding Scheme (MCS) index for
        802.11n supported rates.
        The specified maximum MCS index for 802.11n supported rates must be no
        less than the specified maximum MCS index for 802.11n mandatory rates."
    DEFVAL { 76 }
    ::= { ibDot11nRadioCfg2Entry 13 }

ibDot11nRadioCfg2MandatoryMaxMcs OBJECT-TYPE
    SYNTAX Integer32 (0..76 | 255)
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "Specify the maximum modulation and coding scheme (MCS) index for
        802.11n mandatory rates.
        The value 255 indicates that no maximum MCS index
        is specified.  No maximum MCS index is specified for 802.11n
        mandatory rates by default.
        Besides 255, the specified maximum MCS index for 802.11n supported
        rates must be no less than the specified maximum MCS index for 802.11n
        mandatory rates."
    DEFVAL { 255 }
    ::= { ibDot11nRadioCfg2Entry 14 }

-- *****************************************************************************
-- *  End of ibDot11nRadioCfg2Table Definition
-- *****************************************************************************

-- *****************************************************************************
-- *  ibDot11CfgNotifyGroup Definition
-- *****************************************************************************
ibDot11CfgNotifications OBJECT IDENTIFIER ::= { ibDot11CfgNotifyGroup 0 }

ibDot11CfgCipherChange NOTIFICATION-TYPE
    OBJECTS
    {
        ibDot11SSIDName,
        ibDot11SecurityCiphers
    }
    STATUS      current
    DESCRIPTION
        "This notification is sent when cipher type of corresponding service
        template is changed."
    ::= { ibDot11CfgNotifications 1 }

ibDot11CfgPSKChange NOTIFICATION-TYPE
    OBJECTS
    {
        ibDot11SSIDName
    }
    STATUS      current
    DESCRIPTION
        "This notification is sent when pre-shared key of corresponding
        service template is changed."
    ::= { ibDot11CfgNotifications 2 }

ibDot11SSIDWepIDConflictTrap NOTIFICATION-TYPE
    OBJECTS
    {
        ibDot11PreConflictTemplateNum,
        ibDot11CurrConflictTemplateNum,
        ibDot11ConflictCipherIdx,
        ibDot11ConfigureAPID,
        ibDot11ConfigureRadioID,
        ibDot11ConfigureAPMacAddress,
        ibDot11PreConflictTemplateSSID,
        ibDot11CurrConflictTemplateSSID
    }
    STATUS      current
    DESCRIPTION
        "This notification will be sent when the same wep ID is configured
         on two service policies."
    ::= { ibDot11CfgNotifications 3 }

-- CFG Notification variable object

ibDot11CfgTrapVarObjects OBJECT IDENTIFIER
    ::= { ibDot11CfgNotifyGroup 1 }

ibDot11PreConflictTemplateNum OBJECT-TYPE
    SYNTAX      Integer32 (1..1024)
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "Represents previous service policy ID when radio bind service policy
         and current service policy conflict with previous service policy
         in cipher configuration."
    ::= { ibDot11CfgTrapVarObjects 1 }

ibDot11CurrConflictTemplateNum OBJECT-TYPE
    SYNTAX      Integer32 (1..1024)
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "Represents current service policy ID when radio bind service policy
         and current service policy conflict with previous service policy
         in cipher configuration."
    ::= { ibDot11CfgTrapVarObjects 2 }

ibDot11ConflictCipherIdx OBJECT-TYPE
    SYNTAX      Integer32 (1..4)
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "Represents wep ID that conflicted with."
    ::= { ibDot11CfgTrapVarObjects 3 }

ibDot11ConfigureAPID OBJECT-TYPE
    SYNTAX      IbDot11ObjectIDType
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "Represents which AP is configured."
    ::= { ibDot11CfgTrapVarObjects 4 }

ibDot11ConfigureRadioID OBJECT-TYPE
    SYNTAX      IbDot11RadioScopeType
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "Represents which radio in AP is configured."
    ::= { ibDot11CfgTrapVarObjects 5 }

ibDot11ConfigureAPMacAddress OBJECT-TYPE
    SYNTAX      MacAddress
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "Represents MAC Address of AP."
    ::= { ibDot11CfgTrapVarObjects 6 }

ibDot11PreConflictTemplateSSID OBJECT-TYPE
    SYNTAX      IbDot11SSIDStringType
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "Represents the previous SSID of the AP."
    ::= { ibDot11CfgTrapVarObjects 7 }

ibDot11CurrConflictTemplateSSID OBJECT-TYPE
    SYNTAX      IbDot11SSIDStringType
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "Represents the current SSID of the AP."
    ::= { ibDot11CfgTrapVarObjects 8 }

-- *****************************************************************************
-- *  End of ibDot11CfgNotifyGroup Definition
-- *****************************************************************************

-- *****************************************************************************
-- *  ibDot11LocalACTemplateTable Definition
-- *****************************************************************************
ibDot11LocalACTemplateTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbDot11LocalACTemplateEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "By local AC template CLI, administrator will configure the parameters of LocalAC
        by convenience.
        Before local AC boot up and connect to AC by CAPWAP tunnel, administrator
        will create local AC template for each local AC. When local AC connect to AC,
        AC will assign configuration to local AC by ibDot11LocalACSerialID
        information in local AC template."
    ::= { ibDot11LocalACConfigGroup 1 }

ibDot11LocalACTemplateEntry OBJECT-TYPE
    SYNTAX      IbDot11LocalACTemplateEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry contains configuration information for each LocalAC."
    INDEX
        {
            ibDot11LocalACTemplateName
        }
    ::= { ibDot11LocalACTemplateTable 1 }

IbDot11LocalACTemplateEntry ::= SEQUENCE
    {
        ibDot11LocalACTemplateName             OCTET STRING,
        ibDot11LocalACName                     OCTET STRING,
        ibDot11LocalACSerialID                 OCTET STRING,
        ibDot11TemLocalACModelAlias            OCTET STRING,
        ibDot11LocalACTempRowStatus            RowStatus,
        ibDot11LocalACStatus                   INTEGER,
        ibDot11LocalACIPAddress                IpAddress,
        ibDot11LocalACIPv6Address              OCTET STRING,
        ibDot11EchoInterval                    Integer32,
        ibDot11RetransInterval                 Integer32,
        ibDot11RetransCount                    Integer32,
        ibDot11FirmwareUpgrade                 INTEGER
    }

ibDot11LocalACTemplateName OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(0..127))
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Represents the name of LocalAC template."
    ::= { ibDot11LocalACTemplateEntry 1 }

ibDot11LocalACName OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the LocalAC name."
    ::= { ibDot11LocalACTemplateEntry 2 }

ibDot11LocalACSerialID OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents LocalAC serial id."
    ::= { ibDot11LocalACTemplateEntry 3 }

ibDot11TemLocalACModelAlias OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the alias of LocalAC model name."
    ::= { ibDot11LocalACTemplateEntry 4 }

ibDot11LocalACTempRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The status of this table entry."
    ::= { ibDot11LocalACTemplateEntry 5 }

ibDot11LocalACStatus OBJECT-TYPE
    SYNTAX      INTEGER
        {
            join(1),
            joinConfirm(2),
            download(3),
            config(4),
            run(5),
            idle(6)
        }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the capwap status of local AC.
        The following values are supported:
        join         - local AC is joining to cnetral AC.
        joinConfirm  - cnetral AC confirm whether permit local AC to
        - join.
        download     - local AC is downloading software from cnetral
        - AC.
        config       - local AC and cnetral AC exchange configuration
        - before local AC provide WLAN service.
        run          - The local AC is run.
        idle         - The local AC is idle."
    ::= { ibDot11LocalACTemplateEntry 6 }

ibDot11LocalACIPAddress OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the IP address of an LocalAC."
    ::= { ibDot11LocalACTemplateEntry 7 }

ibDot11LocalACIPv6Address OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the IPv6 address of LocalAC."
    ::= { ibDot11LocalACTemplateEntry 8 }

ibDot11EchoInterval OBJECT-TYPE
    SYNTAX      Integer32(0 | 5..255)
    UNITS       "second"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the interval of echo."
    DEFVAL      { 10 }
    ::= { ibDot11LocalACTemplateEntry 9 }

ibDot11RetransInterval OBJECT-TYPE
    SYNTAX      Integer32(3..8)
    UNITS       "second"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the interval of retransmit."
    DEFVAL      { 5 }
    ::= { ibDot11LocalACTemplateEntry 10 }

ibDot11RetransCount OBJECT-TYPE
    SYNTAX      Integer32(2..5)
    UNITS       "second"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the maximum of retransmit."
    DEFVAL      { 3 }
    ::= { ibDot11LocalACTemplateEntry 11 }

ibDot11FirmwareUpgrade OBJECT-TYPE
    SYNTAX      INTEGER
        {
            enable(1),
            disable(2)
        }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the status of firmware upgrade for an local AC.
        The following values are supported:
        enable      - local AC firmware upgrade will be enabled.
        disable     - local AC firmware upgrade will be disabled."
        DEFVAL      { enable }
    ::= { ibDot11LocalACTemplateEntry 12 }

-- *****************************************************************************
-- *  End of  ibDot11LocalACTemplateTable Definition
-- *****************************************************************************

-- *****************************************************************************
-- *  ibDot11RemoteCfgApTable Definition
-- *****************************************************************************
ibDot11RemoteCfgApTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbDot11RemoteCfgApEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Remote configuration for an AP."
    ::= { ibDot11RemoteConfigGroup  1 }

ibDot11RemoteCfgApEntry OBJECT-TYPE
    SYNTAX      IbDot11RemoteCfgApEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry contains configuration information for each AP."
    INDEX
        {
            ibDot11RmtApName
        }
    ::= { ibDot11RemoteCfgApTable 1 }

IbDot11RemoteCfgApEntry ::= SEQUENCE
    {
        ibDot11RmtApName                 OCTET STRING,
        ibDot11RmtVlanList               OCTET STRING,
        ibDot11RmtCfgEnable              TruthValue
    }

ibDot11RmtApName OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(1..127))
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Represents the name of an AP."
    ::= { ibDot11RemoteCfgApEntry 1 }

ibDot11RmtVlanList OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents the VLAN list on an AP."
    ::= { ibDot11RemoteCfgApEntry 2 }

ibDot11RmtCfgEnable OBJECT-TYPE
    SYNTAX TruthValue
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "Enables remote configuration synchronization."
    DEFVAL { false }
    ::= { ibDot11RemoteCfgApEntry 3 }
-- *****************************************************************************
-- *  End of  ibDot11RemoteCfgApTable Definition
-- *****************************************************************************

-- *****************************************************************************
-- *  ibDot11RemoteCfgIFTable Definition
-- *****************************************************************************
ibDot11RemoteCfgIFTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbDot11RemoteCfgIFEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Remote configuration for interface on an AP."
    ::= { ibDot11RemoteConfigGroup  2 }

ibDot11RemoteCfgIFEntry OBJECT-TYPE
    SYNTAX      IbDot11RemoteCfgIFEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry contains configuration information for each interface on an AP."
    INDEX
        {
            ibDot11RmtIfApName,
            ibDot11RmtIfType,
            ibDot11RmtIfNum
        }
    ::= { ibDot11RemoteCfgIFTable 1 }

IbDot11RemoteCfgIFEntry ::= SEQUENCE
    {
        ibDot11RmtIfApName                OCTET STRING,
        ibDot11RmtIfType                  Integer32,
        ibDot11RmtIfNum                   Integer32,
        ibDot11RmtIfName                  OCTET STRING,
        ibDot11RmtIfLinkType              INTEGER,
        ibDot11RmtIfAccessVlan            Integer32,
        ibDot11RmtIfTrunkPvidVlan         Integer32,
        ibDot11RmtIfTrunkVlanlist         OCTET STRING,
        ibDot11RmtIfHybridPvidVlan        Integer32,
        ibDot11RmtIfHybVlanListTag        OCTET STRING,
        ibDot11RmtIfHybVlanListUnTag      OCTET STRING,
        ibDot11RmtIfIsolate               TruthValue,
        ibDot11RmtIfLinkAggGroupId        Integer32,
        ibDot11RmtIfManagement            TruthValue
    }

ibDot11RmtIfApName OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(1..127))
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Represents the name of an AP."
    ::= { ibDot11RemoteCfgIFEntry 1 }

ibDot11RmtIfType OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Represents the type of the interface.
         ethernet (1),
         gigabitethernet (2),
         ten-gigabitethernet (3),
         smartrate-ethernet(4)."
    ::= { ibDot11RemoteCfgIFEntry 2 }

ibDot11RmtIfNum OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Represents the ID of the interface."
    ::= { ibDot11RemoteCfgIFEntry 3 }

ibDot11RmtIfName OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the name of the interface."
    ::= { ibDot11RemoteCfgIFEntry 4 }

ibDot11RmtIfLinkType OBJECT-TYPE
    SYNTAX      INTEGER
        {
            access (1),
            trunk (2),
            hybrid (3)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Set the link type to access, hybrid or trunk.
         Setting recommend one and setup more related nodes."
    ::= { ibDot11RemoteCfgIFEntry 5 }

ibDot11RmtIfAccessVlan OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Specify the access VLAN for a port."
    ::= { ibDot11RemoteCfgIFEntry 6 }

ibDot11RmtIfTrunkPvidVlan OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Specify the default VLAN for a port."
    ::= { ibDot11RemoteCfgIFEntry 7 }

ibDot11RmtIfTrunkVlanlist OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Specify the permitted VLAN list for a port."
    ::= { ibDot11RemoteCfgIFEntry 8 }

ibDot11RmtIfHybridPvidVlan OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Specify the default VLAN for the hybrid port."
    ::= { ibDot11RemoteCfgIFEntry 9 }

ibDot11RmtIfHybVlanListTag OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Assign the hybrid port to tagged VLANs."
    ::= { ibDot11RemoteCfgIFEntry 10 }

ibDot11RmtIfHybVlanListUnTag OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Assign the hybrid port to untagged VLANs."
    ::= { ibDot11RemoteCfgIFEntry 11 }

ibDot11RmtIfIsolate OBJECT-TYPE
    SYNTAX TruthValue
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "Enable or disable port isolation."
    DEFVAL { true }
    ::= { ibDot11RemoteCfgIFEntry 12 }

ibDot11RmtIfLinkAggGroupId OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Specify link aggregation group number for the port."
    ::= { ibDot11RemoteCfgIFEntry 13 }

ibDot11RmtIfManagement OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Interface management of the port."
    DEFVAL { true }
    ::= { ibDot11RemoteCfgIFEntry 14 }
-- *****************************************************************************
-- *  End of  ibDot11RemoteCfgIFTable Definition
-- *****************************************************************************

END
