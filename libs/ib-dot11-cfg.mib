-- *****************************************************************************
-- *  ibDot11RadioConfigTable Definition
-- *****************************************************************************
ibDot11RadioConfigTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbDot11RadioConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Before AP connects to AC, administrator will configure radio
        parameter to each radio."
    ::= { ibDot11APConfigGroup 8 }

ibDot11RadioConfigEntry OBJECT-TYPE
    SYNTAX      IbDot11RadioConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry contains configuration information for each radio."
    INDEX
        {
            ibDot11RCAPSerialID,
            ibDot11RCRadioID
        }
    ::= { ibDot11RadioConfigTable 1 }

IbDot11RadioConfigEntry ::= SEQUENCE
    {
        ibDot11RCAPSerialID               OCTET STRING,
        ibDot11RCRadioID                  IbDot11RadioScopeType,
        ibDot11RCRadioType                IbDot11RadioType,
        ibDot11RCChannel                  IbDot11ChannelScopeType,
        ibDot11RCPreambleLen              IbDot11PreambleType,
        ibDot11RCPwrAttValue              Integer32,
        ibDot11RCApPowerLevel             IbDot11TxPwrLevelScopeType,
        ibDot11RCDynamicChlState          TruthValue,
        ibDot11RCDynamicPowerState        TruthValue,
        ibDot11RCRadioStatus              TruthValue,
        ibDot11RCRadioRate                OCTET STRING,
        ibDot11RCPwrAdjustStepLength      Integer32,
        ibDot11RCRadioType2               IbDot11RadioType2,
        ibDot11RCPreambleLenCM            INTEGER,
        ibDot11RCDynamicChlStateCM        INTEGER,
        ibDot11RCRadioStatusCM            INTEGER,
        ibDot11RCRadioRateCM              OCTET STRING,
        ibDot11RCDynamicPowerStateCM      IbDot11TruthValueCM,
        ibDot11RCRssiThresholdCM          Integer32,
        ibDot11RCDynamicChlStateSelfDecisiveCM    IbDot11TruthValueCM,
        ibDot11RCDynamicPowerStateSelfDecisiveCM  IbDot11TruthValueCM
    }

ibDot11RCAPSerialID OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(0..127))
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This object represents the Serial ID of AP."
    ::= { ibDot11RadioConfigEntry 1 }

ibDot11RCRadioID OBJECT-TYPE
    SYNTAX      IbDot11RadioScopeType
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This object represents each radio."
    ::= { ibDot11RadioConfigEntry 2 }

ibDot11RCRadioType OBJECT-TYPE
    SYNTAX      IbDot11RadioType
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object represents the radio will work with which 802.11 radio
         type."
    ::= { ibDot11RadioConfigEntry 3 }

ibDot11RCChannel OBJECT-TYPE
    SYNTAX      IbDot11ChannelScopeType
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object represents the channel allocation of radio."
    ::= { ibDot11RadioConfigEntry 4 }

ibDot11RCPreambleLen OBJECT-TYPE
    SYNTAX      IbDot11PreambleType
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object represents whether use long or short preamble for 802.11
         MAC frame.
        When ibDot11CfgRadioType is either dot11b, dot11g or dot11gn,
        then value for ibDot11PreambleLen will be valid.
        Otherwise, the value for it will be invalid."
    DEFVAL      { short }
    ::= { ibDot11RadioConfigEntry 5 }

ibDot11RCPwrAttValue OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object represents the value of power attenuation."
    ::= { ibDot11RadioConfigEntry 6 }

ibDot11RCApPowerLevel OBJECT-TYPE
    SYNTAX      IbDot11TxPwrLevelScopeType
    UNITS       "dbm"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents the maximal transmit power scope for AP."
    ::= { ibDot11RadioConfigEntry 7 }

ibDot11RCDynamicChlState OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Enable/Disable dynamic channel selection."
    ::= { ibDot11RadioConfigEntry 8 }

ibDot11RCDynamicPowerState OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Enable/Disable dynamic power selection for the band."
    ::= { ibDot11RadioConfigEntry 9 }

ibDot11RCRadioStatus OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents the radio admin status whether is enabled
        or disabled."
    ::= { ibDot11RadioConfigEntry 10 }

ibDot11RCRadioRate OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(0..255))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Set data transmit rates for the radio, in the format of 1,2,5.5...
        For 802.11n radios, the format can be MCS X.  The data transmit rates
        between AP and STA must fall into the configured rates.
        (Note: For 802.11n radios, the first value will be treated as a mandatory rate,
        and the second value will be treated as a supported rate.
        If you configure more than two rates, the configuration cannot take effect.
        If you configure only one rate, it is considered as the mandatory rate by default.)"
    ::= { ibDot11RadioConfigEntry 11 }

ibDot11RCPwrAdjustStepLength OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Power adjustment step-length in dBm."
    ::= { ibDot11RadioConfigEntry 12 }

ibDot11RCRadioType2 OBJECT-TYPE
    SYNTAX      IbDot11RadioType2
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object represents the radio will work with which 802.11 radio
         type."
    ::= { ibDot11RadioConfigEntry 13 }

ibDot11RCPreambleLenCM OBJECT-TYPE
    SYNTAX      INTEGER
        {
            long(0),
            short(1)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object represents whether use long or short preamble for 802.11
         MAC frame.
        When ibDot11CfgRadioType is either dot11b, dot11g or dot11gn,
        then value for ibDot11PreambleLenCM will be valid.
        Otherwise, the value for it will be invalid."
    DEFVAL      { short }
    ::= { ibDot11RadioConfigEntry 14 }

ibDot11RCDynamicChlStateCM OBJECT-TYPE
    SYNTAX      INTEGER
        {
            off(0),
            on(1)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Enable/Disable dynamic channel selection."
    ::= { ibDot11RadioConfigEntry 15 }

ibDot11RCRadioStatusCM OBJECT-TYPE
    SYNTAX      INTEGER
        {
            up(1),
            down(2),
            testing(3)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents the radio admin status whether is enabled
        or disabled."
    ::= { ibDot11RadioConfigEntry 16 }

ibDot11RCRadioRateCM OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(0..255))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Set data transmit rates for the radio, in the format of 1,2,5.5...
        For 802.11n radios, the format of each value can be MCS X.  The data transmit
        rates between AP and client should be one of the configured rates.
        (Note: For 802.11n radios, the first value is mandatory rate, and the second
        value is supported rate.  If more than two rate values are configured, the
        configuration will not take effect.  If only one rate value is configured,
        the mandatory rate will be set to the default value.)"
    ::= { ibDot11RadioConfigEntry 17 }

ibDot11RCDynamicPowerStateCM OBJECT-TYPE
    SYNTAX      IbDot11TruthValueCM
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Enable/disable dynamic power selection for the band."
    ::= { ibDot11RadioConfigEntry 18 }

ibDot11RCRssiThresholdCM OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "dBm"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents the minimum value of the RSSI.
        The clients with RSSI below this value will be refused to associate."
    DEFVAL { 1 }
    ::= { ibDot11RadioConfigEntry 19 }

ibDot11RCDynamicChlStateSelfDecisiveCM OBJECT-TYPE
    SYNTAX      IbDot11TruthValueCM
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Enable/disable dynamic channel selection self decisive for the band."
    ::= { ibDot11RadioConfigEntry 20 }

ibDot11RCDynamicPowerStateSelfDecisiveCM OBJECT-TYPE
    SYNTAX      IbDot11TruthValueCM
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Enable/disable dynamic power selection self decisive for the band."
    ::= { ibDot11RadioConfigEntry 21 }

-- *****************************************************************************
-- * End of ibDot11RadioConfigTable Definition
-- *****************************************************************************

ibDot11RadioSSIDCfgTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbDot11RadioSSIDCfgEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "For each radio, it could configure multiple service policies.
        The table describes the relationship between radio and
        service policies."
    ::= { ibDot11APConfigGroup 9 }

ibDot11RadioSSIDCfgEntry OBJECT-TYPE
    SYNTAX      IbDot11RadioSSIDCfgEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry contains SSID configuration information for each radio."
    INDEX
        {
            ibDot11RadioSSIDSerialID,
            ibDot11RadioSSIDRadioID,
            ibDot11RadioSSIDWLANID
        }
    ::= { ibDot11RadioSSIDCfgTable 1 }

IbDot11RadioSSIDCfgEntry ::= SEQUENCE
    {
        ibDot11RadioSSIDSerialID    IbDot11ObjectIDType,
        ibDot11RadioSSIDRadioID     IbDot11RadioScopeType,
        ibDot11RadioSSIDWLANID      Integer32,
        ibDot11RadioSSIDIndex       IbDot11ServicePolicyIDType,
        ibDot11RadioBSSID           MacAddress,
        ibDot11RadioSSIDRowStatus   RowStatus
    }

ibDot11RadioSSIDSerialID OBJECT-TYPE
    SYNTAX      IbDot11ObjectIDType
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This object represents the serial ID of AP."
    ::= { ibDot11RadioSSIDCfgEntry 1 }

ibDot11RadioSSIDRadioID OBJECT-TYPE
    SYNTAX      IbDot11RadioScopeType
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This object represents each radio."
    ::= { ibDot11RadioSSIDCfgEntry 2 }

ibDot11RadioSSIDWLANID OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This object represents the WLAN ID of the BSSID."
    ::= { ibDot11RadioSSIDCfgEntry 3 }

ibDot11RadioSSIDIndex OBJECT-TYPE
    SYNTAX      IbDot11ServicePolicyIDType
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object represents the ID of each service policy used by the
         radio."
    ::= { ibDot11RadioSSIDCfgEntry 4 }

ibDot11RadioBSSID OBJECT-TYPE
    SYNTAX      MacAddress
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "As MAC address format, it is used to identify BSS."
    ::= { ibDot11RadioSSIDCfgEntry 5 }

ibDot11RadioSSIDRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "RowStatus."
    ::= { ibDot11RadioSSIDCfgEntry 6 }

-- *****************************************************************************
-- *  ibDot11APSerialIDTable Definition
-- *****************************************************************************

ibDot11APSerialIDTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbDot11APSerialIDEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The table will provide the general information about APs."
    ::= { ibDot11APConfigGroup 10 }

ibDot11APSerialIDEntry OBJECT-TYPE
    SYNTAX      IbDot11APSerialIDEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry contains the general information of an AP."
    INDEX
        {
            ibDot11SIDAPSerialID
        }
    ::= { ibDot11APSerialIDTable 1 }

IbDot11APSerialIDEntry ::= SEQUENCE
    {
        ibDot11SIDAPSerialID               OCTET STRING,
        ibDot11SIDAPWorkMode               INTEGER,
        ibDot11SIDAPGetIPMethod            INTEGER,
        ibDot11SIDAPTemplateName           OCTET STRING,
        ibDot11SIDModelAlias               OCTET STRING,
        ibDot11SIDAPDescription            OCTET STRING,
        ibDot11SIDRowStatus                RowStatus,
        ibDot11SIDAPName                   OCTET STRING,
        ibDot11SIDStatisInterv             Integer32,
        ibDot11SIDAPBroadcastProbeReply    TruthValue,
        ibDot11SIDAPStaIdleTimerInterv     Integer32,
        ibDot11SIDStaKeepAliveTimerInterv  Integer32,
        ibDot11SIDAPCir                    Integer32,
        ibDot11SIDAPCbs                    Integer32,
        ibDot11SIDAPPriorityLevel          Integer32,
        ibDot11SIDAPElementID              Integer32,
        ibDot11SIDAPDevDetectEnable        TruthValue,
        ibDot11SIDAPStatisIntervMode       INTEGER,
        ibDot11SIDAPWorkModeCM             INTEGER,
        ibDot11SIDEchoInterval             Integer32
    }

ibDot11SIDAPSerialID OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Represents AP serial id."
    ::= { ibDot11APSerialIDEntry 1 }

ibDot11SIDAPWorkMode OBJECT-TYPE
    SYNTAX      INTEGER
        {
            normal(1),
            monitor(2),
            hybrid(3)
        }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents what work mode for AP.
        The following values are supported:
        normal   - AP will supply WLAN service.
        monitor  - AP will be a monitor to find attack.
        hybrid   - AP will supply WLAN service and Device detection is
        enabled."
    DEFVAL      { normal }
    ::= { ibDot11APSerialIDEntry 2 }

ibDot11SIDAPGetIPMethod OBJECT-TYPE
    SYNTAX      INTEGER
    {
        dhcpAlloc(1),
        static(2)
    }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object represents the method how AP gets IP address."
    DEFVAL      { dhcpAlloc }
    ::= { ibDot11APSerialIDEntry 3 }
ibDot11SIDAPTemplateName OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the name of AP template."
    ::= { ibDot11APSerialIDEntry 4 }
ibDot11SIDModelAlias OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the alias of AP model name."
    ::= { ibDot11APSerialIDEntry 5 }

ibDot11SIDAPDescription OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This attribute will give more information about AP.
        Administrator could suggest the location, owner or
        other information for each AP.
        If the value is set to NULL, the description will be removed."
    ::= { ibDot11APSerialIDEntry 6 }

ibDot11SIDRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The status of this table entry."
    ::= { ibDot11APSerialIDEntry 7 }

ibDot11SIDAPName OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the AP name."
    ::= { ibDot11APSerialIDEntry 8 }

ibDot11SIDStatisInterv OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "second"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the interval of radio statistic."
    ::= { ibDot11APSerialIDEntry 9 }

ibDot11SIDAPBroadcastProbeReply OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents enable the AP to reply broadcast probe or not.
        'true' : The AP to reply broadcast probe is enabled.
        'false': The AP to reply broadcast probe is disabled"
    DEFVAL      { true }
    ::= { ibDot11APSerialIDEntry 10 }

ibDot11SIDAPStaIdleTimerInterv OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "second"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the interval for which the link between AP and STA
        ( power-save or awake ) can be idle."
    ::= { ibDot11APSerialIDEntry 11 }

ibDot11SIDStaKeepAliveTimerInterv OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "second"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the interval of the keep alive requests sent between
        AP and STA."
    ::= { ibDot11APSerialIDEntry 12 }

ibDot11SIDAPCir OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "Kbps"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the committed information rate between AP and AC."
    ::= { ibDot11APSerialIDEntry 13 }

ibDot11SIDAPCbs OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "Bytes"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the committed burst size between AP and AC."
    ::= { ibDot11APSerialIDEntry 14 }

ibDot11SIDAPPriorityLevel OBJECT-TYPE
    SYNTAX      Integer32 (0..7)
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object represents the connection priority for the AP.
        If the connection priority is 7, the AC is considered as the
        primary AC for the AP."
    DEFVAL      { 4 }
    ::= { ibDot11APSerialIDEntry 15 }

ibDot11SIDAPElementID OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object represents the AP element ID."
    ::= { ibDot11APSerialIDEntry 16 }

ibDot11SIDAPDevDetectEnable OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents whether device detection is enabled or not.
         'true' : Device detection is enabled.
         'false': Device detection is disabled.
         If device detection is enabled, the AP will provide both device
         detection and WLAN data services.
         By default, the AP only provides WLAN data services.  When an AP works
         in monitor mode, that is, the value of object ibDot11APWorkMode is
         monitor, device detection can not be enabled with object
         ibDot11SIDAPDevDetectEnable."
    DEFVAL      { false }
    ::= { ibDot11APSerialIDEntry 17 }

ibDot11SIDAPStatisIntervMode OBJECT-TYPE
    SYNTAX      INTEGER
    {
        normal(1),
        realtime(2)
    }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object represents the mode of statics interval.
         normal    - statics interval value between 10s to 240s
         realtime  - statics interval value between 0s to 5s"
    DEFVAL      { normal }
    ::= { ibDot11APSerialIDEntry 18 }

ibDot11SIDAPWorkModeCM OBJECT-TYPE
    SYNTAX      INTEGER
        {
            normal(0),
            monitor(1),
            semimonitor(2)
        }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents what work mode for AP.
        The following values are supported:
        normal   - AP will supply WLAN service.
        monitor  - AP will be a monitor to find attack.
        SemiMonitor   - AP will supply WLAN service and Device detection is
        enabled."
    DEFVAL      { normal }
    ::= { ibDot11APSerialIDEntry 19 }

ibDot11SIDEchoInterval OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "second"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents the interval for send echo requests."
    ::= { ibDot11APSerialIDEntry 20 }

-- *****************************************************************************
-- *  ibDot11RadioIntfConfigTable Definition
-- *****************************************************************************
ibDot11RadioIntfConfigTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbDot11RadioIntfConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The table defines the parameters for radio in AP, as per
        radio."
    ::= { ibDot11RadioIntfConfigGroup 1 }

ibDot11RadioIntfConfigEntry OBJECT-TYPE
    SYNTAX      IbDot11RadioIntfConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry contains information of each radio in AP."
    INDEX
        {
            ibDot11RadioIfIdx
        }
    ::= { ibDot11RadioIntfConfigTable 1 }

IbDot11RadioIntfConfigEntry ::= SEQUENCE
    {
        ibDot11RadioIfIdx               Integer32,
        ibDot11RadioCfgBeaconIntvl      Integer32,
        ibDot11RadioCfgDtimIntvl        Integer32,
        ibDot11RadioCfgRtsThreshold     Integer32,
        ibDot11RadioCfgFragThreshold    Integer32,
        ibDot11RadioCfgShtRetryThld     Integer32,
        ibDot11RadioCfglongRtrThld      Integer32,
        ibDot11RadioCfgMaxRxLifetime    Unsigned32,
        ibDot11RadioCfgType             IbDot11RadioType,
        ibDot11RadioCfgChannel          IbDot11ChannelScopeType,
        ibDot11RadioCfgMaxTxPwrLvl      IbDot11TxPwrLevelScopeType,
        ibDot11RadioCfgPreambleLen      IbDot11PreambleType,
        ibDot11RadioCfgWorkMode         IbDot11WorkMode,
        ibDot11RadioCfgOnly11gEnable    TruthValue,
        ibDot11RadioCfgType2            IbDot11RadioType2,
        ibDot11RadioCfgRssithresholdCM  Integer32
    }

ibDot11RadioIfIdx OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Represents the Radio IfIndex of radio in AP."
    ::= { ibDot11RadioIntfConfigEntry 1 }

ibDot11RadioCfgBeaconIntvl OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "TU"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents the interval for Beacon management frame."
    DEFVAL { 100 }
    ::= { ibDot11RadioIntfConfigEntry 2 }

ibDot11RadioCfgDtimIntvl OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents the number of beacon intervals that shall elapse
        between transmission of Beacons frames containing a Traffic
        Indication Map (TIM) element whose DTIM Count field is 0.
        This value is transmitted in the Delivery TIM (DTIM) Period
        field of Beacon frames."
    DEFVAL { 1 }
    ::= { ibDot11RadioIntfConfigEntry 3 }

ibDot11RadioCfgRtsThreshold OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "Byte"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents the current minimum size,in octets,of the MAC
        protocol data unit (MPDU), below which a Request to send/Clear
        to send (RTS/CTS) handshake shall not be performed."
    DEFVAL { 2346 }
    ::= { ibDot11RadioIntfConfigEntry 4 }

ibDot11RadioCfgFragThreshold OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "Byte"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents the current maximum size, in octets, of the MPDU that
        may be delivered to the PHY.  A MSDU shall be broken into
        fragments if its size exceeds the value of this attribute after
        adding MAC headers and trailers."
    DEFVAL { 2346 }
    ::= { ibDot11RadioIntfConfigEntry 5 }

ibDot11RadioCfgShtRetryThld OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents the maximum number of transmission attempts of a
        frame, the length of which is less than or equal to
        ibDot11RadioRTSThreshold, that shall be made before a failure
        condition is indicated."
    DEFVAL { 5 }
    ::= { ibDot11RadioIntfConfigEntry 6 }

ibDot11RadioCfglongRtrThld OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents the maximum number of transmission attempts of a
        frame, the length of which is longer than
        ibDot11RadioRTSThreshold, that shall be made before a
        failure condition is indicated."
    DEFVAL { 5 }
    ::= { ibDot11RadioIntfConfigEntry 7 }

ibDot11RadioCfgMaxRxLifetime OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "millisecond"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents the elapsed time.  After the time specified by
        ibDot11RadioCfgMaxRxLifetime, the further attempts to reassemble
        the MSDU shall be terminated."
    DEFVAL { 2000 }
    ::= { ibDot11RadioIntfConfigEntry 8 }

ibDot11RadioCfgType OBJECT-TYPE
    SYNTAX      IbDot11RadioType
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents the radio will work with which 802.11 radio type."
    ::= { ibDot11RadioIntfConfigEntry 9 }

ibDot11RadioCfgChannel OBJECT-TYPE
    SYNTAX      IbDot11ChannelScopeType
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents the channel allocation of radio."
    DEFVAL { 1 }
    ::= { ibDot11RadioIntfConfigEntry 10 }

ibDot11RadioCfgMaxTxPwrLvl OBJECT-TYPE
    SYNTAX      IbDot11TxPwrLevelScopeType
    UNITS       "dbm"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents the maximal transmit power scope for AP."
    ::= { ibDot11RadioIntfConfigEntry 11 }

ibDot11RadioCfgPreambleLen OBJECT-TYPE
    SYNTAX      IbDot11PreambleType
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents whether use long or short preamble for 802.11 MAC
        frame."
    DEFVAL { short }
    ::= { ibDot11RadioIntfConfigEntry 12 }

ibDot11RadioCfgWorkMode OBJECT-TYPE
    SYNTAX      IbDot11WorkMode
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents work mode of the radio."
    ::= { ibDot11RadioIntfConfigEntry 13 }

ibDot11RadioCfgOnly11gEnable OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents whether enable only 802.11g radio mode or not.
        'true' : Only 802.11g radio mode is enabled.
        'false': Only 802.11g radio mode is disabled."
    DEFVAL { false }
    ::= { ibDot11RadioIntfConfigEntry 14 }

ibDot11RadioCfgType2 OBJECT-TYPE
    SYNTAX      IbDot11RadioType2
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents the radio will work with which 802.11 radio type."
    ::= { ibDot11RadioIntfConfigEntry 15 }

ibDot11RadioCfgRssithresholdCM OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "dBm"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents the minimum value of the RSSI.
        The clients with RSSI below this value will be refused to associate."
    DEFVAL { 1 }
    ::= { ibDot11RadioIntfConfigEntry 16 }

-- *****************************************************************************
-- *  End of ibDot11RadioIntfConfigTable Definition
-- *****************************************************************************

-- *****************************************************************************
-- *  ibDot11RadioIntfBindTable Definition
-- *****************************************************************************
ibDot11RadioIntfBindTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbDot11RadioIntfBindEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The table defines the binding information for each radio in AP.
        "
    ::= { ibDot11RadioIntfConfigGroup 2 }

ibDot11RadioIntfBindEntry OBJECT-TYPE
    SYNTAX      IbDot11RadioIntfBindEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry contains the radio interface binding information
        in AP."
    INDEX
        {
            ibDot11RadioIfIdx,
            ibDot11RadioIntfBindSvcPlcyID
        }
    ::= { ibDot11RadioIntfBindTable 1 }

IbDot11RadioIntfBindEntry ::= SEQUENCE
    {
        ibDot11RadioIntfBindSvcPlcyID    IbDot11ServicePolicyIDType,
        ibDot11RadioIntfBindIfIdx        Unsigned32,
        ibDot11RadioIntfBindRowStatus    RowStatus
    }

ibDot11RadioIntfBindSvcPlcyID OBJECT-TYPE
    SYNTAX      IbDot11ServicePolicyIDType
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Represents the ID of each service policy."
    ::= { ibDot11RadioIntfBindEntry 1 }

ibDot11RadioIntfBindIfIdx OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the Ifindex of interface which is binded to
        the radio."
    ::= { ibDot11RadioIntfBindEntry 2 }

ibDot11RadioIntfBindRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The status of this table entry."
    ::= { ibDot11RadioIntfBindEntry 3 }
-- *****************************************************************************
-- *  End of ibDot11RadioIntfBindTable Definition
-- *****************************************************************************

-- *****************************************************************************
-- *  ibDot11DataRateConfigTable Definition
-- *****************************************************************************
ibDot11DataRateConfigTable OBJECT-TYPE
