-- * End of ibDot11APSysInfoSetTable Definition
-- *****************************************************************************

-- *****************************************************************************
-- *  ibDot11APLimitTable Definition
-- *****************************************************************************
ibDot11APLimitTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbDot11APLimitEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "For each connected APs, administrator could configure a series of
        limits.  This table describes the limit information for AP(s)."
    ::= { ibDot11APConfigGroup 5 }

ibDot11APLimitEntry OBJECT-TYPE
    SYNTAX      IbDot11APLimitEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry contains the limit information for each AP."
    INDEX
        {
            ibDot11APElementIndex
        }
    ::= { ibDot11APLimitTable 1 }

IbDot11APLimitEntry ::= SEQUENCE
    {
        ibDot11APSsidNumLimit     Integer32,
        ibDot11APUserCntLimit     Integer32,
        ibDot11APUserThreshold    Integer32
    }

ibDot11APSsidNumLimit OBJECT-TYPE
    SYNTAX      Integer32(0..65535)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents the upper limit of service template binded
        with this AP.  All radios of AP share this limit.
        Default value 0 : There is no upper limit of service template
        binded with this AP."
    DEFVAL      { 0 }
    ::= { ibDot11APLimitEntry 1 }

ibDot11APUserCntLimit OBJECT-TYPE
    SYNTAX      Integer32(0..65535)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents the upper limit of user which has access to this AP.
        All BSSs binded with AP share this limit.
        Default value 0 : There is no upper limit of user which has access
        to this AP."
    DEFVAL      { 0 }
    ::= { ibDot11APLimitEntry 2 }

ibDot11APUserThreshold OBJECT-TYPE
    SYNTAX      Integer32(0..65535)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents the threshold of user which has access to this
        AP.  All BSSs binded with AP share this limit.  If the user counter
        is overflow, the trap will be sent.
        Default value 0 : There is no threshold of user which has access
        to this AP."
    DEFVAL      { 0 }
    ::= { ibDot11APLimitEntry 3 }
-- *****************************************************************************
-- * End of ibDot11APLimitTable Definition
-- *****************************************************************************

