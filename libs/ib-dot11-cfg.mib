ibDot11IPLearningTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbDot11IPLearningEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The table specifies the IP learning type and status
        of service templates."
    ::= { ibDot11PolicyConfigGroup 8 }
ibDot11IPLearningEntry OBJECT-TYPE
    SYNTAX      IbDot11IPLearningEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry contains information about an IP learning entry."
    INDEX
        {
            ibDot11IPLearningServiceName,
            ibDot11IPLearningType
        }
    ::= { ibDot11IPLearningTable 1 }
IbDot11IPLearningEntry ::=
    SEQUENCE {
        ibDot11IPLearningServiceName      OCTET STRING,
        ibDot11IPLearningType             INTEGER,
        ibDot11IPLearningStatus           TruthValue,
        ibDot11IPLearningVlanBitMapL      OCTET STRING,
        ibDot11IPLearningVlanBitMapH      OCTET STRING
    }
ibDot11IPLearningServiceName  OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE (1..255))
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Represents the service name."
::= { ibDot11IPLearningEntry 1 }
ibDot11IPLearningType OBJECT-TYPE
    SYNTAX      INTEGER {
                invalid(1),
                arp(2),
                dhcp(3),
                dhcp6(4),
                nd(5)
                }
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Represents the IP learning type."
    ::= { ibDot11IPLearningEntry 2 }
ibDot11IPLearningStatus OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents the ip learning status of the interface.
         Setting this attribute to true enables IP learning on the interface."
    ::= { ibDot11IPLearningEntry 3 }
ibDot11IPLearningVlanBitMapL OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(1..256))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Each octet within this value specifies a set of eight
         VLANs, with the first octet specifying VLANs 1 through 8 and
         the second octet specifying VLANs 9 through 16, etc.
         Within each octet, the most significant bit represents
         the highest numbered VLAN, and the least significant bit
         represents the lowest numbered VLAN. Thus, each untagged
         VLAN of the hybrid port is represented by a single bit
         within the value of this object. If that bit has a value
         of '1', that VLAN is untagged in the set of VLANs; the
         VLAN is not untagged if its bit has a value of '0'."
    ::= { ibDot11IPLearningEntry 4 }
ibDot11IPLearningVlanBitMapH OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(1..256))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Each octet within this value specifies a set of eight
         VLANs, with the first octet specifying VLANs 1 through 8 and
         the second octet specifying VLANs 9 through 16, etc.
         Within each octet, the most significant bit represents
         the highest numbered VLAN, and the least significant bit
         represents the lowest numbered VLAN. Thus, each untagged
         VLAN of the hybrid port is represented by a single bit
         within the value of this object. If that bit has a value
         of '1', that VLAN is untagged in the set of VLANs; the
         VLAN is not untagged if its bit has a value of '0'."
    ::= { ibDot11IPLearningEntry 5 }
