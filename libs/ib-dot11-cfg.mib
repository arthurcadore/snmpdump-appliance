    STATUS      current
    DESCRIPTION
        "Represents what security schemes are supported for CAPWAP data
        tunnel.  The cleartxt is the default scheme."
    DEFVAL      { cleartxt }
    ::= { ibDot11GlobeConfigGroup 7 }

ibDot11ACAutoAPSupport OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents enable the auto AP functionality or not.
        'true' : The auto AP functionality is enabled.
        'false': The auto AP functionality is disabled"
    DEFVAL      { false }
    ::= { ibDot11GlobeConfigGroup 8 }

ibDot11AutoAPName OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(0..127))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents the name of auto AP that needs to be made persistent.
        When read the value from the node, NULL value be returned."
    ::= { ibDot11GlobeConfigGroup 9 }

ibDot11PersistentName OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(0..127))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents the new name of auto AP after being made persistent.
        When read the value from the node, NULL value be returned."
    ::= { ibDot11GlobeConfigGroup 10 }

ibDot11IntfTrapThreshold OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "dbm"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents threshold of interference trap.  If signal strength of
        the device exceeds this threshold, corresponding trap will
        be sent."
    ::= { ibDot11GlobeConfigGroup 11 }

ibDot11MonitorInterval OBJECT-TYPE
    SYNTAX      Unsigned32(0 | 5..15)
    UNITS       "minute"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents the monitoring interval.
        Default value 0 : The monitoring functionality is changed to
        real-time mode."
    DEFVAL      { 0 }
    ::= { ibDot11GlobeConfigGroup 12 }

ibDot11SampleInterval OBJECT-TYPE
    SYNTAX      Unsigned32(0 | 10..300)
    UNITS       "second"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents the sampling interval.
        Default value 0 : The sampling functionality is disabled."
    DEFVAL      { 0 }
    ::= { ibDot11GlobeConfigGroup 13 }

ibDot11ChnlSwitChkInterval OBJECT-TYPE
    SYNTAX      Unsigned32(8..180)
    UNITS       "minute"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents the checking interval of channel switch."
    DEFVAL      { 8 }
    ::= { ibDot11GlobeConfigGroup 14 }

ibDot11APUserUplimit OBJECT-TYPE
    SYNTAX      Unsigned32(0..65535)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents the upper limit of user which has access to this AP."
    ::= { ibDot11GlobeConfigGroup 15 }

ibDot11APL2IsolateEnable OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents enable the layer2 isolation or not.
        'true' : The layer2 isolation functionality is enabled.
        'false': The layer2 isolation functionality is disabled."
    DEFVAL      { false }
    ::= { ibDot11GlobeConfigGroup 16 }

ibDot11APBSSIDSupportNum OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the max number of BSSID supported by Current AP."
    ::= { ibDot11GlobeConfigGroup 17 }

ibDot11APLastUpdateStatTime OBJECT-TYPE
    SYNTAX      DateAndTime
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents date and time update statistic information lately."
    ::= { ibDot11GlobeConfigGroup 18 }

ibDot11APDoSProtectEnable OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents enable the DOS protection or not.
        'true' : The DOS protection functionality is enabled.
        'false': The DOS protection functionality is disabled."
    DEFVAL      { false }
    ::= { ibDot11GlobeConfigGroup 19 }

ibDot11MaxAPPerIf OBJECT-TYPE
    SYNTAX      Unsigned32(0..65535)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents max AP number that can be connected to AC interface."
    ::= { ibDot11GlobeConfigGroup 20 }

ibDot11SampleTimeStamp OBJECT-TYPE
    SYNTAX      DateAndTime
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the start time of current sample period."
    ::= { ibDot11GlobeConfigGroup 21 }

ibDot11UplinkTrackId OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents the track id of AP.
        Default value 0 : The uplink track function for AP is disabled."
    DEFVAL      { 0 }
    ::= { ibDot11GlobeConfigGroup 22 }

ibDot11RtCollectSwitch OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object is used to turn on/off real-time statistics collection
         between AC and AP.  If the value is true, ibDot11RtCollectIntvl will
         take effect.  Otherwise, ibDot11RglCollectIntvl will take effect."
    DEFVAL      { false }
    ::= { ibDot11GlobeConfigGroup 23 }

ibDot11RglCollectIntvl OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "second"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object represents interval to regularly collect statistics from
         AC to AP."
    ::= { ibDot11GlobeConfigGroup 24 }

ibDot11RtCollectIntvl OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "second"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object represents interval to collect real-time statistics from
         AC to AP."
    ::= { ibDot11GlobeConfigGroup 25 }

ibDot11AllAPCpuUsageThreshold OBJECT-TYPE
    SYNTAX      Integer32(0..100)
    UNITS       "onepercent"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The CPU usage threshold for all AP."
    ::= { ibDot11GlobeConfigGroup 26 }

ibDot11AllAPMemUsageThreshold OBJECT-TYPE
    SYNTAX      Integer32(0..100)
    UNITS       "onepercent"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The memory usage threshold for all AP."
    ::= { ibDot11GlobeConfigGroup 27 }

ibDot11AdjIntfTrapThreshold OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "dbm"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents threshold of adjacent interference trap.  If signal strength
        of the device exceeds this threshold, corresponding trap will be sent."
    ::= { ibDot11GlobeConfigGroup 28 }

ibDot11AllAPMonitorMode OBJECT-TYPE
    SYNTAX      INTEGER
        {
            normal(1),
            monitor(2),
            hybrid(3)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents what work mode for All AP.
        The following values are supported:
        normal,     - AP will supply WLAN service.
        monitor     - AP will be a monitor to find attack.
        hybrid      - AP will supply WLAN service and Device detection is
        enabled."
    DEFVAL      { normal }
    ::= { ibDot11GlobeConfigGroup 29 }

ibDot11GlobalApFmwUpdState OBJECT-TYPE
    SYNTAX      INTEGER
        {
            enable(1),
            disable(2)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents the status of version auto-update for an AP.
        The following values are supported:
        enable      - AP version will be updated automatically.
        disable     - AP version will not be updated automatically."
    DEFVAL      { enable }
    ::= { ibDot11GlobeConfigGroup 30 }

ibDot11ACNasIDCM OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents the NAS-ID of current AC."
    ::= { ibDot11GlobeConfigGroup 31 }

ibDot11ACRole OBJECT-TYPE
    SYNTAX      INTEGER
        {
            normal(1),
            central(2),
            local(3)
        }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Current network status of AC.
        The following values are supported:
        normal,     - normal ac.
        central     - A central AC is an AC where local ACs are
                      configured, and it establishes CAPWAP tunnels
                      with the local ACs.
        local       - Local ACs provide network access to APs
                      and process client traffic. They are managed
                      by the central AC."
    DEFVAL      { normal }
    ::= { ibDot11GlobeConfigGroup 32 }

ibDot11GlobalLocalACState OBJECT-TYPE
    SYNTAX      INTEGER
        {
            enable(1),
            disable(2)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents the status of local-ac for an AC.
        The following values are supported:
        enable      - LocalAC state will be enabled.
        disable     - LocalAC state will not be enabled."
    DEFVAL      { disable }
    ::= { ibDot11GlobeConfigGroup 33 }

ibDot11CentralACIPAddress OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents the CentralAC IP address of LocalAC."
    ::= { ibDot11GlobeConfigGroup 34 }

ibDot11CentralACIPv6Address OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents the CentralAC IPv6 address of LocalAC."
    ::= { ibDot11GlobeConfigGroup 35 }

ibDot11iMcIP OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents the iMc IP address."
    ::= { ibDot11GlobeConfigGroup 36 }

ibDot11iMcPort OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents the iMc Port."
    ::= { ibDot11GlobeConfigGroup 37 }

ibDot11APProvisionSave OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(0..127))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Sends the provisioned configuration to APs.
        When you read from this node, the system returns NULL."
    ::= { ibDot11GlobeConfigGroup 38 }

ibDot11ApRenameCurrentName OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(0..127))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents the current name of the AP that needs to be changed.
         When you read from the node, the system returns NULL."
    ::= { ibDot11GlobeConfigGroup 39 }

ibDot11ApRenameNewName OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(0..127))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Represents the new name of the AP.
         When you read from the node, the system returns NULL."
    ::= { ibDot11GlobeConfigGroup 40 }

ibDot11ACDescription OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This attribute will give more information about AC.
        Administrator could suggest the location, owner or
        other information for each AC.
        If the value is set to NULL, the description will be removed."
    ::= { ibDot11GlobeConfigGroup 41 }
-- *****************************************************************************
-- * End of ibDot11GlobeConfigGroup Definition
-- *****************************************************************************

-- *****************************************************************************
-- *  ibDot11RadioPolicyTable Definition
-- *****************************************************************************
ibDot11RadioPolicyTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbDot11RadioPolicyEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The table defines the parameters for radio policy, as per
        radio policy of CLI.
        One radio policy could be assigned to the different radios (AP)."
    ::= { ibDot11PolicyConfigGroup 1 }

ibDot11RadioPolicyEntry OBJECT-TYPE
    SYNTAX      IbDot11RadioPolicyEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry contains information of each radio policy."
    INDEX
        {
            IMPLIED ibDot11RadioPolicyName
        }
    ::= { ibDot11RadioPolicyTable 1 }

IbDot11RadioPolicyEntry ::= SEQUENCE
    {
        ibDot11RadioPolicyName        OCTET STRING,
        ibDot11BeaconInterval         Integer32,
        ibDot11DtimInterval           Integer32,
        ibDot11RtsThreshold           Integer32,
        ibDot11FragThreshold          Integer32,
        ibDot11ShortRetryThreshold    Integer32,
        ibDot11LongRetryThreshold     Integer32,
        ibDot11MaxRxLifetime          Unsigned32,
        ibDot11RdoPolicyRowStatus     RowStatus,
        ibDot11RdoClientMaxCount      Integer32,
        ibDot11BeaconIntervalMs       Integer32
    }

ibDot11RadioPolicyName OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(0..127))
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Represents the name of radio policy."
    ::= { ibDot11RadioPolicyEntry 1 }

ibDot11BeaconInterval OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "TU"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the interval for Beacon management frame."
    DEFVAL      { 100 }
    ::= { ibDot11RadioPolicyEntry 2 }

ibDot11DtimInterval OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the number of beacon intervals that shall elapse
        between transmission of Beacons frames containing a Traffic
        Indication Map (TIM) element whose DTIM Count field is 0.
        This value is transmitted in the Delivery TIM (DTIM) Period
        field of Beacon frames."
    DEFVAL      { 1 }
    ::= { ibDot11RadioPolicyEntry 3 }

ibDot11RtsThreshold OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "byte"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the current minimum size, in octets, of the MAC
        protocol data unit (MPDU), below which a Request to send/Clear
        to send (RTS/CTS) handshake shall not be performed."
    DEFVAL      { 2346 }
    ::= { ibDot11RadioPolicyEntry 4 }

ibDot11FragThreshold OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "byte"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the current maximum size, in octets, of the MPDU that
        may be delivered to the PHY.  A MSDU shall be broken into
        fragments if its size exceeds the value of this attribute after
        adding MAC headers and trailers."
    DEFVAL      { 2346 }
    ::= { ibDot11RadioPolicyEntry 5 }

ibDot11ShortRetryThreshold OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the maximum number of transmission attempts of a
        frame, the length of which is less than or equal to
        ibDot11RTSThreshold, which shall be made before a failure
        condition is indicated."
    DEFVAL      { 7 }
    ::= { ibDot11RadioPolicyEntry 6 }

ibDot11LongRetryThreshold OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the maximum number of transmission attempts of a
        frame, the length of which is longer than ibDot11RTSThreshold,
        that shall be made before a failure condition is indicated."
    DEFVAL      { 4 }
    ::= { ibDot11RadioPolicyEntry 7 }

ibDot11MaxRxLifetime OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "millisecond"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the elapsed time.  After the time specified by
        ibDot11MaxRxLifetime, the further attempts to reassemble the
        MSDU shall be terminated."
    DEFVAL      { 2000 }
    ::= { ibDot11RadioPolicyEntry 8 }

ibDot11RdoPolicyRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The status of this table entry."
    ::= { ibDot11RadioPolicyEntry 9 }

ibDot11RdoClientMaxCount OBJECT-TYPE
    SYNTAX      Integer32(0..2147483647)
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represent the maximum number of stations supported by
        this radio policy."
    ::= { ibDot11RadioPolicyEntry 10 }

ibDot11BeaconIntervalMs OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "ms"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the interval for Beacon management frame."
    ::= { ibDot11RadioPolicyEntry 11 }
-- *****************************************************************************
-- * End of ibDot11RadioPolicyTable Definition
-- *****************************************************************************

-- *****************************************************************************
-- *  ibDot11ServicePolicyTable Definition
-- *****************************************************************************
ibDot11ServicePolicyTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbDot11ServicePolicyEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The table defines the parameters for ESS, as per service policy
        of CLI.
        One service policy could be assigned to the different radios
        (AP)."
    ::= { ibDot11PolicyConfigGroup 2 }

ibDot11ServicePolicyEntry OBJECT-TYPE
    SYNTAX      IbDot11ServicePolicyEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry contains information of each service policy."
    INDEX
        {
            ibDot11ServicePolicyID
        }
    ::= { ibDot11ServicePolicyTable 1 }

IbDot11ServicePolicyEntry ::= SEQUENCE
    {
        ibDot11ServicePolicyID       IbDot11ServicePolicyIDType,
        ibDot11SSIDName              IbDot11SSIDStringType,
        ibDot11SSIDHidden            TruthValue,
        ibDot11AuthenMode            IbDot11AuthenType,
        ibDot11SSIDEncryptionMode    IbDot11SSIDEncryptModeType,
        ibDot11WlanInfBindingType    OCTET STRING,
        ibDot11WlanInfBindingID      Integer32,
        ibDot11SrvPolicyRowStatus    RowStatus,
        ibDot11ClientMaxCount        Integer32,
        ibDot11SPInCirMode           IbDot11CirMode,
        ibDot11SPInCirValue          Integer32,
        ibDot11SPOutCirMode          IbDot11CirMode,
        ibDot11SPOutCirValue         Integer32,
        ibDot11WlanInfPVID           Integer32,
        ibDot11SPInCirStaticValue    Integer32,
        ibDot11SPOutCirStaticValue   Integer32,
        ibDot11SPIsolate             TruthValue,
        ibDot11WlanexAuthServerIP    IpAddress,
        ibDot11SPBeaconMeasEnable    TruthValue,
        ibDot11SPBeaconMeasType      INTEGER,
        ibDot11SPBeaconMeasInterval  Integer32,
        ibDot11AuthenModeCM          INTEGER,
        ibDot11SecIEStatusCM         INTEGER,
        ibDot11SecurityCiphersCM     INTEGER,
        ibDot11SrvPolicyStatusCM     INTEGER,
        ibDot11SSIDHiddenCM          IbDot11TruthValueCM,
        ibDot11SPIsolateCM           IbDot11TruthValueCM,
        ibDot11FwdVlanBitMapLow      OCTET STRING,
        ibDot11FwdVlanBitMapHigh     OCTET STRING,
        ibDot11ServicePolicyName     OCTET STRING,
        ibDot11SecurityModeCM        INTEGER,
        ibDot11SPInCbsValue          Integer32,
        ibDot11SPOutCbsValue         Integer32,
        ibDot11WlanInfCustomerPVID   Integer32
    }

ibDot11ServicePolicyID OBJECT-TYPE
    SYNTAX      IbDot11ServicePolicyIDType
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Represents the ID of each service policy."
    ::= { ibDot11ServicePolicyEntry 1 }

ibDot11SSIDName OBJECT-TYPE
    SYNTAX      IbDot11SSIDStringType
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the ESS.  As per CLI, the value of
        service policy name will be SSID."
    ::= { ibDot11ServicePolicyEntry 2 }

ibDot11SSIDHidden OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This attribute controls whether the SSID will be advertised in
        the beacon frame.
        If it is true, then SSID will not be advertised in the beacon
        frame."
    DEFVAL      { false }
    ::= { ibDot11ServicePolicyEntry 3 }

ibDot11AuthenMode OBJECT-TYPE
    SYNTAX      IbDot11AuthenType
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the Authentication mode defined by 802.11."
    ::= { ibDot11ServicePolicyEntry 4 }

ibDot11SSIDEncryptionMode OBJECT-TYPE
    SYNTAX      IbDot11SSIDEncryptModeType
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the encryption mode for this ESS."
    ::= { ibDot11ServicePolicyEntry 5 }

ibDot11WlanInfBindingType OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the service policy will bind to what kind of
        data forwarding interface."
    DEFVAL      { "WLAN-ESS" }
    ::= { ibDot11ServicePolicyEntry 6 }

ibDot11WlanInfBindingID OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the interface ID of the interface that ESS will
        bind to.
        If the value is set to -1, the binding will be removed."
    ::= { ibDot11ServicePolicyEntry 7 }

ibDot11SrvPolicyRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The status of this table entry."
    ::= { ibDot11ServicePolicyEntry 8 }

ibDot11ClientMaxCount OBJECT-TYPE
    SYNTAX      Integer32(0..2147483647)
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represent the maximum number of stations supported by
        this service policy."
    ::= { ibDot11ServicePolicyEntry 9 }

ibDot11SPInCirMode OBJECT-TYPE
    SYNTAX      IbDot11CirMode
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "
        CIR(committed information rate) mode of inbound direction.
        This object is useful only when the corresponding CIR is nonzero.
        If value of this object is changed, corresponding CIR will be reset."
    DEFVAL      { static }
    ::= { ibDot11ServicePolicyEntry 10 }

ibDot11SPInCirValue OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "Kbps"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the CIR of inbound direction of this service policy.
        If the value is 0, it means that no flow control is applied to
        the service policy, and the corresponding CIR mode is meaningless."
    DEFVAL      { 0 }
    ::= { ibDot11ServicePolicyEntry 11 }

ibDot11SPOutCirMode OBJECT-TYPE
    SYNTAX      IbDot11CirMode
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "
        CIR(committed information rate) mode of outbound direction.
        This object is useful only when the corresponding CIR is nonzero.
        If value of this object is changed, corresponding CIR will be reset."
    DEFVAL      { static }
    ::= { ibDot11ServicePolicyEntry 12 }

ibDot11SPOutCirValue OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "Kbps"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the CIR of outbound direction of this service policy.
        If the value is 0, it means that no flow control is applied to
        the service policy, and the corresponding CIR mode is meaningless."
    DEFVAL      { 0 }
    ::= { ibDot11ServicePolicyEntry 13 }

ibDot11WlanInfPVID OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the VLAN ID of SSID."
    DEFVAL      { 1 }
    ::= { ibDot11ServicePolicyEntry 14 }

ibDot11SPInCirStaticValue  OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "Kbps"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the CIR of inbound direction for static CIR mode."
    DEFVAL      { 0 }
        ::= { ibDot11ServicePolicyEntry 15 }

ibDot11SPOutCirStaticValue  OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "Kbps"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the CIR of outbound direction for static CIR mode."
    DEFVAL      { 0 }
        ::= { ibDot11ServicePolicyEntry 16 }

ibDot11SPIsolate OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "If it is true, then stations connected with the service policy are
         isolated."
    DEFVAL      { false }
    ::= { ibDot11ServicePolicyEntry 17 }

ibDot11WlanexAuthServerIP OBJECT-TYPE
    SYNTAX     IpAddress
    MAX-ACCESS read-create
    STATUS     current
    DESCRIPTION
        "Represents the IP address of the Wlanex Authentication server"
    ::= {  ibDot11ServicePolicyEntry 18 }

ibDot11SPBeaconMeasEnable OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "If it is true, then beacon-measurement is enabled."
    DEFVAL      { false }
    ::= { ibDot11ServicePolicyEntry 19  }

ibDot11SPBeaconMeasType OBJECT-TYPE
      SYNTAX      INTEGER
        {
            passive(1),
            active(2),
            beaconTable(3)
        }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents what the work mode for BeaconMeasurement type.
        The following values are supported:
        passive,      - BeaconMeasurement will use the passive type.
        active        - BeaconMeasurement will use the active type.
        beaconTable   - BeaconMeasurement will use the beacon table type."
    DEFVAL      { beaconTable }
    ::= { ibDot11ServicePolicyEntry 20 }

ibDot11SPBeaconMeasInterval OBJECT-TYPE
    SYNTAX      Integer32(10..200)
    UNITS       "second"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the beacon-measurement interval."
    DEFVAL      { 60 }
    ::= { ibDot11ServicePolicyEntry 21 }

ibDot11AuthenModeCM OBJECT-TYPE
    SYNTAX      INTEGER
        {
            opensystem(0),
            sharedkey(1)
        }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the Authentication mode defined by 802.11."
    ::= { ibDot11ServicePolicyEntry 22 }

ibDot11SecIEStatusCM OBJECT-TYPE
    SYNTAX      INTEGER
        {
            none(0),
            wpa(1),
            wpa2(2),
            wlanex(3)
        }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "To enable the security Information element in the beacon and
        probe response frames sent by AP."
    ::= { ibDot11ServicePolicyEntry 23 }

ibDot11SecurityCiphersCM OBJECT-TYPE
    SYNTAX      INTEGER
        {
            none(0),
            wep40(1),
            wep104(2),
            tkip(3),
            aesccmp(4),
            wpisms4(5)
        }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The WLAN system support the following cipher type for data frame
        encryption.
        none
        wep40
        wep104
        tkip
        aesccmp
        wpi-SMS4
        When the value of node equals to zero, it means no configuration or
        undo operation."
    ::= { ibDot11ServicePolicyEntry 24 }

ibDot11SrvPolicyStatusCM OBJECT-TYPE
    SYNTAX      INTEGER
        {
            off(0),
            on(1)
        }
    MAX-ACCESS read-create
    STATUS     current
    DESCRIPTION
        "The status of the Service Policy."
    ::= {  ibDot11ServicePolicyEntry 25 }

ibDot11SSIDHiddenCM OBJECT-TYPE
    SYNTAX      IbDot11TruthValueCM
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This attribute controls whether the SSID will be advertised in
        beacon frames.
        If it is dot11true, the SSID will not be advertised in beacon
        frames."
    DEFVAL      { dot11false }
    ::= { ibDot11ServicePolicyEntry 26 }

ibDot11SPIsolateCM OBJECT-TYPE
    SYNTAX      IbDot11TruthValueCM
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "If it is dot11true, the clients connected with the service policy are
        isolated to each other."
    DEFVAL      { dot11false }
    ::= { ibDot11ServicePolicyEntry 27 }

ibDot11FwdVlanBitMapLow OBJECT-TYPE
    SYNTAX OCTET STRING (SIZE(0..256))
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "Each octet within this value specifies a set of eight
         VLANs, with the first octet specifying VLANs 1 through 8,
         the second octet specifying VLANs 9 through 16, etc.
         Within each octet, the most significant bit represents
         the highest numbered VLAN, and the least significant bit
         represents the lowest numbered VLAN. Thus, each untagged
         VLAN of the hybrid port is represented by a single bit
         within the value of this object. If that bit has a value
         of '1' then that VLAN is untagged in the set of VLANs; the
         VLAN is not untagged if its bit has a value of '0'."
    ::= { ibDot11ServicePolicyEntry 28 }

ibDot11FwdVlanBitMapHigh OBJECT-TYPE
    SYNTAX OCTET STRING (SIZE(0..256))
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "Each octet within this value specifies a set of eight
         VLANs, with the first octet specifying VLANs 1 through 8,
         the second octet specifying VLANs 9 through 16, etc.
         Within each octet, the most significant bit represents
         the highest numbered VLAN, and the least significant bit
         represents the lowest numbered VLAN. Thus, each untagged
         VLAN of the hybrid port is represented by a single bit
         within the value of this object. If that bit has a value
         of '1' then that VLAN is untagged in the set of VLANs; the
         VLAN is not untagged if its bit has a value of '0'."
    ::= { ibDot11ServicePolicyEntry 29 }

ibDot11ServicePolicyName OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the name of each service policy."
    ::= { ibDot11ServicePolicyEntry 30 }

ibDot11SecurityModeCM OBJECT-TYPE
    SYNTAX INTEGER
        {
            none(0),
            psk(1),
            radius(2),
            wlanex(3)
        }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "Determines the security modes of the WLAN port.

        none  - The default value. No security mode is set.

        psk  - Access to the port is denied until the port client is
        authorized (by 802.1X key negotiation).

        radius  - Access to the port is denied until the port
        client is authorized (by 802.1X).

        wlanex  - Access to the port is denied until the port client is
        authenticated by wlan cert."
    ::= { ibDot11ServicePolicyEntry 31 }

 ibDot11SPInCbsValue  OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "byte"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the CBS of inbound direction of this service policy.
        If the value is 0, it means that the CBS is automatically calculated
        from the value of CIR."
    DEFVAL      { 0 }
    ::= { ibDot11ServicePolicyEntry 32 }

ibDot11SPOutCbsValue  OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "byte"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the CBS of outbound direction of this service policy.
        If the value is 0, it means that the CBS is automatically calculated
        from the value of CIR."
    DEFVAL      { 0 }
    ::= { ibDot11ServicePolicyEntry 33 }

ibDot11WlanInfCustomerPVID OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the customer PVID."
    ::= { ibDot11ServicePolicyEntry 34 }
-- *****************************************************************************
-- * End of ibDot11ServicePolicyTable Definition
-- *****************************************************************************

-- *****************************************************************************
-- *  ibDot11ServicePolicyExtTable Definition
-- *****************************************************************************
ibDot11ServicePolicyExtTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbDot11ServicePolicyExtEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The table defines the parameters for ESS, as per service policy
        of CLI.
        When the value of ibDot11SSIDEncryptionMode equals to cipher(2),
        more configuration for a ESS will be put into the table."
    ::= { ibDot11PolicyConfigGroup 3 }

ibDot11ServicePolicyExtEntry OBJECT-TYPE
    SYNTAX      IbDot11ServicePolicyExtEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry contains information of each service policy under cipher(2)
        mode."
    INDEX
        {
            ibDot11ServicePolicyExtID
        }
    ::= { ibDot11ServicePolicyExtTable 1 }

IbDot11ServicePolicyExtEntry ::= SEQUENCE
    {
        ibDot11ServicePolicyExtID       IbDot11ServicePolicyIDType,
        ibDot11SecIEStatus              IbDot11SecIEStatusType,
        ibDot11SecurityCiphers          Integer32,
        ibDot11CipherKeyIndex           Integer32,
        ibDot11CipherKey                OCTET STRING,
        ibDot11SrvPolicyExtRowStatus    RowStatus,
        ibDot11CipherKeyType            INTEGER,
        ibDot11AkmMode                  INTEGER,
        ibDot11PskKey                   OCTET STRING
    }

ibDot11ServicePolicyExtID OBJECT-TYPE
    SYNTAX      IbDot11ServicePolicyIDType
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Represents the ID of each service policy."
    ::= { ibDot11ServicePolicyExtEntry 1 }

ibDot11SecIEStatus OBJECT-TYPE
    SYNTAX      IbDot11SecIEStatusType
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "To enable the security Information element in the beacon and
        probe response frames sent by AP."
    ::= { ibDot11ServicePolicyExtEntry 2 }

ibDot11SecurityCiphers OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The WLAN system support the following cipher type for data frame
        encryption.
        wep40(2)
        tkip(4)
        aesccmp(16)
        wep104(32)
        wpisms4(64)
        wep128(128)
        For each service policy, it could support multiple cipher types at
        the same time.  So the value of object could combine multiple value
        of cipher type above.
        When the value of node equals to zero, it means no configuration or
        undo operation."
    ::= { ibDot11ServicePolicyExtEntry 3 }

ibDot11CipherKeyIndex OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the key index of cipher key for packet.
        When ibDot11SecurityCiphers value is wep40 or wep104,
        the configuration will take effective."
    DEFVAL      { 1 }
    ::= { ibDot11ServicePolicyExtEntry 4 }

ibDot11CipherKey OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the cipher key for packet.
        When ibDot11SecurityCiphers value is wep40 or wep104,
        the configuration will take effective."
    ::= { ibDot11ServicePolicyExtEntry 5 }

ibDot11SrvPolicyExtRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The status of this table entry."
    ::= { ibDot11ServicePolicyExtEntry 6 }

ibDot11CipherKeyType OBJECT-TYPE
    SYNTAX      INTEGER
    {
        char(1),
        hex(2)
    }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the cipher key type of ibDot11CipherKey."
    DEFVAL    {1}
    ::= { ibDot11ServicePolicyExtEntry 7 }

ibDot11AkmMode OBJECT-TYPE
    SYNTAX      INTEGER
    {
        none(0),
        dot1x(1),
        psk(2)
    }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The authentication and key management mode."
    DEFVAL    {0}
    ::= { ibDot11ServicePolicyExtEntry 8 }

ibDot11PskKey OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the preshared key.  This configuration takes effect
        if the ibDot11AkmMode value is psk."
    ::= { ibDot11ServicePolicyExtEntry 9 }

-- *****************************************************************************
-- * End of ibDot11ServicePolicyExtTable Definition
-- *****************************************************************************

-- *****************************************************************************
-- *  ibDot11RadioPolicyExtTable Definition
-- *****************************************************************************
ibDot11RadioPolicyExtTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbDot11RadioPolicyExtEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The table defines the radio policy of each radio.
        It can't be modified if the radio policy is a default radio policy."
    ::= { ibDot11PolicyConfigGroup 4 }

ibDot11RadioPolicyExtEntry OBJECT-TYPE
    SYNTAX      IbDot11RadioPolicyExtEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry contains information of each radio policy."
    INDEX
        {
            ibDot11RPAPSerialID,
            ibDot11RPRadioID
        }
    ::= { ibDot11RadioPolicyExtTable 1 }

IbDot11RadioPolicyExtEntry ::= SEQUENCE
    {
        ibDot11RPAPSerialID             OCTET STRING,
        ibDot11RPRadioID                IbDot11RadioScopeType,
        ibDot11RPBeaconInterval         Integer32,
        ibDot11RPDtimInterval           Integer32,
        ibDot11RPRtsThreshold           Integer32,
        ibDot11RPFragThreshold          Integer32,
        ibDot11RPShortRetryThreshold    Integer32,
        ibDot11RPLongRetryThreshold     Integer32,
        ibDot11RPClientMaxCount         Integer32,
        ibDot11RPBeaconIntervalCM       Integer32
    }

ibDot11RPAPSerialID OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(0..127))
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This object represents the Serial ID of AP."
    ::= { ibDot11RadioPolicyExtEntry 1 }

ibDot11RPRadioID OBJECT-TYPE
    SYNTAX      IbDot11RadioScopeType
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This object represents each radio."
    ::= { ibDot11RadioPolicyExtEntry 2 }

ibDot11RPBeaconInterval OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "milliseconds"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object represents the interval for Beacon management frame."
    DEFVAL      { 100 }
    ::= { ibDot11RadioPolicyExtEntry 3 }

ibDot11RPDtimInterval OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object represents the number of beacon intervals that shall elapse
        between transmissions of Beacons frames containing a Traffic
        Indication Map (TIM) element whose DTIM count field is 0.
        This value is transmitted in the Delivery TIM (DTIM) Period
        field of Beacon frames."
    DEFVAL      { 1 }
    ::= { ibDot11RadioPolicyExtEntry 4 }

ibDot11RPRtsThreshold OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "byte"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object represents the current minimum size, in octets, of the MAC
        protocol data unit (MPDU), below which a Request to send/Clear
        to send (RTS/CTS) handshake shall not be performed."
    DEFVAL      { 2346 }
    ::= { ibDot11RadioPolicyExtEntry 5 }

ibDot11RPFragThreshold OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "byte"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object represents the current maximum size, in octets, of the MPDU
         that may be delivered to the PHY.  A MSDU shall be broken into
        fragments if its size exceeds the value of this attribute after
        adding MAC headers and trailers."
    DEFVAL      { 2346 }
    ::= { ibDot11RadioPolicyExtEntry 6 }

ibDot11RPShortRetryThreshold OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object represents the maximum number of transmission attempts of a
        frame, the length of which is less than or equal to
        ibDot11RTSThreshold, that shall be made before a failure
        condition is indicated."
    DEFVAL      { 7 }
    ::= { ibDot11RadioPolicyExtEntry 7 }

ibDot11RPLongRetryThreshold OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object represents the maximum number of transmission attempts of a
        frame, the length of which is longer than ibDot11RTSThreshold,
        and that shall be made before a failure condition is indicated."
    DEFVAL      { 4 }
    ::= { ibDot11RadioPolicyExtEntry 8 }

ibDot11RPClientMaxCount OBJECT-TYPE
    SYNTAX      Integer32(0..2147483647)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object represents the maximum number of stations supported by
        this radio policy."
    ::= { ibDot11RadioPolicyExtEntry 9 }

ibDot11RPBeaconIntervalCM OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "timeunit"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object represents beacon interval."
    DEFVAL      { 100 }
    ::= { ibDot11RadioPolicyExtEntry 10 }

-- *****************************************************************************
-- * End of ibDot11RadioPolicyTable Definition
-- *****************************************************************************
-- *****************************************************************************
-- *  ibDot11ServicePolicyPortSecurityTable Definition
-- *****************************************************************************
ibDot11SrvPortSecurityTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbDot11SrvPortSecurityEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table defines the WLAN security status for ESS, BSS and
        WLAN-Ethernet interface which binding the crypto service-template.
        Otherwise, when binding the clear service-template,
        ibPortSecurityTables should be used to configure port security."
    ::= { ibDot11PolicyConfigGroup 5 }

ibDot11SrvPortSecurityEntry OBJECT-TYPE
    SYNTAX      IbDot11SrvPortSecurityEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "There is a row in this table for service policy."
    INDEX
        {
            ibDot11SecurityServicePolicyID
        }
    ::= { ibDot11SrvPortSecurityTable 1 }

IbDot11SrvPortSecurityEntry ::= SEQUENCE
    {
        ibDot11SecurityServicePolicyID           IbDot11ServicePolicyIDType,
        ibDot11SrvPortSecurityMode               INTEGER,
        ibDot11SrvSecurityKeyType                INTEGER,
        ibDot11SrvSecurityPskKeyMode             INTEGER,
        ibDot11SrvSecurityPskKeyString           DisplayString,
        ibDot11SrvPortSecurityModeCM             INTEGER
    }

ibDot11SecurityServicePolicyID OBJECT-TYPE
    SYNTAX      IbDot11ServicePolicyIDType
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Represents the ID of each service policy."
    ::= { ibDot11SrvPortSecurityEntry 1 }

ibDot11SrvPortSecurityMode OBJECT-TYPE
    SYNTAX      INTEGER
        {
            noRestrictions(1),
            userLoginSecureExt(2),
            psk(3),
            macAddressAndPsk(4),
            userLoginSecureExtOrPsk(5),
            ext(6)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Determines the security modes of the WLAN port.

        noRestrictions  The default value, no any security mode is set.

        userLoginSecureExt  Access to the port is denied until the port
        client is authorized (by 802.1X).

        psk  Access to the port is denied until the port client is
        authorized (by 802.1X key negotiation).

        macAddressAndPsk  The MAC address is first authenticated and only if
        this succeeds does the psk then attempt user authentication.

        userLoginSecureExtOrPsk  This selects both the userLoginSecureExt and
        psk modes together such that either is allowed to authorized access.
        Which mode works for client depends on the client's choice.

        ext  Access to the port is denied until the port client is
        authenticated (by ext)."
    ::= { ibDot11SrvPortSecurityEntry 2 }

ibDot11SrvSecurityKeyType OBJECT-TYPE
    SYNTAX      INTEGER
        {
            userLoginTxKeyTypeNone(1),
            userLoginTxKeyTypeDot11Key(2),
            userLoginTxKeyTypeRsaRC4Key(3)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This indicates the key type in the 802.1X EAPOL_Key packet.

        userLoginTxKeyTypeNone  - This means the key type is not set yet.

        userLoginTxKeyTypeDot11Key  - This selects the 802.11 key type.

        userLoginTxKeyTypeRsaRC4Key  - This selects the RC4 key for RSA RC4
        session encryption."
    ::= { ibDot11SrvPortSecurityEntry 3 }

ibDot11SrvSecurityPskKeyMode OBJECT-TYPE
    SYNTAX      INTEGER
        {
            pskKeyModeNone(1),
            pskKeyModePassPhrase(2),
            pskKeyModeRawKey(3)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This is the password input mode.  When input as pskKeyModePassPhrase,
        ibDot11SrvSecurityPskKeyString must be 8 to 63 characters;
        when input as pskKeyModeRawKey, ibDot11SrvSecurityPskKeyString
        must be 64 characters and each character must be '0'-'9', 'a'-'f' and
        'A'-'F'."
    ::= { ibDot11SrvPortSecurityEntry 4 }

ibDot11SrvSecurityPskKeyString OBJECT-TYPE
    SYNTAX      DisplayString(SIZE(0..64))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This is the password string used for psk authentication."
    ::= { ibDot11SrvPortSecurityEntry 5 }

ibDot11SrvPortSecurityModeCM OBJECT-TYPE
    SYNTAX INTEGER
        {
            none(0),
            psk(1),
            radius(2),
            wlanex(3)
        }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "Determines the security modes of the WLAN port.

        none  - The default value, no any security mode is set.

        psk  - Access to the port is denied until the port client is
        authorized (by 802.1X key negotiation).

        radius  - Access to the port is denied until the port
        client is authorized (by 802.1X).

        wlanex  - Access to the port is denied until the port client is
        authenticated by wlan cert."
    ::= { ibDot11SrvPortSecurityEntry 6 }

ibDot11SrvL2AuthenTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbDot11SrvL2AuthenEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table defines the Layer 2 authentication status of service
         templates."
    ::= { ibDot11PolicyConfigGroup 7 }

ibDot11SrvL2AuthenEntry OBJECT-TYPE
    SYNTAX      IbDot11SrvL2AuthenEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "There is a row for a service template."
    INDEX
        {
            ibDot11SrvL2AuthenID
        }
    ::= { ibDot11SrvL2AuthenTable 1 }

IbDot11SrvL2AuthenEntry ::= SEQUENCE
    {
        ibDot11SrvL2AuthenID               IbDot11ServicePolicyIDType,
        ibDot11L2AuthenMode                INTEGER,
        ibDot11L2IntrusProtectEnable       TruthValue,
        ibDot11L2IntrusProtectOpt          INTEGER,
        ibDot11TempServiceStopTimer        Integer32,
        ibDot11TempBlockMACTimer           Integer32,
        ibDot11L2IgnoreAuthorization       TruthValue,
        ibDot11L2FailVLAN                  Integer32,
        ibDot11L2CriticalVLAN              Integer32,
        ibDot11L2AuthorFailOffline         TruthValue,
        ibDot11L2AccountFailOffline        TruthValue,
        ibDot11Dot1xHSEnable               TruthValue,
        ibDot11Dot1xSecureHSEnable         TruthValue,
        ibDot11Dot1xReauthenEnable         TruthValue,
        ibDot11Dot1xMandatoryDomain        OCTET STRING,
        ibDot11Dot1xMaxUserCount           Integer32,
        ibDot11MACAuthenDomain             OCTET STRING,
        ibDot11MACAuthenMaxUserCount       Integer32
    }

ibDot11SrvL2AuthenID OBJECT-TYPE
    SYNTAX      IbDot11ServicePolicyIDType
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Represents the ID of a service template."
    ::= { ibDot11SrvL2AuthenEntry 1 }

ibDot11L2AuthenMode OBJECT-TYPE
    SYNTAX      INTEGER
        {
            bypass(1),
            macAuthentication(2),
            macThenDot1xAuthentication(3),
            dot1xAuthentication(4),
            dot1xThenMacAuthentication(5),
            ouiThenDot1x(6),
            macAndDot1x(7)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Authentication mode of the service template.

        bypass  No authentication is required.

        macAuthentication  MAC authentication.

        macThenDot1xAuthentication  MAC authentication is executed first.
        If MAC authentication fails, 802.1X authentication is executed.

        dot1xAuthentication  Access to the port is denied until the client
        is authorized by 802.1X.

        dot1xThenMacAuthentication  802.1X authentication is executed first.
        If 802.1X authentication fails , MAC authentication is executed.

        ouiThenDot1x  OUI authentication is first executed.
        If OUI authentication fails, 802.1X authentication is executed."
    ::= { ibDot11SrvL2AuthenEntry 2 }

