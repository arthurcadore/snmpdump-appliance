ibWIPSVsdConfigGroup OBJECT IDENTIFIER ::= { ibWIPSConfigGroup 2 }

ibWIPSVsdTable OBJECT-TYPE
    SYNTAX SEQUENCE OF IbWIPSVsdEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "The table provides the list of virtual security domain."
    ::= { ibWIPSVsdConfigGroup 1 }

ibWIPSVsdEntry OBJECT-TYPE
    SYNTAX IbWIPSVsdEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "Each entry contains information of each VSD."
    INDEX { ibWIPSVsdNameCfg }
    ::= { ibWIPSVsdTable 1 }

IbWIPSVsdEntry ::=
    SEQUENCE {
        ibWIPSVsdNameCfg
            OCTET STRING,
        ibWIPSVsdRowStatus
            RowStatus,
        ibWIPSVsdAtkDctPolicyNameCfg
            OCTET STRING,
        ibWIPSVsdCtmPolicyNameCfg
            OCTET STRING,
        ibWIPSVsdSigPolicyNameCfg
            OCTET STRING,
        ibWIPSVsdMalPktPolicyNameCfg
