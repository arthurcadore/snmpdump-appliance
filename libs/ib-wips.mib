    ::= { ibWIPSGlobalConfigGroup 2 }

ibWIPSBlocklistAction OBJECT-TYPE
    SYNTAX INTEGER
        {
        block(1),
        unblock(2)
        }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "Represents whether the block list feature is enabled or not.
        'block'  : Enable the block list feature to filter unwanted devices.
        'unblock' : Disable the block list feature."
    ::= { ibWIPSGlobalConfigGroup 3 }

ibWIPSAPInactiveTime OBJECT-TYPE
