ibDot11RrmNbrNF OBJECT-TYPE
    SYNTAX Integer32
    UNITS "dBm"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Noise floor of the AP in dBm."
    ::= { ibDot11RRMNbrInfoEntry 8 }

ibDot11RRMHistoryTable OBJECT-TYPE
    SYNTAX SEQUENCE OF IbDot11RRMHistoryEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "This table shows the details of the latest three channel changes
        and power changes applied on all APs, including time of change,
        reason of the change and the channel, power, interference parameters."
    ::= { ibDot11RRMDetectGroup 3 }

ibDot11RRMHistoryEntry OBJECT-TYPE
    SYNTAX IbDot11RRMHistoryEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "Each entry shows the details of channel and power changes."
    INDEX { ibDot11RRMRadioIndex,
            ibDot11RRMHistoryId,
            ibDot11RRMHistoryRecIndctr }
    ::= { ibDot11RRMHistoryTable 1 }

IbDot11RRMHistoryEntry ::=
    SEQUENCE {
        ibDot11RRMHistoryId
            Integer32,
        ibDot11RRMHistoryRecIndctr
            INTEGER,
        ibDot11RRMHistoryChl
            IbDot11ChannelScopeType,
        ibDot11RRMHistoryPwr
            Integer32,
        ibDot11RRMHistoryLoad
            Integer32,
        ibDot11RRMHistoryUtlz
            Integer32,
        ibDot11RRMHistoryIntrf
            Integer32,
        ibDot11RRMHistoryNoise
            Integer32,
        ibDot11RRMHistoryPER
            Integer32,
        ibDot11RRMHistoryRetryRate
            Integer32,
        ibDot11RRMHistoryChgReason
            BITS,
        ibDot11RRMHistoryChgDateTime
            DateAndTime
     }

ibDot11RRMHistoryId OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "History number of the change."
    ::= { ibDot11RRMHistoryEntry 1 }

ibDot11RRMHistoryRecIndctr OBJECT-TYPE
    SYNTAX INTEGER
        {
        before(1),
        after(2)
        }
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "History record type of the change."
    ::= { ibDot11RRMHistoryEntry 2 }

ibDot11RRMHistoryChl OBJECT-TYPE
    SYNTAX IbDot11ChannelScopeType
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Channel on which the radio operates before/after the change
        of channel or power."
    ::= { ibDot11RRMHistoryEntry 3 }

ibDot11RRMHistoryPwr OBJECT-TYPE
    SYNTAX Integer32
    UNITS "dBm"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Power of the radio before/after the change of channel or
        power."
    ::= { ibDot11RRMHistoryEntry 4 }

ibDot11RRMHistoryLoad OBJECT-TYPE
    SYNTAX Integer32 (0..100)
    UNITS "percent"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Load observed on the radio in percentage before/after the
        change of channel or power."
    ::= { ibDot11RRMHistoryEntry 5 }

ibDot11RRMHistoryUtlz OBJECT-TYPE
    SYNTAX Integer32 (0..100)
    UNITS "percent"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Utilization of the radio in percentage before/after the
        change of channel or power."
    ::= { ibDot11RRMHistoryEntry 6 }

ibDot11RRMHistoryIntrf OBJECT-TYPE
    SYNTAX Integer32 (0..100)
    UNITS "percent"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Interference observed on the radio in percentage
        before/after the change of channel or power."
    ::= { ibDot11RRMHistoryEntry 7 }

ibDot11RRMHistoryNoise OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Noise observed on the radio before/after the change
        of channel or power."
    ::= { ibDot11RRMHistoryEntry 8 }

ibDot11RRMHistoryPER OBJECT-TYPE
    SYNTAX Integer32 (0..100)
    UNITS "percent"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Packet error rate observed on the radio in percentage
        before/after the change of channel or power."
    ::= { ibDot11RRMHistoryEntry 9 }

ibDot11RRMHistoryRetryRate OBJECT-TYPE
    SYNTAX Integer32 (0..100)
    UNITS "percent"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Percentage of retransmission happened on the radio
        before/after the change of channel or power."
    ::= { ibDot11RRMHistoryEntry 10 }

ibDot11RRMHistoryChgReason OBJECT-TYPE
    SYNTAX BITS
        {
        others(0),
        coverage(1),
        radar(2),
        retransmission(3),
        packetsDiscarded(4),
        interference(5)
        }
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Reason for the change of channel or power.
        The various bit positions are:
        |0        |Others                    |
        |1        |Coverage                  |
        |2        |Radar                     |
        |3        |Retransmission            |
        |4        |Packets discarded         |
        |5        |Interference              |
        0 is returned when the history record type is after."
    ::= { ibDot11RRMHistoryEntry 11 }

ibDot11RRMHistoryChgDateTime OBJECT-TYPE
    SYNTAX DateAndTime
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The time when the channel or power change occurred."
    ::= { ibDot11RRMHistoryEntry 12 }

ibDot11RRMRadioNbrInfoTable OBJECT-TYPE
    SYNTAX SEQUENCE OF IbDot11RRMRadioNbrInfoEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "This table shows the RRM neighbor information of each radio
        on all APs."
    ::= { ibDot11RRMDetectGroup 4 }

ibDot11RRMRadioNbrInfoEntry OBJECT-TYPE
    SYNTAX IbDot11RRMRadioNbrInfoEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "Each entry contains information of RRM neighbor information
        of the radio on an AP."
    INDEX { ibDot11RRMRadioNbrAPID, ibDot11RRMRadioNbrRadioID }
    ::= { ibDot11RRMRadioNbrInfoTable 1 }

IbDot11RRMRadioNbrInfoEntry ::=
    SEQUENCE {
        ibDot11RRMRadioNbrAPID
            IbDot11ObjectIDType,
        ibDot11RRMRadioNbrRadioID
