        ibDot11RRMNbrIntrf
            Integer32,
        ibDot11RrmNbrRSSI
            Integer32,
        ibDot11RrmNbrType
            INTEGER,
        ibDot11RrmNbrSSID
            IbDot11SSIDStringType,
        ibDot11RrmNbrSNR
            Integer32,
        ibDot11RrmNbrNF
            Integer32
     }

ibDot11RrmNbrBSSID OBJECT-TYPE
    SYNTAX MacAddress
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "MAC address of the AP."
    ::= { ibDot11RRMNbrInfoEntry 1 }

ibDot11RrmNbrChl OBJECT-TYPE
    SYNTAX IbDot11ChannelScopeType
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Channel number on which the neighbor was found."
    ::= { ibDot11RRMNbrInfoEntry 2 }

ibDot11RRMNbrIntrf OBJECT-TYPE
    SYNTAX Integer32 (0..100)
    UNITS "percent"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Interference observed on the channel in percentage by neighbor."
    ::= { ibDot11RRMNbrInfoEntry 3 }

ibDot11RrmNbrRSSI OBJECT-TYPE
    SYNTAX Integer32
    UNITS "dBm"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Signal strength of the AP in dBm."
