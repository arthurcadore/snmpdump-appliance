     }

ibDot11RRMRadioNbrAPID OBJECT-TYPE
    SYNTAX IbDot11ObjectIDType
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "AP serial id."
    ::= { ibDot11RRMRadioNbrInfoEntry 1 }

ibDot11RRMRadioNbrRadioID OBJECT-TYPE
    SYNTAX IbDot11RadioScopeType
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "Radio Id on AP."
    ::= { ibDot11RRMRadioNbrInfoEntry 2 }

ibDot11RRMRadioNbrSSID OBJECT-TYPE
    SYNTAX  OCTET STRING
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "All neighbour SSIDs scanned by the selected radio."
    ::= { ibDot11RRMRadioNbrInfoEntry 3 }

