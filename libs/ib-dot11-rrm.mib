ibDot11RRMResChgNotifications OBJECT IDENTIFIER ::= { ibDot11RRMNotifyGroup 2 }

ibDot11RRMResChgNtfPrefix OBJECT IDENTIFIER ::= { ibDot11RRMResChgNotifications 0 }

ibDot11RRMPowerChange NOTIFICATION-TYPE
    OBJECTS { ibDot11RRMRadioIndex, ibDot11NewPower, ibDot11OldPower }
    STATUS current
    DESCRIPTION
        "This notification will be sent when power changed on the radio
        automatically."
    ::= { ibDot11RRMResChgNtfPrefix 1 }

ibDot11RRMNotificationsVar OBJECT IDENTIFIER ::= { ibDot11RRMNotifyGroup 3 }

ibDot11NewPower OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS accessible-for-notify
    STATUS current
    DESCRIPTION
        "Power of the radio after the change of power."
    ::= { ibDot11RRMNotificationsVar 1 }

ibDot11OldPower OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS accessible-for-notify
    STATUS current
    DESCRIPTION
        "Power of the radio before the change of power."
    ::= { ibDot11RRMNotificationsVar 2 }

ibDot11MonitorDetectedGroup OBJECT IDENTIFIER ::= { ibDot11RRM 4 }

ibDot11MonitorDetectedDevTable OBJECT-TYPE
    SYNTAX SEQUENCE OF IbDot11MonitorDetectedDevEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "This table shows the devices of AP detected"
    ::= { ibDot11MonitorDetectedGroup 1 }

ibDot11MonitorDetectedDevEntry OBJECT-TYPE
    SYNTAX      IbDot11MonitorDetectedDevEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry contains information of detected devices."
    INDEX
        {
            ibDot11MonitorDevMAC, ibDot11APElementIndex
        }
    ::= { ibDot11MonitorDetectedDevTable 1 }

IbDot11MonitorDetectedDevEntry ::=
    SEQUENCE {
        ibDot11MonitorDevMAC
            MacAddress,
        ibDot11MonitorDevType
            INTEGER,
        ibDot11MonitorDevVendor
            OCTET STRING,
        ibDot11MonitorDevSSID
            OCTET STRING,
        ibDot11MonitorDevBSSID
            MacAddress,
        ibDot11MonitorDevChannel
            IbDot11ChannelScopeType,
        ibDot11MonitorRadioId
            IbDot11RadioScopeType,
        ibDot11MonitorDevMaxRSSI
            Integer32,
        ibDot11MonitorDevBeaconIntvl
            Integer32,
        ibDot11MonitorDevFstDctTime
            DateAndTime,
        ibDot11MonitorDevLstDctTime
            DateAndTime,
        ibDot11MonitorDevClear
            TruthValue,
        ibDot11MonitorDevSNR
            Integer32
     }

ibDot11MonitorDevMAC OBJECT-TYPE
    SYNTAX      MacAddress
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Represents MAC address of the device detected."
    ::= { ibDot11MonitorDetectedDevEntry 1 }

ibDot11MonitorDevType OBJECT-TYPE
    SYNTAX      INTEGER
                {
                    client(1),
                    ap(2),
                    adhoc(3),
                    wirelessBridge(4),
                    unknown(5)
                }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents type of the device detected."
    ::= { ibDot11MonitorDetectedDevEntry 2 }

ibDot11MonitorDevVendor OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents vendor of the detected device."
    ::= { ibDot11MonitorDetectedDevEntry 3 }

ibDot11MonitorDevSSID OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the service set identifier for the ESS of the device which
         type is ap or adhoc."
    ::= { ibDot11MonitorDetectedDevEntry 4 }

ibDot11MonitorDevBSSID OBJECT-TYPE
    SYNTAX      MacAddress
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the basic service set identifier of the detected device."
    ::= { ibDot11MonitorDetectedDevEntry 5 }

ibDot11MonitorDevChannel OBJECT-TYPE
    SYNTAX      IbDot11ChannelScopeType
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the channel in which the device was last detected.  AP will
         choose the channel which has maximum signal strength as effective
         channel, as there is interference between adjacent channels."
    ::= { ibDot11MonitorDetectedDevEntry 6 }

ibDot11MonitorRadioId OBJECT-TYPE
    SYNTAX      IbDot11RadioScopeType
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the radio ID of the AP that detected the device."
    ::= { ibDot11MonitorDetectedDevEntry 7 }

ibDot11MonitorDevMaxRSSI OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "dbm"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the maximum detected RSSI of the device in a scan report cycle."
    ::= { ibDot11MonitorDetectedDevEntry 8 }

ibDot11MonitorDevBeaconIntvl OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "millisecond"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the beacon interval for the detected device(not include
         the device which type is client)."
    ::= { ibDot11MonitorDetectedDevEntry 9 }

ibDot11MonitorDevFstDctTime OBJECT-TYPE
    SYNTAX      DateAndTime
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the time at which the device was first detected."
    ::= { ibDot11MonitorDetectedDevEntry 10 }

ibDot11MonitorDevLstDctTime OBJECT-TYPE
    SYNTAX      DateAndTime
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the time at which the device was detected last time."
    ::= { ibDot11MonitorDetectedDevEntry 11 }

ibDot11MonitorDevClear OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object is used to clear the information of the device detected
         in the WLAN.
         It will return false for get operation."
    ::= { ibDot11MonitorDetectedDevEntry 12 }

ibDot11MonitorDevSNR OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the SNR of the device in a scan report cycle."
    ::= { ibDot11MonitorDetectedDevEntry 13 }

END
