ibCfgExecuteResultEntry OBJECT-TYPE
    SYNTAX IbCfgExecuteResultEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "The result entries of configuration execute operation."
    INDEX { ibCfgExecuteResultIndex }
    ::= { ibCfgExecuteResultTable 1 }

IbCfgExecuteResultEntry ::=
    SEQUENCE
        {
            ibCfgExecuteResultIndex            Integer32,
            ibCfgExecuteResultOptIndex         Integer32,
            ibCfgExecuteResultOpType           ConfigOperationType,
            ibCfgExecuteState                  ConfigOperationStateType,
            ibCfgExecuteTime                   TimeTicks,
            ibCfgExecuteEndTime                TimeTicks
        }

ibCfgExecuteResultIndex OBJECT-TYPE
    SYNTAX Integer32 (1..2147483647)
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "The index of Table, which is an incremental integer.
        The maximum value of the node is 2147483647.  The agent should wrap the
        value to 1 and flush all the existing entries when the maximum value
