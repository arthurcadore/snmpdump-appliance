        { ibCfgOperateGlobalEntryLimit, ibCfgOperateEntryAgeOutTime, ibCfgOperateType, ibCfgOperateProtocol, ibCfgOperateFileName,
        ibCfgOperateServerAddress, ibCfgOperateUserName, ibCfgOperateUserPassword, ibCfgOperateTime,  ibCfgOperateEndNotificationSwitch,
        ibCfgOperateResultGlobalEntryLimit, ibCfgOperateState, ibCfgOperateRowStatus, ibCfgOperateResultOptIndex,
        ibCfgOperateResultOpType, ibCfgOperateEndTime, ibCfgOperFailReason, ibCfgOperateServerPort }
    STATUS current
    DESCRIPTION
