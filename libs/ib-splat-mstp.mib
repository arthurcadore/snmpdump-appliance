  ibMstpEventsV2 OBJECT-IDENTITY
     STATUS      current
     DESCRIPTION "Definition point for Mstp notifications."
     ::= { ibdot1sMstp 0 }

  ibPortMstiStateForwarding NOTIFICATION-TYPE
     OBJECTS  { ibdot1sInstanceID , ibdot1sMstiPortIndex}
     STATUS   current
     DESCRIPTION
         "The SNMP trap that is generated when a port turns into
         forwarding state form other state."
     ::= { ibMstpEventsV2 1 }

  ibPortMstiStateDiscarding NOTIFICATION-TYPE
     OBJECTS  { ibdot1sInstanceID , ibdot1sMstiPortIndex}
     STATUS   current
     DESCRIPTION
         "The SNMP trap that is generated when a port turns into
         discarding state form forwarding state."
     ::= { ibMstpEventsV2 2 }


  ibBridgeLostRootPrimary NOTIFICATION-TYPE
     OBJECTS  { ibdot1sInstanceID }
     STATUS   current
     DESCRIPTION
         "The SNMP trap that is generated when the bridge is no longer
         the root bridge of the instance.  Another switch with higher
         priority has already been the root bridge of the instance."
     ::= { ibMstpEventsV2 3 }

  ibPortMstiRootGuarded NOTIFICATION-TYPE
     OBJECTS  { ibdot1sInstanceID , ibdot1sMstiPortIndex}
     STATUS   current
     DESCRIPTION
         "The SNMP trap that is generated when a root-guard port
         receives a superior message on the relevant instance."
     ::= { ibMstpEventsV2 4 }

  ibPortMstiBpduGuarded NOTIFICATION-TYPE
     OBJECTS  { dot1dStpPort , ibdot1sMstiDesignatedBridge}
     STATUS   current
     DESCRIPTION
         "The SNMP trap is generated when an edged port of the BPDU-guard
         device receives a BPDU.  If the designated bridge ID can't be obtained
         from the BPDU, the designated bridge ID will be zero."
     ::= { ibMstpEventsV2 5 }

  ibPortMstiLoopGuarded NOTIFICATION-TYPE
     OBJECTS  { ibdot1sInstanceID , ibdot1sMstiPortIndex}
     STATUS   current
     DESCRIPTION
         "The SNMP trap that is generated when an Alternate-Port
         or Root-Port is aged out."
     ::= { ibMstpEventsV2 6 }

  ibMstiNewRoot NOTIFICATION-TYPE
     OBJECTS  { ibdot1sInstanceID , ibdot1sMstiDesignedRoot , ibdot1sMstiDesignedRoot}
     STATUS   current
     DESCRIPTION
         "The SNMP trap is generated when the sending device becomes the new
         root of the spanning tree.  The first root bridge ID means the
         old root bridge ID, and the second one means the new root bridge ID."
     ::= { ibMstpEventsV2 7 }

  ibPortPvstBpduProtection NOTIFICATION-TYPE
     OBJECTS  { dot1dStpPort , ibdot1sMstiDesignatedBridge}
     STATUS   current
     DESCRIPTION
         "The SNMP trap is generated when a PVST BPDU is received on an MSTP port
         that is enabled with the PVST protection feature.  If the designated
         bridge ID can't be obtained from the BPDU, the designated bridge ID
         will be zero."
     ::= { ibMstpEventsV2 8 }

  ibMstiTopologyChange NOTIFICATION-TYPE
     OBJECTS  { ibdot1sInstanceID }
     STATUS   current
     DESCRIPTION
         "A topology change trap is sent by a bridge when any of
         its configured ports transits from the Learning state
         to the Forwarding state, or from the Forwarding state to
         the Blocking state."
     ::= { ibMstpEventsV2 9 }

  ibPortMstiLostEdge NOTIFICATION-TYPE
     OBJECTS  { ibdot1sMstiPortIndex, ifDescr, ibdot1sInstanceID }
     STATUS   current
     DESCRIPTION
         "The SNMP trap is generated when an edge port
         receives a BPDU packet, and the port is no longer an edge port."
     ::= { ibMstpEventsV2 10 }

  ibMstiTcGuarded NOTIFICATION-TYPE
     OBJECTS  { ibdot1sInstanceID }
     STATUS   current
     DESCRIPTION
         "The SNMP trap is generated when a port received a BPDU after the
         number of TC BPDUs received by an instance exceeded the threshold."
     ::= { ibMstpEventsV2 11 }

  ibMstiProTcGuarded NOTIFICATION-TYPE
     OBJECTS  { ibdot1sInstanceID, ibMstiTcGuardVal }
     STATUS   current
     DESCRIPTION
         "The SNMP trap is generated when a port received a BPDU after the
         number of TC BPDUs received by an instance exceeded the threshold."
     ::= { ibMstpEventsV2 12 }

  ibPortMstiLoopbackGuarded NOTIFICATION-TYPE
     OBJECTS  { ibdot1sInstanceID, ifDescr }
     STATUS   current
     DESCRIPTION
         "The SNMP trap is generated when a loop is detected
         on a port that is based on an instance."
