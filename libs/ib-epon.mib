--
-- ibEponStatTable
--

ibEponStatTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbEponStatEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "
        This table contains line code error statistics of epon port.
        "
    ::= { ibEponStat 1 }

ibEponStatEntry OBJECT-TYPE
    SYNTAX      IbEponStatEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "
        The entry of ibEponStatTable.
        "
    INDEX
        {
            ifIndex
        }
    ::= { ibEponStatTable 1 }

IbEponStatEntry ::= SEQUENCE
    {
        ibEponStatFER    Counter64,
        ibEponStatBER    Counter64
    }

ibEponStatFER OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "
        The frame error rate of epon port, measured in 1e-9 unit.
        "
    ::= { ibEponStatEntry 1 }

ibEponStatBER OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "
        The bit error rate of epon port, measured in 1e-9 unit.
        "
    ::= { ibEponStatEntry 2  }

