--
-- ibOnuRS485PropertiesTable
--

ibOnuRS485PropertiesTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbOnuRS485PropertiesEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "Property table."
    ::= { ibEponOnuMan 31 }

ibOnuRS485PropertiesEntry OBJECT-TYPE
    SYNTAX          IbOnuRS485PropertiesEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION
            "The entry of property table."
    INDEX      {
                ifIndex,
                ibOnuRS485SerialIndex
               }
    ::= { ibOnuRS485PropertiesTable 1 }

IbOnuRS485PropertiesEntry ::=
    SEQUENCE{
        ibOnuRS485SerialIndex
            Integer32,
        ibOnuRS485BaudRate
            INTEGER,
        ibOnuRS485DataBits
            INTEGER,
        ibOnuRS485Parity
            INTEGER,
        ibOnuRS485StopBits
            INTEGER,
        ibOnuRS485FlowControl
            INTEGER,
        ibOnuRS485TXOctets
            Integer32,
        ibOnuRS485RXOctets
            Integer32,
        ibOnuRS485TXErrOctets
            Integer32,
        ibOnuRS485RXErrOctets
            Integer32,
        ibOnuRS485ResetStatistics
            INTEGER
    }

ibOnuRS485SerialIndex OBJECT-TYPE
    SYNTAX          Integer32 (1..64)
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION
        "The index of serial port."
    ::= { ibOnuRS485PropertiesEntry 1 }

ibOnuRS485BaudRate OBJECT-TYPE
    SYNTAX          INTEGER {
                        baudRate300(1),
                        baudRate600(2),
                        baudRate1200(3),
                        baudRate2400(4),
                        baudRate4800(5),
                        baudRate9600(6),
                        baudRate19200(7),
                        baudRate38400(8),
                        baudRate57600(9),
                        baudRate115200(10)
                        }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
            "The port baud rate."
    DEFVAL          { baudRate9600 }
    ::= { ibOnuRS485PropertiesEntry 2 }

ibOnuRS485DataBits OBJECT-TYPE
    SYNTAX          INTEGER {
                        five(1),            -- 5 data bits
                        six(2),             -- 6 data bits
                        seven(3),           -- 7 data bits
                        eight(4)            -- 8 data bits
                    }
    UNITS           "bit"
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
            "The port number of data bits in a character."
    DEFVAL          { eight }
    ::= { ibOnuRS485PropertiesEntry 3 }

ibOnuRS485Parity OBJECT-TYPE
    SYNTAX          INTEGER {
                        none(1),
                        odd(2),
                        even(3),
                        mark(4),
                        space(5)
                    }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
            "The port sense of a character parity bit."
    DEFVAL          { none }
    ::= { ibOnuRS485PropertiesEntry 4 }

ibOnuRS485StopBits OBJECT-TYPE
    SYNTAX          INTEGER {
                        one(1),               -- 1 stop bit
                        two(2),               -- 2 stop bits
                        oneAndHalf(3)         -- 1.5 stop bits
                    }
    UNITS           "bit"
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
            "The port number of stop bits."
    DEFVAL          { one }
    ::= { ibOnuRS485PropertiesEntry 5 }

ibOnuRS485FlowControl OBJECT-TYPE
    SYNTAX          INTEGER{
                        none(1),
                        hardware(2),
                        xonOrxoff(3)
                    }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
            "The port type of input flow control.
             'none' indicates no flow control at this level.
             'hardware' indicates using hardware signals.
             'xonOrxoff' indicates using software function."
    DEFVAL          { none }
    ::= { ibOnuRS485PropertiesEntry 6 }

ibOnuRS485TXOctets OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
            "The number of output octets about the port.
             The unit is byte."
    ::= { ibOnuRS485PropertiesEntry 7 }

ibOnuRS485RXOctets OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
            "The number of input octets about the port.
             The unit is byte."
    ::= { ibOnuRS485PropertiesEntry 8 }

ibOnuRS485TXErrOctets OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
            "The number of output error octets about the port.
             The unit is byte."
    ::= { ibOnuRS485PropertiesEntry 9 }

ibOnuRS485RXErrOctets OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
            "The number of input error octets about the port.
             The unit is byte."
    ::= { ibOnuRS485PropertiesEntry 10 }

ibOnuRS485ResetStatistics OBJECT-TYPE
    SYNTAX          INTEGER {
                        counting(1),
                        clear(2)
                    }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
            "Reset the counters to zero, including ibOnuRS485TXOctets,
             ibOnuRS485RXOctets, ibOnuRS485TXErrOctets and
             ibOnuRS485RXErrOctets."
    DEFVAL          { counting }
    ::= { ibOnuRS485PropertiesEntry 11 }

