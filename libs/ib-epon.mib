--
-- ibRoltInterfaceTable
--

ibRoltInterfaceTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbRoltInterfaceEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "
        The table of redundant OLT interfaces.
        "
    ::= { ibEponRoltMan 1 }

ibRoltInterfaceEntry  OBJECT-TYPE
    SYNTAX      IbRoltInterfaceEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "
        The entry of the table ibRoltInterfaceTable.
        "
    INDEX
        {
            ibRoltInterfaceNumber
        }
    ::= { ibRoltInterfaceTable 1 }

IbRoltInterfaceEntry ::= SEQUENCE
    {
        ibRoltInterfaceNumber       Integer32,
        ibRoltInterfaceIfIndex      Integer32,
        ibRoltInterfaceRowStatus    RowStatus
    }

ibRoltInterfaceNumber OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "
        The number of an ROLT interface.
        "
    ::= { ibRoltInterfaceEntry 1 }

ibRoltInterfaceIfIndex OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "
        The ifIndex of an ROLT interface.
        "
    ::= { ibRoltInterfaceEntry 2 }

ibRoltInterfaceRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "
        Create or delete a redundant OLT interface.
        Now only two operations are supported:
        createAndGo and destroy.
        "
    ::= { ibRoltInterfaceEntry 3 }

--
-- ibRoltMemberTable
--
ibRoltMemberTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbRoltMemberEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "
        This table is used for adding or deleting the OLT member
        interfaces of an redundant OLT interface.
        "
    ::= { ibEponRoltMan 2 }

ibRoltMemberEntry  OBJECT-TYPE
    SYNTAX      IbRoltMemberEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "
        The entry of the table ibRoltMemberTable.
        "
    INDEX
        {
            ibRoltInterfaceNumber,
            ibRoltMemberOltIfindex
        }
    ::= { ibRoltMemberTable 1 }

IbRoltMemberEntry ::= SEQUENCE
    {
        ibRoltMemberOltIfindex      Integer32,
        ibRoltMemberOltPriority     Integer32,
        ibRoltMemberRowStatus       RowStatus,
        ibRoltMemberForwardStatus   TruthValue
    }

ibRoltMemberOltIfindex OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "
        The ifindex of an OLT interface.
        "
    ::= { ibRoltMemberEntry 1 }

ibRoltMemberOltPriority OBJECT-TYPE
    SYNTAX      Integer32 (1..255)
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "
        The priority of an OLT interface that will be assigned to an ROLT interface.
        "
    ::= { ibRoltMemberEntry 2 }

ibRoltMemberRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "
        Operation status.  Now only two states are supported:
        createAndGo and destroy.
        "
    ::= { ibRoltMemberEntry 3 }

ibRoltMemberForwardStatus OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "
        The forwarding status of an OLT member interface.
        true(1)  - Active.
        false(2) - Inactive.
        "
    ::= { ibRoltMemberEntry 4 }
END
