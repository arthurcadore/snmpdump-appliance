-- ==================================================================
-- =================== ibVsiNotifications definition ==========
-- ==================================================================

ibVsiNotifications OBJECT IDENTIFIER ::= { ibVsi 0 }

-- =============================================================================
-- object definition begin
-- =============================================================================

ibVsiObjects      OBJECT IDENTIFIER ::= { ibVsi 1 }

ibVsiScalarGroup OBJECT IDENTIFIER ::= { ibVsiObjects 1 }

ibVsiNextAvailableVsiIndex    OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Next available VSI entry index for creating VSI.  Its value ranges
        from 0x1 to 0xFFFFFFFF.  The invalid value 0xFFFFFFFF indicates that
        no VSI entry can be created."
    ::= { ibVsiScalarGroup 1 }

ibVsiL2vpnStatus    OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The global configuration of L2VPN."
    ::= { ibVsiScalarGroup 2 }

-- =============================================================================
-- ibVsiTable Definition
-- =============================================================================
ibVsiTable  OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbVsiEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A table for configuring VSI parameters."
    ::= { ibVsiObjects 2 }

ibVsiEntry  OBJECT-TYPE
    SYNTAX      IbVsiEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "An entry for configuring VSI parameters."
    INDEX
    {
        ibVsiIndex
    }
    ::= { ibVsiTable 1 }

IbVsiEntry ::=
    SEQUENCE
    {
        ibVsiIndex             Unsigned32,
        ibVsiName              OCTET STRING,
        ibVsiMode              INTEGER,
        ibMinmIsid             Integer32,
        ibVsiId                Unsigned32,
        ibVsiTransMode         INTEGER,
        ibVsiEnableHubSpoke    INTEGER,
        ibVsiAdminState        INTEGER,
        ibVsiRowStatus         RowStatus,
        ibVsiSpbIsid           Integer32,
        ibVsiVxlanID           Unsigned32,
        ibVsiArpSuppression    TruthValue,
        ibVsiFlooding          TruthValue,
        ibVsiLocalMacCount     Unsigned32,
        ibVsiInterfaceID       Unsigned32,
        ibVsiStatistics        TruthValue,
        ibVsiNvgreID           Unsigned32,
        ibVsiMacCount          Unsigned32,
        ibVsiAcCount           Unsigned32
    }

ibVsiIndex    OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Index of VSI.  Its value ranges from 0x1 to 0xFFFFFFFE."
    ::= { ibVsiEntry 1 }

ibVsiName    OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(1..31))
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Name of VSI.  Max string length of VSI name is 31."
    ::= { ibVsiEntry 2 }

ibVsiMode    OBJECT-TYPE
    SYNTAX      INTEGER
    {
        default(0),
        martini(1),
        minm(2),
        martiniAndMinm(3),
        kompella(4),
        kompellaAndMinm(5),
        minmpxp(6),
        martiniAndMinmpxp(7),
        kompellaAndMinmpxp(8),
        vxlan(9)
    }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Mode of VSI.  Martini mode indicated this VSI supports VPLS service
         signalled using LDP.  Kompella indicated this VSI supports VPLS
         service signalled using BGP.  Minm indicated this VSI supports
         MAC-in-MAC service.
         Default indicated this VSI supports all services."

    ::= { ibVsiEntry 3 }

ibMinmIsid    OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The PBB I-SID of VSI in minm or martiniAndMinm mode.  It
        must be different for every VSI.  Its value ranges from 0x1 to
        0xFFFFFF.  In other VSI modes, its value is invalid value 0x0."
    ::= { ibVsiEntry 4 }

ibVsiId    OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Identifier of VSI in martini or martiniAndMinm mode, by default,
        it's the PW ID of this VSI.  Its value ranges from 1 to 4294967295.
        In other VSI modes, its value is invalid value 0."
    ::= { ibVsiEntry 5 }

ibVsiTransMode    OBJECT-TYPE
    SYNTAX      INTEGER
    {
        vlan(1),
        ethernet(2)
    }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Transit Mode of VSI."
    ::= { ibVsiEntry 6 }

ibVsiEnableHubSpoke    OBJECT-TYPE
    SYNTAX      INTEGER
    {
        disable(1),
        enable(2)
    }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Config HubSpoke property of this VSI."
    ::= { ibVsiEntry 7 }

ibVsiAdminState   OBJECT-TYPE
    SYNTAX      INTEGER
    {
        adminUp(1),
        adminDown(2)
    }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Set VSI admin state as adminUp or adminDown."
    ::= { ibVsiEntry 8 }

ibVsiRowStatus     OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Operation status of this table entry."
    ::= { ibVsiEntry 9 }

ibVsiSpbIsid    OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The SPB I-SID of VSI in minm or martiniAndMinm mode.  It
        must be different for every VSI.  Its value ranges from 0xFF to
        0xFFFFFF.  In other VSI modes, its value is invalid value 0x0."
    ::= { ibVsiEntry 10 }

ibVsiVxlanID    OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "VXLAN ID, in the range of 0x0 to 0xFFFFFF.  Value 0xFFFFFFFF means
        this VSI does not enable VXLAN service."
    ::= { ibVsiEntry 11 }

ibVsiArpSuppression    OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The capability of ARP suppression."
    DEFVAL { false }
    ::= { ibVsiEntry 12 }

ibVsiFlooding    OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The capability of MAC(Media Access Control) flooding."
    DEFVAL { true }
    ::= { ibVsiEntry 13 }

ibVsiLocalMacCount    OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The number of VSI local MAC addresses."
    ::= { ibVsiEntry 14 }

ibVsiInterfaceID    OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "VSI interface ID.  A value of 0xFFFFFFFF indicates that no gateway
        interface has been assigned to the VSI."
    ::= { ibVsiEntry 15 }

ibVsiStatistics    OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The enable status of VSI traffic flow statistics."
    DEFVAL { false }
    ::= { ibVsiEntry 16 }

ibVsiNvgreID    OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "NVGRE ID, in the range of 4096 to 16777214.  Value 4294967295 means
        this VSI does not enable NVGRE service."
    ::= { ibVsiEntry 17 }

ibVsiMacCount    OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The number of VSI MAC addresses."
    ::= { ibVsiEntry 18 }

ibVsiAcCount    OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The number of VSI Ac."
    ::= { ibVsiEntry 19 }

-- =============================================================================
-- End of ibVsiTable Definition
-- =============================================================================

-- =============================================================================
-- ibVsiXconnectTable Definition
-- =============================================================================
ibVsiXconnectTable  OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbVsiXconnectEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A table for configuring xconnect parameters."
    ::= { ibVsiObjects 3 }

ibVsiXconnectEntry  OBJECT-TYPE
    SYNTAX      IbVsiXconnectEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "An entry for configuring xconnect parameters.  Each entry means
        connecting a service instance of a interface to a VSI."
    INDEX
    {
        ibVsiXconnectIfIndex,
        ibVsiXconnectEvcSrvInstId
    }
    ::= { ibVsiXconnectTable 1 }

IbVsiXconnectEntry ::=
    SEQUENCE
    {
        ibVsiXconnectIfIndex          Unsigned32,
        ibVsiXconnectEvcSrvInstId     Unsigned32,
        ibVsiXconnectVsiName          OCTET STRING,
        ibVsiXconnectAccessMode       INTEGER,
        ibVsiXconnectHubSpoke         INTEGER,
        ibVsiXconnectRowStatus        RowStatus,
        ibVsiXconnectInOctets         Counter64,
        ibVsiXconnectInPackets        Counter64,
        ibVsiXconnectInErrors         Counter64,
        ibVsiXconnectInDiscards       Counter64,
        ibVsiXconnectInUcastPackets   Counter64,
        ibVsiXconnectInMcastPackets   Counter64,
        ibVsiXconnectInBcastPackets   Counter64,
        ibVsiXconnectOutOctets        Counter64,
        ibVsiXconnectOutPackets       Counter64,
        ibVsiXconnectOutErrors        Counter64,
        ibVsiXconnectOutDiscards      Counter64,
        ibVsiXconnectOutUcastPackets  Counter64,
        ibVsiXconnectOutMcastPackets  Counter64,
        ibVsiXconnectOutBcastPackets  Counter64
    }
ibVsiXconnectIfIndex    OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Index of the interface associated with the VSI."
    ::= { ibVsiXconnectEntry 1 }

ibVsiXconnectEvcSrvInstId    OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Index of the service instance associated with the VSI.  The value is always 0 when the
         interface isn't a layer 2 Ethernet interface."
    ::= { ibVsiXconnectEntry 2 }


ibVsiXconnectVsiName    OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(1..31))
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Name of VSI.  Max string length of VSI name is 31."
    ::= { ibVsiXconnectEntry 3 }

ibVsiXconnectAccessMode    OBJECT-TYPE
    SYNTAX      INTEGER
    {
        vlan(1),
        ethernet(2)
    }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Access mode of this service instance."
    ::= { ibVsiXconnectEntry 4 }

ibVsiXconnectHubSpoke    OBJECT-TYPE
    SYNTAX      INTEGER
    {
        none(1),
        hub(2),
        spoke(3)
    }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Config HubSpoke property of this service instance."
    ::= { ibVsiXconnectEntry 5 }

ibVsiXconnectRowStatus     OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Operation status of this table entry."
    ::= { ibVsiXconnectEntry 6 }

ibVsiXconnectInOctets    OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Total number of octets received by the AC."
    ::= { ibVsiXconnectEntry 7 }

ibVsiXconnectInPackets    OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Total number of packets received by the AC."
    ::= { ibVsiXconnectEntry 8 }

ibVsiXconnectInErrors    OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Number of error packets received by the AC."
    ::= { ibVsiXconnectEntry 9 }

ibVsiXconnectInDiscards    OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Number of incoming packets discarded by the AC."
    ::= { ibVsiXconnectEntry 10 }

ibVsiXconnectInUcastPackets    OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "Unicast packets sent by the AC."
    ::= { ibVsiXconnectEntry 11 }

ibVsiXconnectInMcastPackets    OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "Rate of packets sent by the AC."
    ::= { ibVsiXconnectEntry 12 }

ibVsiXconnectInBcastPackets    OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "Rate of packets sent by the AC."
    ::= { ibVsiXconnectEntry 13 }

ibVsiXconnectOutOctets    OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Total number of octets sent by the AC."
    ::= { ibVsiXconnectEntry 14 }

ibVsiXconnectOutPackets    OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Total number of packets sent by the AC."
    ::= { ibVsiXconnectEntry 15 }

ibVsiXconnectOutErrors    OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Number of packets that failed to be sent because of a AC error."
    ::= { ibVsiXconnectEntry 16 }

ibVsiXconnectOutDiscards    OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "Number of packets discarded by the AC because of transmission failure."
    ::= { ibVsiXconnectEntry 17 }

ibVsiXconnectOutUcastPackets    OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "Unicast packets received by the AC."
    ::= { ibVsiXconnectEntry 18 }

ibVsiXconnectOutMcastPackets    OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "Rate of packets received by the AC."
    ::= { ibVsiXconnectEntry 19 }

ibVsiXconnectOutBcastPackets    OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
       "Rate of packets sent by the AC."
    ::= { ibVsiXconnectEntry 20 }

-- =============================================================================
-- End of ibVsiXconnectTable Definition
-- =============================================================================

-- =============================================================================
-- ibVsiPwBindTable Definition
-- =============================================================================
ibVsiPwBindTable  OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbVsiPwBindEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A table for PW(PseudoWire) mapping to VSI."
    ::= { ibVsiObjects 4 }

ibVsiPwBindEntry  OBJECT-TYPE
    SYNTAX      IbVsiPwBindEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry means a PW mapping to a VSI."
    INDEX
    {
        ibVsiIndex,
        ibVsiPwIndex
    }
    ::= { ibVsiPwBindTable 1 }

IbVsiPwBindEntry ::=
    SEQUENCE
    {
        ibVsiPwIndex            Unsigned32,
        ibVsiPwBindAttributes   BITS,
        ibVsiPwBindRowStatus    RowStatus
    }

ibVsiPwIndex    OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A unique index for the conceptual row identifying
         a PW in the pwTable."
    ::= { ibVsiPwBindEntry 1 }

ibVsiPwBindAttributes    OBJECT-TYPE
    SYNTAX      BITS {
                     noSplitHorizon (0),
                     hub (1)
                    }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This bit mask indicates optional values for this PW.
         The following describes these bit fields:

         noSplitHorizon flag indicates that the PW disable
         split horizon forwarding.

         hub flag indicates that the PW as the hub link the VSI."
    ::= { ibVsiPwBindEntry 2 }

ibVsiPwBindRowStatus     OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Operation status of this table entry.  A row entry cannot
        be modified when the value of this object is 'active'."
    ::= { ibVsiPwBindEntry 3 }

-- =============================================================================
-- End of ibVsiPwBindTable Definition
-- =============================================================================

-- =============================================================================
-- ibVsiFloodMacTable Definition
-- =============================================================================
ibVsiFloodMacTable  OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbVsiFloodMacEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A table of MAC addresses enabled with selective flooding."
    ::= { ibVsiObjects 5 }

ibVsiFloodMacEntry  OBJECT-TYPE
    SYNTAX      IbVsiFloodMacEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry represents a selective flooding MAC."
    INDEX
    {
        ibVsiIndex,
        ibVsiFloodMac
    }
    ::= { ibVsiFloodMacTable 1 }

IbVsiFloodMacEntry ::=
    SEQUENCE
    {
        ibVsiFloodMac               MacAddress,
        ibVsiFloodMacRowStatus      RowStatus
    }

ibVsiFloodMac    OBJECT-TYPE
    SYNTAX      MacAddress
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "MAC address."
    ::= { ibVsiFloodMacEntry 1 }

ibVsiFloodMacRowStatus     OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Operation status of this table entry."
    ::= { ibVsiFloodMacEntry 2 }

-- =============================================================================
-- End of ibVsiFloodMacTable Definition
-- =============================================================================

-- =============================================================================
-- ibVsiLocalMacTable Definition
-- =============================================================================
ibVsiLocalMacTable  OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbVsiLocalMacEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A table for VSI local MAC addresses."
    ::= { ibVsiObjects 6 }

ibVsiLocalMacEntry  OBJECT-TYPE
    SYNTAX      IbVsiLocalMacEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A local MAC address of VSI."
    INDEX
    {
        ibVsiIndex,
        ibVsiLocalMacAddr
    }
    ::= { ibVsiLocalMacTable 1 }

IbVsiLocalMacEntry ::=
    SEQUENCE
    {
        ibVsiLocalMacAddr            MacAddress,
        ibVsiLocalMacIfIndex         InterfaceIndex,
        ibVsiLocalMacSrvID           Unsigned32
    }

ibVsiLocalMacAddr    OBJECT-TYPE
    SYNTAX      MacAddress
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "MAC address."
    ::= { ibVsiLocalMacEntry 1 }

ibVsiLocalMacIfIndex    OBJECT-TYPE
    SYNTAX      InterfaceIndex
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Index of the interface associated with the MAC address."
    ::= { ibVsiLocalMacEntry 2 }

ibVsiLocalMacSrvID    OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Index of the service instance associated with the MAC address."
    ::= { ibVsiLocalMacEntry 3 }

-- =============================================================================
-- End of ibVsiLocalMacTable Definition
-- =============================================================================

-- =============================================================================
-- ibVsiPerfTable Definition
-- =============================================================================
ibVsiPerfTable  OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbVsiPerfEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table contains performance statistics for each VSI."
    ::= { ibVsiObjects 7 }

ibVsiPerfEntry  OBJECT-TYPE
