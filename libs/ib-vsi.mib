IB-VSI-MIB DEFINITIONS ::= BEGIN
IMPORTS
        RowStatus, TruthValue, MacAddress
    FROM SNMPv2-TC
        MODULE-IDENTITY,
        OBJECT-TYPE,
        Integer32,
        Counter64,
        Unsigned32,
        NOTIFICATION-TYPE,
        IpAddress
    FROM SNMPv2-SMI
        InterfaceIndex
    FROM IF-MIB
        ibCommon
    FROM IB-OID-MIB;

