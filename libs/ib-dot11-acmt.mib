-- *****************************************************************************
-- *     Notifications OF ibDot11ACMtNotifyGroup
-- *****************************************************************************
-- AC Management Notification

        ibDot11ACMtTraps OBJECT IDENTIFIER
    ::= { ibDot11ACMtNotifyGroup 0 }

ibDot11ACMtTunnelSetupTrap NOTIFICATION-TYPE
    OBJECTS
        {
            ibDot11CurrTunnelAPID,
            ibDot11ACMtTrapTunlUpInfo,
            ibDot11ACMtTrapTnlAPName,
            ibDot11ACMtTrapTnlAPIPAddr,
            ibDot11ACMtTrapAPIPv6Addr,
            ibDot11ACMtFirstTrapTime,
            ibDot11ACMtTrapAPMacAddress
        }
    STATUS      current
    DESCRIPTION
        "This notification is sent by AC when CAPWAP tunnel becomes up.
        By this way, NMS will immediately know tunnel up event.
        It is unnecessary to identify whether it is control or data
        tunnel."
    ::= { ibDot11ACMtTraps 1 }

ibDot11ACMtTunnelDownTrap NOTIFICATION-TYPE
    OBJECTS
        {
            ibDot11CurrTunnelAPID,
            ibDot11ACMtTrapTunlDwnInfo,
            ibDot11ACMtTrapTnlAPName,
            ibDot11ACMtTrapTnlAPIPAddr,
            ibDot11ACMtTrapAPIPv6Addr,
            ibDot11ACMtTrapTunlDwnCount,
            ibDot11ACMtTrapTnlAPSysName,
            ibDot11ACMtFirstTrapTime,
            ibDot11ACMtTrapAPMacAddress
        }
    STATUS      current
    DESCRIPTION
        "This notification is sent by AC when CAPWAP tunnel becomes down.
        By this way, NMS will immediately know tunnel down event.
        It is unnecessary to identify whether it is control or data
        tunnel."
    ::= { ibDot11ACMtTraps 2 }

ibDot11ACMtBackupSwtTrap NOTIFICATION-TYPE
    OBJECTS
        {
            ibDot11ACMtTrapACMacAddress,
            ibDot11ACMtTrapBackupSwitchInfo,
            ibDot11ACMtFirstTrapTime
        }
    STATUS      current
    DESCRIPTION
        "This trap is sent by AC when AC switches from master to slave
        or from slave to master."
    ::= { ibDot11ACMtTraps 3 }

ibDot11ACLoadBalanceTrap NOTIFICATION-TYPE
    OBJECTS
        {
            ibDot11LoadBalanceType,
            ibDot11LoadBalanceThreshold
        }
    STATUS      current
    DESCRIPTION
        "This trap is sent by AC when load-balance is enabled on AC."
    ::= { ibDot11ACMtTraps 4 }

ibDot11ACStaFullTrap NOTIFICATION-TYPE
    OBJECTS
        {
            ibDot11ACMaxStaNum
        }
    STATUS current
    DESCRIPTION
        "This notification will be sent when STA is full on AC."
    ::={ ibDot11ACMtTraps 5 }

ibDot11ACStatusSwitchTrap NOTIFICATION-TYPE
    OBJECTS
        {
            ibDot11ACStatusSwitchInfo,
            ibDot11SourceACNmsIP
        }
    STATUS current
    DESCRIPTION
        "This notification will be sent when AC switches from standby to active
        or from standby to active."
    ::={ ibDot11ACMtTraps 6}

ibDot11RunAPNumOverload NOTIFICATION-TYPE
    OBJECTS
        {
            ibDot11MaxAPNumPermitted,
            ibDot11APConnectCount
        }
    STATUS current
    DESCRIPTION
        "The notification will be sent by AC when run AP number exceeds the threshold."
    ::= { ibDot11ACMtTraps 7}

ibDot11RunAPNumOverloadRecover NOTIFICATION-TYPE
    OBJECTS
        {
            ibDot11MaxAPNumPermitted,
            ibDot11APConnectCount
        }
    STATUS current
    DESCRIPTION
        "The notification will be sent by AC when run AP number
        recover normal from overload state."
    ::= { ibDot11ACMtTraps 8 }

ibDot11APInvalidCertificateTrap NOTIFICATION-TYPE
    OBJECTS
        {
            ibDot11ACMtTrapACMacAddress,
            ibDot11ACMtTrapAPMacAddress
        }
    STATUS current
    DESCRIPTION
        "The AC sends the notification once the certificate of an AP expires."
    ::= { ibDot11ACMtTraps 9 }

-- AC Management Notification Variable object

        ibDot11ACMtTrapVarObjects OBJECT IDENTIFIER
    ::= { ibDot11ACMtNotifyGroup 1 }

ibDot11ACMtTrapTunlDwnInfo OBJECT-TYPE
    SYNTAX      INTEGER
        {
            tunnelTimeout(1),
            keyUpdateFailure(2),
            apReset(3),
            apCrash(4),
            apDeleted(5),
            apCfgChange(6)
        }
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "Represents the reason for tunnel down.
        The following values are supported:
        tunnelTimeout     - When AC detects the tunnel is broken,
        - it will notify NMS.
        keyUpdateFailure  - rekey process is failed, tunnel will be broken.
        apReset           - Administrator reset AP.
        apCrash           - AP reboot for system crash.
        apDeleted         - Delete AP from AC and trigger tunnel down.
        apCfgChange       - The configuration for AP was changed."
    ::= { ibDot11ACMtTrapVarObjects 1 }

ibDot11ACMtTrapTunlUpInfo OBJECT-TYPE
    SYNTAX      INTEGER
        {
            up(1)
        }
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "Represents the reason for tunnel up.
        The following values are supported:
        up(1)  - The AP joined AC.
        "
    ::= { ibDot11ACMtTrapVarObjects 2 }

ibDot11ACMtTrapBackupSwitchInfo OBJECT-TYPE
    SYNTAX      INTEGER
        {
            masterToSlave(1),
            slaveToMaster(2)
        }
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "Represents status which the AC switches to.
        The following values are supported:
        masterToSlave  - The AC switches from master to slave.
        slaveToMaster  - The AC switches from slave to master.
        "
    ::= { ibDot11ACMtTrapVarObjects 3 }

ibDot11ACMtTrapTnlAPName OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "Represents name of the AP which sets up the tunnel with the AC.
        "
