ibDot11ACIFLoadInfoTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbDot11ACIFLoadInfoEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Represents AP number information of interface."
    ::= { ibDot11ACLoadInfo 3 }

ibDot11ACIFLoadInfoEntry OBJECT-TYPE
    SYNTAX      IbDot11ACIFLoadInfoEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry represents AP number information of an interface"
    INDEX
        {
            ibDot11ACIfIndex
        }
    ::= { ibDot11ACIFLoadInfoTable 1 }

IbDot11ACIFLoadInfoEntry ::= SEQUENCE
    {
        ibDot11ACIfIndex       Integer32,
        ibDot11ACIfApNum       Integer32,
        ibDot11ACIfStaNum      Integer32,
        ibDot11ACIfName        OCTET STRING
    }

ibDot11ACIfIndex OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "Represents index of AC interface."
    ::= { ibDot11ACIFLoadInfoEntry 1 }

ibDot11ACIfApNum OBJECT-TYPE
