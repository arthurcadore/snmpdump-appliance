IB-RADIUS-MIB DEFINITIONS ::= BEGIN
    IMPORTS
        ibCommon
            FROM IB-OID-MIB
        IpAddress, Integer32, Counter32, OBJECT-TYPE, MODULE-IDENTITY, NOTIFICATION-TYPE,
        Unsigned32,Counter64, TimeTicks
            FROM SNMPv2-SMI
        RowStatus, TruthValue, DisplayString
            FROM SNMPv2-TC
        InetAddressType, InetAddress
            FROM INET-ADDRESS-MIB
        Ipv6Address
            FROM IPV6-TC
        radiusAuthServerIndex, radiusAuthServerAddress, radiusAuthClientServerPortNumber
            FROM RADIUS-AUTH-CLIENT-MIB
        radiusAccServerIndex, radiusAccServerAddress, radiusAccClientServerPortNumber
            FROM RADIUS-ACC-CLIENT-MIB;

    ibRadius MODULE-IDENTITY
    LAST-UPDATED "202203031800Z"            -- March 3, 2022 at 18:00 GMT
    ORGANIZATION
        "Intelbras S.A. Industria de Telecomunicacao Eletronica Brasileria."
    CONTACT-INFO
        ""
    DESCRIPTION
        "The IB-RADIUS-MIB contains objects to
        Manage configuration and Monitor running state
        for RADIUS feature."
    REVISION "202203031800Z"        -- March 3, 2022 at 18:00 GMT
    DESCRIPTION
         "Modified node ibRadiusAccCachePackets."
    REVISION "202112131800Z"        -- December 13 2021 at 18:00 GMT
    DESCRIPTION
         "Added node ibRadiusAccCachePacketTrap.
         Added node ibRadiusAccCacheVarObjects.
         Added node ibRadiusAccCacheNotifications.
         Added node ibRadiusAccCacheMaxPackets.
         Added node ibRadiusAccCacheUpperValue.
         Added node ibRadiusAccCacheLowerValue.
         Added node ibRadiusAccCacheMemoryUsage.
         Added node ibRadiusAccCachePackets.
         Added node ibRadiusAccCacheAlarm.
         Added node ibRadiusAccCacheResume.
         Added node ibRadiusAccCachePacketDiscard."
    REVISION "202109061800Z"        -- Sept 06, 2021 at 18:00 GMT
    DESCRIPTION
         "Added node ibRdAccStopPktTimeResend.
         Modified the properties of ibRdGroupName.
         Modified the properties of ibRdAccGroupName."
    REVISION "201901121800Z"        -- January 12, 2019 at 18:00 GMT
    DESCRIPTION
        "Added node ibRadiusAuthenticationServerUpTrap.
         Added node ibRadiusAccountingServerUpTrap.
         Added node ibRadiusAuthenticationServerDownTrap.
         Added node ibRadiusAccountingServerDownTrap."
    REVISION "201611031800Z"        -- November 3, 2016 at 18:00 GMT
    DESCRIPTION
        "Modified the value range of ibRdAccRealTime.
        Added node ibRdAccRealTimeUnit."
    REVISION "201406071800Z"        -- June 7, 2014 at 18:00 GMT
    DESCRIPTION
        "Modified the description of ibRdSecondaryAuthRowStatus.
        Modified the description of ibRdSecondaryAccRowStatus."
    ::= { ibCommon 13 }

    ibRdObjects OBJECT IDENTIFIER ::= { ibRadius 1 }

    ibRdInfoTable  OBJECT-TYPE
        SYNTAX SEQUENCE OF IbRdInfoEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
        "The (conceptual) table listing RADIUS authentication servers."
    ::= { ibRdObjects 1 }

    ibRdInfoEntry OBJECT-TYPE
        SYNTAX IbRdInfoEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
        "An entry (conceptual row) representing a RADIUS authentication server."
        INDEX { ibRdGroupName }
    ::= { ibRdInfoTable 1 }

    IbRdInfoEntry ::= SEQUENCE {
        ibRdGroupName          DisplayString,
        ibRdPrimAuthIp         IpAddress,
        ibRdPrimUdpPort        Integer32,
        ibRdPrimState          INTEGER,
        ibRdSecAuthIp          IpAddress,
        ibRdSecUdpPort         Integer32,
        ibRdSecState           INTEGER,
        ibRdKey                DisplayString,
        ibRdRetry              Integer32,
        ibRdTimeout            Integer32,
        ibRdPrimAuthIpAddrType InetAddressType,
        ibRdPrimAuthIpAddr     InetAddress,
        ibRdSecAuthIpAddrType  InetAddressType,
        ibRdSecAuthIpAddr      InetAddress,
        ibRdServerType         INTEGER,
        ibRdQuietTime          Integer32,
        ibRdUserNameFormat     INTEGER,
        ibRdRowStatus          RowStatus,
        ibRdSecKey             DisplayString,
        ibRdPrimVpnName        DisplayString,
        ibRdSecVpnName         DisplayString,
        ibRdAuthNasIpAddrType  InetAddressType,
        ibRdAuthNasIpAddr      IpAddress,
        ibRdAuthNasIpv6Addr    Ipv6Address
        }

    ibRdGroupName OBJECT-TYPE
        SYNTAX DisplayString(SIZE (1..32))
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
        "The name of the RADIUS authentication group referred to in this table
        entry."
    ::= { ibRdInfoEntry 1 }


    ibRdPrimAuthIp OBJECT-TYPE
        SYNTAX IpAddress
        MAX-ACCESS read-create
        STATUS deprecated
        DESCRIPTION
        "The IP address of primary RADIUS authentication server."
    ::= { ibRdInfoEntry 2 }

    ibRdPrimUdpPort OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
        "The UDP port the client is using to send requests to
        primary RADIUS authentication server.  Default value is 1812."
    ::= { ibRdInfoEntry 3 }

    ibRdPrimState OBJECT-TYPE
        SYNTAX INTEGER
        {
            active(1),
            block(2)
        }
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
        "The state of the primary RADIUS authentication server.
        1 (active) The primary authentication server is in active state.
        2 (block)  The primary authentication server is in block state."
    ::= { ibRdInfoEntry 4 }

    ibRdSecAuthIp OBJECT-TYPE
        SYNTAX IpAddress
        MAX-ACCESS read-create
        STATUS deprecated
        DESCRIPTION
        "The IP address of secondary RADIUS authentication server."
    ::= { ibRdInfoEntry 5 }

    ibRdSecUdpPort OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
        "The UDP port the client is using to send requests to
        secondary RADIUS authentication server.  Default value is 1812."
    ::= { ibRdInfoEntry 6 }

    ibRdSecState OBJECT-TYPE
        SYNTAX INTEGER
        {
            active(1),
            block(2)
        }
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
        "The state of the secondary RADIUS authentication server.
        1 (active) The secondary authentication server is in active state.
        2 (block)  The secondary authentication server is in block state."
    ::= { ibRdInfoEntry 7 }

    ibRdKey OBJECT-TYPE
        SYNTAX DisplayString(SIZE (0..64))
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
        "The secret shared between the RADIUS client and RADIUS
        authentication server used in encoding and decoding
        sensitive data.
        When read, ibRdKey always returns an Octet String of length zero."
    ::= { ibRdInfoEntry 8 }

    ibRdRetry OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
        "The number of attempts the client will make when trying to send
        requests to a server before it will consider the attempt failed.
        Default value is 3."
    ::= { ibRdInfoEntry 9 }

    ibRdTimeout OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
        "The timeout value the client will use when sending requests to a server.
        The unit is second.  Default value is 3."
    ::= { ibRdInfoEntry 10 }

    ibRdPrimAuthIpAddrType OBJECT-TYPE
        SYNTAX InetAddressType
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
        "The IP addresses type (IPv4 or IPv6) of primary RADIUS authentication
        server."
    ::= { ibRdInfoEntry 11 }

    ibRdPrimAuthIpAddr OBJECT-TYPE
        SYNTAX InetAddress
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
        "The IP address of primary RADIUS authentication server."
    ::= { ibRdInfoEntry 12 }

    ibRdSecAuthIpAddrType OBJECT-TYPE
        SYNTAX InetAddressType
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
        "The IP addresses type (IPv4 or IPv6) of secondary RADIUS authentication
        server."
    ::= { ibRdInfoEntry 13 }

    ibRdSecAuthIpAddr OBJECT-TYPE
        SYNTAX InetAddress
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
        "The IP address of secondary RADIUS authentication server."
    ::= { ibRdInfoEntry 14 }

    ibRdServerType OBJECT-TYPE
        SYNTAX INTEGER
        {
            standard(1),
            iphotel(2),
            portal(3),
            extended(4)
        }
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
        "Specify the type of RADIUS server.
        1 (standard) - Server based on RFC protocol(s).
        2 (iphotel) - Server for IP-Hotel or 201+ system.
        3 (portal) - Server for iTellin Portal system.
        4 (extended) - Server based on RADIUS extensions.
        Default type is standard."
    ::= { ibRdInfoEntry 15 }

    ibRdQuietTime OBJECT-TYPE
        SYNTAX Integer32 (0..255)
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
        "The time for server returning active.  The unit is minute.
         When the value is 0, the server state retains active.  Default value is 5."
    ::= { ibRdInfoEntry 16 }

    ibRdUserNameFormat OBJECT-TYPE
        SYNTAX INTEGER
        {
            withoutdomain(1),
            withdomain(2),
            keeporignal(3)
        }
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
        "Specify the user-name format that is sent to RADIUS server.
        1 (withoutdomain) - send the user-name without domain.
        2 (withdomain) - send the user-name with domain.
        3 (keeporignal) - send the user-name as it is entered.
        Default format is withdomain."
    ::= { ibRdInfoEntry 17 }


    ibRdRowStatus   OBJECT-TYPE
        SYNTAX   RowStatus
        MAX-ACCESS  read-create
        STATUS   current
        DESCRIPTION
        "This object is responsible for managing the creation, deletion and
        modification of rows, which support active status and CreateAndGo,
        Destroy operation.  To create a new row, ibRdGroupName must be specified.
        To destroy an existent row, the ibRdGroupName MUST NOT be referred by
        ibDomainTable in ibDomainRadiusGroupName column."
    ::= { ibRdInfoEntry 18 }


    ibRdSecKey OBJECT-TYPE
        SYNTAX DisplayString(SIZE (0..64))
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
        "The secret shared between the RADIUS client and the secondary RADIUS
        authentication server used in encoding and decoding
        sensitive data.
        When read, ibRdSecKey always returns an Octet String of length zero."
    ::= { ibRdInfoEntry 19 }

    ibRdPrimVpnName OBJECT-TYPE
        SYNTAX DisplayString(SIZE (0..31))
        MAX-ACCESS  read-create
        STATUS current
        DESCRIPTION
        "The human-readable name of the VPN in which the primary RADIUS
        authentication server is placed."
    ::= { ibRdInfoEntry 20 }

    ibRdSecVpnName OBJECT-TYPE
        SYNTAX DisplayString(SIZE (0..31))
        MAX-ACCESS  read-create
        STATUS current
        DESCRIPTION
        "The human-readable name of the VPN in which the secondary RADIUS
        authentication server is placed."
    ::= { ibRdInfoEntry 21 }

    ibRdAuthNasIpAddrType OBJECT-TYPE
        SYNTAX InetAddressType
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
        "The type (IPv4 or IPv6) of the source IP used to communicate with
        RADIUS authentication server."
    ::= { ibRdInfoEntry 22 }

    ibRdAuthNasIpAddr OBJECT-TYPE
        SYNTAX IpAddress
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
        "The source IPv4 address used to communicate with the RADIUS authentication
        server."
    ::= { ibRdInfoEntry 23 }

    ibRdAuthNasIpv6Addr OBJECT-TYPE
        SYNTAX Ipv6Address
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
        "The source IPv6 address used to communicate with the RADIUS authentication
        server."
    ::= { ibRdInfoEntry 24 }

-- ***********************************************************************
--
--  Accounting Server Table
--
-- ***********************************************************************
    ibRdAccInfoTable  OBJECT-TYPE
        SYNTAX SEQUENCE OF IbRdAccInfoEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
        "The (conceptual) table listing RADIUS accounting servers."
    ::= { ibRdObjects 2 }

    ibRdAccInfoEntry OBJECT-TYPE
        SYNTAX IbRdAccInfoEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
        "An entry (conceptual row) representing a RADIUS accounting server."
        INDEX { ibRdAccGroupName }
    ::= { ibRdAccInfoTable 1 }


    IbRdAccInfoEntry ::= SEQUENCE {
        ibRdAccGroupName           DisplayString,
        ibRdPrimAccIpAddrType      InetAddressType,
        ibRdPrimAccIpAddr          InetAddress,
        ibRdPrimAccUdpPort         Integer32,
        ibRdPrimAccState           INTEGER,
        ibRdSecAccIpAddrType       InetAddressType,
        ibRdSecAccIpAddr           InetAddress,
        ibRdSecAccUdpPort          Integer32,
        ibRdSecAccState            INTEGER,
        ibRdAccKey                 DisplayString,
        ibRdAccRetry               Integer32,
        ibRdAccTimeout             Integer32,
        ibRdAccServerType          INTEGER,
        ibRdAccQuietTime           Integer32,
        ibRdAccFailureAction       INTEGER,
        ibRdAccRealTime            Integer32,
        ibRdAccRealTimeRetry       Integer32,
        ibRdAccSaveStopPktEnable   TruthValue,
        ibRdAccStopRetry           Integer32,
        ibRdAccDataFlowUnit        INTEGER,
        ibRdAccPacketUnit          INTEGER,
        ibRdAccRowStatus           RowStatus,
        ibRdAcctOnEnable           TruthValue,
        ibRdAcctOnSendTimes        Integer32,
        ibRdAcctOnSendInterval     Integer32,
        ibRdSecAccKey              DisplayString,
        ibRdPrimAccVpnName         DisplayString,
        ibRdSecAccVpnName          DisplayString,
        ibRdAccNasIpAddrType       InetAddressType,
        ibRdAccNasIpAddr           IpAddress,
        ibRdAccNasIpv6Addr         Ipv6Address,
        ibRdAccRealTimeUnit        INTEGER,
        ibRdAccStopPktTimeResend   Integer32
        }

    ibRdAccGroupName OBJECT-TYPE
        SYNTAX DisplayString(SIZE (1..32))
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
        "The name of the RADIUS group referred to in this table entry."
    ::= { ibRdAccInfoEntry 1 }

    ibRdPrimAccIpAddrType OBJECT-TYPE
        SYNTAX      InetAddressType
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
        "The IP addresses type (IPv4 or IPv6) of primary RADIUS accounting server."
    ::= { ibRdAccInfoEntry 2 }

    ibRdPrimAccIpAddr OBJECT-TYPE
        SYNTAX      InetAddress
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
        "The IP address of primary RADIUS accounting server."
    ::= { ibRdAccInfoEntry 3 }

    ibRdPrimAccUdpPort OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
        "The UDP port the client is using to send requests to primary
        RADIUS accounting server.  Default value is 1813."
    ::= { ibRdAccInfoEntry 4 }

    ibRdPrimAccState OBJECT-TYPE
        SYNTAX INTEGER
        {
            active(1),
            block(2)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
        "The state of the primary RADIUS accounting server.
        1 (active) The primary accounting server is in active state.
        2 (block)  The primary accounting server is in block state."
    ::= { ibRdAccInfoEntry 5 }

    ibRdSecAccIpAddrType OBJECT-TYPE
        SYNTAX      InetAddressType
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
        "The IP addresses type (IPv4 or IPv6) of secondary RADIUS accounting
        server."
    ::= { ibRdAccInfoEntry 6 }

    ibRdSecAccIpAddr OBJECT-TYPE
        SYNTAX      InetAddress
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
        "The IP address of secondary RADIUS accounting server."
    ::= { ibRdAccInfoEntry 7 }

    ibRdSecAccUdpPort OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
        "The UDP port the client is using to send requests to secondary
        RADIUS accounting server.  Default value is 1813."
    ::= { ibRdAccInfoEntry 8 }

    ibRdSecAccState OBJECT-TYPE
        SYNTAX INTEGER
        {
            active(1),
            block(2)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
        "The state of the secondary RADIUS accounting server.
        1 (active) The secondary accounting server is in active state.
        2 (block)  The secondary accounting server is in block state."
    ::= { ibRdAccInfoEntry 9 }

    ibRdAccKey OBJECT-TYPE
        SYNTAX DisplayString(SIZE (0..64))
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
        "The secret shared between the RADIUS client and RADIUS
        accounting server used in encoding and decoding sensitive data.
        When read, ibRdAccKey always returns an Octet String of length zero."
    ::= { ibRdAccInfoEntry 10 }


    ibRdAccRetry OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
        "The number of attempt the client will make when trying to send
        requests to a server before it will consider the attempt failed.
        Default value is 3."
    ::= { ibRdAccInfoEntry 11 }

    ibRdAccTimeout OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
        "The timeout value the client will use when sending requests to a server.
        The unit is second.  Default value is 3."
    ::= { ibRdAccInfoEntry 12 }

    ibRdAccServerType OBJECT-TYPE
        SYNTAX INTEGER
        {
            standard(1),
            iphotel(2),
            portal(3),
            extended(4)
        }
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
        "Specify the type of RADIUS server.
        1 (standard) - Server based on RFC protocol(s).
        2 (iphotel) - Server for IP-Hotel or 201+ system.
        3 (portal) - Server for iTellin Portal system.
        4 (extended) - Server based on RADIUS extensions.
        Default type is standard."
    ::= { ibRdAccInfoEntry 13 }

    ibRdAccQuietTime OBJECT-TYPE
        SYNTAX Integer32 (0..255)
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
        "The time for server returning active.  The unit is minute.
         When the value is 0, the server state retains active.  Default value is 5."
    ::= { ibRdAccInfoEntry 14 }

    ibRdAccFailureAction OBJECT-TYPE
        SYNTAX INTEGER
        {
            ignore(1),
            reject(2)
        }
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
        "Defines the action that authentication should take if
        authentication succeeds but the associated accounting start
        fails.
        1 (ignore) - treat as authentication success; ignore
            accounting start failure.
        2 (reject) - treat as authentication failed if
            corresponding accounting start fails.
        Default value is 1(reject)."
    ::= { ibRdAccInfoEntry 15 }

    ibRdAccRealTime OBJECT-TYPE
        SYNTAX Integer32 (0..71582)
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
        "Interval of realtime-accounting packets.
        The unit is defined by ibRdAccRealTimeUnit.
        When the value is 0, the device doesn't send realtime-accounting
        packets.  Default value is 12 minutes."
    ::= { ibRdAccInfoEntry 16 }

    ibRdAccRealTimeRetry OBJECT-TYPE
        SYNTAX Integer32 (1..255)
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
        "The number of attempt the client will make when trying to send
        realtime-accounting packet to accounting server before it will
        consider the attempt failed.  Default value is 5."
        DEFVAL { 5 }
    ::= { ibRdAccInfoEntry 17 }

    ibRdAccSaveStopPktEnable OBJECT-TYPE
        SYNTAX TruthValue
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
        "The control of whether save stop-accounting packet in local buffer
        and resend later when the accounting server doesn't respond.  When
        SaveStopPktEnable is set to false, the value of AccStopRetry will be
        ignored.  Default value is true."
    ::= { ibRdAccInfoEntry 18 }

    ibRdAccStopRetry OBJECT-TYPE
        SYNTAX Integer32 (10..65535)
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
        "The number of attempt the client will make when trying to send
        stop-accounting packet to accounting server.  Default value is 500."
    ::= { ibRdAccInfoEntry 19 }

    ibRdAccDataFlowUnit OBJECT-TYPE
        SYNTAX INTEGER
        {
            byte(1),
            kiloByte(2),
            megaByte(3),
            gigaByte(4)
        }
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
        "Specify data flow format that is sent to RADIUS server.  The value
        SHOULD be set the same as the value of corresponding server.
        1 (byte) -  Specify 'byte' as the unit of data flow.
        2 (kiloByte) - Specify 'kilo-byte' as the unit of data flow.
        3 (megaByte) - Specify 'mega-byte' as the unit of data flow.
        4 (gigaByte) - Specify 'giga-byte' as the unit of data flow.
        Default value is 1."
    ::= { ibRdAccInfoEntry 20 }

    ibRdAccPacketUnit OBJECT-TYPE
        SYNTAX INTEGER
        {
            onePacket(1),
            kiloPacket(2),
            megaPacket(3),
            gigaPacket(4)
        }
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
        "Specify packet format that is sent to RADIUS server.  The value
        SHOULD be set the same as the value of corresponding server.
        1 (onePacket)  - Specify 'one-packet' as the unit of packet.
        2 (kiloPacket) - Specify 'kilo-packet' as the unit of packet.
        3 (megaPacket) - Specify 'mega-packet' as the unit of packet.
        4 (gigaPacket) - Specify 'giga-packet' as the unit of packet.
        Default value is 1."
    ::= { ibRdAccInfoEntry 21 }

    ibRdAccRowStatus   OBJECT-TYPE
        SYNTAX   RowStatus
        MAX-ACCESS  read-create
        STATUS   current
        DESCRIPTION
        "This object is responsible for managing the creation, deletion and
        modification of rows, which support active status and CreateAndGo,
        Destroy operation.  To create a new row, ibRdAccGroupName must be specified.
        To destroy an existent row, the ibRdAccGroupName MUST NOT be referred by
        ibDomainTable in ibDomainRadiusGroupName column."
    ::= { ibRdAccInfoEntry 22 }

    ibRdAcctOnEnable OBJECT-TYPE
        SYNTAX TruthValue
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
        "The control of Accounting-On function.  The Accounting-On function is
        used by the client to mark the start of accounting (for example, upon
        booting) by sending Accounting-On packets and to mark the end of
        accounting (for example, just before a scheduled reboot) by sending
        Accounting-Off packets.  Default value is false."
    ::= { ibRdAccInfoEntry 23 }

    ibRdAcctOnSendTimes OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
        "The number of Accounting-On packets the client will send before it
        considers the accounting server failed.  Default value is 50."
        DEFVAL { 50 }
    ::= { ibRdAccInfoEntry 24 }

    ibRdAcctOnSendInterval OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
        "Interval of Accounting-On packets.  The unit is second.
        Default value is 3."
        DEFVAL { 3 }
    ::= { ibRdAccInfoEntry 25 }


    ibRdSecAccKey OBJECT-TYPE
        SYNTAX DisplayString(SIZE (0..64))
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
        "The secret shared between the RADIUS client and the secondary RADIUS
        accounting server used in encoding and decoding sensitive data.
        When read, ibRdSecAccKey always returns an Octet String of length zero."
    ::= { ibRdAccInfoEntry 26 }

    ibRdPrimAccVpnName OBJECT-TYPE
        SYNTAX DisplayString(SIZE (0..31))
        MAX-ACCESS  read-create
        STATUS current
        DESCRIPTION
        "The human-readable name of the VPN in which the primary RADIUS
        accounting server is placed."
    ::= { ibRdAccInfoEntry 27 }

    ibRdSecAccVpnName OBJECT-TYPE
        SYNTAX DisplayString(SIZE (0..31))
        MAX-ACCESS  read-create
        STATUS current
        DESCRIPTION
        "The human-readable name of the VPN in which the secondary RADIUS
        accounting server is placed."
    ::= { ibRdAccInfoEntry 28 }

    ibRdAccNasIpAddrType OBJECT-TYPE
        SYNTAX InetAddressType
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
        "The type (IPv4 or IPv6) of the source IP used to communicate with
        RADIUS accounting server."
    ::= { ibRdAccInfoEntry 29 }

    ibRdAccNasIpAddr OBJECT-TYPE
        SYNTAX IpAddress
