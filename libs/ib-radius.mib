    IbRadiusSchAccEntry ::= SEQUENCE {
        ibRadiusSchAccGroupName           DisplayString,
        ibRadiusSchAccPrimIpAddr          IpAddress,
        ibRadiusSchAccPrimUdpPort         Integer32,
        ibRadiusSchAccPrimKey             DisplayString,
        ibRadiusSchAccSecIpAddr           IpAddress,
        ibRadiusSchAccSecUdpPort          Integer32,
        ibRadiusSchAccSecKey              DisplayString,
        ibRadiusSchAccRowStatus           RowStatus
        }

    ibRadiusSchAccGroupName OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
        "The name of the RADIUS accounting server group referred to in this table entry."
    ::= { ibRadiusSchAccEntry 1 }

    ibRadiusSchAccPrimIpAddr OBJECT-TYPE
        SYNTAX      IpAddress
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
        "The IP address of primary RADIUS accounting server."
    ::= { ibRadiusSchAccEntry 2 }

    ibRadiusSchAccPrimUdpPort OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
        "The UDP port the client is using to send requests to primary
        RADIUS accounting server.  Default value is 1813."
        DEFVAL { 1813 }
    ::= { ibRadiusSchAccEntry 3 }

    ibRadiusSchAccPrimKey OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
        "The secret shared between the RADIUS client and the primary RADIUS
        accounting server used in encoding and decoding sensitive data."
    ::= { ibRadiusSchAccEntry 4 }

    ibRadiusSchAccSecIpAddr OBJECT-TYPE
        SYNTAX      IpAddress
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
        "The IP address of secondary RADIUS accounting server."
    ::= { ibRadiusSchAccEntry 5 }

    ibRadiusSchAccSecUdpPort OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
        "The UDP port the client is using to send requests to secondary
        RADIUS accounting server.  Default value is 1813."
        DEFVAL { 1813 }
    ::= { ibRadiusSchAccEntry 6 }

    ibRadiusSchAccSecKey OBJECT-TYPE
        SYNTAX DisplayString
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
        "The secret shared between the RADIUS client and the secondary RADIUS
        accounting server used in encoding and decoding sensitive data."
    ::= { ibRadiusSchAccEntry 7 }

    ibRadiusSchAccRowStatus   OBJECT-TYPE
        SYNTAX   RowStatus
        MAX-ACCESS  read-create
        STATUS   current
        DESCRIPTION
        "This object is responsible for managing the creation, deletion and
        modification of rows, which support active status and CreateAndGo,
        Destroy operation.
        To create a new row, ibRadiusSchAccGroupName must be specified,
        and this action will create a corresponding domain that
        ibDomainRadiusGroupName is the same as ibRadiusSchAccGroupName.
        To destroy an existent row, the ibRadiusSchAccGroupName MUST NOT be
        referred by ibDomainTable in ibDomainRadiusGroupName column."
    ::= { ibRadiusSchAccEntry 8 }
-- ***********************************************************************
--
-- RADIUS ACCOUNT STATISTIC
--
-- ***********************************************************************
ibRadiusStatistic OBJECT IDENTIFIER ::= { ibRadius 6 }

ibRadiusStatAccReq OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "It shows the number of radius account request to the
        radius server."
    ::= { ibRadiusStatistic 1 }

ibRadiusStatAccAck OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "It shows the number of radius account response from the
         radius server."
    ::= { ibRadiusStatistic 2 }

ibRadiusStatLogoutReq OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "It shows the number of logout request to the radius server."
    ::= { ibRadiusStatistic 3 }

ibRadiusStatLogoutAck OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "It shows the number of logout response from the radius server."
    ::= { ibRadiusStatistic 4 }

ibRadiusServerTrapVarObjects OBJECT IDENTIFIER ::= { ibRadius 7 }

ibRadiusServerFirstTrapTime OBJECT-TYPE
    SYNTAX      TimeTicks
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "Represents the first trap time."
    ::= { ibRadiusServerTrapVarObjects 1 }

-- ***********************************************************************
--
-- RADIUS STOP-ACCOUNTING CACHE TRAP
--
-- ***********************************************************************
ibRadiusAccCachePacketTrap OBJECT IDENTIFIER ::= { ibRadius 8 }

ibRadiusAccCacheNotifications OBJECT IDENTIFIER ::= { ibRadiusAccCachePacketTrap 0 }

ibRadiusAccCacheAlarm NOTIFICATION-TYPE
    OBJECTS     { ibRadiusAccCacheMaxPackets,
                 ibRadiusAccCacheUpperValue}
    STATUS      current
    DESCRIPTION
        "This trap is generated when the ratio of the number of buffered
         RADIUS stop-accounting packets to the maximum number reaches
         the upper threshold."
    ::= { ibRadiusAccCacheNotifications 1 }

ibRadiusAccCacheResume NOTIFICATION-TYPE
    OBJECTS     { ibRadiusAccCacheMaxPackets,
                 ibRadiusAccCacheLowerValue}
    STATUS      current
    DESCRIPTION
        "This trap is generated when the ratio of the number of buffered
         RADIUS stop-accounting packets to the maximum number drops below
         the lower threshold."
    ::= { ibRadiusAccCacheNotifications 2 }

ibRadiusAccCachePacketDiscard NOTIFICATION-TYPE
    OBJECTS     { ibRadiusAccCacheMemoryUsage,
                 ibRadiusAccCachePackets}
    STATUS      current
    DESCRIPTION
        "This trap is generated when the device discards
         RADIUS stop-accounting packets because the memory usage
         has reached the minor alarm threshold."
    ::= { ibRadiusAccCacheNotifications 3 }

ibRadiusAccCacheVarObjects OBJECT IDENTIFIER ::= { ibRadiusAccCachePacketTrap 1 }

ibRadiusAccCacheMaxPackets OBJECT-TYPE
    SYNTAX Integer32 (256..2147483647)
    MAX-ACCESS accessible-for-notify
    STATUS current
    DESCRIPTION
        "Maximum number of RADIUS stop-accounting packets that can be buffered."
    ::= { ibRadiusAccCacheVarObjects 1 }

ibRadiusAccCacheUpperValue OBJECT-TYPE
    SYNTAX Integer32 (1..100)
    MAX-ACCESS accessible-for-notify
    STATUS current
    DESCRIPTION
        "Upper threshold for the ratio of the number of buffered
         RADIUS stop-accounting packets to the maximum number of
         RADIUS stop-accounting packets that can be buffered."
    ::= { ibRadiusAccCacheVarObjects 2 }

ibRadiusAccCacheLowerValue OBJECT-TYPE
    SYNTAX Integer32 (0..99)
    MAX-ACCESS accessible-for-notify
    STATUS current
    DESCRIPTION
        "Lower threshold for the ratio of the number of buffered
         RADIUS stop-accounting packets to the maximum number of
         RADIUS stop-accounting packets that can be buffered."
    ::= { ibRadiusAccCacheVarObjects 3 }

ibRadiusAccCacheMemoryUsage OBJECT-TYPE
    SYNTAX Integer32 (0..100)
    MAX-ACCESS accessible-for-notify
    STATUS current
    DESCRIPTION
        "Current memory usage of the device."
    ::= { ibRadiusAccCacheVarObjects 4 }

ibRadiusAccCachePackets OBJECT-TYPE
    SYNTAX Integer32 (0..2147483647)
    MAX-ACCESS accessible-for-notify
    STATUS current
    DESCRIPTION
        "Current number of buffered RADIUS stop-accounting packets."
    ::= { ibRadiusAccCacheVarObjects 5 }

END
