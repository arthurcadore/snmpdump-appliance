ibPosaTables OBJECT IDENTIFIER ::= { ibPosa 2 }

--
-- definition of ibPosaAppTable
--

ibPosaAppTable OBJECT-TYPE
    SYNTAX SEQUENCE OF IbPosaAppEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "This table shows the configuration information and
        running status of POS application."
    ::= { ibPosaTables 1 }

ibPosaAppEntry OBJECT-TYPE
    SYNTAX IbPosaAppEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "This table shows the configuration information and
        running status of POS application."
    INDEX { ibPosaAppID }
    ::= { ibPosaAppTable 1 }

IbPosaAppEntry ::=
    SEQUENCE {
        ibPosaAppID
            Integer32,
        ibPosaAppServiceType
            IbAppServiceType,
        ibPosaAppIfIndex
            Integer32,
        ibPosaAppMode
            IbAppMode,
        ibPosaAppHostIPType
            InetAddressType,
        ibPosaAppHostIP
            InetAddress,
        ibPosaAppHostPort
            Integer32,
        ibPosaAppRouterIPType
            InetAddressType,
        ibPosaAppRouterIP
            InetAddress,
        ibPosaAppKeepAliveInterval
            Integer32,
        ibPosaAppKeepAliveCount
            Integer32,
        ibPosaAppConnectTimeout
            Integer32,
        ibPosaAppState
            IbPeerState,
        ibPosaAppRowStatus
            RowStatus,
        ibPosaAppName
            OCTET STRING,
        ibPosaCallerIDTransEnable
            TruthValue,
        ibPosaTpduChangeStrategy
            IbTpduChangeStrategy,
        ibPosaBackupAppID
            Integer32,
        ibPosaQuietTimeOut
            Integer32,
        ibPosaAppHello
            TruthValue,
        ibPosaAppHelloInterval
            Integer32,
        ibPosaAppRouterPort
            Integer32
     }

ibPosaAppID OBJECT-TYPE
    SYNTAX Integer32 (1..1024)
    MAX-ACCESS accessible-for-notify
    STATUS current
    DESCRIPTION
        "The identification of a POS application."
    ::= { ibPosaAppEntry 1 }

ibPosaAppServiceType OBJECT-TYPE
    SYNTAX IbAppServiceType
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "The service type of a POS application.  It means the way
        in which the router connects with the application server."
    DEFVAL { tcp }
    ::= { ibPosaAppEntry 2 }

ibPosaAppIfIndex OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "This attribute is the ifindex through which the router connecting with
        the application server, if the service type of POS application is flow(2).
        It is invalid if the service type is tcp(1).
        At preasent, it supports 5 types of interface: FCM, SERIAL, ASYNC,
        AM (Analog Modem) and AUX."
    REFERENCE
        "ifindex"
    ::= { ibPosaAppEntry 3 }

ibPosaAppMode OBJECT-TYPE
    SYNTAX IbAppMode
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "The work mode of a POS application."
    DEFVAL { normal }
    ::= { ibPosaAppEntry 4 }

ibPosaAppHostIPType OBJECT-TYPE
    SYNTAX InetAddressType
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "The IP address type of the application server.  And now it must be IPV4.
        It is invalid if the service type is flow(2)."
    ::= { ibPosaAppEntry 5 }

ibPosaAppHostIP OBJECT-TYPE
    SYNTAX InetAddress
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "The IP address of the application server.  And now it only supports IPV4.
        It is invalid if the service type is flow(2)."
    ::= { ibPosaAppEntry 6 }

ibPosaAppHostPort OBJECT-TYPE
    SYNTAX Integer32 (0..65535)
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "The listening port of the application server.  If the information of
        the application server is not specified, this object will return the
        value 0.
        It is invalid if the service type is flow(2)."
    ::= { ibPosaAppEntry 7 }

ibPosaAppRouterIPType OBJECT-TYPE
    SYNTAX InetAddressType
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "The source IP address type of the access router.  Now it must be ipv4.
        It is invalid if the service type is flow(2)."
    ::= { ibPosaAppEntry 8 }

ibPosaAppRouterIP OBJECT-TYPE
    SYNTAX InetAddress
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "The source IP address of access router used in connecting with
        application server.  Now it only supports ipv4.
        It is invalid if the service type is flow(2)."
    ::= { ibPosaAppEntry 9 }

ibPosaAppKeepAliveInterval OBJECT-TYPE
    SYNTAX Integer32 (1..7200)
    UNITS "seconds"
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "The TCP keep alive interval of the connection between the router and
        the application server.  In unit of seconds.
        The default value is 2 seconds.
        It is valid only when the service type is tcp(1)."
    DEFVAL { 2 }
    ::= { ibPosaAppEntry 10 }

ibPosaAppKeepAliveCount OBJECT-TYPE
    SYNTAX Integer32 (2..100)
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "The maximum number of sending TCP keep alive packets.
        It is valid only when the service type is tcp(1)."
    DEFVAL { 3 }
    ::= { ibPosaAppEntry 11 }

ibPosaAppConnectTimeout OBJECT-TYPE
    SYNTAX Integer32 (1..20)
    UNITS "seconds"
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "The maximum interval that the router waits for the reply from
        application server after sending the request for establishing a TCP
        connection.  It is invalid if the service type is flow(2)."
    DEFVAL { 20 }
    ::= { ibPosaAppEntry 12 }

ibPosaAppState OBJECT-TYPE
    SYNTAX IbPeerState
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The current running state of application server."
    ::= { ibPosaAppEntry 13 }

ibPosaAppRowStatus OBJECT-TYPE
    SYNTAX RowStatus
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "Operation status."
    ::= { ibPosaAppEntry 14 }

ibPosaAppName OBJECT-TYPE
    SYNTAX OCTET STRING (SIZE (0..32))
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "The description of a POS application."
    ::= { ibPosaAppEntry 15 }

ibPosaCallerIDTransEnable OBJECT-TYPE
    SYNTAX TruthValue
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "Enable or disable the caller-id transmission function."
    DEFVAL { false }
    ::= { ibPosaAppEntry 16 }

ibPosaTpduChangeStrategy OBJECT-TYPE
    SYNTAX IbTpduChangeStrategy
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "Set the strategy to modify the address of the packet from terminals
        before delivering it to application server."
    DEFVAL { changeTpduSrc }
    ::= { ibPosaAppEntry 17 }

ibPosaBackupAppID OBJECT-TYPE
    SYNTAX Integer32 (0..1024)
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "Specify the backup application ID, 0 indicates not using backup application.
        It is invalid if the service type is flow(2)."
    ::= { ibPosaAppEntry 18 }

ibPosaQuietTimeOut OBJECT-TYPE
    SYNTAX Integer32 (10..600)
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "Specify the wait time for re-activating application server.
        It is invalid if the service type is flow(2)."
    ::= { ibPosaAppEntry 19 }

ibPosaAppHello OBJECT-TYPE
    SYNTAX TruthValue
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "Specify the switch of hello packet for the application server."
    DEFVAL { false }
    ::= { ibPosaAppEntry 20 }

ibPosaAppHelloInterval OBJECT-TYPE
    SYNTAX Integer32 (1..600)
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "Specify the interval time in minute(s) of hello packet."
    DEFVAL { 1 }
    ::= { ibPosaAppEntry 21 }

ibPosaAppRouterPort OBJECT-TYPE
    SYNTAX Integer32 (0..4999)
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "The source port of access router used in connecting with
        application server.  0 indicates not to specify the source port.
        It is valid only if the service type is tcp(1) and mode is normal(1)."
    ::= { ibPosaAppEntry 22 }

--
-- definition of ibPosaTerminalTable
--

ibPosaTerminalTable OBJECT-TYPE
    SYNTAX SEQUENCE OF IbPosaTerminalEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "This table shows the configuration information and
        running status of POS terminal."
    ::= { ibPosaTables 2 }

ibPosaTerminalEntry OBJECT-TYPE
    SYNTAX IbPosaTerminalEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "This table shows the configuration information and
        running status of POS terminal."
    INDEX { ibPosaTerminalID }
    ::= { ibPosaTerminalTable 1 }

IbPosaTerminalEntry ::=
    SEQUENCE {
        ibPosaTerminalID
            Integer32,
        ibPosaTerminalAccessType
            IbTerminalAccessType,
        ibPosaTerminalIfIndex
            Integer32,
        ibPosaTerminalTransAppID
            Integer32,
        ibPosaTerminalListenPort
            Integer32,
        ibPosaTerminalState
            IbPeerState,
        ibPosaTerminalRowStatus
            RowStatus,
        ibPosaTerminalName
            OCTET STRING,
        ibPosaTerminalCfgIfIndex
            Integer32
     }

ibPosaTerminalID OBJECT-TYPE
    SYNTAX Integer32 (1..1024)
    MAX-ACCESS accessible-for-notify
    STATUS current
    DESCRIPTION
        "The identification of a POS terminal."
    ::= { ibPosaTerminalEntry 1 }

ibPosaTerminalAccessType OBJECT-TYPE
    SYNTAX IbTerminalAccessType
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "The access type of a POS terminal."
    DEFVAL { fcm }
    ::= { ibPosaTerminalEntry 2 }

ibPosaTerminalIfIndex OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "The ifindex of the interface through which connects the POS terminal
        and the router.  It is invalid if the access type is TCP."
    REFERENCE
        "ifindex"
    ::= { ibPosaTerminalEntry 3 }

ibPosaTerminalTransAppID OBJECT-TYPE
    SYNTAX Integer32 (0..1024)
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "The identification of the application server to which all the packets
        from this POS terminal will be sent.
        If this object is set to 0, the destination application server to which
        a packet from this POS terminal will be sent is determined by its TPDU
        (Transport Protocol Data Unit) destination address.
        It is invalid if the access type is TCP."
    DEFVAL { 0 }
    ::= { ibPosaTerminalEntry 4 }

ibPosaTerminalListenPort OBJECT-TYPE
    SYNTAX Integer32 (0..65535)
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "The listening port accepting the connection from POS terminals.
        It is valid only when the access type is TCP."
    ::= { ibPosaTerminalEntry 5 }

ibPosaTerminalState OBJECT-TYPE
    SYNTAX IbPeerState
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The current running status of a POS terminal."
    ::= { ibPosaTerminalEntry 6 }

ibPosaTerminalRowStatus OBJECT-TYPE
    SYNTAX RowStatus
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "Operation status."
    ::= { ibPosaTerminalEntry 7 }

ibPosaTerminalName OBJECT-TYPE
    SYNTAX OCTET STRING (SIZE (0..32))
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "The description of a POS terminal."
    ::= { ibPosaTerminalEntry 8 }

ibPosaTerminalCfgIfIndex OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The ifindex of the interface on which the terminal configuration is.
        If it is a physical interface, its value is equal to that of
        ibPosaTerminalIfIndex."
    REFERENCE
        "ifindex"
    ::= { ibPosaTerminalEntry 9 }

--
-- definition of ibPosaMapTable
--

ibPosaMapTable OBJECT-TYPE
    SYNTAX SEQUENCE OF IbPosaMapEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "This table describe the application-mapping information."
    ::= { ibPosaTables 3 }

ibPosaMapEntry OBJECT-TYPE
    SYNTAX IbPosaMapEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "This table describe the application-mapping information."
    INDEX { ibPosaMapSrcCode, ibPosaMapDestCode }
    ::= { ibPosaMapTable 1 }

IbPosaMapEntry ::=
    SEQUENCE {
        ibPosaMapDestCode
            OCTET STRING,
        ibPosaMapAppID
            Integer32,
        ibPosaMapRowStatus
            RowStatus,
        ibPosaMapSrcCode
            OCTET STRING
     }

ibPosaMapDestCode OBJECT-TYPE
    SYNTAX OCTET STRING (SIZE (4..7))
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "The destination code of the mapping item.  Generally, the destination
        code is a 4-character-string.  If the destination address in POS packet
        matches the code, the packet will be deliveried to the corresponding
        application server.  If any code can not be matched, the packet will be
        deliveried to the default server.  A special destination code, 'default',
        is for default mapping item."
    ::= { ibPosaMapEntry 1 }

ibPosaMapAppID OBJECT-TYPE
    SYNTAX Integer32 (1..1024)
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "The identifier of POS application.  It is the index of ibPosaAppTable."
    ::= { ibPosaMapEntry 2 }

ibPosaMapRowStatus OBJECT-TYPE
    SYNTAX RowStatus
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "Operation status."
    ::= { ibPosaMapEntry 3 }

ibPosaMapSrcCode OBJECT-TYPE
    SYNTAX OCTET STRING (SIZE (4..7))
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "The source code of the mapping item.  Generally, the source code is a
        4-character-string.  If the source address in POS packet matches the
        code, the packet will be deliveried to the corresponding application
        server.  If any code can not be matched, the packet will be deliveried
        to the default server.  A special source code, 'default', is for
        default mapping item."
    ::= { ibPosaMapEntry 4 }

--
-- definition of ibPosaFcmStatTable
--

ibPosaFcmStatTable OBJECT-TYPE
    SYNTAX SEQUENCE OF IbPosaFcmStatEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "This table shows the statistics information of FCM interface."
    ::= { ibPosaTables 4 }

ibPosaFcmStatEntry OBJECT-TYPE
    SYNTAX IbPosaFcmStatEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "This table shows the statistics information of FCM interface."
    INDEX { ibPosaFcmStatIfIndex }
    ::= { ibPosaFcmStatTable 1 }

IbPosaFcmStatEntry ::=
    SEQUENCE {
        ibPosaFcmStatIfIndex
            Integer32,
        ibPosaFcmStatTimeoutCnts
            Counter32,
        ibPosaFcmStatConnectFailCnts
            Counter32,
        ibPosaFcmStatTransCnts
            Gauge32,
        ibPosaFcmStatTransSuccessCnts
            Gauge32,
        ibPosaFcmStatTransCntsClear
            TruthValue
     }

ibPosaFcmStatIfIndex OBJECT-TYPE
    SYNTAX Integer32 (0..2147483647)
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "The index of FCM interface."
    ::= { ibPosaFcmStatEntry 1 }

ibPosaFcmStatTimeoutCnts OBJECT-TYPE
    SYNTAX Counter32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The number of disconnections because of trade timeouts."
    ::= { ibPosaFcmStatEntry 2 }

ibPosaFcmStatConnectFailCnts OBJECT-TYPE
    SYNTAX Counter32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The number of connection failures because of unsuccessful negotiations."
    ::= { ibPosaFcmStatEntry 3 }
ibPosaFcmStatTransCnts OBJECT-TYPE
    SYNTAX Gauge32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The number of all transactions made through the interface."
    ::= { ibPosaFcmStatEntry 4 }

ibPosaFcmStatTransSuccessCnts OBJECT-TYPE
    SYNTAX Gauge32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The number of successful transactions made through the interface."
    ::= { ibPosaFcmStatEntry 5 }

ibPosaFcmStatTransCntsClear OBJECT-TYPE
    SYNTAX TruthValue
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "Clear the number of successful transactions and all transactions."
    DEFVAL { false }
    ::= { ibPosaFcmStatEntry 6 }

--
-- definition of ibPosaAppStatTable
--

ibPosaAppStatTable OBJECT-TYPE
    SYNTAX SEQUENCE OF IbPosaAppStatEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "This table shows the statistics information of application."
    ::= { ibPosaTables 5 }

ibPosaAppStatEntry OBJECT-TYPE
    SYNTAX IbPosaAppStatEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "This table shows the statistics information of application."
    INDEX { ibPosaAppID }
    ::= { ibPosaAppStatTable 1 }

IbPosaAppStatEntry ::=
    SEQUENCE {
        ibPosaAppRecvPkts
            Counter32,
        ibPosaAppSendPkts
            Counter32,
        ibPosaAppErrPkts
            Counter32,
        ibPosaAppDistributeErrCnts
            Counter32,
        ibPosaAppInDiscardedPkts
            Counter32,
        ibPosaAppOutDiscardedPkts
            Counter32
     }

ibPosaAppRecvPkts OBJECT-TYPE
    SYNTAX Counter32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The number of packets recieved from the application server."
    ::= { ibPosaAppStatEntry 1 }

ibPosaAppSendPkts OBJECT-TYPE
    SYNTAX Counter32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The number of packets sent to the application server."
    ::= { ibPosaAppStatEntry 2 }

ibPosaAppErrPkts OBJECT-TYPE
    SYNTAX Counter32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The number of inbound packets that contained errors."
    ::= { ibPosaAppStatEntry 3 }

ibPosaAppDistributeErrCnts OBJECT-TYPE
    SYNTAX Counter32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The number of packets that are discarded when no corresponding POS
        terminal can be found."
    ::= { ibPosaAppStatEntry 4 }

ibPosaAppInDiscardedPkts OBJECT-TYPE
    SYNTAX Counter32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The number of inbound packets that are discarded when the incoming
        buffer is full."
    ::= { ibPosaAppStatEntry 5 }

ibPosaAppOutDiscardedPkts OBJECT-TYPE
    SYNTAX Counter32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The number of outbound packets that are discarded because of link
        errors between the router and the application server."
    ::= { ibPosaAppStatEntry 6 }

--
-- definition of ibPosaTerminalStatTable
--

ibPosaTerminalStatTable OBJECT-TYPE
    SYNTAX SEQUENCE OF IbPosaTerminalStatEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "This table shows the statistics information of application."
    ::= { ibPosaTables 6 }

ibPosaTerminalStatEntry OBJECT-TYPE
    SYNTAX IbPosaTerminalStatEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "This table shows the statistics information of application."
    INDEX { ibPosaTerminalID }
    ::= { ibPosaTerminalStatTable 1 }

IbPosaTerminalStatEntry ::=
    SEQUENCE {
        ibPosaTerminalRecvPkts
            Counter32,
        ibPosaTerminalSendPkts
            Counter32,
        ibPosaTerminalErrPkts
            Counter32,
        ibPosaTerminalMapErrCnts
            Counter32,
        ibPosaTerminalInDiscardedPkts
            Counter32,
        ibPosaTerminalOutDiscardedPkts
            Counter32,
        ibPosaTermianlTcpConnectionNumber
            Integer32
     }

ibPosaTerminalRecvPkts OBJECT-TYPE
    SYNTAX Counter32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The number of packets recieved from the POS terminal."
    ::= { ibPosaTerminalStatEntry 1 }

ibPosaTerminalSendPkts OBJECT-TYPE
    SYNTAX Counter32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The number of packets sent to the POS terminal."
    ::= { ibPosaTerminalStatEntry 2 }

ibPosaTerminalErrPkts OBJECT-TYPE
    SYNTAX Counter32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The number of inbound packets that contained errors."
    ::= { ibPosaTerminalStatEntry 3 }

ibPosaTerminalMapErrCnts OBJECT-TYPE
    SYNTAX Counter32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The number of packets that are discarded because of mapping errors."
    ::= { ibPosaTerminalStatEntry 4 }

ibPosaTerminalInDiscardedPkts OBJECT-TYPE
    SYNTAX Counter32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The number of inbound packets that are discarded when the incoming
        buffer is full."
    ::= { ibPosaTerminalStatEntry 5 }

ibPosaTerminalOutDiscardedPkts OBJECT-TYPE
    SYNTAX Counter32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The number of outbound packets that are discarded because of link
        errors between the router and the POS terminal."
    ::= { ibPosaTerminalStatEntry 6 }

ibPosaTermianlTcpConnectionNumber OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The number of TCP connections between the router and the POS terminal."
    ::= { ibPosaTerminalStatEntry 7 }

--
-- definition of ibPosaBatchTerminalTable
--

ibPosaBatchTerminalTable OBJECT-TYPE
    SYNTAX SEQUENCE OF IbPosaBatchTerminalEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "The table of the configuration information relevant to
        a batch of POS terminals."
    ::= { ibPosaTables 7 }

ibPosaBatchTerminalEntry OBJECT-TYPE
    SYNTAX IbPosaBatchTerminalEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "The configuration information relevant to a batch of
        POS terminals."
    INDEX { ifIndex }
    ::= { ibPosaBatchTerminalTable 1 }

IbPosaBatchTerminalEntry ::=
    SEQUENCE {
        ibPosaBatchTerminalFirstID
            Integer32,
        ibPosaBatchTerminalRowStatus
            RowStatus
     }

ibPosaBatchTerminalFirstID OBJECT-TYPE
    SYNTAX Integer32 (1..1024)
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "The first ID of the batch of POS terminals."
    ::= { ibPosaBatchTerminalEntry 1 }

ibPosaBatchTerminalRowStatus OBJECT-TYPE
    SYNTAX RowStatus
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "Operation status."
    ::= { ibPosaBatchTerminalEntry 2 }

--
-- definition of ibPosaTcpTermStatTable
--

ibPosaTcpTermStatTable OBJECT-TYPE
    SYNTAX SEQUENCE OF IbPosaTcpTermStatEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "This table shows the statistics information of terminal whose
        access type is tcp."
    ::= { ibPosaTables 8 }

ibPosaTcpTermStatEntry OBJECT-TYPE
    SYNTAX IbPosaTcpTermStatEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "This table shows the statistics information of terminal whose
        access type is tcp."
    INDEX { ibPosaTcpTermStatIndex }
    ::= { ibPosaTcpTermStatTable 1 }

IbPosaTcpTermStatEntry ::=
    SEQUENCE {
        ibPosaTcpTermStatIndex
            Integer32,
        ibPosaTcpTermStatIPType
            InetAddressType,
        ibPosaTcpTermStatIP
            InetAddress,
        ibPosaTcpTermStatIPMask
            InetAddress,
        ibPosaTcpTermRecvPkts
            Counter64,
        ibPosaTcpTermSendPkts
            Counter64,
        ibPosaTcpTermErrPkts
            Counter64,
        ibPosaTcpTermMapErrCnts
            Counter64,
        ibPosaTcpTermInDiscardedPkts
            Counter64,
        ibPosaTcpTermOutDiscardedPkts
            Counter64,
        ibPosaTcpTermStatRowStatus
            RowStatus
     }

ibPosaTcpTermStatIndex OBJECT-TYPE
    SYNTAX Integer32 (1..64)
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "The index of entity."
    ::= { ibPosaTcpTermStatEntry 1 }

ibPosaTcpTermStatIPType OBJECT-TYPE
    SYNTAX InetAddressType
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "The type of Internet address.  And now it must be ipv4."
    ::= { ibPosaTcpTermStatEntry 2 }

ibPosaTcpTermStatIP OBJECT-TYPE
    SYNTAX InetAddress
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "The IP address or the subnet of the statistics terminal(s).
        When the vlaue is a subnet, the statistics of all the
        terminals in the subnet will be added up.  And now it only
        supports ipv4."
    ::= { ibPosaTcpTermStatEntry 3 }

ibPosaTcpTermStatIPMask OBJECT-TYPE
    SYNTAX InetAddress
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "The mask associated with the IP address or the subnet.  And
        now it only supports ipv4."
    ::= { ibPosaTcpTermStatEntry 4 }

ibPosaTcpTermRecvPkts OBJECT-TYPE
    SYNTAX Counter64
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The number of packets recieved from the POS terminal(s)."
    ::= { ibPosaTcpTermStatEntry 5 }

ibPosaTcpTermSendPkts OBJECT-TYPE
    SYNTAX Counter64
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The number of packets sent to the POS terminal(s)."
    ::= { ibPosaTcpTermStatEntry 6 }

ibPosaTcpTermErrPkts OBJECT-TYPE
    SYNTAX Counter64
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The number of inbound packets that contained errors."
    ::= { ibPosaTcpTermStatEntry 7 }

ibPosaTcpTermMapErrCnts OBJECT-TYPE
    SYNTAX Counter64
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The number of packets that are discarded because of mapping errors."
    ::= { ibPosaTcpTermStatEntry 8 }

ibPosaTcpTermInDiscardedPkts OBJECT-TYPE
    SYNTAX Counter64
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The number of inbound packets that are discarded when the incoming
        buffer is full."
    ::= { ibPosaTcpTermStatEntry 9 }

ibPosaTcpTermOutDiscardedPkts OBJECT-TYPE
    SYNTAX Counter64
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The number of outbound packets that are discarded because of link
        errors between the router and the POS terminal(s)."
    ::= { ibPosaTcpTermStatEntry 10 }

ibPosaTcpTermStatRowStatus OBJECT-TYPE
    SYNTAX RowStatus
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "Operation status."
    ::= { ibPosaTcpTermStatEntry 11 }

ibPosaFcmConfTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbPosaFcmConfEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
        "This table shows the configuration information of FCM (Fast Connection
        Modem) interfaces."
    ::= { ibPosaTables 9 }

ibPosaFcmConfEntry OBJECT-TYPE
    SYNTAX         IbPosaFcmConfEntry
    MAX-ACCESS     not-accessible
    STATUS         current
    DESCRIPTION
        "This entry shows the configuration information of FCM (Fast Connection
        Modem) interfaces."
    INDEX { ifIndex }
    ::= { ibPosaFcmConfTable 1 }

IbPosaFcmConfEntry ::= SEQUENCE  {
        ibPosaFcmNegoHookOff         Integer32,
        ibPosaFcmNegoSilence         Integer32,
        ibPosaFcmNegoScrmbBinary1    Integer32,
        ibPosaFcmNegoUnscrmbBinary1  Integer32,
        ibPosaFcmThresholdRlsdOff    Integer32,
        ibPosaFcmThresholdRlsdOn     Integer32,
        ibPosaFcmThresholdTxPower    Integer32,
        ibPosaFcmThresholdAnswerTone Integer32
        }

ibPosaFcmNegoHookOff     OBJECT-TYPE
    SYNTAX   Integer32 (100..6000)
    UNITS       "milliseconds"
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
        "Delay time for hookoff."
    DEFVAL { 500 }
    ::= { ibPosaFcmConfEntry 1 }

ibPosaFcmNegoSilence     OBJECT-TYPE
    SYNTAX   Integer32 (0..3000)
    UNITS       "milliseconds"
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
        "Silence time for modem negotiation."
    DEFVAL { 0 }
    ::= { ibPosaFcmConfEntry 2 }

ibPosaFcmNegoScrmbBinary1 OBJECT-TYPE
    SYNTAX   Integer32 (100..1500)
    UNITS       "milliseconds"
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
        "Scramble-binary1 time for modem negotiation."
    DEFVAL { 250 }
    ::= { ibPosaFcmConfEntry 3 }

ibPosaFcmNegoUnscrmbBinary1 OBJECT-TYPE
    SYNTAX   Integer32 (300..1500)
    UNITS       "milliseconds"
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
        "Unscramble-binary1 time for modem negotiation."
    DEFVAL { 400 }
    ::= { ibPosaFcmConfEntry 4 }

ibPosaFcmThresholdRlsdOff  OBJECT-TYPE
    SYNTAX   Integer32 (0..75)
    UNITS       "-dBm"
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
        "RLSD (Received Line Signal Detect) off threshold for modem."
    DEFVAL { 48 }
    ::= { ibPosaFcmConfEntry 5 }

ibPosaFcmThresholdRlsdOn  OBJECT-TYPE
    SYNTAX   Integer32 (0..75)
    UNITS       "-dBm"
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
        "RLSD (Received Line Signal Detect) on threshold for modem."
    DEFVAL { 43 }
    ::= { ibPosaFcmConfEntry 6 }

ibPosaFcmThresholdTxPower OBJECT-TYPE
    SYNTAX   Integer32 (1..42)
    UNITS       "-dBm"
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
        "TX power for modem."
    DEFVAL { 10 }
    ::= { ibPosaFcmConfEntry 7 }

ibPosaFcmThresholdAnswerTone OBJECT-TYPE
    SYNTAX   Integer32 (1..42)
    UNITS       "-dBm"
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
        "Answer tone level for modem."
    DEFVAL { 9 }
    ::= { ibPosaFcmConfEntry 8 }

ibPosaCallerStatTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbPosaCallerStatEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
        "This table shows the statistics information of terminal according the
