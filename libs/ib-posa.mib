--
-- traps defined
--

ibPosaTrap OBJECT IDENTIFIER ::= { ibPosa 3 }

ibPosaTrapPrex OBJECT IDENTIFIER ::= { ibPosaTrap 0 }

ibPosaServerStatusChange NOTIFICATION-TYPE
    OBJECTS { ibPosaServerEnable }
    STATUS current
    DESCRIPTION
        "This trap is generated when the POS function is started or stopped."
    ::= { ibPosaTrapPrex 1 }

ibPosaAppStateChange NOTIFICATION-TYPE
    OBJECTS { ibPosaAppID, ibPosaAppStateChangeObject }
    STATUS current
    DESCRIPTION
        "This trap is generated whenever the availability of application server
        changes."
    ::= { ibPosaTrapPrex 2 }

ibPosaTerminalHangUp NOTIFICATION-TYPE
    OBJECTS { ifIndex, ifDescr, ibPosaCallerStatCallerID }
    STATUS current
    DESCRIPTION
        "This trap is generated whenever the POS connection through a FCM (Fast
        Connection Modem) interface is hanged up by POS terminal."
    ::= { ibPosaTrapPrex 3 }

ibPosaFcmLinkNegoFailed NOTIFICATION-TYPE
    OBJECTS { ifIndex, ifDescr , ibPosaCallerStatCallerID }
    STATUS current
    DESCRIPTION
        "This trap is generated whenever the link layer negotiation is failed
        between the POS terminal and router through a FCM (Fast Connection
        Modem) interface."
    ::= { ibPosaTrapPrex 4 }

ibPosaFcmPhyNegoFailed NOTIFICATION-TYPE
    OBJECTS { ifIndex, ifDescr , ibPosaCallerStatCallerID }
    STATUS current
    DESCRIPTION
        "This trap is generated whenever the physical layer negotiation is failed
        between the POS terminal and router through a FCM (Fast Connection
        Modem) interface."
    ::= { ibPosaTrapPrex 5 }

ibPosaTcpConnectionExceed NOTIFICATION-TYPE
    OBJECTS { ibPosaTcpConnectionThreshold }
    STATUS current
    DESCRIPTION
        "This trap is generated whenever the number of connections exceeds the threshold."
    ::= { ibPosaTrapPrex 6 }

ibPosaFcmConnectionExceed NOTIFICATION-TYPE
    OBJECTS { ibPosaFcmConnectionThreshold }
    STATUS current
    DESCRIPTION
        "This trap is generated whenever the number of connections exceeds the threshold."
    ::= { ibPosaTrapPrex 7 }

ibPosaTcpTradeExceed NOTIFICATION-TYPE
    OBJECTS { ibPosaTcpTradeLimit, ibPosaTerminalID }
    STATUS current
    DESCRIPTION
        "This trap is generated whenever the number of trades in any TCP connect exceeds
        the limit."
    ::= { ibPosaTrapPrex 8 }

ibPosaTradeSuccessFalling NOTIFICATION-TYPE
    OBJECTS { ibPosaNiiStatIndex, ibPosaTradeSuccessFallingThreshold }
    STATUS current
    DESCRIPTION
        "This trap is generated whenever the transaction success rate of bank server is smaller than or equal to the threshold."
    ::= { ibPosaTrapPrex 9 }

ibPosaE1DialFalling NOTIFICATION-TYPE
    OBJECTS { ifIndex, ifDescr, ibPosaE1DialFallingThreshold }
    STATUS current
    DESCRIPTION
        "This trap is generated whenever the dialing success rate of E1-controller is smaller than or equal to the threshold."
    ::= { ibPosaTrapPrex 10 }

ibPosaFcmTradeAbnormal NOTIFICATION-TYPE
    OBJECTS { ifIndex, ifDescr, ibPosaCallerStatCallerID }
    STATUS current
    DESCRIPTION
        "This trap is generated whenever the dialing link is abnormal."
    ::= { ibPosaTrapPrex 11 }

ibPosaTrapObjects OBJECT IDENTIFIER ::= { ibPosaTrap 1 }

ibPosaAppStateChangeObject OBJECT-TYPE
    SYNTAX INTEGER
        {
        available(1),
        unavailable(2)
        }
    MAX-ACCESS accessible-for-notify
    STATUS current
    DESCRIPTION
        "This is a dummy MIB object referenced by the ibPosaAppStateChange trap.
        This object shows the availability changing of corresponding application
        server."
    ::= { ibPosaTrapObjects 1 }

