-- Reaction Table
ibNqaReactionTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbNqaReactionEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A table of NQA test monitoring notification and
        reaction configuration definitions.

        An entry is removed from this table when its
        corresponding pingCtlEntry is deleted.

         When PingCtlType of the test is ibNqaTwampLight,
         it does not support the table."
  ::= { ibNqaObjects 13 }

ibNqaReactionEntry OBJECT-TYPE
    SYNTAX      IbNqaReactionEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Defines an entry in the ibNqaReactionTable. The first two index
        elements identify the pingCtlEntry that a ibNqaReactItemIndex
        belongs to. The third index element selects a single reaction."
    INDEX
        {
            ibNqaReactOwnerIndex,
            ibNqaReactTestName,
            ibNqaReactItemIndex
        }
    ::= { ibNqaReactionTable 1 }

IbNqaReactionEntry ::=
    SEQUENCE
        {
            ibNqaReactOwnerIndex                   SnmpAdminString,
            ibNqaReactTestName                     SnmpAdminString,
            ibNqaReactItemIndex                    Unsigned32,
            ibNqaReactCheckedElement               INTEGER,
            ibNqaReactThresholdUpperLimit          Unsigned32,
            ibNqaReactThresholdLowerLimit          Unsigned32,
            ibNqaReactThresholdType                INTEGER,
            ibNqaReactThresholdConsecNum           Unsigned32,
            ibNqaReactThresholdAccumNum            Unsigned32,
            ibNqaReactActionType                   INTEGER,
            ibNqaReactCurrentStatus                INTEGER,
            ibNqaReactRowStatus                    RowStatus,
            ibNqaReactCheckedNum                   Unsigned32,
            ibNqaReactThresholdNum                 Unsigned32
        }

ibNqaReactOwnerIndex OBJECT-TYPE
    SYNTAX      SnmpAdminString (SIZE(0..32))
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "The first index of ibNqaReactionTable, it is the same as
        pingCtlOwnerIndex in pingCtlTable. ibNqaReactOwnerIndex and
        ibNqaReactTestName identify an existed test."
    ::= { ibNqaReactionEntry 1 }

ibNqaReactTestName OBJECT-TYPE
    SYNTAX      SnmpAdminString (SIZE(0..32))
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "The second index of ibNqaReactionTable, it is the same as
        pingCtlTestName in pingCtlTable. ibNqaReactOwnerIndex and
        ibNqaReactTestName identify an existed test."
    ::= { ibNqaReactionEntry 2 }

ibNqaReactItemIndex OBJECT-TYPE
    SYNTAX      Unsigned32 (1..10)
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "The third index of ibNqaReactionTable, it definitely
        identifies one of the reaction configurations for a test."
    ::= { ibNqaReactionEntry 3 }

ibNqaReactCheckedElement OBJECT-TYPE
    SYNTAX      INTEGER {
                          probetime(1),
                          probefailure(2),
                          jitterrtt(3),
                          jitterpacketloss(4),
                          jittersd(5),
                          jitterds(6),
                          icpif(7),
                          mos(8),
                          jitterOwdSD(9),
                          jitterOwdDS(10),
                          testfailure(11),
                          testresultchange(12)
                        }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "A list of reaction condition type.
        'probetime' and 'probefailure' are valid when pingCtlType of
        the test is not ibNqajitter.

        'jitterOwdSD': one way delay from source to destination in
                       jitter test.
        'jitterOwdDS': one way delay from destination to source in
                       jitter test.

        'jitterrtt', 'jitterpacketloss', 'jittersd', 'jitterds',
        'jitterOwdSD' and 'jitterOwdDS' are valid when pingCtlType
        of the test is ibNqajitter.

        'icpif' and 'mos' are valid when pingCtlType of the test is
        ibNqajitter and ibNqaCtlCodecType is 'g711Alaw' or 'g711Ulaw'
        or 'g729A'."
     ::= { ibNqaReactionEntry 4 }

ibNqaReactThresholdUpperLimit OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object defines an administrative upper-threshold value for
        ibNqaReactCheckedElement.

        The value of this object is zero if ibNqaReactCheckedElement is
        specified as 'jitterpacketloss' and 'probefailure'."
    DEFVAL { 0 }
    ::= { ibNqaReactionEntry 5 }

ibNqaReactThresholdLowerLimit OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object defines an administrative lower-threshold value for
        ibNqaReactCheckedElement.

        The value of ibNqaReactThresholdLowerLimit should be equal or less
        than the value of ibNqaReactThresholdUpperLimit.

        The value of this object is zero if ibNqaReactCheckedElement is
        specified as 'jitterpacketloss' and 'probefailure'."
    DEFVAL { 0 }
    ::= { ibNqaReactionEntry 6 }

ibNqaReactThresholdType OBJECT-TYPE
    SYNTAX      INTEGER {
                           invalid(0),
                           average(1),
                           consecutive(2),
                           accumulative(3)
                       }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object defines the reaction threshold types.

        invalid(0):     Invalid value.

        average(1):     If reaction threshold type is average and the average
                        value of watched operation exceeds threshold limit,
                        an action specified in ibNqaReactActionType is
                        generated when ibNqaReactCurrentStatus changes.

        consecutive(2): If reaction threshold type is consecutive and
                        the times of watched operation consecutively exceeds
                        the limit specified by ibNqaReactThresholdConsecNum,
                        an action specified in ibNqaReactActionType is
                        generated when ibNqaReactCurrentStatus changes.

        accumulative(3): If reaction threshold type is accumulative and
                        the times of watched operation accumulatively exceeds
                        the limit specified by ibNqaReactThresholdAccumNum,
                        an action specified in ibNqaReactActionType is
                        generated when ibNqaReactCurrentStatus changes.

        If reaction threshold type is not configured, the value of this
        object is 'invalid'."
    ::= { ibNqaReactionEntry 7 }

ibNqaReactThresholdConsecNum OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object defines a consecutive threshold limit.

        The value is zero when ibNqaReactThresholdType
        is not 'consecutive'."
    DEFVAL { 0 }
    ::= { ibNqaReactionEntry 8 }

ibNqaReactThresholdAccumNum OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object defines an accumulative threshold limit.

        The value is zero when the ibNqaReactThresholdType
        is not 'accumulative'."
    DEFVAL { 0 }
    ::= { ibNqaReactionEntry 9 }

ibNqaReactActionType OBJECT-TYPE
    SYNTAX      INTEGER {
                           none(0),
                           trapOnly(1),
                           triggerOnly(2),
                           trapAndTrigger(3)
                        }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Specifies the type of reaction to generate if the specified
        violation type occurs for the monitored element:

        none               - No reaction is generated
        trapOnly           - A trap is generated
        triggerOnly        - All trigger actions defined for this
                             entry are generated
        trapAndTrigger     - Both a trap and all trigger actions
                             are generated "
    DEFVAL { none }
    ::= { ibNqaReactionEntry 10 }

ibNqaReactCurrentStatus OBJECT-TYPE
    SYNTAX      INTEGER {
                            invalid(1),
                            overThreshold(2),
                            belowThreshold(3),
                            overUpperThreshold(4),
                            belowLowerThreshold(5)
                        }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Specifies the current status of this reaction entry:

        invalid  - The status of the reaction entry is unknown.

        overThreshold       - The status of the reaction entry is overThreshold
                              when the monitored result exceeds the threshold
                              value for the ibNqaReactThresholdType.

        belowThreshold      - The status of the reaction entry is belowThreshold
                              when the monitored result is under the threshold
                              value for the ibNqaReactThresholdType.

        overUpperThreshold  - The status of the reaction entry is overThreshold
                              when the monitored result is exceeds the
                              upper-threshold value for the
                              ibNqaReactThresholdType.

        belowLowerThreshold - The status of the reaction entry is belowThreshold
                              when the monitored result is under the
                              lower-threshold value for
                              ibNqaReactThresholdType."

    ::= { ibNqaReactionEntry 11 }

ibNqaReactRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object allows entries to be created and deleted
        in the ibNqaReactionTable.

        Creation of an entry in this table results in a new set of reaction
        configurations which take effect immediately.

        Deletion of an entry in this table results in corresponding reaction
        configurations are being deleted and stop taking effect."
    REFERENCE
        "See definition of RowStatus in RFC 2579, 'Textual
        Conventions for SMIv2.'"
    ::= { ibNqaReactionEntry 12 }

ibNqaReactCheckedNum OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The number of checked element samples that are monitored."
    DEFVAL { 0 }
    ::= { ibNqaReactionEntry 13 }

ibNqaReactThresholdNum OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The number of checked element samples that are over threshold."
    DEFVAL { 0 }
    ::= { ibNqaReactionEntry 14 }

-- Statistics Reaction Table
ibNqaStatisticsReactionTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbNqaStatisticsReactionEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A table of NQA test monitoring notification and
        reaction configuration definitions.

        An entry is removed from this table when its
        corresponding pingCtlEntry is deleted."
  ::= { ibNqaObjects 14 }

ibNqaStatisticsReactionEntry OBJECT-TYPE
    SYNTAX      IbNqaStatisticsReactionEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Defines an entry in the ibNqaStatisticsReactionTable. The
        first two index elements identify the pingCtlEntry that a
        ibNqaReactItemIndex belongs to. The third index element
        identifies an entry in the ibNqaStatisticsResultsTable.
        The forth index element selects a single reaction."
    INDEX
        {
            ibNqaStatReactOwnerIndex,
            ibNqaStatReactTestName,
            ibNqaStatReactIndex,
            ibNqaStatReactItemIndex
        }
    ::= { ibNqaStatisticsReactionTable 1 }

IbNqaStatisticsReactionEntry ::=
    SEQUENCE
        {
            ibNqaStatReactOwnerIndex               SnmpAdminString,
            ibNqaStatReactTestName                 SnmpAdminString,
            ibNqaStatReactIndex                    Unsigned32,
            ibNqaStatReactItemIndex                Unsigned32,
            ibNqaStatReactCheckedNum               Counter32,
            ibNqaStatReactThresholdNum             Counter32
        }

ibNqaStatReactOwnerIndex OBJECT-TYPE
    SYNTAX      SnmpAdminString (SIZE(0..32))
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The first index of ibNqaStatisticsReactionTable, it is the same
        as pingCtlOwnerIndex in pingCtlTable."
    ::= { ibNqaStatisticsReactionEntry 1 }

ibNqaStatReactTestName OBJECT-TYPE
    SYNTAX      SnmpAdminString (SIZE(0..32))
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The second index of ibNqaStatisticsReactionTable, it is the same
        as pingCtlTestName in pingCtlTable."
    ::= { ibNqaStatisticsReactionEntry 2 }

ibNqaStatReactIndex OBJECT-TYPE
    SYNTAX      Unsigned32 (1..10)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The third index of ibNqaStatisticsReactionTable. An entry in
        this table is created when the statistics result of a NQA probe
        is determined.

        An implementation MUST start assigning ibNqaStatResIndex values
        at 1 and wrap after exceeding the maximum possible value as
        defined by the limit of this object ('ffffffff'h)."
    ::= { ibNqaStatisticsReactionEntry 3 }

ibNqaStatReactItemIndex OBJECT-TYPE
    SYNTAX      Unsigned32 (0..'ffffffff'h)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The forth index of ibNqaStatisticsReactionTable, it definitely
        identifies one of the reaction configurations for a test"
    ::= { ibNqaStatisticsReactionEntry 4 }

ibNqaStatReactCheckedNum OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The number of checked element samples that are monitored."
    ::= { ibNqaStatisticsReactionEntry 5 }

ibNqaStatReactThresholdNum OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The number of checked element samples that are over threshold."
    ::= { ibNqaStatisticsReactionEntry 6 }

-- =================================================================
--   NQA server configuration extended information
-- =================================================================

ibNqaTcpServerExtendTable OBJECT-TYPE
    SYNTAX SEQUENCE OF IbNqaTcpServerExtendEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A table which contains the status of extended NQA TCP server
        configuration information."
    ::= { ibNqaObjects 15 }

ibNqaTcpServerExtendEntry OBJECT-TYPE
    SYNTAX      IbNqaTcpServerExtendEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A table which contains the status of extended NQA TCP server
        configuration information.
        This entry includes the TCP server IP address, port,
        VPN name and DSField."
    INDEX {
            ibNqaTcpServerExtIpAddress,
            ibNqaTcpServerExtPort,
            ibNqaTcpServerExtVPNType,
            ibNqaTcpServerExtVPNInstance
          }
    ::= { ibNqaTcpServerExtendTable 1 }

IbNqaTcpServerExtendEntry  ::= SEQUENCE
{
    ibNqaTcpServerExtIpAddress                InetAddress,
    ibNqaTcpServerExtPort                     Integer32,
    ibNqaTcpServerExtVPNType                  VpnInstanceType,
    ibNqaTcpServerExtVPNInstance              OCTET STRING,
    ibNqaTcpServerExtDSField                  Unsigned32,
    ibNqaTcpServerExtRowStatus                RowStatus
}

ibNqaTcpServerExtIpAddress          OBJECT-TYPE
    SYNTAX      InetAddress
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "TCP server's IP address."
    ::= { ibNqaTcpServerExtendEntry 1 }

ibNqaTcpServerExtPort          OBJECT-TYPE
    SYNTAX      Integer32 (0..65536)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "TCP server's port number."
    ::= { ibNqaTcpServerExtendEntry 2 }

ibNqaTcpServerExtVPNType OBJECT-TYPE
    SYNTAX      VpnInstanceType
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "TCP server's VPN instance type."
    ::= { ibNqaTcpServerExtendEntry 3 }

ibNqaTcpServerExtVPNInstance OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(0..31))
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "TCP server's VPN name.If the NQA server listens on the public network,
        the value of ibNqaTcpServerExtVPNType must be public(1) and the value
        of this object can be set to any non-printable character string.If the
        NQA server listens on the private network, the value of ibNqaTcpServerExtVPNType
        must be vpn(2) and the value of this object can be set to any non-printable
        character string."
    ::= { ibNqaTcpServerExtendEntry 4 }

ibNqaTcpServerExtDSField OBJECT-TYPE
    SYNTAX      Unsigned32 (0..256)
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Specifies the value to store in the Differentiated
        Services (DS) Field in the IP packet used to
        encapsulate the probe.  The DS Field is defined
        as the Type of Service (TOS) octet in a IPv4 header
        or as the Traffic Class octet in a IPv6 header.

        The value of this object must be a decimal integer
        in the range of 0 to 255.  Not all values are legal or
        meaningful.  A value of 0 means that the function
        represented by this option is not supported.  The value
        of 256 indicates that TOS is not configured, and this
        value is not supported by IP implementations.  DS Field
        usage is often not supported by IP implementations and
        not all values are supported.  Refer to RFC 2474 for
        guidance on usage of this field."
    REFERENCE
        "Refer to RFC 2474 for the definition of the
        Differentiated Services Field and to RFC 1812
        Section 5.3.2 for Type of Service (TOS)."
    ::= { ibNqaTcpServerExtendEntry 5 }

ibNqaTcpServerExtRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "ibNqaTcpServerIpAddress, ibNqaTcpServerPort, ibNqaTcpServerExtVPNType
        and ibNqaTcpServerVPNInstance must be set during a creation operation."
    REFERENCE
        "RFC 2579"
    ::= { ibNqaTcpServerExtendEntry 6 }

ibNqaUdpServerExtendTable OBJECT-TYPE
