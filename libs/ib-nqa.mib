-- =================================================================
--   Tcp server configuration information Statistics Table
-- =================================================================


ibNqaTcpServerTable OBJECT-TYPE
    SYNTAX SEQUENCE OF IbNqaTcpServerEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A table which contains the status of NQA tcp server
        configuration information."
    ::= { ibNqaObjects 6 }

ibNqaTcpServerEntry OBJECT-TYPE
    SYNTAX      IbNqaTcpServerEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A table which contains the status of NQA tcp server
        configuration information.

        This entry includes not only the tcp server ip address
        but also the tcp server port."
    INDEX {
            ibNqaTcpServerIpAddress,
            ibNqaTcpServerPort
          }
    ::= { ibNqaTcpServerTable 1 }

IbNqaTcpServerEntry  ::= SEQUENCE
{
    ibNqaTcpServerIpAddress                InetAddress,
    ibNqaTcpServerPort                     Integer32,
    ibNqaTcpServerRowStatus                RowStatus
}

ibNqaTcpServerIpAddress          OBJECT-TYPE
    SYNTAX      InetAddress
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Tcp server's ip address."
    ::= { ibNqaTcpServerEntry 1 }

ibNqaTcpServerPort          OBJECT-TYPE
    SYNTAX      Integer32 (0..65536)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Tcp server's port number."
    ::= { ibNqaTcpServerEntry 2 }

ibNqaTcpServerRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "ibNqaTcpServerIpAddress and ibNqaTcpServerPort
        must be set during a creation operation."
    REFERENCE
        "RFC 2579"
    ::= { ibNqaTcpServerEntry 3 }

ibNqaUdpServerTable OBJECT-TYPE
    SYNTAX SEQUENCE OF IbNqaUdpServerEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
