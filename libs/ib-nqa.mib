                disable(2)
                }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Enable NQA server function."
    DEFVAL { disable }
    ::= { ibNqaObjects 8 }

ibNqaStatsMaxGroupNumber OBJECT-TYPE
    SYNTAX     Integer32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
        "The maximum number saved per group. The maximum value of
        ibNqaCtlStatisticsGroupNumber can not exceed the value of
        this object."
    ::= { ibNqaObjects 9 }


ibNqaStatisticsCtlTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbNqaStatisticsCtlEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Defines the NQA statistics control table for providing, via SNMP,
        the capability of performing NQA operations at a remote host.
        The results of these operations are stored in the
        ibNqaStatisticsResultsTable and the ibNqaGroupStatsJitterTable."
    ::= { ibNqaObjects 10 }

ibNqaStatisticsCtlEntry OBJECT-TYPE
    SYNTAX      IbNqaStatisticsCtlEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Defines an entry in the ibNqaStatisticsCtlTable.
        The ibNqaStatisticsCtlTable has the same index as the
        pingCtlTable in order for a ibNqaStatisticsCtlTable to
        correspond to the pingCtlEntry."
    AUGMENTS    { pingCtlEntry }
    ::= { ibNqaStatisticsCtlTable 1 }

IbNqaStatisticsCtlEntry ::=
    SEQUENCE
        {
            ibNqaCtlStatisticsInterval       Unsigned32,
            ibNqaCtlStatisticsGroupNumber    Unsigned32,
            ibNqaCtlStatisticsKeptTime       Unsigned32,
            ibNqaCtlBeginTime                DateAndTime,
            ibNqaCtlLifeTime                 Unsigned32
        }

ibNqaCtlStatisticsInterval OBJECT-TYPE
    SYNTAX     Unsigned32
    UNITS      "minutes"
    MAX-ACCESS read-create
    STATUS     current
    DESCRIPTION
        "Statistic information will be calculated after the time that is
        the value of ibNqaCtlStatisticsInterval."
    ::= { ibNqaStatisticsCtlEntry 1 }

ibNqaCtlStatisticsGroupNumber OBJECT-TYPE
    SYNTAX     Unsigned32
    MAX-ACCESS read-create
    STATUS     current
    DESCRIPTION
        "The value of this object indicates the number of group saved.
        The length of this object is from 1 to ibNqaStatsMaxGroupNumber."
    DEFVAL     { 2 }
    ::= { ibNqaStatisticsCtlEntry 2 }

ibNqaCtlStatisticsKeptTime OBJECT-TYPE
    SYNTAX     Unsigned32(1..1440)
    UNITS      "minutes"
    MAX-ACCESS read-create
    STATUS     current
    DESCRIPTION
        "To indicate how long the statistic information will be saved after
        the test stops."
    DEFVAL     { 120 }
    ::= { ibNqaStatisticsCtlEntry 3 }

ibNqaCtlBeginTime OBJECT-TYPE
    SYNTAX     DateAndTime
    MAX-ACCESS read-create
    STATUS     current
    DESCRIPTION
        "Time at which the type-specified test will start."
    ::= { ibNqaStatisticsCtlEntry 4 }

ibNqaCtlLifeTime OBJECT-TYPE
    SYNTAX     Unsigned32
    UNITS      "seconds"
    MAX-ACCESS read-create
    STATUS     current
    DESCRIPTION
        "To indicate how long the type-specified test will last."
    DEFVAL     { 0 }
    ::= { ibNqaStatisticsCtlEntry 5 }

ibNqaStatisticsResultsTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbNqaStatisticsResultsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Defines the NQA statistics results table for providing
        the capability of performing NQA operations at a remote
        host. The statistics results of these operations in a statistic cycle
        are stored in the ibNqaStatisticsResultsTable. The statistic cycle is
        the value of ibNqaCtlStatisticsInterval.
        The first two index identify the pingCtlEntry that the probe statistics
        result (IbNqaStatisticsResultsEntry) belongs to."

    ::= { ibNqaObjects 11 }


ibNqaStatisticsResultsEntry OBJECT-TYPE
    SYNTAX       IbNqaStatisticsResultsEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
        "Defines an entry in the ibNqaStatisticsResultsTable. The
        ibNqaStatisticsResultsTable has the same index as the
        pingCtlTable in order for a ibNqaStatisticsResultsEntry to
        correspond to the pingCtlEntry that caused it to be created."
    INDEX
        {
            pingCtlOwnerIndex,
            pingCtlTestName,
            ibNqaStatResIndex
        }
    ::= { ibNqaStatisticsResultsTable 1 }

IbNqaStatisticsResultsEntry ::=
    SEQUENCE
        {
            ibNqaStatResIndex                  Unsigned32,
            ibNqaStatResIpTargetAddressType    InetAddressType,
            ibNqaStatResIpTargetAddress        InetAddress,
            ibNqaStatResMinRtt                 Gauge32,
            ibNqaStatResMaxRtt                 Gauge32,
            ibNqaStatResAverageRtt             Gauge32,
            ibNqaStatResProbeResponses         Counter32,
            ibNqaStatResSentProbes             Counter32,
            ibNqaStatResRttSumOfSquares        Counter32,
            ibNqaStatResStartTime              DateAndTime,
            ibNqaStatResInterval               Gauge32,
            ibNqaStatResRttNumDisconnects      Counter32,
            ibNqaStatResRttTimeouts            Counter32,
            ibNqaStatResRttBusies              Counter32,
            ibNqaStatResRttNoConnections       Counter32,
            ibNqaStatResRttDrops               Counter32,
            ibNqaStatResRttSequenceErrors      Counter32,
            ibNqaStatResRttErrors              Counter32,
            ibNqaStatResLostPacketRatio        Gauge32,
            ibNqaStatResPacketLateArrival      Counter32,
            ibNqaStatResRttSum                 Counter32,
            ibNqaStatResNumOfDelaySD           Counter32,
            ibNqaStatResMinDelaySD             Gauge32,
            ibNqaStatResMaxDelaySD             Gauge32,
            ibNqaStatResSumDelaySD             Counter32,
            ibNqaStatResSum2DelaySD            Counter32,
            ibNqaStatResNumOfDelayDS           Counter32,
            ibNqaStatResMinDelayDS             Gauge32,
            ibNqaStatResMaxDelayDS             Gauge32,
            ibNqaStatResSumDelayDS             Counter32,
            ibNqaStatResSum2DelayDS            Counter32,
