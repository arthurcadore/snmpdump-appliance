IB-NQA-MIB DEFINITIONS ::= BEGIN

        IMPORTS
            MODULE-IDENTITY, OBJECT-TYPE, Unsigned32,
            OBJECT-IDENTITY, NOTIFICATION-TYPE, Integer32, Counter32, Gauge32
                FROM SNMPv2-SMI
            DateAndTime, DisplayString, RowStatus, TEXTUAL-CONVENTION
                FROM SNMPv2-TC
            InetAddressType, InetAddress
                FROM INET-ADDRESS-MIB
            ibRhw
                FROM IB-OID-MIB
            pingCtlEntry, pingCtlOwnerIndex, pingCtlTestName, pingCtlType,
            pingCtlTargetAddress, pingCtlTargetAddressType, pingCtlDescr
                FROM DISMAN-PING-MIB
            SnmpAdminString
                FROM SNMP-FRAMEWORK-MIB;          -- RFC2571

    ibNqa MODULE-IDENTITY
        LAST-UPDATED "202301100000Z"
        ORGANIZATION
            "Intelbras S.A. Industria de Telecomunicacao Eletronica Brasileria."
        CONTACT-INFO
            ""
            DESCRIPTION
                "The private MIB file includes the test information of the
                device. It is supplementary for rfc2925.txt."
            REVISION "202301100000Z"
            DESCRIPTION
                "Add ibNqaHttp2"
            REVISION "202212010000Z"
            DESCRIPTION
                "Add ibNqaScheduleFailure for trap.
                 Add ibNqaServerFailure for trap."
            REVISION "202209010000Z"
            DESCRIPTION
                "Add ibNqaJitterOwdDSOverThreshold to ibNqaNotifications.
                 Add ibNqaJitterOwdSDOverThreshold to ibNqaNotifications.
                 Add ibNqaTestFailure to ibNqaNotifications.
                 Add ibNqaTestResultChange to ibNqaNotifications."
            REVISION "202003250000Z"
            DESCRIPTION
                "Add ibNqaProbeTimeAboveThreshold to ibNqaNotifications.
                 Add ibNqaProbeTimeBelowThreshold to ibNqaNotifications.
                 Add ibNqaProbeFailAboveThreshold to ibNqaNotifications.
                 Add ibNqaProbeFailBelowThreshold to ibNqaNotifications."
        ::= { ibRhw 3 }

 -- Textual Conventions
VpnInstanceType ::= TEXTUAL-CONVENTION
     STATUS      current
     DESCRIPTION
         "A value that represents a type of VPN instance.
          public(1)   Public network.
          vpn(2)      Private network."
     SYNTAX      INTEGER {
                     public(1),
                     vpn(2)
                 }

    ibNqaObjects OBJECT IDENTIFIER ::= { ibNqa 1 }

 -- The registration node (point) for remote NQA(Network Quality Analysis) test
 -- implementation types

ibNqaImplementationTypeDomains  OBJECT IDENTIFIER ::= { ibNqa 2 }

ibNqaUdpEcho OBJECT-IDENTITY
    STATUS      current
    DESCRIPTION
        "Indicates that an implementation is using the UDP based echo server.
        Notes: The ibNqaCtlTargetPort and ibNqaCtlSourcePort need to be set,
        while in the pingUdpEcho Node, ibNqaCtlTargetPort must be set to the
        default value."
    ::= { ibNqaImplementationTypeDomains 1 }

ibNqaTcpconnect OBJECT-IDENTITY
    STATUS      current
    DESCRIPTION
        "Indicates that an implementation is attempting to
        connect to a TCP port in order to calculate a round
        trip time.
        Notes: The ibNqaCtlTargetPort and ibNqaCtlSourcePort need to be set,
        while in the pingTcpConnectionAttempt Node, ibNqaCtlTargetPort must
        be set to the default value."
    ::= { ibNqaImplementationTypeDomains 2 }

ibNqajitter OBJECT-IDENTITY
    STATUS      current
    DESCRIPTION
        "Indicates that an implementation is using udp for packet transfers.
        'jitter' will cause the NQA(Network Quality Analysis) application
        to perform delay variance analysis."
    ::= { ibNqaImplementationTypeDomains 3 }

ibNqaHttp OBJECT-IDENTITY
    STATUS      current
    DESCRIPTION
        "Indicates that an implementation is using udp for name resolution,
        tcp connect and tcp data transfer mechanisms for HTTP data download
        from a particular HTTP Server.
        It causes the NQA application to perform a download of the object
        specified in the URL."
    ::= { ibNqaImplementationTypeDomains 4 }


ibNqadlsw OBJECT-IDENTITY
    STATUS      current
    DESCRIPTION
        "Indicates that an implementation is using tcp for sending keepalives.
        'dlsw' will cause the NQA application to perform a keepalive operation
        to measure the response time of a DLSw peer."
    ::= { ibNqaImplementationTypeDomains 5 }

ibNqadhcp OBJECT-IDENTITY
    STATUS      current
    DESCRIPTION
        "Indicates that an implementation is using udp for sending dhcp
        requests. 'dhcp' will cause the NQA application
        to perform an IP Address lease request/teardown operation."
    ::= { ibNqaImplementationTypeDomains 6 }

ibNqaftp OBJECT-IDENTITY
    STATUS      current
    DESCRIPTION
        "Indicates that an implementation is using tcp packet for ftp connect
         and data transfer."
    ::= { ibNqaImplementationTypeDomains 7 }

ibNqaTwampLight OBJECT-IDENTITY
    STATUS      current
    DESCRIPTION
        "Indicates that a measurement for two-way network quality through UDP
         packets. The type only supports sending traps."
    ::= { ibNqaImplementationTypeDomains 8 }

ibNqaArp OBJECT-IDENTITY
    STATUS      current
    DESCRIPTION
        "Indicates that an implementation is using the Address
         Resolution Protocol (ARP) test facility.
         The type only supports IPv4 network."
    ::= { ibNqaImplementationTypeDomains 9 }

ibNqaHttp2 OBJECT-IDENTITY
    STATUS      current
    DESCRIPTION
        "Indicates that an implementation is using udp for name resolution,
        tcp connect and tcp data transfer mechanisms for HTTP2 data download
        from a particular HTTP Server.
        It causes the NQA application to perform a download of the object
        specified in the URL."
    ::= { ibNqaImplementationTypeDomains 10 }

-- MIB contains 3 groups

ibNqaMIBVersion OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Version string of this MIB.
        The format will be: 'Version.Release.Patch: Description'
        An instance likes: '1.0.0: Initial NQA MIB'
        "
    ::= { ibNqaObjects 1 }

ibNqaCtlTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbNqaCtlEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Defines the NQA Control Extend Table for providing, via SNMP,
        the capability of performing network analysis operations at a
        remote host."
   ::= { ibNqaObjects 2 }

ibNqaCtlEntry OBJECT-TYPE
    SYNTAX      IbNqaCtlEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Defines an entry in the ibNqaCtlTable. The operation of this
        table is same as that of pingResultsTable."
    AUGMENTS    { pingCtlEntry }
    ::= { ibNqaCtlTable 1 }

IbNqaCtlEntry ::=
    SEQUENCE {
    ibNqaCtlTargetPort               Integer32,
    ibNqaCtlSourcePort               Integer32,
    ibNqaCtlTTL                      Integer32,
    ibNqaCtlJitterAdminInterval      Integer32,
    ibNqaCtlJitterAdminNumPackets    Integer32,
    ibNqaCtlHttpOperationType        INTEGER,
    ibNqaCtlHttpOperationString      DisplayString,
    ibNqaCtlFtpOperationType         INTEGER,
    ibNqaCtlFtpUsername              DisplayString,
    ibNqaCtlFtpPassword              DisplayString,
    ibNqaCtlFtpOperationString       DisplayString,
    ibNqaCtlVPNInstance              DisplayString,
    ibNqaCtlHistoryKeptTime          Integer32,
    ibNqaCtlHistoryEnable            INTEGER,
    ibNqaCtlICPIFAdvFactor           Integer32,
    ibNqaCtlCodecType                INTEGER
    }

ibNqaCtlTargetPort          OBJECT-TYPE
    SYNTAX      Integer32 (0..65536)
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The destination port that may be used by udpEcho,
        tcpConnect and jitter probes."
    DEFVAL      { 0 }
    ::= { ibNqaCtlEntry 1 }

ibNqaCtlSourcePort          OBJECT-TYPE
    SYNTAX      Integer32 (0..65536)
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The source port that may be used by all tests except
        for dns and dlsw.
        If no source port is specified, system will create
        a random port as source port."
    DEFVAL      { 0 }
    ::= { ibNqaCtlEntry 2 }

ibNqaCtlTTL OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Specifies the value to store in the TTL Field in the IP packet used to
        encapsulate the NQA probe."
    DEFVAL      { 20 }
    ::= { ibNqaCtlEntry 3 }

ibNqaCtlJitterAdminInterval OBJECT-TYPE
    SYNTAX      Integer32 (0..60000)
    UNITS       "milliseconds"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Time value in milliseconds that is an interval between two continuous
        probes in jitter test.

        This object is only used by jitter test."
    DEFVAL      { 20 }
    ::= { ibNqaCtlEntry 4 }

ibNqaCtlJitterAdminNumPackets OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Packet number that specifies how many packets will be sent in
        one jitter test.

        This object is only used by jitter test."
    DEFVAL      { 10 }
    ::= { ibNqaCtlEntry 5 }

ibNqaCtlHttpOperationType OBJECT-TYPE
    SYNTAX      INTEGER{
                get(1),
                post(2),
                raw(3)
                }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Http operation type: get, post or raw."
    DEFVAL      { get }
    ::= { ibNqaCtlEntry 6 }

ibNqaCtlHttpOperationString OBJECT-TYPE
    SYNTAX      DisplayString(SIZE(0..1023))
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Http operation string. For example, if the http operation type is get,
        the string will be url, home page and http version."
    DEFVAL      { "" }
    ::= { ibNqaCtlEntry 7 }

ibNqaCtlFtpOperationType OBJECT-TYPE
    SYNTAX      INTEGER{
                get(1),
                put(2)
                }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Ftp operation type."
    DEFVAL      { get }
    ::= {ibNqaCtlEntry 8 }

ibNqaCtlFtpUsername OBJECT-TYPE
    SYNTAX      DisplayString(SIZE(0..32))
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Ftp username."
    DEFVAL      { "" }
    ::= { ibNqaCtlEntry 9 }

ibNqaCtlFtpPassword OBJECT-TYPE
    SYNTAX      DisplayString(SIZE(0..32))
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Ftp password."
    DEFVAL      { "" }
    ::= { ibNqaCtlEntry 10 }

ibNqaCtlFtpOperationString OBJECT-TYPE
    SYNTAX      DisplayString(SIZE(0..255))
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Ftp operation string. If the ftp operation type is get,
        the string must be the file name which will be gotten from ftp server."
    DEFVAL      { "" }
    ::= { ibNqaCtlEntry 11 }

ibNqaCtlVPNInstance OBJECT-TYPE
    SYNTAX      DisplayString(SIZE(0..255))
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "VPN name by which system finds route in the specified VPN
        to send NQA probe packets."
    DEFVAL      { "" }
    ::= { ibNqaCtlEntry 12 }

ibNqaCtlHistoryKeptTime OBJECT-TYPE
    SYNTAX     Integer32(1..1440)
    UNITS      "minutes"
    MAX-ACCESS read-create
    STATUS     current
    DESCRIPTION
        "To indicate how long the history records will be saved
        in the pingProbeHistoryTable table after the test stopped."
    DEFVAL     { 120 }
    ::= { ibNqaCtlEntry 13 }

ibNqaCtlHistoryEnable OBJECT-TYPE
    SYNTAX      INTEGER
        {
            enabled(1),  --history will be recorded
            disabled(2)  --history will not be recorded
        }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "To indicate whether the history should be recorded in the
        PingProbeHistoryTable or not.
            enabled(1)   --history will be recorded
            disabled(2)  --history will not be recorded."
    DEFVAL      { disabled }
    ::= {ibNqaCtlEntry 14 }

ibNqaCtlICPIFAdvFactor OBJECT-TYPE
    SYNTAX  Integer32
    MAX-ACCESS read-create
    STATUS  current
    DESCRIPTION
        "The advantage factor depends on the type of access and how the service
        is to be used. This object will be used while calculating the ICPIF
        (Calculated Planning Impairment Factor) values.  This is applicable
        only for the jitter probe.

        Suggestion values:

        |-----------------------------------|-------|
        |          Service                  |factor |
        |-----------------------------------|-------|
        |conventional wire-line             |  0    |
        |-----------------------------------|-------|
        |mobility within Building           |  5    |
        |-----------------------------------|-------|
        |mobility within geographical area  |  10   |
        |-----------------------------------|-------|
        |access to hard-to-reach location   |  20   |
        |-----------------------------------|-------|
        "
    DEFVAL  {0}
    ::= { ibNqaCtlEntry 15 }

ibNqaCtlCodecType OBJECT-TYPE
    SYNTAX     INTEGER
        {
            notDefined(1),     -- no codec type is defined or codec is unknown
            g711Alaw(2),       -- uses G.711 A-Law
            g711Ulaw(3),       -- uses G.711 muHmm-Law
            g729A(4),          -- uses G.729A
            icmpTimestamp(5)   -- uses icmp timestamp packet type
        }
    MAX-ACCESS read-create
    STATUS     current
    DESCRIPTION
        "To indicate the codec type to be used with jitter probe. This is
        applicable only for the jitter probe."
    DEFVAL     { notDefined }
    ::= { ibNqaCtlEntry 16 }

-- NQA Test Results Table

ibNqaResultsTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbNqaResultsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Defines the NQA Results Extend Table for providing
        the capability of performing NQA operations at a remote
        host. The results of these operations are stored in the
        ibNqaResultsTable. The operation of this table is same
        as that of pingResultsTable."
   ::= { ibNqaObjects 3 }

ibNqaResultsEntry OBJECT-TYPE
    SYNTAX      IbNqaResultsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The operation of this table is same as that of pingResultsTable."
    INDEX {
             pingCtlOwnerIndex,
             pingCtlTestName
          }
    ::= { ibNqaResultsTable 1 }

IbNqaResultsEntry ::=
    SEQUENCE {
        ibNqaResultsRttNumDisconnects   Unsigned32,
        ibNqaResultsRttTimeouts         Unsigned32,
        ibNqaResultsRttBusies           Unsigned32,
        ibNqaResultsRttNoConnections    Unsigned32,
        ibNqaResultsRttDrops            Unsigned32,
        ibNqaResultsRttSequenceErrors   Unsigned32,
        ibNqaResultsRttStatsErrors      Unsigned32,
        ibNqaResultsMaxDelaySD          Unsigned32,
        ibNqaResultsMaxDelayDS          Unsigned32,
        ibNqaResultsLostPacketRatio     Unsigned32,
        ibNqaResultsPacketLateArrival   Unsigned32,
        ibNqaResultsRttSum              Unsigned32,
        ibNqaResultsNumOfDelaySD        Unsigned32,
        ibNqaResultsMinDelaySD          Unsigned32,
        ibNqaResultsSumDelaySD          Unsigned32,
        ibNqaResultsSum2DelaySD         Unsigned32,
        ibNqaResultsNumOfDelayDS        Unsigned32,
        ibNqaResultsMinDelayDS          Unsigned32,
        ibNqaResultsSumDelayDS          Unsigned32,
        ibNqaResultsSum2DelayDS         Unsigned32
    }

ibNqaResultsRttNumDisconnects OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The number of times that a NQA probe failed because
        the destination became disconnected.

        For connectionless protocol pingCtlTypes, this number should be
        zero."
    ::= { ibNqaResultsEntry 1 }


ibNqaResultsRttTimeouts OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The number of times that a NQA probe failed because the
        probe timed out."
    ::= { ibNqaResultsEntry 2 }


ibNqaResultsRttBusies OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The number of times that a NQA probe failed because all
         NQA tasks have been running.

        If a new probe fails to start, the value of this object
        is incremented. The next test will not start until the
        next Frequency."
   ::= { ibNqaResultsEntry 3 }


ibNqaResultsRttNoConnections OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The number of times that a NQA probe failed because the probe
        destination is unavailable.

        For connectionless protocol pingCtlTypes, this number should be
        zero."
    ::= { ibNqaResultsEntry 4 }


ibNqaResultsRttDrops OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The number of times that a NQA operation failed to initiated
        for unavailibility of necessary internal resources."
    ::= { ibNqaResultsEntry 5 }


ibNqaResultsRttSequenceErrors OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The number of packets arrived out of sequence."
   ::= { ibNqaResultsEntry 6 }

ibNqaResultsRttStatsErrors OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The number of some other errors."
    ::= { ibNqaResultsEntry 7 }

ibNqaResultsMaxDelaySD OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "milliseconds"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The maximal delay from source device to destination device.
         Currently this value is only used for jitter test, and this
         value must be 0 in other type test. In jitter test, if
         destination system time is not valid, the SD delay may be set 0."
    ::= { ibNqaResultsEntry 8 }

