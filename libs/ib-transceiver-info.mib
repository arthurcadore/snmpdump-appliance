IB-TRANSCEIVER-INFO-MIB DEFINITIONS ::= BEGIN
IMPORTS
    MODULE-IDENTITY, OBJECT-TYPE, Integer32, Unsigned32, NOTIFICATION-TYPE
        FROM SNMPv2-SMI
    TruthValue, DateAndTime
        FROM SNMPv2-TC
    ifIndex, ifName
        FROM IF-MIB
    ibCommon
        FROM IB-OID-MIB;

    ibTransceiver MODULE-IDENTITY
        LAST-UPDATED "202308110000Z"
        ORGANIZATION
            "Intelbras S.A. Industria de Telecomunicacao Eletronica Brasileria."
        CONTACT-INFO
            ""
        DESCRIPTION
            "Add enumeration values for ibTransceiverChanLoopback,Change description of ibTransceiverChanCurPMD,
            ibTransceiverChanPMDAvg, ibTransceiverChanPMDMin, ibTransceiverChanPMDMax."
        REVISION "202308110000Z"
        DESCRIPTION
            "
            Add the ibTransceiverPERFCollInterval, ibTransceiverPERFCollEnable, ibTransceiverLCHTable.
            Added ibTransceiverModulationType, ibTransceiverFecMode to ibTransceiverInfoTable.
            Added ibTransceiverChanCfgTxPower, ibTransceiverChanLoopback, ibTransceiverChanFrequence,
            ibTransceiverChanGrid, ibTransceiverChanCurCD, ibTransceiverChanCDAvg,
            ibTransceiverChanCDMin, ibTransceiverChanCDMax, ibTransceiverChanCurPMD ,
            ibTransceiverChanPMDAvg, ibTransceiverChanPMDMin, ibTransceiverChanPMDMax ,
            ibTransceiverChanCurPDL, ibTransceiverChanPDLAvg,
            ibTransceiverChanPDLMin, ibTransceiverChanPDLMax,
            ibTransceiverChanCurSopRoc, ibTransceiverChanSopRocAvg, ibTransceiverChanSopRocMin,
            ibTransceiverChanSopRocMax, ibTransceiverChanCurOsnr, ibTransceiverChanOsnrAvg,
            ibTransceiverChanOsnrMin, ibTransceiverChanOsnrMax, ibTransceiverChanCurEsnr,
            ibTransceiverChanEsnrAvg, ibTransceiverChanEsnrMin, ibTransceiverChanEsnrMax,
            ibTransceiverChanCurCFO, ibTransceiverChanCFOAvg,
            ibTransceiverChanCFOMin, ibTransceiverChanCFOMax, ibTransceiverChanCurPreFecBer ,
            ibTransceiverChanPreFecBerAvg, ibTransceiverChanPreFecBerMin, ibTransceiverChanPreFecBerMax ,
            ibTransceiverChanPostFecFer, ibTransceiverChanFecUEBlocks, ibTransceiverChanEcBits,
            ibTransceiverChanUcWords, ibTransceiverChanTXPowerAvg, ibTransceiverChanTXPowerMin,
            ibTransceiverChanTXPowerMax, ibTransceiverChanRXPowerAvg, ibTransceiverChanRXPowerMin,
            ibTransceiverChanRXPowerMax, ibTransceiverChanCurHgSoPmd , ibTransceiverChanHgSoPmdAvg,
            ibTransceiverChanHgSoPmdMin, ibTransceiverChanHgSoPmdMax, ibTransceiverChanCurLgSoPmd,
            ibTransceiverChanLgSoPmdAvg, ibTransceiverChanLgSoPmdMin, ibTransceiverChanLgSoPmdMax,
            ibTransceiverChanLaserTemp to ibTransceiverChannelTable."
        REVISION "202307130000Z"
        DESCRIPTION
            "Add ibTransceiverTotalTXPower and ibTransceiverTotalRXPower."
        REVISION "202303010000Z"
        DESCRIPTION
            "The objects in this MIB module are used to display the
            information of transceiver on interface."
        REVISION "202207210000Z"
        DESCRIPTION
            "Add ibTransceiverCurrentPower and ibTransceiverMaxPower to ibTransceiverInfoTable.
            Add ibTransceiverCurrentTotalPower and ibTransceiverMaxTotalPower."
        REVISION "202205120000Z"
        DESCRIPTION
            "Add ibTransceiverRelySlotMfgName and ibTransceiverRelySlotMfgDate
            to ibTransceiverInfoTable."
        REVISION "202202220000Z"
        DESCRIPTION
            "Add the ibTransceiverLaneTable. Added ibTransceiverLaneCurRxPower,
             ibTransceiverLaneCurTxPower and ibTransceiverLaneBiasCurrent to ibTransceiverLaneTable."
        REVISION "202112020000Z"
        DESCRIPTION
            "Add the objects of ibTransceiverExtrAlarmEnable and ibTransceiverMaybeFake."
        REVISION "202103180000Z"
        DESCRIPTION
            "Add the objects of ibTransceiverPwrOutHiAlarmDbm, ibTransceiverPwrOutLoAlarmDbm,
            ibTransceiverPwrOutHiWarnDbm, ibTransceiverPwrOutLoWarnDbm, ibTransceiverRcvPwrHiAlarmDbm,
            ibTransceiverRcvPwrLoAlarmDbm, ibTransceiverRcvPwrHiWarnDbm, ibTransceiverRcvPwrLoWarnDbm.
            Added ibTransceiverChanTXPwrHiAmDbm, ibTransceiverChanTXPwrLoAmDbm
            to ibTransceiverChannelTable."
        REVISION "201908090000Z"
        DESCRIPTION
            "Added ibTransceiverOriginalSN to ibTransceiverInfoTable."
        REVISION "201901030000Z"
        DESCRIPTION
            "Added ibTransceiverPartNumber and ibTransceiverProductCode
            to ibTransceiverInfoTable."
        REVISION "201801030000Z"
        DESCRIPTION
            "Added ibTransceiverChannelBiasHiAm, ibTransceiverChannelBiasLoAm,
            ibTransceiverChannelTXPwrHiAm, ibTransceiverChannelTXPwrLoAm
            to ibTransceiverChannelTable."
        REVISION "201603090000Z"
        DESCRIPTION
            "Added ibTransceiverCurWaveErr, ibTransceiverWaveErrHiAlarm, ibTransceiverWaveErrLoAlarm,
            ibTransceiverCurFreqErr, ibTransceiverFreqErrHiAlarm and ibTransceiverFreqErrLoAlarm
            to ibTransceiverInfoTable."
        REVISION "201512250000Z"
        DESCRIPTION
            "Added ibTransceiverFrequency and ibTransceiverActiveITUChannel to ibTransceiverInfoTable.
            Added ibTransceiverITUChanTable for ITU channel information."
        REVISION "201408111050Z"
        DESCRIPTION
            "Add ibTransceiverVendorOUI and ibTransceiverRevisionNumber."
        REVISION "201306060000Z"
        DESCRIPTION
            "Add ibTransceiverChannelTable to transceiver channel statistics."
        REVISION "201206060000Z"
        DESCRIPTION
            "Add the objects of ibTransceiverTempHiAlarm, ibTransceiverTempLoAlarm,
            ibTransceiverTempHiWarn, ibTransceiverTempLoWarn, ibTransceiverVccHiAlarm,
            ibTransceiverVccLoAlarm, ibTransceiverVccHiWarn, ibTransceiverVccLoWarn,
            ibTransceiverBiasHiAlarm, ibTransceiverBiasLoAlarm, ibTransceiverBiasHiWarn,
            ibTransceiverBiasLoWarn, ibTransceiverPwrOutHiAlarm, ibTransceiverPwrOutLoAlarm,
            ibTransceiverPwrOutHiWarn, ibTransceiverPwrOutLoWarn, ibTransceiverRcvPwrHiAlarm,
            ibTransceiverRcvPwrLoAlarm, ibTransceiverRcvPwrHiWarn, ibTransceiverRcvPwrLoWarn,
            ibTransceiverErrors."
        REVISION "200912290000Z"
        DESCRIPTION
            "Add the objects of ibTransceiverDiagnostic, ibTransceiverCurTXPower,
            ibTransceiverMaxTXPower, ibTransceiverMinTXPower, ibTransceiverCurRXPower,
            ibTransceiverMaxRXPower, ibTransceiverMinRXPower, ibTransceiverTemperature,
            ibTransceiverVoltage, ibTransceiverBiasCurrent."
        REVISION "200606080000Z"
        DESCRIPTION
            "Change description of ibTransceiverWaveLength."
        REVISION "200601100000Z"
        DESCRIPTION
            "The initial revision of this MIB module."
            ::= { ibCommon 70 }

    ibTransceiverInfoAdm OBJECT IDENTIFIER ::= { ibTransceiver 1 }

    ibTransceiverInfoTable OBJECT-TYPE
        SYNTAX SEQUENCE OF IbTransceiverInfoEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "This table shows transceiver information on an interface."
        ::= { ibTransceiverInfoAdm 1 }

    ibTransceiverInfoEntry OBJECT-TYPE
        SYNTAX IbTransceiverInfoEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "The entry of the ibTransceiverInfoTable."
        INDEX { ifIndex }
        ::= { ibTransceiverInfoTable 1 }

    IbTransceiverInfoEntry ::=
        SEQUENCE {
            ibTransceiverHardwareType      OCTET STRING,
            ibTransceiverType              OCTET STRING,
            ibTransceiverWaveLength        Integer32,
            ibTransceiverVendorName        OCTET STRING,
            ibTransceiverSerialNumber      OCTET STRING,
            ibTransceiverFiberDiameterType INTEGER,
            ibTransceiverTransferDistance  Integer32,
            ibTransceiverDiagnostic        TruthValue,
            ibTransceiverCurTXPower        Integer32,
            ibTransceiverMaxTXPower        Integer32,
            ibTransceiverMinTXPower        Integer32,
            ibTransceiverCurRXPower        Integer32,
            ibTransceiverMaxRXPower        Integer32,
            ibTransceiverMinRXPower        Integer32,
            ibTransceiverTemperature       Integer32,
            ibTransceiverVoltage           Integer32,
            ibTransceiverBiasCurrent       Integer32,
            ibTransceiverTempHiAlarm       Integer32,
            ibTransceiverTempLoAlarm       Integer32,
            ibTransceiverTempHiWarn        Integer32,
            ibTransceiverTempLoWarn        Integer32,
            ibTransceiverVccHiAlarm        Integer32,
            ibTransceiverVccLoAlarm        Integer32,
            ibTransceiverVccHiWarn         Integer32,
            ibTransceiverVccLoWarn         Integer32,
            ibTransceiverBiasHiAlarm       Integer32,
            ibTransceiverBiasLoAlarm       Integer32,
            ibTransceiverBiasHiWarn        Integer32,
            ibTransceiverBiasLoWarn        Integer32,
            ibTransceiverPwrOutHiAlarm     Integer32,
            ibTransceiverPwrOutLoAlarm     Integer32,
            ibTransceiverPwrOutHiWarn      Integer32,
            ibTransceiverPwrOutLoWarn      Integer32,
            ibTransceiverRcvPwrHiAlarm     Integer32,
            ibTransceiverRcvPwrLoAlarm     Integer32,
            ibTransceiverRcvPwrHiWarn      Integer32,
            ibTransceiverRcvPwrLoWarn      Integer32,
            ibTransceiverErrors            BITS,
            ibTransceiverVendorOUI         OCTET STRING,
            ibTransceiverRevisionNumber    OCTET STRING,
            ibTransceiverFrequency         Integer32,
            ibTransceiverActiveITUChannel  Unsigned32,
            ibTransceiverCurWaveErr        Integer32,
            ibTransceiverWaveErrHiAlarm    Integer32,
            ibTransceiverWaveErrLoAlarm    Integer32,
            ibTransceiverCurFreqErr        Integer32,
            ibTransceiverFreqErrHiAlarm    Integer32,
            ibTransceiverFreqErrLoAlarm    Integer32,
            ibTransceiverPartNumber        OCTET STRING,
            ibTransceiverProductCode       OCTET STRING,
            ibTransceiverOriginalSN        OCTET STRING,
            ibTransceiverPwrOutHiAlarmDbm  Integer32,
            ibTransceiverPwrOutLoAlarmDbm  Integer32,
            ibTransceiverPwrOutHiWarnDbm   Integer32,
            ibTransceiverPwrOutLoWarnDbm   Integer32,
            ibTransceiverRcvPwrHiAlarmDbm  Integer32,
            ibTransceiverRcvPwrLoAlarmDbm  Integer32,
            ibTransceiverRcvPwrHiWarnDbm   Integer32,
            ibTransceiverRcvPwrLoWarnDbm   Integer32,
            ibTransceiverRelySlotMfgName   OCTET STRING,
            ibTransceiverRelySlotMfgDate   DateAndTime,
            ibTransceiverCurrentPower      Integer32,
            ibTransceiverMaxPower          Integer32,
            ibTransceiverTotalTXPower      Integer32,
            ibTransceiverTotalRXPower      Integer32,
            ibTransceiverModulationType    OCTET STRING,
            ibTransceiverFecMode           OCTET STRING
        }

    ibTransceiverHardwareType OBJECT-TYPE
        SYNTAX OCTET STRING
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Hardware type of the interface, such as SM(single mode)."
        ::= { ibTransceiverInfoEntry 1 }

    ibTransceiverType OBJECT-TYPE
        SYNTAX OCTET STRING
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Type of the interface, such as SFP/XFP/GBIC."
        ::= { ibTransceiverInfoEntry 2 }

    ibTransceiverWaveLength OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Wave length of the interface, measured in nanometer.
            The unit is nm."
        ::= { ibTransceiverInfoEntry 3 }

    ibTransceiverVendorName OBJECT-TYPE
        SYNTAX OCTET STRING
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Vendor name of the interface."
        ::= { ibTransceiverInfoEntry 4 }

    ibTransceiverSerialNumber OBJECT-TYPE
        SYNTAX OCTET STRING
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Serial number of the interface."
        ::= { ibTransceiverInfoEntry 5 }

    ibTransceiverFiberDiameterType OBJECT-TYPE
        SYNTAX INTEGER
            {
              fiber9(1),
              fiber50(2),
              fiber625(3),
              copper(4),
              unknown(65535)
            }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The diameter of the fiber, measured in micron.
            fiber9 - 9 micron multi-mode fiber
            fiber50 - 50 micron multi-mode fiber
            fiber625 - 62.5 micron multi-mode fiber
            copper - copper cable."
        ::= { ibTransceiverInfoEntry 6 }

    ibTransceiverTransferDistance OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The maximum distance which the interface can transmit,
            measured in meter."
        ::= { ibTransceiverInfoEntry 7 }

    ibTransceiverDiagnostic OBJECT-TYPE
        SYNTAX      TruthValue
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Indicating the digital diagnostic monitoring function."
        ::= { ibTransceiverInfoEntry 8 }

    ibTransceiverCurTXPower OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Indicating the current transmitted power.
            The unit is in hundredths of dBm."
        ::= { ibTransceiverInfoEntry 9 }

    ibTransceiverMaxTXPower OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Indicating the maximum transmitted power.
            The unit is in hundredths of dBm."
        ::= { ibTransceiverInfoEntry 10 }

    ibTransceiverMinTXPower OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Indicating the minimum transmitted power.
            The unit is in hundredths of dBm."
        ::= { ibTransceiverInfoEntry 11 }

    ibTransceiverCurRXPower OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Indicating the current received power.
            The unit is in hundredths of dBm."
        ::= { ibTransceiverInfoEntry 12 }

    ibTransceiverMaxRXPower OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Indicating the maximum received power.
            The unit is in hundredths of dBm."
        ::= { ibTransceiverInfoEntry 13 }

    ibTransceiverMinRXPower OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Indicating the minimum received power.
            The unit is in hundredths of dBm."
        ::= { ibTransceiverInfoEntry 14 }

    ibTransceiverTemperature OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Indicating the current temperature.
             The unit is Celsius centigrade."
        ::= { ibTransceiverInfoEntry 15 }

    ibTransceiverVoltage OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Indicating the current voltage.
            The unit is in hundredths of V"
        ::= { ibTransceiverInfoEntry 16 }

    ibTransceiverBiasCurrent OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Indicating the current bias electric current.
            The unit is in hundredths of mA"
        ::= { ibTransceiverInfoEntry 17 }

    ibTransceiverTempHiAlarm OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Transceiver temperature high alarm threshold
             in thousandths of degrees Celsius.
             As an example: 49120 is 49.120 degrees Celsius."
        ::= { ibTransceiverInfoEntry 18 }

    ibTransceiverTempLoAlarm OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Transceiver temperature low alarm threshold
             in thousandths of degrees Celsius.
             As an example: 49120 is 49.120 degrees Celsius."
        ::= { ibTransceiverInfoEntry 19 }

    ibTransceiverTempHiWarn OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Transceiver temperature high warning threshold
             in thousandths of degrees Celsius.
             As an example: 49120 is 49.120 degrees Celsius."
        ::= { ibTransceiverInfoEntry 20 }

    ibTransceiverTempLoWarn OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Transceiver temperature low warning threshold
             in thousandths of degrees Celsius.
             As an example: 49120 is 49.120 degrees Celsius."
        ::= { ibTransceiverInfoEntry 21 }

    ibTransceiverVccHiAlarm OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Transceiver VCC high alarm threshold
             in hundreds of microvolts.
             As an example: 32928 is 3.2928 volts.
             Returns zero if not supported on the transceiver."
        ::= { ibTransceiverInfoEntry 22 }

    ibTransceiverVccLoAlarm OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Transceiver VCC low alarm threshold
             in hundreds of microvolts.
             As an example: 32928 is 3.2928 volts.
             Returns zero if not supported on the transceiver."
        ::= { ibTransceiverInfoEntry 23 }

    ibTransceiverVccHiWarn OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Transceiver VCC high warning threshold
             in hundreds of microvolts.
             As an example: 32928 is 3.2928 volts.
             Returns zero if not supported on the transceiver."
        ::= { ibTransceiverInfoEntry 24 }

    ibTransceiverVccLoWarn OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Transceiver VCC low warning threshold
             in hundreds of microvolts.
             As an example: 32928 is 3.2928 volts.
             Returns zero if not supported on the transceiver."
        ::= { ibTransceiverInfoEntry 25 }

    ibTransceiverBiasHiAlarm OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Transceiver bias high alarm threshold in microamps."
        ::= { ibTransceiverInfoEntry 26 }

    ibTransceiverBiasLoAlarm OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Transceiver bias low alarm threshold in microamps."
        ::= { ibTransceiverInfoEntry 27 }

    ibTransceiverBiasHiWarn OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Transceiver bias high warning threshold in microamps."
        ::= { ibTransceiverInfoEntry 28 }

    ibTransceiverBiasLoWarn OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Transceiver bias low warning threshold in microamps."
        ::= { ibTransceiverInfoEntry 29 }

    ibTransceiverPwrOutHiAlarm OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Transceiver transmit power high alarm
             threshold in tenths of microwatts.
             As an example: 10000 is 1 milliwatt."
        ::= { ibTransceiverInfoEntry 30 }

    ibTransceiverPwrOutLoAlarm OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Transceiver transmit power low alarm
             threshold in tenths of microwatts.
             As an example: 10000 is 1 milliwatt."
        ::= { ibTransceiverInfoEntry 31 }

    ibTransceiverPwrOutHiWarn OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Transceiver transmit power high warning
             threshold in tenths of microwatts
             As an example: 10000 is 1 milliwatt."
        ::= { ibTransceiverInfoEntry 32 }

    ibTransceiverPwrOutLoWarn OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
