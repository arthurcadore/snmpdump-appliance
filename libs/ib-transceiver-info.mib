-- Transceiver ITUChannel Table

    ibTransceiverITUChanTable OBJECT-TYPE
        SYNTAX SEQUENCE OF IbTransceiverITUChanEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "This table provides information about ITU channels supported on the transceiver that is connected to this interface."
        ::= { ibTransceiverInfoAdm 3 }

    ibTransceiverITUChanEntry OBJECT-TYPE
        SYNTAX IbTransceiverITUChanEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "The entry of the ibTransceiverITUChanTable."
        INDEX { ifIndex, ibTransceiverITUChanIdx }
        ::= { ibTransceiverITUChanTable 1 }

    IbTransceiverITUChanEntry ::=
        SEQUENCE {
            ibTransceiverITUChanIdx            Unsigned32,
            ibTransceiverITUChanFreq          Integer32,
            ibTransceiverITUChanWaveLth        Integer32
        }

    ibTransceiverITUChanIdx OBJECT-TYPE
        SYNTAX      Unsigned32 (1..4294967295)
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The index of the specified ITU channel on the transceiver that is connected to this interface."
        ::= { ibTransceiverITUChanEntry 1 }

    ibTransceiverITUChanFreq OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The frequency of the specified ITU channel on the transceiver that is connected to this interface.
            The unit is hundredths of THz."
        ::= { ibTransceiverITUChanEntry 2 }

    ibTransceiverITUChanWaveLth OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The wave length of the specified ITU channel on the transceiver that is connected to this interface.
            The unit is pm."
        ::= { ibTransceiverITUChanEntry 3 }

