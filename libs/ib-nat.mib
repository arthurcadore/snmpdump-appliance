IB-NAT-MIB DEFINITIONS ::= BEGIN

                IMPORTS
                        ibCommon
                                FROM IB-OID-MIB
                        ifIndex
                                FROM IF-MIB
                        InetAddress, InetAddressType
                                FROM INET-ADDRESS-MIB
                        TimeTicks, IpAddress, Integer32, Counter32, Counter64, Unsigned32, OBJECT-TYPE,
                        MODULE-IDENTITY, NOTIFICATION-TYPE
                                FROM SNMPv2-SMI
                        RowStatus, DisplayString
                                FROM SNMPv2-TC;


                ibNat MODULE-IDENTITY
                        LAST-UPDATED "202208031105Z"             -- August 3, 2022 at 11:05 GMT
                        ORGANIZATION
                            "Intelbras S.A. Industria de Telecomunicacao Eletronica Brasileria."
                        CONTACT-INFO
                            ""
                        DESCRIPTION
                                "Add the ibNatIPPoolIpAllocFailTrap,
                                 ibNatIPPoolIpAllocFailRecoveryTrap,
                                 ibNatIPPoolIpApplyFailTrap,
                                 ibNatPortBlockAllocFailTrap,
                                 ibNatPortBlockAllocFailRecoveryTrap,
                                 ibNatPortAllocFailTrap,
                                 ibNatPortAllocFailRecoveryTrap,
                                 ibNatPortUsageTrap,
                                 ibNatPortUsageRecoveryTrap"

                        REVISION "202208031105Z"                -- August 3, 2022 at 11:05 GMT
                        DESCRIPTION
                                "Add the ibNat444PortBlockOverTrap,ibNatEIMOverTrap,ibNatEIMBelowTrap,
                                ibNat444PortBlockBelowTrap,ibNatNoPatOverTrap,ibNatNoPatBelowTrap,
                                ibNatIPPoolAddrUsageRecoveryTraps,ibNatNqaTrap, ibNatNqaExcessiveTrap,
                ibNatNqaBelowTrap,ibNatPortSplitTrap, ibNatPortRecycleTrap,
                ibNatTrapChassis, ibNatTrapSlot, ibNatTrapCpu, ibNatAddrNum, ibNat444PortBlockCount,
                ibNat444PortBlockUseCount, ibNat444PortBlockUsage, ibNatNqaAddr, ibNatConflictVar"
            REVISION "202109171711Z"                -- september 17, 2021 at 17:11 GMT
                        DESCRIPTION
                                "Add the ibNatIPPoolGroupTable,
                                ibNatIPPoolAddrUsageExceedTraps,
                                ibNatIPPoolAddrUsageRecoveryTraps"

                        REVISION "202012291440Z"                -- December 29, 2020 at 14:40 GMT
                        DESCRIPTION
                                "This MIB contains objects to manage configuration of NAT.
                                There are no constraints on this MIB."

                        REVISION "201912011446Z"                -- December 1, 2019 at 14:46 GMT
                        DESCRIPTION
                                "Add the ibInstAddrGrpUsgTable,
                                ibInstAddrGrpMemberUsgTable,
                                ibInstAddrGrpUsageTraps."

                        REVISION "201910101752Z"                -- October 10, 2019 at 17:52 GMT
