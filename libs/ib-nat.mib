    OBJECTS
        {
            ibNATBandwidthRatio,
            ibNATChassis,
            ibNATSlot,
            ibNATCpu
        }
    STATUS      current
    DESCRIPTION
    "Send trap when the bandwidth usage of a CGN card has reached the ibNATBandwidthRatio."
    ::= { ibNATBandwidthPrefix 1 }
ibNATBandwidthRecoveryTrap NOTIFICATION-TYPE
    OBJECTS
        {
            ibNATChassis,
            ibNATSlot,
            ibNATCpu
         }
    STATUS      current
    DESCRIPTION
    "Send trap when the CGN card bandwidth usage has dropped below the threshold."
    ::= { ibNATBandwidthPrefix 2 }
