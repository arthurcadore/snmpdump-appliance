                        DESCRIPTION
                                "Add the ibNATTotalNATSessionCount,
                                 ibNATEimTableCount,
                                 ibNATInboundNoPATTableCount,
                                 ibNATOutboundNoPATTableCount,
                                 ibNATMaxDynamicPortblock,
                                 ibNATMaxStaticPortblock,
                                 ibNATActiveDynamicPortblock,
                                 ibNATActiveStaticPortblock."

                        REVISION "200501201518Z"                -- January 20, 2005 at 15:18 GMT
                        DESCRIPTION
                                "The initial revision of this MIB module."
                        ::= { ibCommon 18 }




--
-- Node definitions
--

-- Global Variants
                ibNATGlobalVars OBJECT IDENTIFIER ::= { ibNat 1 }


--                     **************Global Variants***************
-- NAT Clear Session
                ibNATClearSession OBJECT IDENTIFIER ::= { ibNATGlobalVars 1 }


                ibNATClearSessionSlotNo OBJECT-TYPE
                        SYNTAX Integer32 (1..14 | 255)
                        MAX-ACCESS read-write
                        STATUS current
                        DESCRIPTION
                                "The number of NAT board in which administrator wants to
                                clear data of the session. In router, the value is 255
                                because no NAT board in it. This node only supports 'set' operation.
                                The return value of 'get' operation is meaningless."
                        ::= { ibNATClearSession 1 }


-- NAT blacklist UserConnectLimitPara
                ibNATBLConnectLimitPara OBJECT IDENTIFIER ::= { ibNATGlobalVars 2 }


                ibNATBLConnectHighValue OBJECT-TYPE
                        SYNTAX Integer32 (20..20000)
                        MAX-ACCESS read-write
                        STATUS current
                        DESCRIPTION
                                "The maximal connection amount.  If the connection amount exceeds the
                                value, the IP address will be added into blacklist.  The IP address
                                will not be removed from blacklist until the connection amount is
                                less than the minimal connection amount."
                        DEFVAL { 500 }
                        ::= { ibNATBLConnectLimitPara 1 }


                ibNATBLConnectLowValue OBJECT-TYPE
                        SYNTAX Integer32 (20..20000)
                        MAX-ACCESS read-write
                        STATUS current
                        DESCRIPTION
                                "The minimal connection amount."
                        DEFVAL { 200 }
                        ::= { ibNATBLConnectLimitPara 2 }


                ibNATBLConnectHighRate OBJECT-TYPE
                        SYNTAX Integer32 (20..21474836)
                        MAX-ACCESS read-write
                        STATUS current
                        DESCRIPTION
                                "The maximal connection rate(in second).  If the connection rate exceed the
                                value, the IP address will be added into blacklist.  The IP address
                                will not be removed from blacklist until the connection rate is
                                less than the minimal connection rate."
                        DEFVAL { 250 }
                        ::= { ibNATBLConnectLimitPara 3 }


                ibNATBLConnectLowRate OBJECT-TYPE
                        SYNTAX Integer32 (20..21474836)
                        MAX-ACCESS read-write
                        STATUS current
                        DESCRIPTION
                                "The minimal connection rate(in second)."
                        DEFVAL { 100 }
                        ::= { ibNATBLConnectLimitPara 4 }


                ibNATBLSpecialConnectHighRate OBJECT-TYPE
                        SYNTAX Integer32 (20..21474836)
                        MAX-ACCESS read-write
                        STATUS current
                        DESCRIPTION
                                "The maximal special connection rate(in second).  When administrator
                                wants to control the speed of connection, he can selects the connection
                                speed rate or special connection rate."
                        DEFVAL { 250 }
                        ::= { ibNATBLConnectLimitPara 5 }


                ibNATBLSpecialConnectLowRate OBJECT-TYPE
                        SYNTAX Integer32 (20..21474836)
                        MAX-ACCESS read-write
                        STATUS current
                        DESCRIPTION
                                "The minimal special connection rate(in second)."
                        DEFVAL { 100 }
                        ::= { ibNATBLConnectLimitPara 6 }


-- NAT blacklist property control Enable
                ibNATBLCtrlEnable OBJECT IDENTIFIER ::= { ibNATGlobalVars 3 }


                ibNATBLConnectSumEnable OBJECT-TYPE
                        SYNTAX INTEGER
                                {
                                enable(1),
                                disable(2)
                                }
                        MAX-ACCESS read-write
                        STATUS current
                        DESCRIPTION
                                "Enable or disable the connection amount control."
                        DEFVAL { disable }
                        ::= { ibNATBLCtrlEnable 1 }


                ibNATBLConnectRateEnable OBJECT-TYPE
                        SYNTAX INTEGER
                                {
                                enable(1),
                                disable(2)
                                }
                        MAX-ACCESS read-write
                        STATUS current
                        DESCRIPTION
                                "Enable or disable the connection rate control."
                        DEFVAL { disable }
                        ::= { ibNATBLCtrlEnable 2 }


-- NAT NP aging-time
                ibNATNPTimer OBJECT IDENTIFIER ::= { ibNATGlobalVars 4 }


                ibNATNPAgingTime OBJECT-TYPE
                        SYNTAX INTEGER
                                {
                                fast(1),
                                slow(2)
                                }
                        MAX-ACCESS read-write
                        STATUS current
                        DESCRIPTION
                                "There are two kinds of aging time in NP.  One is fast and the other is
                                slow.  Administrator can select one or other.  The default is fast."
                        DEFVAL { fast }
                        ::= { ibNATNPTimer 1 }


                ibNATMibObjects OBJECT IDENTIFIER ::= { ibNat 2 }


--                      **************MibObjects***************
-- NAT address pool table
                ibNATPoolInfoTable OBJECT-TYPE
                        SYNTAX SEQUENCE OF IbNATPoolInfoEntry
                        MAX-ACCESS not-accessible
                        STATUS current
                        DESCRIPTION
                                "NAT address pool table : The address in the pool is
                                the IP address in the global network. The pool can span
                                255 IP addresses.All address pools configured by administrator
                                are recorded in this table."
                        ::= { ibNATMibObjects 1 }


                ibNATPoolInfoEntry OBJECT-TYPE
                        SYNTAX IbNATPoolInfoEntry
                        MAX-ACCESS not-accessible
                        STATUS current
                        DESCRIPTION
                                "The entry of NAT address pool table."
                        INDEX { ibNATPoolIdx }
                        ::= { ibNATPoolInfoTable 1 }


                IbNATPoolInfoEntry ::=
                        SEQUENCE {
                                ibNATPoolIdx
                                        Integer32,
                                ibNATPoolStartIpAddr
                                        IpAddress,
                                ibNATPoolEndIpAddr
                                        IpAddress,
                                ibNATPoolSlotNo
                                        Integer32,
                                ibNATPoolRefCounter
                                        Integer32,
                                ibNATPoolRowStatus
                                        RowStatus
                         }

                ibNATPoolIdx OBJECT-TYPE
                        SYNTAX Integer32 (1..320)
                        MAX-ACCESS not-accessible
                        STATUS current
                        DESCRIPTION
                                "Index of the address pool: The index uniquely indicates
                                the address pool in the system, and its range is from 1 to 320. "
                        ::= { ibNATPoolInfoEntry 1 }


                ibNATPoolStartIpAddr OBJECT-TYPE
                        SYNTAX IpAddress
                        MAX-ACCESS read-create
                        STATUS current
                        DESCRIPTION
                                "The start IP address of address pool:
                                It must be less than or equal to the end IP address."
                        ::= { ibNATPoolInfoEntry 2 }


                ibNATPoolEndIpAddr OBJECT-TYPE
                        SYNTAX IpAddress
                        MAX-ACCESS read-create
                        STATUS current
                        DESCRIPTION
                                "The end IP address of address pool:
                                It must be more than or equal to the start IP address."
                        ::= { ibNATPoolInfoEntry 3 }


                ibNATPoolSlotNo OBJECT-TYPE
                        SYNTAX Integer32 (1..14 | 255)
                        MAX-ACCESS read-create
                        STATUS current
                        DESCRIPTION
                                "The number of NAT board to which the address pool is bound.
                                The address translation is processed by this NAT board .
                                The relation between address pool and slotNo is n:1.
                                When the address pool is not bound to the NAT board,the value is 255. "
                        ::= { ibNATPoolInfoEntry 4 }


                ibNATPoolRefCounter OBJECT-TYPE
                        SYNTAX Integer32
                        MAX-ACCESS read-create
                        STATUS current
                        DESCRIPTION
                                "The counter of reference.  An address pool can be associated with
                                more than one ACL and the counter of reference is recorded in this
                                field.  When the address pool is not associated with ACL, the value is 0."
                        ::= { ibNATPoolInfoEntry 5 }


                ibNATPoolRowStatus OBJECT-TYPE
                        SYNTAX RowStatus
                        MAX-ACCESS read-create
                        STATUS current
                        DESCRIPTION
                                "Only support 'destroy' and 'createAndGo'.
                                If ibNATPoolIdx, ibNATPoolStartIpAddr and
                                ibNATPoolEndIpAddr were provided correctly,
                                its value is changed 'active'."
                        ::= { ibNATPoolInfoEntry 6 }


--     NAT Out bound Table
                ibNATOutboundTable OBJECT-TYPE
                        SYNTAX SEQUENCE OF IbNATOutboundEntry
                        MAX-ACCESS not-accessible
                        STATUS current
                        DESCRIPTION
                                "NAT out bound table: through associating ACL number,
                                address pool with NAT board, administrator can designate
                                which inside IP address can be translated to global IP
                                address in the pool and the translation is processed by
                                which NAT board."
                        ::= { ibNATMibObjects 2 }


                ibNATOutboundEntry OBJECT-TYPE
                        SYNTAX IbNATOutboundEntry
                        MAX-ACCESS not-accessible
                        STATUS current
                        DESCRIPTION
                                "The entry of NAT out bound table."
                        INDEX { ifIndex, ibNATOutboundAclNo }
                        ::= { ibNATOutboundTable 1 }


                IbNATOutboundEntry ::=
                        SEQUENCE {
                                ibNATOutboundAclNo
                                        Integer32,
                                ibNATOutboundPoolIdx
                                        Integer32,
                                ibNATOutboundIsNoPat
                                        INTEGER,
                                ibNATOutboundSlotNo
                                        Integer32,
                                ibNATOutboundRowStatus
                                        RowStatus
                         }

                ibNATOutboundAclNo OBJECT-TYPE
                        SYNTAX Integer32 (2000..3999)
                        MAX-ACCESS not-accessible
                        STATUS current
                        DESCRIPTION
                                "The number of ACL(Access Control List).  When the packet accords with
                                the rule in the ACL, its source IP address will be translated to IP
                                address in Address pool."
                        ::= { ibNATOutboundEntry 1 }


                ibNATOutboundPoolIdx OBJECT-TYPE
                        SYNTAX Integer32 (0..320 | 2147483647)
                        MAX-ACCESS read-create
                        STATUS current
                        DESCRIPTION
                                "The index of address pool in ibNATPoolInfoTable.
                                when administrator doesn't use the address pool and
                                use the IP address of the interface as the address of
                                global network, the value is 2147483647."
                        ::= { ibNATOutboundEntry 2 }


                ibNATOutboundIsNoPat OBJECT-TYPE
                        SYNTAX INTEGER
                                {
                                true(1),
                                false(2)
                                }
                        MAX-ACCESS read-create
                        STATUS current
                        DESCRIPTION
                                "Whether to use no pat manner.  The translation of no pat
                                is that the translation doesn't use the port information of packet."
                        DEFVAL { false }
                        ::= { ibNATOutboundEntry 3 }


                ibNATOutboundSlotNo OBJECT-TYPE
                        SYNTAX Integer32 (1..14 | 255)
                        MAX-ACCESS read-create
                        STATUS current
                        DESCRIPTION
                                "The number of NAT board.  In router, the value is 255 because no NAT board in it."
                        ::= { ibNATOutboundEntry 4 }


                ibNATOutboundRowStatus OBJECT-TYPE
                        SYNTAX RowStatus
                        MAX-ACCESS read-create
                        STATUS current
                        DESCRIPTION
                                "Only support 'destroy' and 'createAndGo'.
                                If ibNATOutboundAclNo or ibNATOutboundAclNo
                                and ibNATOutboundPoolIdx were provided correctly,
                                its value is changed 'active'."
                        ::= { ibNATOutboundEntry 5 }


--     NAT inside Server Table
                ibNATServerTable OBJECT-TYPE
                        SYNTAX SEQUENCE OF IbNATServerEntry
                        MAX-ACCESS not-accessible
                        STATUS current
                        DESCRIPTION
                                "NAT inside Server Table.  Administrator can configure server in the
                                private network which can provide service for people in the public network.
                                The translation map is :
                                (InsideIP     , InsidePort--> GlobalIP, GlobalPort    )
                                (InsideIP + 1 , InsidePort--> GlobalIP, GlobalPort + 1)
                                (InsideIP + 2 , InsidePort--> GlobalIP, GlobalPort + 2)
                                ...
                                ...
                                ...
                                (InsideIP+n(=InsideIP2),InsidePort--> GlobalIP,GlobalPort+n(=GlobalPort2))
                                "
                        ::= { ibNATMibObjects 3 }


                ibNATServerEntry OBJECT-TYPE
                        SYNTAX IbNATServerEntry
                        MAX-ACCESS not-accessible
                        STATUS current
                        DESCRIPTION
                                "The entry of NAT inside server table."
                        INDEX { ifIndex, ibNATServerProType, ibNATServerGlobalIP, ibNATServerStartGlobalPort, ibNATServerVpnIndex
                                 }
                        ::= { ibNATServerTable 1 }


                IbNATServerEntry ::=
                        SEQUENCE {
                                ibNATServerProType
                                        Integer32,
                                ibNATServerGlobalIP
                                        IpAddress,
                                ibNATServerStartGlobalPort
                                        Integer32,
                                ibNATServerEndGlobalPort
                                        Integer32,
                                ibNATServerStartInsideIP
                                        IpAddress,
                                ibNATServerEndInsideIP
                                        IpAddress,
                                ibNATServerInsidePort
                                        Integer32,
                                ibNATServerSlotNo
                                        Integer32,
                                ibNATServerVpnIndex
                                        Integer32,
                                ibNATServerAclNumber
                                        Integer32,
                                ibNATServerRowStatus
                                        RowStatus
                         }

                ibNATServerProType OBJECT-TYPE
                        SYNTAX Integer32 (1..255)
                        MAX-ACCESS not-accessible
                        STATUS current
                        DESCRIPTION
                                "The type of protocol: icmp(1), tcp(6), udp(17) and others."
                        ::= { ibNATServerEntry 1 }


                ibNATServerGlobalIP OBJECT-TYPE
                        SYNTAX IpAddress
                        MAX-ACCESS not-accessible
                        STATUS current
                        DESCRIPTION
                                "The global IP address of Inside Server."
                        ::= { ibNATServerEntry 2 }


                ibNATServerStartGlobalPort OBJECT-TYPE
                        SYNTAX Integer32 (0..65535)
                        MAX-ACCESS not-accessible
                        STATUS current
                        DESCRIPTION
                                "The start global port of Inside Server.
                                When the port need not be configured, the value is 0."
                        ::= { ibNATServerEntry 3 }


                ibNATServerEndGlobalPort OBJECT-TYPE
                        SYNTAX Integer32 (0..65535)
                        MAX-ACCESS read-create
                        STATUS current
                        DESCRIPTION
                                "The end global port of Inside Server.  When administrator doesn't
                                configure a series of inside server, the value is 0.
                                If the value is not zero, the value of (GlobalPort2-GlobalPort)
                                must be equal to (InsideIP2 - InsideIP)."
                        ::= { ibNATServerEntry 4 }


                ibNATServerStartInsideIP OBJECT-TYPE
                        SYNTAX IpAddress
                        MAX-ACCESS read-create
                        STATUS current
                        DESCRIPTION
                                "The start private IP address of Inside Server."
                        ::= { ibNATServerEntry 5 }


                ibNATServerEndInsideIP OBJECT-TYPE
                        SYNTAX IpAddress
                        MAX-ACCESS read-create
                        STATUS current
                        DESCRIPTION
                                "The end private IP address of Inside Server.  When administrator doesn't
                                configure a series of inside server, the value is 0.0.0.0."
                        ::= { ibNATServerEntry 6 }


                ibNATServerInsidePort OBJECT-TYPE
                        SYNTAX Integer32 (0..65535)
                        MAX-ACCESS read-create
                        STATUS current
                        DESCRIPTION
                                "The private port of Inside Server."
                        ::= { ibNATServerEntry 7 }


                ibNATServerSlotNo OBJECT-TYPE
                        SYNTAX Integer32 (1..14 | 255)
                        MAX-ACCESS read-create
                        STATUS current
                        DESCRIPTION
                                "The number of NAT board.  In router, the value is 255 because no NAT board in it."
                        ::= { ibNATServerEntry 8 }


                ibNATServerVpnIndex OBJECT-TYPE
                        SYNTAX Integer32 (0..65535)
                        MAX-ACCESS not-accessible
                        STATUS current
                        DESCRIPTION
                                "The VPN index of server."
                        ::= { ibNATServerEntry 10 }


                ibNATServerAclNumber OBJECT-TYPE
                        SYNTAX Integer32 (1..10000)
                        MAX-ACCESS read-create
                        STATUS current
                        DESCRIPTION
                                "Number of basic or advanced ACL."
                        ::= { ibNATServerEntry 11 }


                ibNATServerRowStatus OBJECT-TYPE
                        SYNTAX RowStatus
                        MAX-ACCESS read-create
                        STATUS current
                        DESCRIPTION
                                "Only support 'destroy' and 'createAndGo'.
                                If ibNATServerProType, ibNATServerGlobalIP, ibNATServerGlobalPort
                                and ibNATServerInsideIP were provided correctly,
                                its value is changed 'active'."
                        ::= { ibNATServerEntry 12 }


--     NAT Time out Table
                ibNATTimeOutTable OBJECT-TYPE
                        SYNTAX SEQUENCE OF IbNATTimeOutEntry
                        MAX-ACCESS not-accessible
                        STATUS current
                        DESCRIPTION
                                "NAT Time out Table. When a connection is established,
                                if there are not any actives in this connection between
                                the time, it will be disconnected."
                        ::= { ibNATMibObjects 4 }


                ibNATTimeOutEntry OBJECT-TYPE
                        SYNTAX IbNATTimeOutEntry
                        MAX-ACCESS not-accessible
                        STATUS current
                        DESCRIPTION
                                "The entry of NAT Time out Table."
                        INDEX { ibNATTimeOutProtocol }
                        ::= { ibNATTimeOutTable 1 }


                IbNATTimeOutEntry ::=
                        SEQUENCE {
                                ibNATTimeOutProtocol
                                        INTEGER,
                                ibNATTimeOutTimeValue
                                        Integer32
                         }

                ibNATTimeOutProtocol OBJECT-TYPE
                        SYNTAX INTEGER
                                {
                                tcp(1),
                                udp(2),
                                icmp(3),
                                pptp(4),
                                dns(5),
                                tcpFin(6),
                                tcpSyn(7),
                                ftpCtrl(8),
                                ftpData(9)
                                }
                        MAX-ACCESS not-accessible
                        STATUS current
                        DESCRIPTION
                                "The type of protocol."
                        ::= { ibNATTimeOutEntry 1 }


                ibNATTimeOutTimeValue OBJECT-TYPE
                        SYNTAX Integer32 (10..86400)
                        MAX-ACCESS read-write
                        STATUS current
                        DESCRIPTION
                                "The time of time out."
                        ::= { ibNATTimeOutEntry 2 }


--     NAT blacklist Enable(start/stop) Table
                ibNATBLEnableTable OBJECT-TYPE
                        SYNTAX SEQUENCE OF IbNATBLEnableEntry
                        MAX-ACCESS not-accessible
                        STATUS current
                        DESCRIPTION
                                "NAT blacklist function Enable Table.  In this table, administrator can decide
                                whether to start the function of blacklist in the NAT board.
                                When the function of blacklist can't be set for a NAT board,
                                the number of NAT board is 256."
                        ::= { ibNATMibObjects 5 }


                ibNATBLEnableEntry OBJECT-TYPE
                        SYNTAX IbNATBLEnableEntry
                        MAX-ACCESS not-accessible
                        STATUS current
                        DESCRIPTION
                                "The entry of NAT blacklist Enable Table."
                        INDEX { ibNATBLEnableSlotNo }
                        ::= { ibNATBLEnableTable 1 }


                IbNATBLEnableEntry ::=
                        SEQUENCE {
                                ibNATBLEnableSlotNo
                                        Integer32,
                                ibNATBLEnable
                                        INTEGER
                         }

                ibNATBLEnableSlotNo OBJECT-TYPE
                        SYNTAX Integer32 (1..14 | 255)
                        MAX-ACCESS not-accessible
                        STATUS current
                        DESCRIPTION
                                "The number of NAT board.
                                When the function of blacklist can't be set for a NAT board,
                                the value is 255."
                        ::= { ibNATBLEnableEntry 1 }


                ibNATBLEnable OBJECT-TYPE
                        SYNTAX INTEGER
                                {
                                enable(1),
                                disable(2)
                                }
                        MAX-ACCESS read-write
                        STATUS current
                        DESCRIPTION
                                "Start or stop of function of blacklist."
                        DEFVAL { disable }
                        ::= { ibNATBLEnableEntry 2 }


--     NAT special IP ConnectLimitPara Table
                ibNATBLIPConnectLimitParaTable OBJECT-TYPE
                        SYNTAX SEQUENCE OF IbNATBLIPConnectLimitParaEntry
                        MAX-ACCESS not-accessible
                        STATUS current
                        DESCRIPTION
                                "The table of connection control of special IP address.  In this table, administrator can decide to
                                adopt which manner to control the connection which IP address is the
                                special IP address."
                        ::= { ibNATMibObjects 6 }


                ibNATBLIPConnectLimitParaEntry OBJECT-TYPE
                        SYNTAX IbNATBLIPConnectLimitParaEntry
                        MAX-ACCESS not-accessible
                        STATUS current
                        DESCRIPTION
                                "The entry of the table of connection control of special IP address."
                        INDEX { ibNATBLIPConnectLimitParaIP }
                        ::= { ibNATBLIPConnectLimitParaTable 1 }


                IbNATBLIPConnectLimitParaEntry ::=
                        SEQUENCE {
                                ibNATBLIPConnectLimitParaIP
                                        IpAddress,
                                ibNATBLIPConnectHighValue
                                        Integer32,
                                ibNATBLIPConnectLowValue
                                        Integer32,
                                ibNATBLIPUseSpecialConnectRate
                                        INTEGER,
                                ibNATBLIPConnectLimitRowStatus
                                        RowStatus
                         }

                ibNATBLIPConnectLimitParaIP OBJECT-TYPE
                        SYNTAX IpAddress
                        MAX-ACCESS not-accessible
                        STATUS current
                        DESCRIPTION
                                "The special IP address.  The IP is private IP address."
                        ::= { ibNATBLIPConnectLimitParaEntry 1 }


                ibNATBLIPConnectHighValue OBJECT-TYPE
                        SYNTAX Integer32 (20..20000)
                        MAX-ACCESS read-create
                        STATUS current
                        DESCRIPTION
                                "The maximal connection amount.  It must be more than the minimal connection amount."
                        DEFVAL { 500 }
                        ::= { ibNATBLIPConnectLimitParaEntry 2 }


                ibNATBLIPConnectLowValue OBJECT-TYPE
                        SYNTAX Integer32 (20..20000)
                        MAX-ACCESS read-create
                        STATUS current
                        DESCRIPTION
                                "The minimal connection amount.  It must be less than the maximal connection amount."
                        DEFVAL { 200 }
                        ::= { ibNATBLIPConnectLimitParaEntry 3 }


