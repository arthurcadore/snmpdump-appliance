        DEFVAL { 0 }
        ::= { ibDvpnMibGlobal 14 }

    ibDvpnMapNumber  OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "
                The number of dvpn maps.
            "
        DEFVAL { 0 }
        ::= { ibDvpnMibGlobal 15 }

    ibDvpnSessionNumber OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
             "
                 The number of dvpn sessiones including the session of all states.
             "
        DEFVAL { 0 }
        ::= {  ibDvpnMibGlobal 16 }

    ibDvpnServerPreSharedKey  OBJECT-TYPE
        SYNTAX OCTET STRING (SIZE (0..127))
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "
                The pre-shared-key of server.
                It is only used on server.
            "
        ::= { ibDvpnMibGlobal 17 }

    ibDvpnMapTrapEnable OBJECT-TYPE
        SYNTAX INTEGER
            {
                enable(1),
                disable(2)
            }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "
                Dvpn map trap enable or disable.
            "
        DEFVAL { enable }
        ::= { ibDvpnMibGlobal 18 }

    ibDvpnSessionTrapEnable OBJECT-TYPE
        SYNTAX INTEGER
            {
                enable(1),
                disable(2)
            }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "
                Dvpn session trap enable or disable.
            "
        DEFVAL { enable }
        ::= { ibDvpnMibGlobal 19 }

    ibDvpnVersion  OBJECT-TYPE
        SYNTAX INTEGER
            {
                version2(1),
                version3(2)
            }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "
                Dvpn version.
            "
        ::= { ibDvpnMibGlobal 20 }

    ibDvpnClearDomainAllConection  OBJECT-TYPE
        SYNTAX Integer32(1..65535)
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "
                Clear all dvpn conects in the domain, the parameter is
                the id of the domain.
            "
        ::= { ibDvpnMibGlobal 21 }

    ibDvpnClearDvpnStaInfo OBJECT-TYPE
        SYNTAX Integer32(1)
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "
                Clear the statistic information of dvpn.
            "
        ::= {  ibDvpnMibGlobal 22 }

    ibDvpnTotalRedirectNumber OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "
                The number of redirect packets.
                It is only used on server.
            "
        DEFVAL { 0 }
        ::= { ibDvpnMibGlobal 23 }


    ibDvpnGlobalAuthenClientType OBJECT-TYPE
        SYNTAX INTEGER
            {
                 none(1),
                 pap(2),
                 chap(3)
            }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "
                Dvpn global authentication type being used by server to client.
                The value is decided by the configuration by the user.
            "
        DEFVAL { none }
        ::= { ibDvpnMibGlobal 24 }



    ibDvpnGlobalUserDefAAADomain OBJECT-TYPE
        SYNTAX OCTET STRING (SIZE (0..24))
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "
                Dvpn global AAA domain of the user. The domain will decide the
                authentication server which is used by the dvpn server to
                authenticate dvpn client.
            "
        ::= { ibDvpnMibGlobal 25 }

    ibDvpnLocalDeviceId OBJECT-TYPE
        SYNTAX OCTET STRING
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "
                The device ID, it is a string.
            "
        ::= {  ibDvpnMibGlobal 26 }

    ibDvpnSessionHisAgeTime OBJECT-TYPE
        SYNTAX Integer32(1..100)
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "
                The age-time of the ibDvpnSessionHisTable table.
                The unit is hour.
            "
        DEFVAL { 50 }
        ::= {  ibDvpnMibGlobal 27 }

    ibDvpnSessionHisReset OBJECT-TYPE
        SYNTAX INTEGER
            {
                 reset(1)
            }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "
                The reset flag of dvpn ibDvpnSessionHisTable table.
            "
        ::= {  ibDvpnMibGlobal 28 }


--ibDvpnPolicyTable

    ibDvpnPolicyTable  OBJECT-TYPE
        SYNTAX SEQUENCE OF IbDvpnPolicyEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "
                The dvpn policy table.
                It is used on dvpn server.
            "
        ::= { ibDvpnMibTableTroop 1 }

    ibDvpnPolicyEntry OBJECT-TYPE
        SYNTAX IbDvpnPolicyEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "
                The entry of ibDvpnPolicyTable.
            "
        INDEX { ibDvpnPolicyName }
        ::= { ibDvpnPolicyTable 1 }

    IbDvpnPolicyEntry ::=
        SEQUENCE {
            ibDvpnPolicyName                            OCTET STRING,
            ibDvpnPoAuthenClientType                    INTEGER,
            ibDvpnPoSessionAlgorithmSuite               DvpnAlgorithmSuite,
            ibDvpnPoSessionIdleTime                     Integer32,
            ibDvpnPoSessionKeepTime                     Integer32,
            ibDvpnPoSessionSetupInterval                Integer32,
            ibDvpnPoDataAlgorithmSuite                  DvpnAlgorithmSuite,
            ibDvpnPoSaSeconds                           Integer32,
            ibDvpnPoUserDefAAADomain                    OCTET STRING,
            ibDvpnPoRefTimes                            Integer32,
            ibDvpnPoRowStatus                           RowStatus
        }

    ibDvpnPolicyName OBJECT-TYPE
        SYNTAX OCTET STRING (SIZE(1..31))
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "
                The name of Dvpn policy.
            "
        ::= { ibDvpnPolicyEntry 1 }

    ibDvpnPoAuthenClientType OBJECT-TYPE
        SYNTAX INTEGER
            {
                none(1),
                pap(2),
                chap(3)
            }
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
            "
                The authentication type used by server to client.
                The value is decided by the policy configuration of the user.
                The default value is none.
            "
        DEFVAL { none }
        ::= { ibDvpnPolicyEntry 2 }

    ibDvpnPoSessionAlgorithmSuite OBJECT-TYPE
        SYNTAX DvpnAlgorithmSuite
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
            "
                The algorithm suite used when session is created.
            "
        DEFVAL { dvpnDesCbcMd5PreShaModp768 }
        ::= { ibDvpnPolicyEntry 3 }

    ibDvpnPoSessionIdleTime OBJECT-TYPE
        SYNTAX Integer32(60..86400)
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
            "
                The session idle time, the unit is second.
            "
        DEFVAL { 300 }
        ::= { ibDvpnPolicyEntry 4 }

    ibDvpnPoSessionKeepTime OBJECT-TYPE
        SYNTAX Integer32(5..300)
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
            "
                The session keepalive time, the unit is second.
            "
        DEFVAL { 10 }
        ::= { ibDvpnPolicyEntry 5 }

    ibDvpnPoSessionSetupInterval OBJECT-TYPE
        SYNTAX Integer32(5..60)
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
            "
                The session setup interval time, the unit is second.
            "
        DEFVAL { 10 }
        ::= { ibDvpnPolicyEntry 6 }

    ibDvpnPoDataAlgorithmSuite OBJECT-TYPE
        SYNTAX DvpnAlgorithmSuite
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
            "
                The algorithm suite used by the session for createing ipsec sa.
            "
        DEFVAL { dvpnDesCbcMd5PreShaModp768 }
        ::= { ibDvpnPolicyEntry 7 }

    ibDvpnPoSaSeconds OBJECT-TYPE
        SYNTAX Integer32(180..604800)
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
            "
                The duration of DVPN IPSec security association.
                The unit is second.
            "
        DEFVAL { 3600 }
        ::= { ibDvpnPolicyEntry 8 }

    ibDvpnPoUserDefAAADomain OBJECT-TYPE
        SYNTAX OCTET STRING (SIZE (0..24))
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
            "
                Dvpn policy AAA domain of the user. The domain will decide the
                authentication server which used by the dvpn server to authenticate
                dvpn client.
            "
        ::= { ibDvpnPolicyEntry 9 }

    ibDvpnPoRefTimes OBJECT-TYPE
        SYNTAX Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "
                The total times of the policy being used by dvpn.
            "
        ::= { ibDvpnPolicyEntry 10 }

    ibDvpnPoRowStatus OBJECT-TYPE
        SYNTAX RowStatus
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
            "
                The status of this conceptual row.
                We support the characteristic of active(1),createAndGo(4),destroy(6).
            "
        ::= { ibDvpnPolicyEntry 11 }



--ibDvpnDomainInfoTable

    ibDvpnDomainInfoTable OBJECT-TYPE
        SYNTAX SEQUENCE OF IbDvpnDomainInfoEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "
                The dvpn domain table.
            "
        ::= { ibDvpnMibTableTroop 2 }

    ibDvpnDomainInfoEntry OBJECT-TYPE
        SYNTAX IbDvpnDomainInfoEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "
                The entry of ibDvpnDomainInfoTable.
            "
        INDEX { ibDvpnDomainID }
        ::= { ibDvpnDomainInfoTable 1 }

    IbDvpnDomainInfoEntry ::=
        SEQUENCE
        {
            ibDvpnDomainID                       Integer32,
            ibDvpnDomainSessionNum               Unsigned32,
            ibDvpnDomainRedirectNum              Unsigned32,
            ibDvpnDomainInputPkt                 Unsigned32,
            ibDvpnDomainDropPkt                  Unsigned32,
            ibDvpnDomainOutputPkt                Unsigned32,
            ibDvpnDomainOutputErrorPkt           Unsigned32,
            ibDvpnDomainEncryptErrorPkt          Unsigned32
        }

    ibDvpnDomainID OBJECT-TYPE
        SYNTAX Integer32(1..65535)
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "
                Dvpn domain ID.
            "
        ::= { ibDvpnDomainInfoEntry 1 }

    ibDvpnDomainSessionNum OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "
                The number of session in this dvpn domain.
            "
        ::= { ibDvpnDomainInfoEntry 2 }

    ibDvpnDomainRedirectNum OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "
               The number of redirect packets in this dvpn domain.
            "
        ::= { ibDvpnDomainInfoEntry 3 }

    ibDvpnDomainInputPkt OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "
               The number of input packets in this dvpn domain.
            "
        ::= { ibDvpnDomainInfoEntry 4 }

    ibDvpnDomainDropPkt OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "
                The number of drop packets in this dvpn domain.
            "
        ::= { ibDvpnDomainInfoEntry 5 }

    ibDvpnDomainOutputPkt OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "
                The number of output packets in this dvpn domain.
            "
        ::= { ibDvpnDomainInfoEntry 6 }

    ibDvpnDomainOutputErrorPkt OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "
                The number of output error packets in this dvpn domain.
            "
        ::= { ibDvpnDomainInfoEntry 7 }

    ibDvpnDomainEncryptErrorPkt OBJECT-TYPE
        SYNTAX Unsigned32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "
                The number of encrypt error packets in this dvpn domain.
            "
        ::= { ibDvpnDomainInfoEntry 8 }





--ibDvpnClassTable

    ibDvpnClassTable OBJECT-TYPE
        SYNTAX SEQUENCE OF IbDvpnClassEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "
                The Dvpn class table.
                It is only used on dvpn client.
            "
        ::= { ibDvpnMibTableTroop 3 }

    ibDvpnClassEntry OBJECT-TYPE
        SYNTAX IbDvpnClassEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "
                The entry of the ibDvpnClassTable.
            "
        INDEX { ibDvpnClassName }
        ::= { ibDvpnClassTable 1 }

    IbDvpnClassEntry ::=
        SEQUENCE {
            ibDvpnClassName                 OCTET STRING,
            ibDvpnClServerPublicIpType      InetAddressType,
            ibDvpnClServerPublicIp          InetAddress,
            ibDvpnClServerPriIpType         InetAddressType,
            ibDvpnClServerPriIp             InetAddress,
            ibDvpnClAlgorithmSuite          DvpnAlgorithmSuite,
            ibDvpnClAuthenServerType        INTEGER,
            ibDvpnClPreShareKey             OCTET STRING,
            ibDvpnClUserName                OCTET STRING,
            ibDvpnClPwdEncrypted            INTEGER,
            ibDvpnClPasswd                  OCTET STRING,
            ibDvpnClassRowStatus            RowStatus
        }

    ibDvpnClassName OBJECT-TYPE
        SYNTAX OCTET STRING (SIZE(1..31))
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "
                The dvpn class name. It is the index of ibDvpnClassTable.
            "
        ::= { ibDvpnClassEntry  1 }

    ibDvpnClServerPublicIpType OBJECT-TYPE
        SYNTAX InetAddressType
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
            "
                Dvpn server public ip type (ipv4, ipv6).
            "
        ::= { ibDvpnClassEntry  2 }

    ibDvpnClServerPublicIp OBJECT-TYPE
        SYNTAX InetAddress
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
            "
                Dvpn server public ip address.
            "
        ::= { ibDvpnClassEntry  3 }

    ibDvpnClServerPriIpType OBJECT-TYPE
        SYNTAX InetAddressType
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
            "
                Dvpn server private ip type (ipv4, ipv6).
            "
        ::= { ibDvpnClassEntry  4 }

    ibDvpnClServerPriIp OBJECT-TYPE
        SYNTAX InetAddress
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
            "
                Dvpn server private ip address.
            "
        ::= { ibDvpnClassEntry  5 }

    ibDvpnClAlgorithmSuite OBJECT-TYPE
        SYNTAX DvpnAlgorithmSuite
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
            "
                The algorithm suite used by the client when sending registry
                packets to the server.
                'dvpnAlgorithmNone' is not supported here.
            "
        DEFVAL { dvpnDesCbcMd5PreShaModp768 }
        ::= { ibDvpnClassEntry  6 }

    ibDvpnClAuthenServerType OBJECT-TYPE
        SYNTAX INTEGER
            {
                none(1),
                preShareKey(2)
            }
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
            "
                The authentication type being used by client to server.
                The value is decided by the class configuration of the user.
                The default value is none.
            "
        DEFVAL { none }
        ::= { ibDvpnClassEntry  7 }

    ibDvpnClPreShareKey OBJECT-TYPE
        SYNTAX OCTET STRING(SIZE(0..127))
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
            "
                Dvpn PreShareKey.
                It is used when the method used by client when authenticating
                server is pre-share-key, only when the value of the key is the
                same as that on the server, the client can authenticate server
                successfully.
            "
        ::= { ibDvpnClassEntry  8 }

    ibDvpnClUserName OBJECT-TYPE
        SYNTAX OCTET STRING(SIZE(0..80))
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
            "
                Dvpn UserName. It is configured by the user.
            "
        ::= { ibDvpnClassEntry  9 }

    ibDvpnClPwdEncrypted OBJECT-TYPE
        SYNTAX INTEGER
            {
                simple(1),
                cipher(2)
            }
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
            "
                Dvpn password encrypted or not.
            "
        ::= { ibDvpnClassEntry  10 }

    ibDvpnClPasswd OBJECT-TYPE
        SYNTAX OCTET STRING
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
            "
                Dvpn password. It is configured by the user.
            "
        ::= { ibDvpnClassEntry  11 }

    ibDvpnClassRowStatus OBJECT-TYPE
        SYNTAX RowStatus
        MAX-ACCESS read-create
        STATUS current
