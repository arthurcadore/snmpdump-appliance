    ibDvpnMibCompliance MODULE-COMPLIANCE
        STATUS current
        DESCRIPTION
            "
                The compliance statement for entities which implement
                the DVPN MIB.
            "
        MODULE
            MANDATORY-GROUPS {
                ibDvpnGlobalGroup,
                ibDvpnDomainInfoGroup,
                ibDvpnPolicyGroup,
                ibDvpnClassGroup,
                ibDvpnTunnelGroup,
                ibDvpnMapGroup,
                ibDvpnSessionGroup,
                ibDvpnSessionHisGroup,
                ibDvpnNotificationGroup
            }
        ::= { ibDvpnMibCompliances 1 }

    ibDvpnMibGroups OBJECT IDENTIFIER ::= { ibDvpnMibConformance 2 }

    ibDvpnGlobalGroup OBJECT-GROUP
        OBJECTS {
            ibDvpnServiceEnable,
            ibDvpnClassNumber,
            ibDvpnClientNumber,
            ibDvpnMapAgeTime,
            ibDvpnClientRegisterInterval,
            ibDvpnClientRegisterDumb,
            ibDvpnClientRegisterRetry,
            ibDvpnInputPkt,
            ibDvpnDropPkt,
            ibDvpnOutputPkt,
            ibDvpnOutputErrorPkt,
            ibDvpnEncryptErrorPkt,
            ibDvpnCurrentDeviceRole,
            ibDvpnDomainNumber,
            ibDvpnMapNumber,
            ibDvpnSessionNumber,
            ibDvpnServerPreSharedKey,
            ibDvpnMapTrapEnable,
            ibDvpnSessionTrapEnable,
            ibDvpnVersion,
            ibDvpnClearDomainAllConection,
            ibDvpnClearDvpnStaInfo,
            ibDvpnTotalRedirectNumber,
            ibDvpnGlobalAuthenClientType,
            ibDvpnGlobalUserDefAAADomain,
            ibDvpnLocalDeviceId,
            ibDvpnSessionHisAgeTime,
            ibDvpnSessionHisReset
        }
        STATUS current
        DESCRIPTION
            "
                A collection of objects providing dvpn global information.
            "
        ::= { ibDvpnMibGroups 1 }

    ibDvpnPolicyGroup OBJECT-GROUP
        OBJECTS {
            ibDvpnPoAuthenClientType,
            ibDvpnPoSessionAlgorithmSuite,
            ibDvpnPoSessionIdleTime,
            ibDvpnPoSessionKeepTime,
            ibDvpnPoSessionSetupInterval,
            ibDvpnPoDataAlgorithmSuite,
            ibDvpnPoSaSeconds,
            ibDvpnPoUserDefAAADomain,
            ibDvpnPoRefTimes,
            ibDvpnPoRowStatus
        }
        STATUS current
        DESCRIPTION
            "
                A collection of objects providing dvpn policy information.
            "
        ::= { ibDvpnMibGroups 2 }

    ibDvpnDomainInfoGroup OBJECT-GROUP
        OBJECTS {
            ibDvpnDomainSessionNum,
            ibDvpnDomainRedirectNum,
            ibDvpnDomainInputPkt,
            ibDvpnDomainDropPkt,
            ibDvpnDomainOutputPkt,
            ibDvpnDomainOutputErrorPkt,
            ibDvpnDomainEncryptErrorPkt
        }
        STATUS current
        DESCRIPTION
            "
                A collection of objects providing dvpn domain information.
            "
        ::= { ibDvpnMibGroups 3 }

    ibDvpnClassGroup OBJECT-GROUP
        OBJECTS {
            ibDvpnClServerPublicIpType,
            ibDvpnClServerPublicIp,
            ibDvpnClServerPriIpType,
            ibDvpnClServerPriIp,
            ibDvpnClAlgorithmSuite,
            ibDvpnClAuthenServerType,
            ibDvpnClPreShareKey,
            ibDvpnClUserName,
            ibDvpnClPwdEncrypted,
            ibDvpnClPasswd,
            ibDvpnClassRowStatus
        }
        STATUS current
        DESCRIPTION
            "
                A collection of objects providing dvpn class information.
            "
        ::= { ibDvpnMibGroups 4 }

    ibDvpnTunnelGroup OBJECT-GROUP
        OBJECTS {
            ibDvpnTunnelInterfaceType,
            ibDvpnTunnelAcl,
            ibDvpnTunnelClientRegType,
            ibDvpnTunnelDvpnId,
            ibDvpnTunnelPolicy,
            ibDvpnTunnelClass
        }
        STATUS current
        DESCRIPTION
            "
                A collection of objects providing dvpn tunnel interface information.
            "
        ::= { ibDvpnMibGroups 5 }

    ibDvpnMapGroup OBJECT-GROUP
        OBJECTS {
            ibDvpnMapIndex,
            ibDvpnMapPeerDeviceId,
            ibDvpnMapDvpnId,
            ibDvpnMapBuildTime,
            ibDvpnMapPeerPriIpType,
            ibDvpnMapPeerPriIp,
            ibDvpnMapPeerPublicIpType,
            ibDvpnMapPeerPublicIp,
            ibDvpnMapLocalPriIpType,
            ibDvpnMapLocalPriIp,
            ibDvpnMapLocalPublicIpType,
            ibDvpnMapLocalPublicIp,
            ibDvpnMapUserName,
            ibDvpnMapUdpPort,
            ibDvpnMapControlId,
            ibDvpnMapType,
            ibDvpnMapState
        }
        STATUS current
        DESCRIPTION
            "
                A collection of objects providing dvpn map information.
            "
        ::= { ibDvpnMibGroups 6 }

    ibDvpnSessionGroup OBJECT-GROUP
        OBJECTS {
            ibDvpnSessionDvpnId,
            ibDvpnSessionPeerDeviceId,
            ibDvpnSessionBuildTime,
            ibDvpnSessionPeerPubIpType,
            ibDvpnSessionPeerPubIp,
            ibDvpnSessionPeerPriIpType,
            ibDvpnSessionPeerPriIp,
            ibDvpnSessionLocalPubIpType,
            ibDvpnSessionLocalPubIp,
            ibDvpnSessionLocalPriIpType,
            ibDvpnSessionLocalPriIp,
            ibDvpnSessionPeerUdpPort,
            ibDvpnSessionInitiator,
            ibDvpnSessionUserName,
            ibDvpnSessionState,
            ibDvpnSessionType,
            ibDvpnSessionPeerType
        }
        STATUS current
        DESCRIPTION
            "
                A collection of objects providing dvpn session information.
            "
        ::= { ibDvpnMibGroups 7 }

    ibDvpnSessionHisGroup OBJECT-GROUP
        OBJECTS {
            ibDvpnSessionHisPeerPriIPType,
            ibDvpnSessionHisSendPkt,
            ibDvpnSessionHisRcvPkt,
            ibDvpnSessionHisOnlineNumber,
            ibDvpnSessionHisFirstUpTime,
            ibDvpnSessionHisLastUpTime,
            ibDvpnSessionHisLastDownTime,
            ibDvpnSessionHisOnlineFlag,
            ibDvpnSessionHisPeerDeviceId
        }
        STATUS current
        DESCRIPTION
            "
                A collection of objects providing dvpn session history information.
            "
        ::= { ibDvpnMibGroups 8 }


    ibDvpnNotificationGroup OBJECT-GROUP
        OBJECTS {
            ibDvpnSessionBuildNotification,
            ibDvpnSessionDelNotification,
            ibDvpnMapBuildNotification,
            ibDvpnMapDelNotification
        }
        STATUS current
        DESCRIPTION
            "
                A collection of objects providing dvpn notification information.
            "
        ::= { ibDvpnMibGroups 9 }

END
