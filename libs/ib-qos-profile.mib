        ibQoSProfPortMappingRowStatus OBJECT-TYPE
            SYNTAX RowStatus
            MAX-ACCESS read-create
            STATUS current
            DESCRIPTION
                "RowStatus,Now support three state:CreateAndGo,Active,Destroy."
            ::= { ibQoSProfPortMappingEntry 3 }

        ibQoSProfPortMappingModeTable OBJECT-TYPE
            SYNTAX SEQUENCE OF IbQoSProfPortMappingModeEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "Port mapping mode table is used to set mapping mode of the port.
                When a qos profile is applied to a port, the profile can have two working ways:
                1)The profile is the same for all the users connected to the port;
                2)The profile can be different for different users connected to the port;
                Port-based mode means the profile is the same for all the users.
                User-based mode means the profile is different for different users.
                Default value is port-based."
            ::= { ibQoSProfPortMapping 2 }

        ibQoSProfPortMappingModeEntry OBJECT-TYPE
            SYNTAX IbQoSProfPortMappingModeEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION             " "
            INDEX { ibQoSProfPortMappingModeIfIndex }
            ::= { ibQoSProfPortMappingModeTable 1 }

        IbQoSProfPortMappingModeEntry ::=
            SEQUENCE {
                ibQoSProfPortMappingModeIfIndex
                    Integer32,
                ibQoSProfPortMappingMode
                    INTEGER
             }

        ibQoSProfPortMappingModeIfIndex OBJECT-TYPE
            SYNTAX Integer32 (1..2147483647)
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "The ifindex which the QoS profile mapping mode is applied to."
            ::= { ibQoSProfPortMappingModeEntry 1 }

        ibQoSProfPortMappingMode OBJECT-TYPE
            SYNTAX INTEGER
                {
                user-based(1),
                port-based(2)
                }
            MAX-ACCESS read-write
            STATUS current
            DESCRIPTION
                "The QoS profile mapping mode of specifed port."
            DEFVAL { 2 }
            ::= { ibQoSProfPortMappingModeEntry 2 }

        ibQoSProfDynPortMappingTable OBJECT-TYPE
            SYNTAX SEQUENCE OF IbQoSProfDynPortMappingEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "Dynamic port mapping table is used to get user' port,MAC address,IP address,VLAN ID and user-specified qos profiles.
                Network managers can allocate different QOS profile groups to different users.
                Once the user is on line, then the user-specified QOS profiles will be dynamically applied to the port the user connects.
                Each port can have different groups of users simultaneously connected.
                Each group of users can have assiociation with multiple QOS profiles simultaneously.
                So the user's port,MAC address,IP address and VLAN ID will replace the source information in the ACL rules refered in the QOS profiles.
                Once the user is off line, then the user-specified QOS profiles will be dynamically deleted from the port the user connects."
            ::= { ibQoSProfPortMapping 3 }

        ibQoSProfDynPortMappingEntry OBJECT-TYPE
            SYNTAX IbQoSProfDynPortMappingEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION             " "
            INDEX { ibQoSProfDynPortMappingIfIndex, ibQoSProfDynPortMappingUserSrcMAC }
            ::= { ibQoSProfDynPortMappingTable 1 }

        IbQoSProfDynPortMappingEntry ::=
            SEQUENCE {
                ibQoSProfDynPortMappingIfIndex
                    Integer32,
                ibQoSProfDynPortMappingUserSrcMAC
                    MacAddress,
                ibQoSProfDynPortMappingUserName
                    OCTET STRING,
                ibQoSProfDynPortMappingUserIPAddr
                    IpAddress,
                ibQoSProfDynPortMappingUserVLANID
                    Integer32,
                ibQoSProfDynPortMappingUserProfName
                    OCTET STRING
             }

        ibQoSProfDynPortMappingIfIndex OBJECT-TYPE
            SYNTAX Integer32 (1..2147483647)
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "if index of the port user profile is applied."
            ::= { ibQoSProfDynPortMappingEntry 1 }

        ibQoSProfDynPortMappingUserSrcMAC OBJECT-TYPE
            SYNTAX MacAddress
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "User sourc MAC address."
            ::= { ibQoSProfDynPortMappingEntry 2 }

        ibQoSProfDynPortMappingUserName OBJECT-TYPE
            SYNTAX OCTET STRING (SIZE (1..32))
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "User name."
            ::= { ibQoSProfDynPortMappingEntry 3 }

        ibQoSProfDynPortMappingUserIPAddr OBJECT-TYPE
            SYNTAX IpAddress
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "User IP address"
            ::= { ibQoSProfDynPortMappingEntry 4 }

        ibQoSProfDynPortMappingUserVLANID OBJECT-TYPE
            SYNTAX Integer32 (1..65535)
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "User VLAN ID."
            ::= { ibQoSProfDynPortMappingEntry 5 }

        ibQoSProfDynPortMappingUserProfName OBJECT-TYPE
            SYNTAX OCTET STRING (SIZE (1..32))
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "User profile name."
            ::= { ibQoSProfDynPortMappingEntry 6 }

        ibQoSProfPortMappingTraps OBJECT IDENTIFIER ::= { ibQosProfile 2 }

        ibQoSProfPortMappingError NOTIFICATION-TYPE
