-- *****************************************************************************
-- *  ibWlanModuleConfigTable Definition
-- *****************************************************************************
ibWlanModuleConfigTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbWlanModuleConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Administrators must configure module parameters for a module to operate."
    ::= { ibWlanModuleConfigGroup 1 }

ibWlanModuleConfigEntry OBJECT-TYPE
    SYNTAX      IbWlanModuleConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry contains module configuration information."
    INDEX
        {
            ibWlanAPSerialID,
            ibWlanModuleID
        }
    ::= { ibWlanModuleConfigTable 1 }

IbWlanModuleConfigEntry ::= SEQUENCE
    {
        ibWlanAPSerialID               OCTET STRING,
        ibWlanModuleID                 Integer32,
        ibWlanModuleType               INTEGER,
        ibWlanModuleStatus             TruthValue,
        ibWlanModuleReset              INTEGER,
        ibWlanModuleRstFac             INTEGER,
        ibWlanModuleUpWareStatus       TruthValue,
        ibWlanModuleTxPower            Integer32,
        ibWlanModuleManualUpdate          OCTET STRING
    }

ibWlanAPSerialID OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(0..127))
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This object represents the serial ID of an AP."
    ::= { ibWlanModuleConfigEntry 1 }

ibWlanModuleID OBJECT-TYPE
    SYNTAX      Integer32(1..65535)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This object represents the module ID of a module."
    ::= { ibWlanModuleConfigEntry 2 }

ibWlanModuleType OBJECT-TYPE
    SYNTAX      INTEGER
        {
          none(0),
          ble(1),
          iot(2)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object represents the type of a module."
    ::= { ibWlanModuleConfigEntry 3 }

ibWlanModuleStatus OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object represents the enabled or disabled status of a module."
    ::= { ibWlanModuleConfigEntry 4 }

ibWlanModuleReset OBJECT-TYPE
    SYNTAX      INTEGER
    {
      none(0),
      reboot(1)
    }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "When read the value from the node
         none: Represents the module has been rebooted and the status of the module is normal.
         When write the value to the node
         reboot: Represents module rebooting.  Other values are not supported."
    ::= { ibWlanModuleConfigEntry 5 }

ibWlanModuleRstFac OBJECT-TYPE
    SYNTAX      INTEGER
    {
      none(0),
      restore(1)
    }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "When read the value from the node
         none: Represents the module has been restored and the status of the module is normal.
         When write the value to the node
         restore: Represents module restoring.  Other values are not supported."
    ::= { ibWlanModuleConfigEntry 6 }

ibWlanModuleUpWareStatus OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object represents the enabled or disabled status of module auto update."
    ::= { ibWlanModuleConfigEntry 7 }

ibWlanModuleTxPower OBJECT-TYPE
    SYNTAX      Integer32(1..4)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object represents the packet transmit power."
    ::= { ibWlanModuleConfigEntry 8 }

ibWlanModuleManualUpdate OBJECT-TYPE
    SYNTAX       OCTET STRING
    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION
        "This object represents the file path for module firmware updating."
    ::= { ibWlanModuleConfigEntry 9 }

-- *****************************************************************************
-- * End of ibWlanModuleConfigTable Definition
-- *****************************************************************************

