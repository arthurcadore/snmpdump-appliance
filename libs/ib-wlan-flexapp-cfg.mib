    INDEX
        {
            ibDot11IOTAPSerialID,
            ibDot11IOTModuleID
        }
    ::= { ibWlanModuleInfoTable 1 }

IbWlanModuleInfoEntry ::= SEQUENCE
    {
        ibDot11IOTAPSerialID          OCTET STRING,
        ibDot11IOTModuleID            Integer32,
        ibDot11IOTModuleType          INTEGER,
        ibDot11IOTModuleModel         OCTET STRING,
        ibDot11IOTModuleHwVersion     OCTET STRING,
        ibDot11IOTModuleSwVersion     OCTET STRING,
        ibDot11IOTModuleSerialId      OCTET STRING
    }

ibDot11IOTAPSerialID OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(0..127))
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This object represents the serial ID of an AP."
    ::= { ibWlanModuleInfoEntry 1 }

ibDot11IOTModuleID OBJECT-TYPE
    SYNTAX      Integer32(1..65535)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This object represents a module."
    ::= { ibWlanModuleInfoEntry 2 }

ibDot11IOTModuleType OBJECT-TYPE
    SYNTAX      INTEGER
        {
          none(0),
          company(1),
          iot(2)
        }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object represents the type of a module."
    ::= { ibWlanModuleInfoEntry 3 }

ibDot11IOTModuleModel OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object represents the model of a module."
    ::= { ibWlanModuleInfoEntry 4 }

ibDot11IOTModuleHwVersion OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object represents the hardware version of a module."
    ::= { ibWlanModuleInfoEntry 5 }

ibDot11IOTModuleSwVersion OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object represents the software version of a module."
    ::= { ibWlanModuleInfoEntry 6 }

ibDot11IOTModuleSerialId OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object represents the sequence ID of a module."
    ::= { ibWlanModuleInfoEntry 7 }

-- *****************************************************************************
-- * End of ibDot11APModuleInfoTable Definition
-- *****************************************************************************

