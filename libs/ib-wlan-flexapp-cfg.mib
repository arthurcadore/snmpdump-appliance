-- *****************************************************************************
-- *  ibWlanBLEConfigTable Definition
-- *****************************************************************************
ibWlanBLEConfigTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbWlanBLEConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Administrators must configure Bluetooth Low Energy (BLE) parameters."
    ::= { ibWlanBLEConfigGroup 1 }

ibWlanBLEConfigEntry OBJECT-TYPE
    SYNTAX      IbWlanBLEConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry contains BLE location configuration information."
    INDEX
        {
            ibWlanBLEAPSerialID
        }
    ::= { ibWlanBLEConfigTable 1 }

IbWlanBLEConfigEntry ::= SEQUENCE
    {
        ibWlanBLEAPSerialID            OCTET STRING,
        ibWlanBLEStatus                TruthValue,
        ibWlanBLEEngineAdd             IpAddress,
        ibWlanBLEEnginePort            Integer32,
        ibWlanBLEVendorPort            Integer32,
        ibWlanBLERssiStatus            TruthValue,
        ibWlanBLERssiThreshold         Integer32,
        ibWlanBLEConnectPassword       OCTET STRING,
        ibWlanBLECommandPassword       OCTET STRING,
        ibWlanBLEReportStatus          TruthValue,
        ibWlanBLEReportInterval        Integer32,
        ibWlanBLEAgingTime             Integer32,
        ibWlanBLERealTimeReportStatus  TruthValue,
        ibWlanBLERealTimePrefix        OCTET STRING
    }

ibWlanBLEAPSerialID OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(0..127))
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This object represents the serial ID of an AP."
    ::= { ibWlanBLEConfigEntry 1 }

ibWlanBLEStatus OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object represents the enabled or disabled status of BLE location."
    ::= { ibWlanBLEConfigEntry 2 }

ibWlanBLEEngineAdd OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object represents the server's IPv4 address to which BLE messages are sent."
    ::= { ibWlanBLEConfigEntry 3 }

ibWlanBLEEnginePort OBJECT-TYPE
    SYNTAX      Integer32(0..65535)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object represents the UDP port of the location server."
    ::= { ibWlanBLEConfigEntry 4 }

ibWlanBLEVendorPort OBJECT-TYPE
    SYNTAX      Integer32(1..65535)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object represents the UDP port to listen for BLE messages from the server."
    ::= { ibWlanBLEConfigEntry 5 }

ibWlanBLERssiStatus OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object represents the enabled or disabled status of BLE RSSI-based packet filtering."
    ::= { ibWlanBLEConfigEntry 6 }

ibWlanBLERssiThreshold OBJECT-TYPE
    SYNTAX      Integer32(5..255)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object represents the max RSSI value of clients' packets."
    ::= { ibWlanBLEConfigEntry 7 }

ibWlanBLEConnectPassword OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(0|4))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object represents the BLE module connection password."
    ::= { ibWlanBLEConfigEntry 8 }

ibWlanBLECommandPassword OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(0|12))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object represents the BLE module command password."
    ::= { ibWlanBLEConfigEntry 9 }

ibWlanBLEReportStatus OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object represents the enabled or disabled status of BLE location reporting."
    ::= { ibWlanBLEConfigEntry 10 }

ibWlanBLEReportInterval OBJECT-TYPE
    SYNTAX      Integer32(1..86400)
    UNITS       "Second"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object represents the interval at which an AP sends BLE messages to the server."
    ::= { ibWlanBLEConfigEntry 11 }

ibWlanBLEAgingTime OBJECT-TYPE
    SYNTAX      Integer32(60..3600)
    UNITS       "Second"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object represents the aging time for iBeacon devices."
    ::= { ibWlanBLEConfigEntry 12 }

ibWlanBLERealTimeReportStatus OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object represents the enabled or disabled status of BLE realtime reporting."
    ::= { ibWlanBLEConfigEntry 13 }

ibWlanBLERealTimePrefix OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(0|8..18))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object represents the manufacturer prefix of BLE devices to
         be located.  The prefix must be an even number."
    ::= { ibWlanBLEConfigEntry 14 }

-- *****************************************************************************
-- * End of ibWlanBLEConfigTable Definition
-- *****************************************************************************

