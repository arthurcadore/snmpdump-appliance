IB-WLAN-FLEXAPP-CFG-MIB DEFINITIONS ::= BEGIN

IMPORTS
        TruthValue,
        MacAddress
    FROM SNMPv2-TC
        MODULE-IDENTITY,
        NOTIFICATION-TYPE,
        OBJECT-TYPE,
        Integer32,
        IpAddress
    FROM SNMPv2-SMI
        ibDot11
    FROM IB-DOT11-REF-MIB;

ibWlanFlexAppCFG MODULE-IDENTITY
    LAST-UPDATED "202309081800Z"        -- Sept 08, 2023 at 18:00 GMT
    ORGANIZATION
        "Intelbras S.A. Industria de Telecomunicacao Eletronica Brasileria."
    CONTACT-INFO
        ""
    DESCRIPTION
        "This MIB provides information about WLAN application configuration."
    REVISION "202309081800Z"        -- Sept 08, 2023 at 18:00 GMT
    DESCRIPTION
        "Modify some words."
    REVISION "202307101800Z"        -- July 10, 2023 at 18:00 GMT
    DESCRIPTION
        "Modify keyword to RTT."
    REVISION "201505261800Z"        -- May 26, 2015 at 18:00 GMT
    DESCRIPTION
        "Added new nodes."
    ::= { ibDot11 19 }
--
-- Node definitions
--

ibWlanModuleConfigGroup OBJECT IDENTIFIER ::= { ibWlanFlexAppCFG 1 }
--  Module Group has the following children:
--  ibWlanModuleConfigTable          ::= { ibWlanModuleConfigGroup 1 }
--  ibWlanModuleInfoTable            ::= { ibWlanModuleConfigGroup 2 }

ibWlanIOTConfigGroup OBJECT IDENTIFIER ::= { ibWlanFlexAppCFG 2 }
--  IOT Configure Group has the following children:
--  ibWlanIOTConfigTable             ::= { ibWlanIOTConfigGroup 1 }

ibWlanModuleNotifyGroup OBJECT IDENTIFIER ::= { ibWlanFlexAppCFG 3 }
--  Module Notify Group has the following children:
--  ibWlanModuleTraps ::= { ibWlanModuleNotifyGroup 0 }
--  ibWlanModuleTrapVarObjects ::= { ibWlanModuleNotifyGroup 1 }

ibWlanBLEConfigGroup OBJECT IDENTIFIER ::= { ibWlanFlexAppCFG 4 }
--  BLE Configure Group has the following children:
--  ibWlanBLEConfigTable             ::= { ibWlanBLEConfigGroup 1 }
--  ibWlanBLEModuleConfigTable       ::= { ibWlanBLEConfigGroup 2 }

ibWlanAEConfigGroup OBJECT IDENTIFIER ::= { ibWlanFlexAppCFG 5 }
--  AE Configure Group has the following children:
--  ibWlanAEConfigTable               ::= { ibWlanAEConfigGroup 1 }
--  ibWlanAERadioConfigTable          ::= { ibWlanAEConfigGroup 2 }

ibWlanCommonConfigGroup OBJECT IDENTIFIER ::= { ibWlanFlexAppCFG 6 }
--  common Configure Group has the following children:
--  ibWlanCommonConfigTable             ::= { ibWlanCommonConfigGroup 1 }

ibWlanRTTConfigGroup OBJECT IDENTIFIER ::= { ibWlanFlexAppCFG 7 }
