-- *****************************************************************************
-- *  ibWlanModuleTraps Definition
-- *****************************************************************************
-- AP Management Notification

ibWlanModuleTraps OBJECT IDENTIFIER
    ::= { ibWlanModuleNotifyGroup 0 }

ibWlanModuleInsertTrap NOTIFICATION-TYPE
    OBJECTS
        {
            ibWlanTrapAPMacAddress,
            ibWlanTrapModuleID,
            ibWlanTrapModulePhyType,
            ibWlanTrapModuleModel,
            ibWlanTrapModuleHwVersion,
            ibWlanTrapModuleSwVersion,
            ibWlanTrapModuleSequenceId
        }
    STATUS current
    DESCRIPTION
        "This notification will be generated when an IOT module is inserted."
    ::= { ibWlanModuleTraps 1 }

ibWlanModuleRomveTrap NOTIFICATION-TYPE
    OBJECTS
        {
            ibWlanTrapAPMacAddress,
            ibWlanTrapModuleID,
            ibWlanTrapModulePhyType,
            ibWlanTrapModuleModel,
            ibWlanTrapModuleHwVersion,
            ibWlanTrapModuleSwVersion,
            ibWlanTrapModuleSequenceId
        }
    STATUS current
    DESCRIPTION
        "This notification will be generated when an IOT module is removed."
    ::= { ibWlanModuleTraps 2 }

ibWlanModuleMissMatch NOTIFICATION-TYPE
    OBJECTS
        {
            ibWlanTrapAPMacAddress,
            ibWlanTrapModuleID,
            ibWlanTrapModuleCfgType,
            ibWlanTrapModulePhyType,
            ibWlanTrapModuleModel
        }
    STATUS current
    DESCRIPTION
        "This notification will be generated when the configured
         type doesn't match the module type."
    ::= { ibWlanModuleTraps 3 }

-- AP Management Notification variable object

ibWlanModuleTrapVarObjects OBJECT IDENTIFIER
    ::= { ibWlanModuleNotifyGroup 1 }

ibWlanTrapAPMacAddress OBJECT-TYPE
    SYNTAX      MacAddress
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "This object represents the MAC address of an AP."
    ::= {ibWlanModuleTrapVarObjects 1 }

ibWlanTrapModuleID OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "This object represents the ID of a module."
    ::= { ibWlanModuleTrapVarObjects 2 }

ibWlanTrapModuleCfgType OBJECT-TYPE
    SYNTAX      INTEGER
        {
          none(0),
          ble(1),
          rfid(2),
          zigbee(3),
          uwb(4)
        }
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "This object represents the configuration type of a module."
    ::= { ibWlanModuleTrapVarObjects 3 }

ibWlanTrapModulePhyType OBJECT-TYPE
    SYNTAX      INTEGER
        {
          none(0),
          company(1),
          iot(2)
        }
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "This object represents the physical type of a module."
    ::= { ibWlanModuleTrapVarObjects 4 }

ibWlanTrapModuleModel OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "This object represents the model of a module."
    ::= { ibWlanModuleTrapVarObjects 5 }

ibWlanTrapModuleHwVersion OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "This object represents the hardware version of a module."
