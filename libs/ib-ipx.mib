--  ibIpxIfStatTable includes all statistic information
--  on each interface.

ibIpxIfStatTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbIpxIfStatEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "The table of the statistic information on each interface."
    ::= { ibIpxStatInterface 1 }

ibIpxIfStatEntry OBJECT-TYPE
    SYNTAX      IbIpxIfStatEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "The entry of the statistic table."
    INDEX       { ibIpxIfStatIndex }
    ::= { ibIpxIfStatTable 1 }

IbIpxIfStatEntry ::=
    SEQUENCE{
        ibIpxIfStatIndex
            Integer32,
        ibIpxIfStatNetId
            OCTET STRING,
        ibIpxIfStatNodeId
            OCTET STRING,
        --ipx
        ibIpxIfStatIpxReceives
            Counter32,
        ibIpxIfStatIpxSends
            Counter32,
        ibIpxIfStatIpxRecvBytes
            Counter32,
        ibIpxIfStatIpxSendBytes
            Counter32,
        -- Rip
        ibIpxIfStatRipReceives
            Counter32,
        ibIpxIfStatRipSends
            Counter32,
        ibIpxIfStatRipDiscards
            Counter32,
        ibIpxIfStatRipSpecReqReceives
            Counter32,
        ibIpxIfStatRipSpecRspSends
            Counter32,
        ibIpxIfStatRipGenReqReceives
            Counter32,
        ibIpxIfStatRipGenRspSends
            Counter32,
        -- Sap
        ibIpxIfStatSapReceives
            Counter32,
        ibIpxIfStatSapSends
            Counter32,
        ibIpxIfStatSapDiscards
            Counter32,
        ibIpxIfStatSapGnsReqReceives
            Counter32,
        ibIpxIfStatSapGnsRspSends
            Counter32
}

ibIpxIfStatIndex OBJECT-TYPE
    SYNTAX      Integer32 (1..2147483647)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "The index of the table's entry. According to the 
                interface's index."
    REFERENCE   "ifIndex"
    ::= { ibIpxIfStatEntry 1 }

ibIpxIfStatNetId OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE(4))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The network number of the IPX address of this interface."
    ::= { ibIpxIfStatEntry 2 }

ibIpxIfStatNodeId OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE(6))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The node number of the IPX address of this interface."
    ::= { ibIpxIfStatEntry 3 }

