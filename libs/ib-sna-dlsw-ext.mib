-- ====================================================================
-- DLSw Extension LLC2 Port Configuration Table
-- ====================================================================
ibdeLlc2PortTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbdeLlc2PortEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table defines parameters for the Ethernet interface."
    ::= { ibdeLlc2 1 }

ibdeLlc2PortEntry OBJECT-TYPE
    SYNTAX      IbdeLlc2PortEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "LLC2 port entry."
    INDEX       { ifIndex }
    ::= { ibdeLlc2PortTable 1 }

IbdeLlc2PortEntry ::=
    SEQUENCE {
        ibdeLlc2PortMaxAck           Integer32,
        ibdeLlc2PortMaxPdu           Integer32,
        ibdeLlc2PortMaxSendQueue     Integer32,
        ibdeLlc2PortMaxTransmission  Integer32,
        ibdeLlc2PortModulo           INTEGER,
        ibdeLlc2PortReceiveWindow    Integer32,
        ibdeLlc2PortTimerAck         Integer32,
        ibdeLlc2PortTimerAckDelay    Integer32,
        ibdeLlc2PortTimerDetect      Integer32,
        ibdeLlc2PortTimerBusy        Integer32,
        ibdeLlc2PortTimerPoll        Integer32,
        ibdeLlc2PortTimerReject      Integer32,
        ibdeLlc2PortRowStatus        RowStatus
    }

ibdeLlc2PortMaxAck OBJECT-TYPE
    SYNTAX      Integer32 (1..127)
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Maximum of I-frames received before ACK."
    DEFVAL      { 3 }
    ::= { ibdeLlc2PortEntry 1 }

ibdeLlc2PortMaxPdu OBJECT-TYPE
    SYNTAX      Integer32 (1..1700)
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Maximum of LLC2 pdu length."
    DEFVAL      { 1493 }
    ::= { ibdeLlc2PortEntry 2 }

ibdeLlc2PortMaxSendQueue OBJECT-TYPE
    SYNTAX      Integer32 (20..200)
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Queue for sending llc2 I-frames."
    DEFVAL      { 50 }
    ::= { ibdeLlc2PortEntry 3 }

ibdeLlc2PortMaxTransmission OBJECT-TYPE
    SYNTAX      Integer32 (1..255)
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Retry times of operations."
    DEFVAL      { 20 }
    ::= { ibdeLlc2PortEntry 4 }

ibdeLlc2PortModulo OBJECT-TYPE
    SYNTAX      INTEGER
                {
                m8(8),
                m128(128)
                }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Modulus of LLC2."
    DEFVAL      { m128 }
    ::= { ibdeLlc2PortEntry 5 }

ibdeLlc2PortReceiveWindow OBJECT-TYPE
    SYNTAX      Integer32 (1..127)
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Maximum number of I-frames to send before received ACK."
    DEFVAL      { 7 }
    ::= { ibdeLlc2PortEntry 6 }

ibdeLlc2PortTimerAck OBJECT-TYPE
    SYNTAX      Integer32 (1..60000)
    UNITS       "milliseconds"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Waiting for ACK time after sending an I-frame."
    DEFVAL      { 200 }
    ::= { ibdeLlc2PortEntry 7 }

ibdeLlc2PortTimerAckDelay OBJECT-TYPE
    SYNTAX      Integer32 (1..60000)
    UNITS       "milliseconds"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Maximum time allows I-frames incoming without replay ACK."
    DEFVAL      { 100 }
    ::= { ibdeLlc2PortEntry 8 }

ibdeLlc2PortTimerDetect OBJECT-TYPE
    SYNTAX      Integer32 (1..60000)
    UNITS       "milliseconds"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Detect the link status."
    DEFVAL      { 100 }
    ::= { ibdeLlc2PortEntry 9 }

ibdeLlc2PortTimerBusy OBJECT-TYPE
    SYNTAX      Integer32 (1..60000)
    UNITS       "milliseconds"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Waiting time while other LLC2 station is in busy state."
    DEFVAL      { 300 }
    ::= { ibdeLlc2PortEntry 10 }

ibdeLlc2PortTimerPoll OBJECT-TYPE
    SYNTAX      Integer32 (1..60000)
    UNITS       "milliseconds"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Waiting time after a P frame is sent."
    DEFVAL      { 5000 }
    ::= { ibdeLlc2PortEntry 11 }

ibdeLlc2PortTimerReject OBJECT-TYPE
    SYNTAX      Integer32 (1..60000)
    UNITS       "milliseconds"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Waiting time after a REJ frame is sent."
    DEFVAL      { 500 }
    ::= { ibdeLlc2PortEntry 12 }

ibdeLlc2PortRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "RowStatus."
    ::= { ibdeLlc2PortEntry 13 }

-- ====================================================================
-- DLSw Reachable Cache MIB-Group
-- ====================================================================
ibdeRchCacheStat OBJECT IDENTIFIER ::= { ibdeReachableCache 1 }

ibdeRchCacheMaxIndex OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Maximum of ibdeRchCacheIndex."
    ::= { ibdeRchCacheStat 1 }

ibdeRchCacheNextIndex OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The next value of ibdeRchCacheIndex to be assigned by
        the agent. A retrieval of this object atomically reserves
        the returned value for use by the manager to create a row
        in ibdeRchCacheTable. This makes it possible for the agent
        to control the index space of the MAC address cache, yet
        allows the manager to administratively create new rows."
    ::= { ibdeRchCacheStat 2 }

ibdeRchCacheTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbdeRchCacheEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Reachable cache table."
    ::= { ibdeReachableCache 3 }

ibdeRchCacheEntry OBJECT-TYPE
    SYNTAX      IbdeRchCacheEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Reachable cache entry."
    INDEX       { ibdeRchCacheIndex }
    ::= { ibdeRchCacheTable 1 }

IbdeRchCacheEntry ::=
    SEQUENCE {
        ibdeRchCacheIndex            Integer32,
        ibdeRchCacheStatus           INTEGER,
        ibdeRchCacheRemainTime       TimeTicks,
        ibdeRchCacheMac              MacAddressNC,
        ibdeRchCacheRemoteIpAddrType InetAddressType,
        ibdeRchCacheRemoteIp         InetAddress,
        ibdeRchCacheRowStatus        RowStatus
    }

ibdeRchCacheIndex OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Index of reachable cache entry."
    ::= { ibdeRchCacheEntry 1 }

ibdeRchCacheStatus OBJECT-TYPE
    SYNTAX      INTEGER
                {
                found(1),
                verify(2),
                noCacheInfo(3),
                exploring(4),
                waiting(5)
                }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Current status of reachable cache entry."
    ::= { ibdeRchCacheEntry 2 }

ibdeRchCacheRemainTime OBJECT-TYPE
    SYNTAX      TimeTicks
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Remain time of reachable cache entry."
    ::= { ibdeRchCacheEntry 3 }

ibdeRchCacheMac OBJECT-TYPE
    SYNTAX      MacAddressNC
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "MAC address of reachable cache entry."
    ::= { ibdeRchCacheEntry 4 }

ibdeRchCacheRemoteIpAddrType OBJECT-TYPE
    SYNTAX      InetAddressType
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Remote peer ip address type."
    ::= { ibdeRchCacheEntry 5 }

ibdeRchCacheRemoteIp OBJECT-TYPE
    SYNTAX      InetAddress
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Remote peer ip address of reachable cache entry."
    ::= { ibdeRchCacheEntry 6 }

ibdeRchCacheRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "RowStatus."
    ::= { ibdeRchCacheEntry 7 }

-- ====================================================================
-- DLSw Ethernet Backup MIB-Group
-- ====================================================================
ibdeEBMacMapStat OBJECT IDENTIFIER ::= { ibdeEthernetBackup 1 }

ibdeEBMacMapMaxIndex OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Maximum of ibdeEBMacMapIndex."
    ::= { ibdeEBMacMapStat 1 }

ibdeEBMacMapNextIndex OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The next value of ibdeEBMacMapIndex to be assigned by
        the agent. A retrieval of this object atomically reserves
        the returned value for use by the manager to create a row
        in ibdeEBMacMapTable. This makes it possible for the agent
        to control the index space of the MAC mapping tables, yet
        allows the manager to administratively create new rows."
    ::= { ibdeEBMacMapStat 2 }

ibdeEBIfTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbdeEBIfEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Information of interface which works for DLSw Ethernet redundancy."
    ::= { ibdeEthernetBackup 3 }

ibdeEBIfEntry OBJECT-TYPE
    SYNTAX      IbdeEBIfEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Parameters defined for DLSw Ethernet redundancy interface."
    INDEX       { ifIndex }
    ::= { ibdeEBIfTable 1 }

IbdeEBIfEntry ::=
    SEQUENCE {
        ibdeEBMulticastMac    MacAddressNC,
        ibdeEBPriority        Integer32,
        ibdeEBtimer           Integer32,
        ibdeEBRowStatus       RowStatus
    }

ibdeEBMulticastMac OBJECT-TYPE
    SYNTAX      MacAddressNC
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Multicast MAC address configured for the Ethernet
        redundancy of dlsw module. When Ethernet redundancy
        is inactive, its value is '000000000000'H.
        To active Ethernet redundancy, give it a valid multicast MAC address."
    DEFVAL      { '000000000000'h }
    ::= { ibdeEBIfEntry 1 }

ibdeEBPriority OBJECT-TYPE
    SYNTAX      Integer32 (1..254)
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Priority configured for the Ethernet redundancy of DLSw module."
    DEFVAL      { 100 }
    ::= { ibdeEBIfEntry 2 }

ibdeEBtimer OBJECT-TYPE
    SYNTAX      Integer32 (100..5000)
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Timer for the Ethernet redundancy of DLSw module."
    DEFVAL      { 500 }
    ::= { ibdeEBIfEntry 3 }

ibdeEBRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "RowStatus."
    ::= { ibdeEBIfEntry 4 }

ibdeEBMacMapTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbdeEBMacMapEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "MAC address mapping table."
    ::= { ibdeEthernetBackup 4 }

ibdeEBMacMapEntry OBJECT-TYPE
    SYNTAX      IbdeEBMacMapEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "MAC address mapping entry."
    INDEX       { ifIndex, ibdeEBMacMapIndex }
    ::= { ibdeEBMacMapTable 1 }

IbdeEBMacMapEntry ::=
    SEQUENCE {
        ibdeEBMacMapIndex         Integer32,
        ibdeEBMacMapLocalMac      MacAddressNC,
        ibdeEBMacMapRemoteMac     MacAddressNC,
        ibdeEBMacMapNeighbour     MacAddressNC,
        ibdeEBMacMapRowStatus     RowStatus
    }

ibdeEBMacMapIndex OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Index of MAC address mapping entry."
    ::= { ibdeEBMacMapEntry 1 }

ibdeEBMacMapLocalMac OBJECT-TYPE
    SYNTAX      MacAddressNC
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Local MAC address in MAC address mapping.
        When creating a new conceptual row, ibdeEBMacMapLocalMac
        and ibdeEBMacMapRemoteMac should be set simultaneously."
    ::= { ibdeEBMacMapEntry 2 }

ibdeEBMacMapRemoteMac OBJECT-TYPE
    SYNTAX      MacAddressNC
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Remote MAC address of MAC address mapping.
        When creating a new conceptual row, ibdeEBMacMapLocalMac
        and ibdeEBMacMapRemoteMac should be set simultaneously."
    ::= { ibdeEBMacMapEntry 3 }

ibdeEBMacMapNeighbour OBJECT-TYPE
    SYNTAX      MacAddressNC
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Neighbour of MAC address mapping."
    ::= { ibdeEBMacMapEntry 4 }

ibdeEBMacMapRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Status of MAC address mapping entry."
    ::= { ibdeEBMacMapEntry 5 }


END
