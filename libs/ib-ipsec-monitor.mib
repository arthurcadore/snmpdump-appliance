-- ===============================================
-- Begin the table of ibIPSecTrafficTable.
-- ===============================================

    ibIPSecTrafficTable OBJECT-TYPE
        SYNTAX SEQUENCE OF IbIPSecTrafficEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "The IPSec Phase-2 Tunnel Traffic Table. "
        ::= { ibIPSecObjects 4 }

    ibIPSecTrafficEntry OBJECT-TYPE
        SYNTAX IbIPSecTrafficEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Information about ibIPSecTrafficTable."
        INDEX { ibIPSecTunIfIndex,ibIPSecTunEntryIndex,ibIPSecTunIndex }
        ::= { ibIPSecTrafficTable 1 }

    IbIPSecTrafficEntry ::=
        SEQUENCE {
            ibIPSecTrafficLocalType
                IbTrafficType,
            ibIPSecTrafficLocalAddr1
                IpAddress,
            ibIPSecTrafficLocalAddr2
                IpAddress,
            ibIPSecTrafficLocalProtocol
                Integer32,
            ibIPSecTrafficLocalPort
                Integer32,
            ibIPSecTrafficRemoteType
                IbTrafficType,
            ibIPSecTrafficRemoteAddr1
                IpAddress,
            ibIPSecTrafficRemoteAddr2
                IpAddress,
            ibIPSecTrafficRemoteProtocol
                Integer32,
            ibIPSecTrafficRemotePort
                Integer32
    }

    ibIPSecTrafficLocalType OBJECT-TYPE
        SYNTAX IbTrafficType
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The type of local peer. Possible values are: a single
             IP address, or an IP address range, or an IP subnet."
        ::= { ibIPSecTrafficEntry 1 }

    ibIPSecTrafficLocalAddr1 OBJECT-TYPE
        SYNTAX IpAddress
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The first specification of local peer's IP address. If
             the local peer type is single IP address, then this is the
             value of the IP address. If the local peer type is IP subnet,
             then this is the value of the subnet. If the local peer type
             is IP address range, then this is the value of beginning IP
             address of the range."
        ::= { ibIPSecTrafficEntry 2 }

    ibIPSecTrafficLocalAddr2 OBJECT-TYPE
        SYNTAX IpAddress
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The second specification of local peer's IP address. If the local
             peer type is single IP address, then this is the value of the
             IP address. If the local peer type is IP subnet, then this is
             the value of the subnet mask. If the local peer type is IP
             address range, then this is the value of ending IP address of
             the range."
        ::= { ibIPSecTrafficEntry 3 }

    ibIPSecTrafficLocalProtocol OBJECT-TYPE
        SYNTAX Integer32 (0..255)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The protocol number of the local peer's traffic."
        ::= { ibIPSecTrafficEntry 4 }

    ibIPSecTrafficLocalPort OBJECT-TYPE
        SYNTAX Integer32 (0..65535)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The port number of the local peer's traffic."
        ::= { ibIPSecTrafficEntry 5 }

    ibIPSecTrafficRemoteType OBJECT-TYPE
        SYNTAX IbTrafficType
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The type of remote peer. Possible values are: a single
             IP address, or an IP address range, or an IP subnet."
        ::= { ibIPSecTrafficEntry 6 }

    ibIPSecTrafficRemoteAddr1 OBJECT-TYPE
        SYNTAX IpAddress
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The first specification of remote peer's IP address.
             If the remote peer type is single IP address, then
             this is the value of the IP address. If the remote
             peer type is IP subnet, then this is the value
             of the subnet. If the remote peer type is IP
             address range, then this is the value of beginning
             IP address of the range."
        ::= { ibIPSecTrafficEntry 7 }

    ibIPSecTrafficRemoteAddr2 OBJECT-TYPE
        SYNTAX IpAddress
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Single IP address, then this is the value of the IP
             address. If the remote peer type is IP subnet,
             then this is the value of the subnet mask. If the
             remote peer type is IP address range, then this
             is the value of ending IP address of the range."
        ::= { ibIPSecTrafficEntry 8 }

    ibIPSecTrafficRemoteProtocol OBJECT-TYPE
        SYNTAX Integer32 (0..255)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The protocol number of the remote peer's traffic."
        ::= { ibIPSecTrafficEntry 9 }

    ibIPSecTrafficRemotePort OBJECT-TYPE
        SYNTAX Integer32 (0..65535)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The port number of the remote peer's traffic."
        ::= { ibIPSecTrafficEntry 10 }

