-- Compliance Statements
-- ===============================================
    ibIPSecCompliance MODULE-COMPLIANCE
        STATUS current
        DESCRIPTION
            " "
        MODULE -- this module
        MANDATORY-GROUPS
            {
                ibIPSecTunnelTableGroup,
                ibIPSecTunnelStatGroup,
                ibIPSecSaGroup,
                ibIPSecTrafficTableGroup,
                ibIPSecGlobalStatsGroup,
                ibIPSecTrapObjectGroup,
                ibIPSecTrapCntlGroup,
                ibIPSecTrapGroup
            }
        ::= { ibIPSecCompliances 1 }

