-- ===============================================
-- definition of traps.
-- ===============================================

    ibIPSecTrap OBJECT IDENTIFIER ::= { ibIPSecObjects 8 }

    ibIPSecNotifications OBJECT IDENTIFIER ::= { ibIPSecTrap 1 }

    ibIPSecTunnelStart NOTIFICATION-TYPE
        OBJECTS { ibIPSecTunLocalAddr,
                  ibIPSecTunRemoteAddr,
                  ibIPSecTunLifeTime,
                  ibIPSecTunLifeSize
                }
        STATUS     current
        DESCRIPTION
            "This notification is generated when an IPSec Phase-2
             Tunnel is created."
        ::= { ibIPSecNotifications 1 }

    ibIPSecTunnelStop NOTIFICATION-TYPE
        OBJECTS { ibIPSecTunLocalAddr,
                  ibIPSecTunRemoteAddr,
                  ibIPSecTunActiveTime
                }
        STATUS     current
        DESCRIPTION
            "This notification is generated when an IPSec Phase-2
             Tunnel is deleted."
        ::= { ibIPSecNotifications 2 }

    ibIPSecNoSaFailure NOTIFICATION-TYPE
        OBJECTS { ibIPSecTunLocalAddr,
                  ibIPSecTunRemoteAddr
                }
        STATUS     current
        DESCRIPTION
            "This notification is generated when an IPSec Phase-2
             non-existent SA error occurs."
        ::= { ibIPSecNotifications 3 }

    ibIPSecAuthFailFailure NOTIFICATION-TYPE
        OBJECTS { ibIPSecTunLocalAddr,
                  ibIPSecTunRemoteAddr
                  }
        STATUS     current
        DESCRIPTION
            "This notification is generated when the IPSec phase-2
             authentication failure occurs."
        ::= { ibIPSecNotifications 4 }

    ibIPSecEncryFailFailure NOTIFICATION-TYPE
        OBJECTS { ibIPSecTunLocalAddr,
                  ibIPSecTunRemoteAddr
                }
        STATUS     current
        DESCRIPTION
            "This notification is generated when the IPSec phase-2
             tunnel has an encrypting failure."
        ::= { ibIPSecNotifications 5 }

    ibIPSecDecryFailFailure NOTIFICATION-TYPE
        OBJECTS { ibIPSecTunLocalAddr,
                  ibIPSecTunRemoteAddr
                }
        STATUS     current
        DESCRIPTION
            "This notification is generated when the IPSec phase-2
             tunnel has a decrypting failure."
        ::= { ibIPSecNotifications 6 }

    ibIPSecInvalidSaFailure NOTIFICATION-TYPE
        OBJECTS { ibIPSecTunLocalAddr,
                  ibIPSecTunRemoteAddr,
                  ibIPSecSpiValue
                }
        STATUS     current
        DESCRIPTION
            "This notification is generated when the IPSec phase-2
             invalid SA failure occurs."
        ::= { ibIPSecNotifications 7 }

    ibIPSecPolicyAdd NOTIFICATION-TYPE
        OBJECTS { ibIPSecPolicyName,
                  ibIPSecPolicySeqNum,
                  ibIPSecPolicySize
                }
        STATUS     current
        DESCRIPTION
            "This notification is generated when an IPSec policy is added."
        ::= { ibIPSecNotifications 8 }

    ibIPSecPolicyDel NOTIFICATION-TYPE
        OBJECTS { ibIPSecPolicyName,
                  ibIPSecPolicySeqNum,
                  ibIPSecPolicySize
                }
        STATUS     current
        DESCRIPTION
            "This notification is generated when an IPSec policy is deleted."
        ::= { ibIPSecNotifications 9 }

    ibIPSecPolicyAttach NOTIFICATION-TYPE
        OBJECTS { ibIPSecPolicyName,
                  ibIPSecPolicySize,
                  ifIndex
                }
        STATUS     current
        DESCRIPTION
            "This notification is generated when an IPSec policy is attached
             with one interface."
        ::= { ibIPSecNotifications 10 }

    ibIPSecPolicyDetach NOTIFICATION-TYPE
        OBJECTS { ibIPSecPolicyName,
                  ibIPSecPolicySize,
                  ifIndex
                }
        STATUS     current
        DESCRIPTION
            "This notification is generated when an IPSec policy is detached
             with one interface."
        ::= { ibIPSecNotifications 11 }

