        ::= { ibIPsecObjectsV2 10 }

    ibIPsecTunnelStatByDescripV2Entry OBJECT-TYPE
        SYNTAX IbIPsecTunnelStatByDescripV2Entry
        MAX-ACCESS not-accessible
        STATUS current
