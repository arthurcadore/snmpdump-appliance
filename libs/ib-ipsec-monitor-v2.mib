        ::= { ibIPsecTunnelStatByDescripV2Table 1 }

    IbIPsecTunnelStatByDescripV2Entry ::=
        SEQUENCE {
            ibIPsecPolicyDescripV2
                OCTET STRING,
            ibIPsecTunInOctetsByDescripV2
                Counter64,
            ibIPsecTunInDecompOctetsByDescripV2
                Counter64,
            ibIPsecTunInPktsByDescripV2
