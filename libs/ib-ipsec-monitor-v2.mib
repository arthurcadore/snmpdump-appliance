                Counter64,
            ibIPsecTunInAuthFailsByDescripV2
                Counter64,
            ibIPsecTunInDecryptFailsByDescripV2
                Counter64,
            ibIPsecTunOutOctetsByDescripV2
                Counter64,
            ibIPsecTunOutUncompOctetsByDescripV2
                Counter64,
            ibIPsecTunOutPktsByDescripV2
                Counter64,
            ibIPsecTunOutDropPktsByDescripV2
                Counter64,
            ibIPsecTunOutEncryptFailsByDescripV2
                Counter64,
            ibIPsecTunNoMemoryDropPktsByDescripV2
