            ifIndex,
            ibEponUniMirrorGroupID
        }
    ::= { ibEponUniMirrorGroupTable 1 }

IbEponUniMirrorGroupEntry ::= SEQUENCE
    {
        ibEponUniMirrorGroupID             Integer32,
        ibEponUniMirrorInboundPortList     OCTET STRING,
        ibEponUniMirrorOutboundPortList    OCTET STRING,
        ibEponUniMonitorPort               Integer32,
        ibEponUniMirrorRowStatus           RowStatus
    }

ibEponUniMirrorGroupID OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Epon UNI mirror group ID."
    ::= { ibEponUniMirrorGroupEntry 1 }

ibEponUniMirrorInboundPortList OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "
        UNI inbound port list.
        Each octet within this value specifies a set of eight ports,
        with the first octet specifying ports 1 through 8, the second
        octet specifying ports 9 through 16, etc.  Within each octet,
        the most significant bit represents the highest numbered UNI,
        and the least significant bit represents the lowest numbered UNI.
        Thus, each UNI port is represented by a single bit within the
        value of this object.  If that bit has a value of '1' then that
        port is allowed in the set of ports.  the port is not allowed if
        its bit has a value of '0'.
        "
    ::= { ibEponUniMirrorGroupEntry 2 }

ibEponUniMirrorOutboundPortList OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "
        UNI outbound port list. For more information, see ibEponUniMirrorInboundPortList.
        "
    ::= { ibEponUniMirrorGroupEntry 3 }

ibEponUniMonitorPort OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "
        UNI monitor port.
        "
    ::= { ibEponUniMirrorGroupEntry 4 }

