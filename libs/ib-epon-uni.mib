    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "
        UNI port policy configuration.
        "
    ::= { ibEponUniSysMan 6 }

ibEponUniPortPolicyEntry OBJECT-TYPE
    SYNTAX      IbEponUniPortPolicyEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "
        The entry of ibEponUniPortPolicyTable.
        "
    INDEX
        {
            ifIndex,
            ibEponUniIndex
        }
    ::= { ibEponUniPortPolicyTable 1 }

IbEponUniPortPolicyEntry ::= SEQUENCE
    {
        ibEponUniPortPolicyStatus                INTEGER,
        ibEponUniPortPolicyCir                   Integer32,
        ibEponUniPortPolicyBucketDepth           Integer32,
        ibEponUniPortPolicyExtraBurst            Integer32,
        ibEponUniPortPolicyInboundCir            Integer32,
        ibEponUniPortPolicyInboundBucketDepth    Integer32,
        ibEponUniPortPolicyInboundExtraBurst     Integer32,
        ibEponUniPortPolicyOutboundCir           Integer32,
        ibEponUniPortPolicyOutboundPir           Integer32
