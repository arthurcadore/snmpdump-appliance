IB-EPON-UNI-MIB DEFINITIONS ::= BEGIN

IMPORTS
        ibEpon
    FROM IB-OID-MIB
        ifIndex, ifDescr
    FROM IF-MIB
        Integer32, Unsigned32, IpAddress, OBJECT-TYPE, MODULE-IDENTITY,
        NOTIFICATION-TYPE
    FROM SNMPv2-SMI
        MacAddress, TruthValue, RowStatus
    FROM SNMPv2-TC;

ibEponUni MODULE-IDENTITY
    LAST-UPDATED "202109101049Z"  --September 10, 2021
    ORGANIZATION
        "Intelbras S.A. Industria de Telecomunicacao Eletronica Brasileria."
    CONTACT-INFO
        ""
    DESCRIPTION
        "The objects in this MIB module are used to manage and
        display current configuration of UNI (user network interface)
        which is based on the ONU(Optical Network Unit)."
    REVISION    "202109101049Z"  -- September 10, 2021
    DESCRIPTION "Modify ibEponUniTrunkPvid and ibEponUniMulticastIndex." 
    REVISION    "202107081049Z"  -- July 08, 2021
    DESCRIPTION "Modify ibEponUniDescr."
    REVISION    "201811281049Z"  -- November 28, 2018
    DESCRIPTION "Add ibUniPoeTable, ibUniPoeInfoTable."
    REVISION    "201706061145Z"  -- June 06, 2017
    DESCRIPTION "Add ibEponUniMacMax to ibEponUniSysManTable."
    REVISION    "201703061145Z"  -- March 06, 2017
    DESCRIPTION "Add the objects of ibEponCTCAlarmTable, ibEponUniErrorInfo,
                 ibEponCTCAlarmTrap, ibEponCTCAlarmRecoverTrap, and ibUniStatisticsTable."
    ::= { ibEpon 5 }

ibEponUniSysMan OBJECT IDENTIFIER ::= { ibEponUni 1 }

ibEponUniSysManTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbEponUniSysManEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table defines several UNI system parameters."
    ::= { ibEponUniSysMan 1 }

ibEponUniSysManEntry OBJECT-TYPE
    SYNTAX      IbEponUniSysManEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The entry of ibEponUniSysManTable."
    INDEX
        {
            ifIndex,
            ibEponUniIndex
        }
    ::= { ibEponUniSysManTable 1 }


IbEponUniSysManEntry ::= SEQUENCE
    {
        ibEponUniIndex                     Integer32,
        ibEponUniDescr                     OCTET STRING,
        ibEponUniAdminStatus               INTEGER,
        ibEponUniMdi                       INTEGER,
        ibEponUniPriority                  Integer32,
        ibEponUniVlanType                  INTEGER,
        ibEponUniAccessVlan                Integer32,
        ibEponUniTrunkPvid                 Integer32,
        ibEponUniVLANTrunkAllowListLow     OCTET STRING,
        ibEponUniVLANTrunkAllowListHigh    OCTET STRING,
        ibEponUniInboundLineRate           Integer32,
        ibEponUniOutboundLineRate          Integer32,
        ibEponUniFlowControl               TruthValue,
        ibEponUniSpeed                     INTEGER,
        ibEponUniDuplex                    INTEGER,
        ibEponUniVlanVPNStatus             TruthValue,
        ibEponUniCountReset                INTEGER,
        ibEponUniPortIsolate               INTEGER,
        ibEponUniVlanConfiguration         OCTET STRING,
        ibEponUniAutoNegotiation           INTEGER,
        ibEponUniRestartAutoNeg            INTEGER,
        ibEponUniLinkStatus                INTEGER,
        ibEponUniInterfaceType             INTEGER,
        ibEponUniVitualCableTest           INTEGER,
        ibEponUniVCTCableStatus            INTEGER,
        ibEponUniVCTCableLength            Integer32,
        ibEponUniVCTImpedanceMismatch      INTEGER,
        ibEponUniVCTPairSkew               Integer32,
        ibEponUniVCTPairSwap               INTEGER,
        ibEponUniVCTPolaritySwap           INTEGER,
        ibEponUniVCTInsertionLoss          Integer32,
        ibEponUniVCTReturnLoss             Integer32,
        ibEponUniVCTNearendCrosstalk       Integer32,
        ibEponUniVlan                      Integer32,
        ibEponUniMacMax                    Integer32
    }

ibEponUniIndex OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "The index value which uniquely identifies the FE(fast Ethernet)
        interface of the ONU."
    ::= { ibEponUniSysManEntry 1 }

ibEponUniDescr OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "A textual string containing information about the interface.  This
        string should include the name of the manufacturer, the product name
        and the version of the interface hardware and software."
    ::= { ibEponUniSysManEntry 2 }

ibEponUniAdminStatus OBJECT-TYPE
    SYNTAX      INTEGER
        {
            up(1),
            down(2),
            testing(3)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The desired state of the user network interface. When a managed
        system initializes, all user network interfaces start with down(2)
        state. The testing(3)state indicates that no operational packets
        can be passed. As a result of either explicit management action or
        per configuration information retained by the managed system,
        ibEponUniAdminStatus is then changed to either the up(1) or
        testing(3) states (or remains in the down(2) state)."
    ::= { ibEponUniSysManEntry 3 }

ibEponUniMdi OBJECT-TYPE
    SYNTAX      INTEGER
        {
            mdi-ii(1),
            mdi-x(2),
            mdi-auto(3)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Type of the line connected to UNI interface:
        mdi-ii(straight-through cable),
        mdi-x (crossover cable),
        mdi-auto (auto-sensing)."
    ::= { ibEponUniSysManEntry 4 }

ibEponUniPriority OBJECT-TYPE
    SYNTAX      Integer32(0..7)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The priority of user network interface.  If the value is larger,
        the priority will be higher."
    ::= { ibEponUniSysManEntry 5 }

ibEponUniVlanType OBJECT-TYPE
    SYNTAX      INTEGER
        {
            vlantrunk(1),
            access(2),
            hybrid(3),
            untagged(4),
            transparent(5),
            doubletagged(6),
            tag(7),
            translation(8),
            aggregation(9)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "UNI vlan types."
    ::= { ibEponUniSysManEntry 6 }

ibEponUniAccessVlan OBJECT-TYPE
    SYNTAX      Integer32(1..4094)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The vlan ID assigned to untagged frames or Priority-Tagged
        frames received on user network interface."
    ::= { ibEponUniSysManEntry 7 }

ibEponUniTrunkPvid OBJECT-TYPE
    SYNTAX      Integer32(1..4094)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "PVID(port vlan ID), the vlan ID assigned to untagged frames or
        Priority-Tagged frames received on user network interface."
    ::= { ibEponUniSysManEntry 8 }

ibEponUniVLANTrunkAllowListLow OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Each octet within this value specifies a set of eight VLANs,
        with the first octet specifying VLANs 1 through 8, the second
        octet specifying VLANs 9 through 16, etc.  Within each octet,
        the most significant bit represents the highest numbered VLAN,
        and the least significant bit represents the lowest numbered VLAN.
        Thus, each allowed vlan of the trunk port is represented by a
        single bit within the value of this object.  If that bit has a value
        of '1' then that vlan is allowed in the set of VLANs.  The vlan
        is not allowed if its bit has a value of '0'."
    ::= { ibEponUniSysManEntry 9 }

ibEponUniVLANTrunkAllowListHigh OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Each octet within this value specifies a set of eight VLANs,
        with the first octet specifying VLANs 2049 through 2056, the
        second octet specifying VLANs 2057 through 2064, etc. Within
        each octet, the most significant bit represents the highest
        numbered vlan, and the least significant bit represents the
        lowest numbered vlan. Thus, each allowed vlan of the trunk port
        is represented by a single bit within the value of this object.
        If that bit has a value of '1' then that vlan is allowed in the
        set of VLANs. The vlan is not allowed if its bit has a value of '0'.
        "
    ::= { ibEponUniSysManEntry 10 }

ibEponUniInboundLineRate OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The value of the inbound line-rate.  measured with kbps.
        The upper limit of the line-rate is the value of
        ibEponUniLineRateMax in the entry.  It must be multiple
        of the value of ibEponUniLineRateStep. The default value of
        ibEponUniLineRate is the value of ibEponUniLineRateMax."
    ::= { ibEponUniSysManEntry 11 }

ibEponUniOutboundLineRate OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The value of the outbound line-rate.  measured with kbps.
        The upper limit of the line-rate is the value of
        ibEponUniLineRateMax in the entry. It must be multiple
        of the value of ibEponUniLineRateStep.  The default value of
        ibEponUniLineRate is the value of ibEponUniLineRateMax."
    ::= { ibEponUniSysManEntry 12 }

ibEponUniFlowControl OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Flow control status.
        true(1)  - Flow control status enable.
        false(2) - Flow control status disable."
    ::= { ibEponUniSysManEntry 13 }

ibEponUniSpeed OBJECT-TYPE
    SYNTAX      INTEGER
        {
            auto(0),
            s10M(10),
            s100M(100),
            s1000M(1000),
            s10000M(10000),
            s24000M(24000)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "User network interface speed."
    ::= { ibEponUniSysManEntry 14 }

ibEponUniDuplex OBJECT-TYPE
    SYNTAX      INTEGER
        {
            full(1),
            half(2),
            auto(3)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "User network interface mode."
    ::= { ibEponUniSysManEntry 15 }

ibEponUniVlanVPNStatus OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "vlan VPN status.
        true(1)  - vlan VPN status enable.
        false(2) - vlan VPN disable."
    ::= { ibEponUniSysManEntry 16 }

ibEponUniCountReset OBJECT-TYPE
    SYNTAX      INTEGER
        {
            reset(1)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Reset the counter which is about packets of the interface,
        then the counter will change to zero.  There is no sense in
        reading this object."
    ::= { ibEponUniSysManEntry 17 }

ibEponUniPortIsolate OBJECT-TYPE
    SYNTAX      INTEGER
        {
            enable(1),
            disable(2)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "
        enable - enable UNI port isolate setting.
        disable - disable UNI port isolate setting.
        "
    ::= { ibEponUniSysManEntry 18 }


ibEponUniVlanConfiguration OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(0..255))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "
        The vlan configuration of the related vlan mode.
        Transparent vlan mode: ibEponUniVlanConfiguration is a
        zero length string.
        Tag vlan mode: the first four octets were the tag
        assigned to untagged frames received on UNI, including TPID,
        VID, CFI and priority.
        Translation vlan mode: the first four octets were the old tag
        (including TPID, VID, CFI and priority), which is replaced
        by the new tag(including TPID, VID, CFI and priority),
        the second four octets.
        Doubletagged vlan mode: the first four octets were the tag
        assigned to untagged or tagged frames received on UNI, including TPID,
        VID, CFI and priority.
        Vlantrunk vlan mode: the first four octets were the old tag
        (including TPID, VID, CFI and priority), which is replaced
        by the new tag(including TPID, VID, CFI and priority) of
        the second four octets, the frames which assigned tag ranges from
        the third four octets to fourth four octets were transmited freely
        and so on for following octets.
        Aggregation vlan mode: the first four octets were the old tag
        (including TPID, VID, CFI and priority), which is replaced
        by the new tag(including TPID, VID, CFI and priority) of
        the second four octets, the frames which assigned tag ranges from
        the third four octets to fourth four octets(using low two octets as vlan tag)
        were replaced by the new tag(using high two octets as target vlan tag)
        and so on for following octets.
        "
    ::= { ibEponUniSysManEntry 23 }

ibEponUniAutoNegotiation OBJECT-TYPE
    SYNTAX      INTEGER
        {
            enable(1),
            disable(2)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "
        enable - enable auto negotiation.
        disable - disable auto negotiation.
        "
    ::= { ibEponUniSysManEntry 25 }

ibEponUniRestartAutoNeg OBJECT-TYPE
    SYNTAX      INTEGER
        {
            autoNegotiation(1)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "
        Force to restart the auto negotiation process.
        "
    ::= { ibEponUniSysManEntry 26 }


ibEponUniLinkStatus OBJECT-TYPE
    SYNTAX      INTEGER
        {
            up(1),
            down(2)
        }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "
        The link status of UNI.
        "
    ::= { ibEponUniSysManEntry 29 }

ibEponUniInterfaceType OBJECT-TYPE
    SYNTAX      INTEGER
        {
            gigabitethernetport(1),
            fastethernetport(2),
            voipport(3),
            e1port(4)
        }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "
        UNI interface type.
        "
    ::= { ibEponUniSysManEntry 30 }

-- BEGIN for UNI VCT
ibEponUniVitualCableTest OBJECT-TYPE
    SYNTAX      INTEGER{true(1),
        false(2)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "
        True(1): Virtual cable test enable.
        False(2): Virtual cable test disable."
    DEFVAL{ false }
    ::= { ibEponUniSysManEntry 31 }

ibEponUniVCTCableStatus OBJECT-TYPE
    SYNTAX      INTEGER{normal(1),
        abnormal(2),
        abnormalOpen(3),
        abnormalShort(4),
        failure(5)
        }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "
        Get the cable status.
        Normal(1),cable is normal.
        Abnormal(2),cable is abnormal.
        AbnormalOpen(3),cable is abnormal because of open circuit.
        AbnormalShort(4),cable is abnormal because of short circuit.
        Failure(5),cable is abnormal because of testing failure.
        "
    ::= { ibEponUniSysManEntry 32 }

ibEponUniVCTCableLength OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "
        Get the cable length.
        When ibEponUniVCTCableStatus is normal,
        the node stands for the length from the UNI port to the port opposite.
        When ibEponUniVCTCableStatus is abnormal,
        it stands for the length from the UNI port to the failure place.
        Unit:meter(m).
        "
    ::= { ibEponUniSysManEntry 33 }

ibEponUniVCTImpedanceMismatch OBJECT-TYPE
    SYNTAX      INTEGER{not-support(1),
        true(2),
        false(3)
        }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "
        Get the cable impedance.
        Not-support(1),the UNI port does not support this function.
        True(2),impedance match.
        False(3),impedance mismatch."
    ::= { ibEponUniSysManEntry 34 }

ibEponUniVCTPairSkew OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "
        Get the symmetry of cable.
        If the function is not supported,
        the default value 0 will be returned.
        Unit:nanosecond(ns).
        "
    ::= { ibEponUniSysManEntry 35 }

ibEponUniVCTPairSwap OBJECT-TYPE
    SYNTAX      INTEGER{notSupport(1),
        true(2),
        false(3)
        }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "
        NotSupport(1),the UNI port does not support this function.
        True(2),pair swap.
        False(3),not pair swap."
    ::= { ibEponUniSysManEntry 36 }

ibEponUniVCTPolaritySwap OBJECT-TYPE
    SYNTAX      INTEGER{notSupport(1),
        true(2),
        false(3)
        }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "
        NotSupport(1),the UNI port does not support this function.
        True(2),polarity swap.
        False(3),not polarity swap."
    ::= { ibEponUniSysManEntry 37 }

ibEponUniVCTInsertionLoss OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "
        If the function is not supported,
        the default value 0 will be returned.
        Unit:decibel(db).
        "
    ::= { ibEponUniSysManEntry 38 }

ibEponUniVCTReturnLoss OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "
        If the function is not supported,
        the default value 0 will be returned.
        Unit:decibel(db).
        "
    ::= { ibEponUniSysManEntry 39 }

ibEponUniVCTNearendCrosstalk OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "
        If the function is not supported,
        the default value 0 will be returned.
        Unit:decibel(db).
        "
    ::= { ibEponUniSysManEntry 40 }
ibEponUniVlan   OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "
        If the function is not supported,
        the default value 0 will be returned.
        Unit:decibel(db).
        "
    ::= { ibEponUniSysManEntry 41 }
ibEponUniMacMax   OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "
        The maximum number of MAC addresses that the UNI port can learn.
        no-limit(65535)
        others(0~65534)
        "
    DEFVAL { 65535 }
    ::= { ibEponUniSysManEntry 42 }
-- END for UNI VCT

ibEponUniCountTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbEponUniCountEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A list of interface counter about frames entries."
    ::= { ibEponUniSysMan 2 }

ibEponUniCountEntry OBJECT-TYPE
    SYNTAX      IbEponUniCountEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "An entry containing management information applicable
        to a particular interface."
    INDEX
        {
            ifIndex,
            ibEponUniIndex
        }
    ::= { ibEponUniCountTable 1 }

IbEponUniCountEntry ::= SEQUENCE
    {
        ibEponUniInStatsPkts                 Unsigned32,
        ibEponUniInStatsUnicastPkts          Unsigned32,
        ibEponUniInStatsBroadcastPkts        Unsigned32,
        ibEponUniInStatsMulticastPkts        Unsigned32,
        ibEponUniInPausePkts                 Unsigned32,
        ibEponUniInTotalErrors               Unsigned32,
        ibEponUniInStatsCRCAlignErrors       Unsigned32,
        ibEponUniInStatsUndersizePkts        Unsigned32,
        ibEponUniInStatsOversizePkts         Unsigned32,
        ibEponUniInErrorbyOther              Unsigned32,
        ibEponUniOutStatsPkts                Unsigned32,
        ibEponUniOutStatsUnicastPkts         Unsigned32,
        ibEponUniOutStatsBroadcastPkts       Unsigned32,
        ibEponUniOutStatsMulticastPkts       Unsigned32,
        ibEponUniOutStatsPausePkts           Unsigned32,
        ibEponUniOutTotalErrors              Unsigned32,
        ibEponUniOutStatsCollisions          Unsigned32,
        ibEponUniOutDelayExceededDiscards    Unsigned32,
        ibEponUniOutErrorbyOther             Unsigned32,
        ibEponUniOutDroppedFrames            Unsigned32
    }

ibEponUniInStatsPkts OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
