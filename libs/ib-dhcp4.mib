ibDhcpServer2PoolSecNwTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbDhcpServer2PoolSecNwEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A table containing the secondary network configuration of
         DHCP pools."
    ::= { ibDhcpServer2Tables 3 }

ibDhcpServer2PoolSecNwEntry OBJECT-TYPE
    SYNTAX      IbDhcpServer2PoolSecNwEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "An entry containing the objects for configuring
         the secondary network for DHCP pools."
    INDEX
        {
            ibDhcpServer2PoolIndex,
            ibDhcpServer2PoolSecNw,
            ibDhcpServer2PoolSecNwMask
        }
    ::= { ibDhcpServer2PoolSecNwTable 1 }

IbDhcpServer2PoolSecNwEntry ::=
    SEQUENCE
        {
            ibDhcpServer2PoolSecNw                InetAddressIPv4,
            ibDhcpServer2PoolSecNwMask            InetAddressIPv4,
            ibDhcpServer2PoolSecNwGwIP            OCTET STRING,
            ibDhcpServer2PoolSecNwStatus          RowStatus
        }

ibDhcpServer2PoolSecNw OBJECT-TYPE
    SYNTAX      InetAddressIPv4
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Network address of the secondary subnets in a DHCP pool."
    ::= { ibDhcpServer2PoolSecNwEntry 1 }

ibDhcpServer2PoolSecNwMask OBJECT-TYPE
    SYNTAX      InetAddressIPv4
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Network mask of the secondary subnets in a DHCP pool."
    ::= { ibDhcpServer2PoolSecNwEntry 2 }

ibDhcpServer2PoolSecNwGwIP OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(0..127))
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Gateway IP addresses of the secondary subnets in a DHCP pool.

         IP addresses are space separated and the leading
         or trailing space will be trimmed.

         Since at most 8 gateway addresses can be configured
         for a secondary network, a string is defined to get
         or configure 8 IP at a time.

         If the object is set with a zero-length string,
         the configuration of the gateway address is removed."
    ::= { ibDhcpServer2PoolSecNwEntry 3 }

ibDhcpServer2PoolSecNwStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "RowStatus.  Three actions are used: active,
         createAndGo, destroy."
    ::= { ibDhcpServer2PoolSecNwEntry 4 }

-- =============================================================================
-- 4th Table of ibDhcpServer2Tables: ibDhcpServer2PoolClassTable
-- =============================================================================
ibDhcpServer2PoolClassTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbDhcpServer2PoolClassEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A table containing address range of the DHCP
         server pool for user classes."
    ::= { ibDhcpServer2Tables 4 }

ibDhcpServer2PoolClassEntry OBJECT-TYPE
    SYNTAX      IbDhcpServer2PoolClassEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "An entry containing address range of DHCP pools
         for user classes."
    INDEX { ibDhcpServer2PoolIndex, ibDhcpServer2PoolClassName }
    ::= { ibDhcpServer2PoolClassTable 1 }

IbDhcpServer2PoolClassEntry ::=
    SEQUENCE
        {
            ibDhcpServer2PoolClassName            OCTET STRING,
            ibDhcpServer2PoolClassStart           InetAddressIPv4,
            ibDhcpServer2PoolClassEnd             InetAddressIPv4,
            ibDhcpServer2PoolClassStatus          RowStatus
        }

ibDhcpServer2PoolClassName OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(1..63))
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "User class name."
    ::= { ibDhcpServer2PoolClassEntry 1 }

ibDhcpServer2PoolClassStart OBJECT-TYPE
    SYNTAX      InetAddressIPv4
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Start address of an address range for a DHCP user class.

         The SET operation to this object ought to be with
         the SET operation of ibDhcpServer2PoolClassEnd together,
         and any SET operation alone to this object will be
         regarded as an invalid operation."
    ::= { ibDhcpServer2PoolClassEntry 2 }

ibDhcpServer2PoolClassEnd OBJECT-TYPE
    SYNTAX      InetAddressIPv4
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "End address of an address range for a DHCP user class.

         The SET operation to this object ought to be with
         the SET of ibDhcpServer2PoolClassStart together,
         and any SET operation alone to this object will be
         regarded as an invalid operation.

         This object must be set with an IP address NOT smaller
         than the value of ibDhcpServer2PoolClassStart."
    ::= { ibDhcpServer2PoolClassEntry 3 }

ibDhcpServer2PoolClassStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "RowStatus.  Three actions are used: active,
         createAndGo, destroy.

         If the value of this object is set to 'createAndGo',
         ibDhcpServer2PoolClassStart and ibDhcpServer2PoolClassEnd
         must also be set."
    ::= { ibDhcpServer2PoolClassEntry 4 }

-- =============================================================================
-- 5th Table of ibDhcpServer2Tables: ibDhcpServer2PoolStaticTable
-- =============================================================================
ibDhcpServer2PoolStaticTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbDhcpServer2PoolStaticEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A table containing the static binding configurations
         in DHCP pools."
    ::= { ibDhcpServer2Tables 5 }

ibDhcpServer2PoolStaticEntry OBJECT-TYPE
    SYNTAX      IbDhcpServer2PoolStaticEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "An entry containing the objects for configuring
         the static binding in DHCP pools."
    INDEX { ibDhcpServer2PoolIndex, ibDhcpServer2PoolStaticIP}
    ::= { ibDhcpServer2PoolStaticTable 1 }

IbDhcpServer2PoolStaticEntry ::=
    SEQUENCE
        {
            ibDhcpServer2PoolStaticIP          InetAddressIPv4,
            ibDhcpServer2PoolStaticMask        InetAddressIPv4,
            ibDhcpServer2PoolStaticCID         OCTET STRING,
            ibDhcpServer2PoolStaticHAddr       OCTET STRING,
            ibDhcpServer2PoolStaticHType       INTEGER,
            ibDhcpServer2PoolStaticStatus      RowStatus
        }

ibDhcpServer2PoolStaticIP OBJECT-TYPE
    SYNTAX      InetAddressIPv4
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Ip address of a static binding in a DHCP pool."
    ::= { ibDhcpServer2PoolStaticEntry 1 }

ibDhcpServer2PoolStaticMask OBJECT-TYPE
    SYNTAX      InetAddressIPv4
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Network mask of a static binding in a DHCP pool."
    ::= { ibDhcpServer2PoolStaticEntry 2 }

ibDhcpServer2PoolStaticCID OBJECT-TYPE
    SYNTAX     OCTET STRING(SIZE(0|4..254))
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Client identifier of a DHCP client that the IP
         address binds to.

         The structure of the OCTET STRING is
         H-H-H... while H indicates 4 hexadecimal characters
         except that the last H indicates 2 or 4 hexadecimal
         characters."
    ::= { ibDhcpServer2PoolStaticEntry 3 }

ibDhcpServer2PoolStaticHAddr OBJECT-TYPE
    SYNTAX     OCTET STRING(SIZE(0|4..39))
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Hardware address of a DHCP client that the IP
         address binds to.

         The structure of the OCTET STRING is
         H-H-H... while H indicates 4 hexadecimal characters
         except that the last H indicates 2 or 4 hexadecimal
         characters."
    ::= { ibDhcpServer2PoolStaticEntry 4 }

ibDhcpServer2PoolStaticHType OBJECT-TYPE
    SYNTAX     INTEGER
        {
            default(1),
            ethernet(2),
            tokenRing(3)
        }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Hardware type of a DHCP client that the IP address binds to.

         The SET operation to this object ought to be with
         the SET operation of ibDhcpServer2PoolStaticHAddr,
         and any SET operation alone to this object will be
         regarded as an invalid operation.

         'default' indicates that the hardware type is unknown.
         If the value of ibDhcpServer2PoolStaticCID is set to a non-zero
         length string, the object is set with 'default' automatically.
         Otherwise it cannot be set with 'default'.

         'ethernet' indicates that the hardware type is ethernet.
         If the value of ibDhcpServer2PoolStaticHAddr is set with
         a non-zero length string, the default value of this object
         is 'ethernet'.

         'tokenRing' indicates that the hardware type is token-ring."
    ::= { ibDhcpServer2PoolStaticEntry 5 }

ibDhcpServer2PoolStaticStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "RowStatus.  Three actions are used: active,
         createAndGo, destroy.

         If the value of this object is set to 'createAndGo',
         ibDhcpServer2PoolStaticCID or (ibDhcpServer2PoolStaticHardAddr
         and ibDhcpServer2PoolStaticHardType) must also be set."
    ::= { ibDhcpServer2PoolStaticEntry 6 }

-- =============================================================================
-- 6th Table of ibDhcpServer2Tables: ibDhcpServer2PoolOptionTable
-- =============================================================================
ibDhcpServer2PoolOptionTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbDhcpServer2PoolOptionEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A table for configuring user-defined options for DHCP pools."
    ::= { ibDhcpServer2Tables 6 }

ibDhcpServer2PoolOptionEntry OBJECT-TYPE
    SYNTAX      IbDhcpServer2PoolOptionEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "An entry containing the objects for configuring user-defined
         options for DHCP pools."
    INDEX { ibDhcpServer2PoolIndex, ibDhcpServer2PoolOptCode }
    ::= { ibDhcpServer2PoolOptionTable 1 }

IbDhcpServer2PoolOptionEntry ::=
    SEQUENCE
        {
            ibDhcpServer2PoolOptCode         Integer32,
            ibDhcpServer2PoolOptType         INTEGER,
            ibDhcpServer2PoolOptAscii        OCTET STRING,
            ibDhcpServer2PoolOptHexStr       OCTET STRING,
            ibDhcpServer2PoolOptIPStr        OCTET STRING,
            ibDhcpServer2PoolOptRowStatus    RowStatus
        }

ibDhcpServer2PoolOptCode OBJECT-TYPE
    SYNTAX      Integer32(2..254)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Option code."
    ::= { ibDhcpServer2PoolOptionEntry 1 }

ibDhcpServer2PoolOptType OBJECT-TYPE
    SYNTAX      INTEGER
        {
            ascii(1),
            hex(2),
            ip(3)
        }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Option type.

         'ascii' means using the ascii string that is defined
         by ibDhcpServer2PoolOptAscii to fill in the option.

         'hex' means using the hex string that is defined by
         ibDhcpServer2PoolOptHexStr to fill in the option.

         'ip' means using the IP string that is defined by
         ibDhcpServer2PoolOptIPStr to fill in the option."
    ::= { ibDhcpServer2PoolOptionEntry 2 }

ibDhcpServer2PoolOptAscii OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(0..255))
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Ascii string of an option.

         If ibDhcpServer2PoolOptType is set with 'ascii',
         this object must be set with a non-zero length
         string."
    ::= { ibDhcpServer2PoolOptionEntry 3 }

ibDhcpServer2PoolOptHexStr OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(0..510))
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Hex string of an option.

         The length of the string must be even.
         If ibDhcpServer2PoolOptType is set with 'hex',
         this object must be set with a non-zero length
         string."
    ::= { ibDhcpServer2PoolOptionEntry 4 }

ibDhcpServer2PoolOptIPStr OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(0..127))
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "IP string of an option.

         IP addresses are space separated and the leading
         or trailing space will be trimmed.

         1 to 8 IP addresses can be configured simultaneously.

         If ibDhcpServer2PoolOptType is set with 'ip',
         this object must be set with a non-zero length string
         that must contain at least one IP address."
    ::= { ibDhcpServer2PoolOptionEntry 5 }

ibDhcpServer2PoolOptRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "RowStatus.  Three actions are used: active, createAndGo,
         destroy.

         If the value of this object is set to 'createAndGo',
         ibDhcpServer2PoolOptType must also be set."
    ::= { ibDhcpServer2PoolOptionEntry 6 }

