-- =============================================================================
ibDhcpServer2RuleTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbDhcpServer2RuleEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A table for configuring option rules used to match DHCP users."
    ::= { ibDhcpServer2Tables 9 }

ibDhcpServer2RuleEntry OBJECT-TYPE
    SYNTAX      IbDhcpServer2RuleEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "An entry for configuring option rules used to match DHCP users."
    INDEX
        {
            ibDhcpServer2ClassName,
            ibDhcpServer2RuleNumber
        }
    ::= { ibDhcpServer2RuleTable 1 }

IbDhcpServer2RuleEntry ::=
    SEQUENCE
        {
            ibDhcpServer2RuleNumber           Integer32,
            ibDhcpServer2RuleOptCode          Integer32,
            ibDhcpServer2RuleOptHexStr        OCTET STRING,
            ibDhcpServer2RuleOptMask          OCTET STRING,
            ibDhcpServer2RuleOptOffset        Integer32,
            ibDhcpServer2RuleOptLength        Integer32,
            ibDhcpServer2RuleRowStatus        RowStatus
        }

ibDhcpServer2RuleNumber OBJECT-TYPE
    SYNTAX      Integer32(1..128)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Rule number."
    ::= { ibDhcpServer2RuleEntry 1 }

ibDhcpServer2RuleOptCode OBJECT-TYPE
    SYNTAX      Integer32(1..254)
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Option code."
    ::= { ibDhcpServer2RuleEntry 2 }

ibDhcpServer2RuleOptHexStr OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(0..510))
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Hex string of an option.

         The length of the string must be even."
    ::= { ibDhcpServer2RuleEntry 3 }

ibDhcpServer2RuleOptMask OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(0..510))
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The mask used to match options.

         The length of this object must be same with that of
         ibDhcpServer2RuleOptHexStr.

         The SET operation of this object must be with
         the SET operation of ibDhcpServer2RuleOptHexStr."
    ::= { ibDhcpServer2RuleEntry 4 }

ibDhcpServer2RuleOptOffset OBJECT-TYPE
    SYNTAX      Integer32(0..254)
