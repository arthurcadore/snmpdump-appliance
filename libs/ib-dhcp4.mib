        {   ibDhcpServer2OptionCode         Integer32,
            ibDhcpServer2OptionType         INTEGER,
            ibDhcpServer2OptionAscii        OCTET STRING,
            ibDhcpServer2OptionHexStr       OCTET STRING,
            ibDhcpServer2OptionIPStr        OCTET STRING,
            ibDhcpServer2OptionRowStatus    RowStatus
        }

