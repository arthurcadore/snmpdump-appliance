-- =============================================================================
-- DHCP relay table definition.
-- =============================================================================
    ibDhcpRelay2Tables OBJECT IDENTIFIER ::= { ibDhcp4 4 }

-- =============================================================================
-- 1st Table of ibDhcpRelay2Tables: ibDhcpRelay2IfConfigTable
-- =============================================================================
ibDhcpRelay2IfConfigTable  OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbDhcpRelay2IfConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A table for configuring relay functions for interfaces."
    ::= { ibDhcpRelay2Tables 1 }

ibDhcpRelay2IfConfigEntry  OBJECT-TYPE
    SYNTAX      IbDhcpRelay2IfConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "An entry for configuring relay functions for an interface."
    INDEX { ifIndex }
    ::= { ibDhcpRelay2IfConfigTable 1 }

IbDhcpRelay2IfConfigEntry ::=
    SEQUENCE
        {
            ibDhcpRelay2IfSelectRelay              TruthValue,
            ibDhcpRelay2IfCheckMac                 TruthValue,
            ibDhcpRelay2IfOpt82Enable              TruthValue,
            ibDhcpRelay2IfOpt82Strategy            INTEGER,
            ibDhcpRelay2IfOpt82CIDMode             INTEGER,
            ibDhcpRelay2IfOpt82CIDNodeType         INTEGER,
            ibDhcpRelay2IfOpt82CIDNodeStr          OCTET STRING,
            ibDhcpRelay2IfOpt82CIDStr              OCTET STRING,
            ibDhcpRelay2IfOpt82CIDFormat           INTEGER,
            ibDhcpRelay2IfOpt82RIDMode             INTEGER,
            ibDhcpRelay2IfOpt82RIDStr              OCTET STRING,
            ibDhcpRelay2IfOpt82RIDFormat           INTEGER
        }

ibDhcpRelay2IfSelectRelay    OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "If the value is 'true', the DHCP relay function
         would be enabled on this interface."
    DEFVAL      { false }
    ::= { ibDhcpRelay2IfConfigEntry 1 }

ibDhcpRelay2IfCheckMac    OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "If the value is 'true', the function of checking MAC address
         of packets received from the interface would be enabled."
    DEFVAL      { false }
    ::= { ibDhcpRelay2IfConfigEntry 2 }

ibDhcpRelay2IfOpt82Enable OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "If DHCP relay supports option 82 functions, the
         value is 'true'.
         If DHCP relay does NOT support option 82 functions,
         the value is 'false'."
    DEFVAL      { false }
    ::= { ibDhcpRelay2IfConfigEntry 3 }

ibDhcpRelay2IfOpt82Strategy     OBJECT-TYPE
    SYNTAX      INTEGER
    {
        drop(1),
        keep(2),
        replace(3)
    }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The strategies of DHCP relay handling option 82.

         'drop' indicates DHCP relay discarding the request
         packet including option 82.

         'keep' indicates DHCP relay accepting the request
         packet without any change of the option 82.

         'replace' indicates DHCP relay accepting the
         request packet on condition that it generates a new
         option 82 to replace the original one."
    DEFVAL      { replace }
    ::= { ibDhcpRelay2IfConfigEntry 4 }

ibDhcpRelay2IfOpt82CIDMode OBJECT-TYPE
    SYNTAX      INTEGER
    {
        normal(1),
        verbose(2),
        userDefine(3)
    }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The mode of the circuit ID suboption of DHCP option 82.

         'normal' is the standard mode.

         'verbose' is the detailed mode.

         'userDefine' is the user-defined mode.

         If the value of this object is set to 'userDefine',
         ibDhcpRelay2IfOpt82CIDStr must also be set."
    DEFVAL      { normal }
    ::= { ibDhcpRelay2IfConfigEntry 5 }

ibDhcpRelay2IfOpt82CIDNodeType OBJECT-TYPE
    SYNTAX      INTEGER
    {
        invalid(1),
        mac(2),
        sysname(3),
        userDefine(4)
    }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Type of access node identifier that is used to fill
         in the circuit ID suboption of option 82 when the value
         of ibDhcpRelay2IfOpt82CIDMode is set to 'verbose'.

         The SET operation of this object must be with the SET
         operation of ibDhcpRelay2IfOpt82CIDMode.

         'invalid' indicates that the node identifier is NOT used
         to fill in the circuit ID suboption of option 82.
         If the value of ibDhcpRelay2IfOpt82CIDMode is NOT 'verbose',
         the value of this object is set to 'invalid' automatically.
         Otherwise the value of this object cannot be set to 'invalid'.

         'mac' indicates that the node identifier is filled in with
         the MAC of DHCP relay input interface.
         If the value of ibDhcpRelay2IfOpt82CIDMode is set to
         'verbose', the default value of this object is 'mac'.

         'sysname' indicates that the node identifier is filled in with
         the sysname of the DHCP relay device.

         'userDefine' indicates that the node identifier is filled in with
         the string defined by the object of ibDhcpRelay2IfOpt82CIDNodeStr.
         If the value is set to 'userDefine', the value of
         ibDhcpRelay2IfOpt82CIDNodeStr must be set to a non-zero
         length string simultaneously."
    ::= { ibDhcpRelay2IfConfigEntry 6 }

ibDhcpRelay2IfOpt82CIDNodeStr      OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(0..50))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
         "The string defined by user to fill in the
          node identifier in the verbose mode.

          If the value of ibDhcpRelay2IfOpt82CIDMode is NOT 'verbose',
          or the value of ibDhcpRelay2IfOpt82CIDNodeType is
          NOT 'userDefine', it is set with a zero-length string
          automatically.
          Otherwise it must be set with a non-zero length string.

          The SET operation of this object must be with the SET
          operation of ibDhcpRelay2IfOpt82CIDMode and
          ibDhcpRelay2IfOpt82CIDNodeType."
    ::= { ibDhcpRelay2IfConfigEntry 7 }

ibDhcpRelay2IfOpt82CIDStr      OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(0|3..63))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The string defined by user to fill in the circuit ID
         suboption of option 82 in the user-define mode.

         If the value of ibDhcpRelay2IfOpt82CIDMode is NOT 'userDefine',
         it is set to a zero-length string automatically.
         Otherwise it must be set with a non-zero length string.

         The SET operation of this object must be with the SET
         operation of ibDhcpRelay2IfOpt82CIDMode."
    ::= { ibDhcpRelay2IfConfigEntry 8 }

ibDhcpRelay2IfOpt82CIDFormat    OBJECT-TYPE
    SYNTAX      INTEGER
    {
        hex(1),
        ascii(2),
        undefine(3)
    }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The format of the circuit ID suboption of DHCP
         option 82.

         'hex' is the hex format.

         'ascii' is the ascii format.

         'undefine' is the undefine format.

         If the value of ibDhcpRelay2IfOpt82CIDMode is
         'normal', the value of this object is set to 'hex'
         automatically and cannot be set to 'undefine'.

         If the value of ibDhcpRelay2IfOpt82CIDMode is
         'verbose', the default value of this object is 'undefine'.

         If the value of ibDhcpRelay2IfOpt82CIDMode is
         'userDefine', the value of this object is set to
         'ascii' automatically and cannot be set to other value.

         The SET operation of this object must be with the SET
         operation of ibDhcpRelay2IfOpt82CIDMode."
    ::= { ibDhcpRelay2IfConfigEntry 9 }

ibDhcpRelay2IfOpt82RIDMode OBJECT-TYPE
    SYNTAX      INTEGER
    {
        normal(1),
        sysname(2),
        userDefine(3)
    }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The mode of the remote ID suboption of DHCP
         option 82.

         'normal' is the standard mode.

         'sysname' is the sysname mode.

         'userDefine' is the user-defined mode.

         If the value of this object is set to 'userDefine',
         ibDhcpRelay2IfOpt82RIDStr must also be set."
    DEFVAL      { normal }
    ::= { ibDhcpRelay2IfConfigEntry 10 }

ibDhcpRelay2IfOpt82RIDStr  OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE(0..63))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The string defined by user to fill in the
         remote ID in the user-defined mode.

         If the value of ibDhcpRelay2IfOpt82RIDMode is NOT
         'userDefine', it is set to a zero-length string automatically.
         Otherwise it must be set with a non-zero length string.

         The SET operation of this object must be with the SET
         operation of ibDhcpRelay2IfOpt82RIDMode."
    ::= { ibDhcpRelay2IfConfigEntry 11 }

ibDhcpRelay2IfOpt82RIDFormat    OBJECT-TYPE
    SYNTAX      INTEGER
    {
        hex(1),
        ascii(2)
    }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The format of the remote ID suboption of DHCP option 82.

         'hex' is the hex format.

         'ascii' is the ascii format.

         If the value of ibDhcpRelay2IfOpt82RIDMode is
         'sysname' or 'userDefine', this object is set with 'ascii'
         automatically and cannot be set with other value.

         If the value of ibDhcpRelay2IfOpt82RIDMode is 'normal',
         the default value of this object is 'hex'.

         The SET operation of this object must be with the SET
         operation of ibDhcpRelay2IfOpt82RIDMode."
    ::= { ibDhcpRelay2IfConfigEntry 12 }

-- =============================================================================
-- 2nd Table of ibDhcpRelay2Tables: ibDhcpRelay2SrvAddrTable
-- =============================================================================
ibDhcpRelay2SrvAddrTable  OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbDhcpRelay2SrvAddrEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A table for configuring a DHCP
         server address on the interface."
    ::= { ibDhcpRelay2Tables 2 }

ibDhcpRelay2SrvAddrEntry  OBJECT-TYPE
    SYNTAX      IbDhcpRelay2SrvAddrEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "An entry for configuring a DHCP
         server address on the interface."
    INDEX
        {
            ifIndex,
            ibDhcpRelay2SrvAddrIP
        }
    ::= { ibDhcpRelay2SrvAddrTable 1 }

IbDhcpRelay2SrvAddrEntry ::=
    SEQUENCE
        {
            ibDhcpRelay2SrvAddrIP                InetAddressIPv4,
            ibDhcpRelay2SrvAddrRowStatus         RowStatus
        }

ibDhcpRelay2SrvAddrIP    OBJECT-TYPE
    SYNTAX      InetAddressIPv4
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "Ip address of DHCP server."
    ::= { ibDhcpRelay2SrvAddrEntry 1 }

ibDhcpRelay2SrvAddrRowStatus    OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Operation status of this table entry.
         Three actions are used: active,
         createAndGo, destroy."
    ::= { ibDhcpRelay2SrvAddrEntry 2 }

-- =============================================================================
-- 3rd Table of ibDhcpRelay2Tables: ibDhcpRelay2UserInfoTable
-- =============================================================================
ibDhcpRelay2UserInfoTable   OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbDhcpRelay2UserInfoEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A table containing the user information of DHCP client."
    ::= { ibDhcpRelay2Tables 3 }

ibDhcpRelay2UserInfoEntry  OBJECT-TYPE
    SYNTAX      IbDhcpRelay2UserInfoEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "An entry containing the user information of DHCP client."
    INDEX
    {
        ibDhcpRelay2UserInfoVpnIndex,
        ibDhcpRelay2UserInfoIpAddr
    }
    ::= { ibDhcpRelay2UserInfoTable 1 }

IbDhcpRelay2UserInfoEntry   ::=
    SEQUENCE
    {
        ibDhcpRelay2UserInfoVpnIndex      Unsigned32,
        ibDhcpRelay2UserInfoIpAddr        InetAddressIPv4,
        ibDhcpRelay2UserInfoMacAddr       MacAddress,
        ibDhcpRelay2UserInfoIfIndex       InterfaceIndexOrZero,
        ibDhcpRelay2UserInfoRowStatus     RowStatus
    }

ibDhcpRelay2UserInfoVpnIndex OBJECT-TYPE
    SYNTAX      Unsigned32(0 .. 65534)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "VPN index of the DHCP client."
    ::= { ibDhcpRelay2UserInfoEntry 1 }

ibDhcpRelay2UserInfoIpAddr     OBJECT-TYPE
    SYNTAX      InetAddressIPv4
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "DHCP client's IP address."
    ::= { ibDhcpRelay2UserInfoEntry 2 }

ibDhcpRelay2UserInfoMacAddr    OBJECT-TYPE
    SYNTAX      MacAddress
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "DHCP client's MAC address."
    ::= { ibDhcpRelay2UserInfoEntry 3 }

ibDhcpRelay2UserInfoIfIndex      OBJECT-TYPE
    SYNTAX      InterfaceIndexOrZero
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The index of the interface that DHCP client adhered to.
         0 means that no interface information is recorded in the
         user information entry."
    ::= { ibDhcpRelay2UserInfoEntry 4 }

ibDhcpRelay2UserInfoRowStatus     OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Operation status of this table entry.
         Only destroy action is allow to use."
    ::= { ibDhcpRelay2UserInfoEntry 5 }

-- =============================================================================
-- DHCP server trap objects definition.
-- =============================================================================
ibDhcpServer2Traps OBJECT IDENTIFIER ::= { ibDhcp4 5 }

ibDhcpServer2TrapNotify OBJECT IDENTIFIER ::= { ibDhcpServer2Traps 0 }

ibDhcpServer2AddrExhaust NOTIFICATION-TYPE
    OBJECTS
    {
        ibDhcpServer2PoolIndex,
        ibDhcpServer2PoolName
    }
    STATUS      current
    DESCRIPTION
        "This trap is generated when addresses are exhausted
         in the DHCP pool."
    ::= { ibDhcpServer2TrapNotify 1 }

ibDhcpServer2AddrExhaustRecov NOTIFICATION-TYPE
    OBJECTS
    {
        ibDhcpServer2PoolIndex,
        ibDhcpServer2PoolName
    }
    STATUS      current
    DESCRIPTION
        "This trap is generated when the DHCP pool recovers from
         address exhaustion."
    ::= { ibDhcpServer2TrapNotify 2 }

ibDhcpServer2IpUsageOverflow NOTIFICATION-TYPE
    OBJECTS
    {
        ibDhcpServer2PoolIndex,
        ibDhcpServer2PoolName
    }
    STATUS      current
    DESCRIPTION
        "This trap is generated when the IP address usage of DHCP
         server pool above the threshold.  The value of the IP address
         usage is sampled for every 5 minutes."
    ::= { ibDhcpServer2TrapNotify 3 }

ibDhcpServer2AllocOverflow NOTIFICATION-TYPE
    STATUS      current
    DESCRIPTION
        "This trap is generated when the success rate of allocation
         of DHCP server IP address in 5 minutes below the threshold."
    ::= { ibDhcpServer2TrapNotify 4 }

ibDhcpServer2IpUsageOverflowRecov NOTIFICATION-TYPE
    OBJECTS
    {
        ibDhcpServer2PoolIndex,
        ibDhcpServer2PoolName
    }
    STATUS      current
    DESCRIPTION
        "This trap is generated when the IP address usage of DHCP
         server pool reduced to 90% of the threshold.
         The value of the IP address usage is sampled for every 5 minutes."
    ::= { ibDhcpServer2TrapNotify 5 }

ibDhcpServer2NetUsageOverflow NOTIFICATION-TYPE
    OBJECTS
    {
        ibDhcpServer2PoolIndex,
        ibDhcpServer2PoolName
    }
    STATUS      current
    DESCRIPTION
        "This trap is generated when the subnet usage of DHCP
         server pool has reached or exceeded the threshold. The IP address usage
         data is polled every 5 minutes."
    ::= { ibDhcpServer2TrapNotify 6 }

ibDhcpServer2NetUsageOverflowRecov NOTIFICATION-TYPE
    OBJECTS
    {
        ibDhcpServer2PoolIndex,
        ibDhcpServer2PoolName
    }
    STATUS      current
    DESCRIPTION
        "This trap is generated when the subnet usage in the DHCP
         server pool has dropped to or below 90% of the threshold.
         The IP address usage data is polled every 5 minutes."
    ::= { ibDhcpServer2TrapNotify 7 }

ibDhcpServer2NetExhaust NOTIFICATION-TYPE
    OBJECTS
    {
        ibDhcpServer2PoolIndex,
        ibDhcpServer2PoolName
    }
    STATUS      current
    DESCRIPTION
        "This trap is generated when all IP addresses
         in the DHCP pool are exhausted."
    ::= { ibDhcpServer2TrapNotify 8 }

ibDhcpServer2NetExhaustRecov NOTIFICATION-TYPE
    OBJECTS
    {
        ibDhcpServer2PoolIndex,
        ibDhcpServer2PoolName
    }
    STATUS      current
    DESCRIPTION
        "This trap is generated when the DHCP pool recovers from
         IP address exhaustion."
    ::= { ibDhcpServer2TrapNotify 9 }

ibDhcpServer2GpNetExhaust NOTIFICATION-TYPE
    OBJECTS
    {
        ibDhcpServer2PoolGpName
    }
    STATUS      current
    DESCRIPTION
        "This trap is generated when all IP addresses
         in the DHCP pool group are exhausted."
    ::= { ibDhcpServer2TrapNotify 10 }

ibDhcpServer2GpNetExhaustRecov NOTIFICATION-TYPE
    OBJECTS
    {
        ibDhcpServer2PoolGpName
    }
    STATUS      current
    DESCRIPTION
        "This trap is generated when the DHCP pool group recovers from
         IP address exhaustion."
    ::= { ibDhcpServer2TrapNotify 11 }

ibDhcpSvr2AllocIpFailed NOTIFICATION-TYPE
    OBJECTS
    {
        ibDhcpServer2OnlineFailReason
    }
    STATUS      current
    DESCRIPTION
        "This trap is generated when DHCP servers fail to alloc address."
    ::= { ibDhcpServer2TrapNotify 12 }

ibDhcpServer2ServerDown NOTIFICATION-TYPE
    OBJECTS
    {
        ibDhcpServer2OnlineFailReason
    }
    STATUS      current
    DESCRIPTION
        "This trap is generated when the DHCP servers fail to alloc address."
    ::= { ibDhcpServer2TrapNotify 13 }

ibDhcpSvr2GpAddrExhaus NOTIFICATION-TYPE
    OBJECTS
    {
        ibDhcpServer2PoolGpName
    }
    STATUS      current
    DESCRIPTION
        "This trap is generated when all IP addresses
         in the DHCP pool group are exhausted."
    ::= { ibDhcpServer2TrapNotify 14 }

ibDhcpSvr2GpAddrExhaustRecov NOTIFICATION-TYPE
    OBJECTS
    {
        ibDhcpServer2PoolGpName
    }
    STATUS      current
    DESCRIPTION
        "This trap is generated when the DHCP pool group recovers from
         IP address exhaustion."
    ::= { ibDhcpServer2TrapNotify 15 }

ibDhcpServer2DevOdapFull NOTIFICATION-TYPE
    STATUS      current
    DESCRIPTION
        "This trap is generated when all ODAP
        address pools on vBRAS device are full."
    ::= { ibDhcpServer2TrapNotify 16 }

ibDhcpServer2OdapApplyFail NOTIFICATION-TYPE
    OBJECTS
    {
        ibDhcpServer2OnlineFailReason
    }
    STATUS      current
    DESCRIPTION
        "This trap is generated when DHCP servers fail to apply for subnet."
    ::= { ibDhcpServer2TrapNotify 17 }

ibDhcpServer2OdapReleaseFail NOTIFICATION-TYPE
    OBJECTS
    {
        ibDhcpServer2OnlineFailReason
    }
    STATUS      current
    DESCRIPTION
        "This trap is generated when DHCP servers fail to release subnet."
    ::= { ibDhcpServer2TrapNotify 18 }

ibDhcpSvr2GpUsageOverflow NOTIFICATION-TYPE
    OBJECTS
    {
        ibDhcpServer2PoolGpName
    }
    STATUS      current
    DESCRIPTION
        "This trap is generated when the usage of DHCP
         server pool group has reached or exceeded the threshold.
         The IP address usage data is polled every 5 minutes."
    ::= { ibDhcpServer2TrapNotify 19 }

ibDhcpSvr2GpUsageOverflowRecov NOTIFICATION-TYPE
    OBJECTS
    {
        ibDhcpServer2PoolGpName
    }
    STATUS      current
    DESCRIPTION
        "This trap is generated when the DHCP pool group recovers from
         IP address exhaustion."
    ::= { ibDhcpServer2TrapNotify 20 }

ibDhcpSvr2UserIpConflict NOTIFICATION-TYPE
    OBJECTS
    {
        ibDhcpServer2PoolName,
        ibDhcpServer2AllocConflictIP
    }
    STATUS      current
    DESCRIPTION
        "This trap is generated when the DHCP Server received a decline
        packet from the client."
    ::= { ibDhcpServer2TrapNotify 21 }

END
