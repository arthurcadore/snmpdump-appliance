-- * End of ibDot11WIDSGlobalConfigGroup Definition
-- *****************************************************************************

-- *****************************************************************************
-- *  ibDot11WIDSPermitVendorTable Definition
-- *****************************************************************************
ibDot11WIDSPermitVendorTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbDot11WIDSPermitVendorEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The table provides the permitted vendor list, and each vendor
        will be identified by OUI.
        The legal device should be made by the permitted vendors."
    ::= { ibDot11WIDSConfigGroup 2 }

ibDot11WIDSPermitVendorEntry OBJECT-TYPE
    SYNTAX      IbDot11WIDSPermitVendorEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry provides the information of permitted vendor."
    INDEX
        {
            ibDot11VendorOUI
        }
    ::= { ibDot11WIDSPermitVendorTable 1 }

IbDot11WIDSPermitVendorEntry ::= SEQUENCE
    {
        ibDot11VendorOUI                OCTET STRING,
        ibDot11PermitVendorRowStatus    RowStatus,
        ibDot11VendorName               OCTET STRING
    }

ibDot11VendorOUI OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(3))
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Represents the vendor OUI information of the wireless device."
    ::= { ibDot11WIDSPermitVendorEntry 1 }

ibDot11PermitVendorRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The status of this table entry."
    ::= { ibDot11WIDSPermitVendorEntry 2 }

ibDot11VendorName OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(0..127))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the vendor name of the wireless device."
    ::= { ibDot11WIDSPermitVendorEntry 3 }
-- *****************************************************************************
-- * End of ibDot11WIDSPermitVendorTable Definition
-- *****************************************************************************

-- *****************************************************************************
