-- *****************************************************************************
-- *  ibDot11WIDSRogueAPExtTable Definition
-- *****************************************************************************
ibDot11WIDSRogueAPExtTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbDot11WIDSRogueAPExtEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "As each rogue AP could be detected by multiple monitor APs, each
        monitor AP could have some kind of detailed information about
        a specific rogue AP.
        In the ibDot11WIDSRogueAPTable table, the detailed
        information for a specific rogue AP will be summarized from
        information in the ibDot11WIDSRogueAPExtTable table.
        For example, multiple monitor APs could receive RF signal of
        one rogue AP, and each monitor AP has its maximum signal strength by
        itself. The information will be kept as
        ibDot11DetectMaxAPSigStrength in the ibDot11WIDSRogueAPExtTable
        table. While only the maximum value among all the
        ibDot11DetectMaxAPSigStrength for each monitor AP will be
        kept in the ibDot11WIDSRogueAPTable as
        ibDot11RogueAPMaxSigStrength."
    ::= { ibDot11WIDSDetectGroup 2 }

ibDot11WIDSRogueAPExtEntry OBJECT-TYPE
    SYNTAX      IbDot11WIDSRogueAPExtEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry contains information of the rogue AP detected
        by each monitor AP."
    INDEX
        {
            ibDot11RogueAPBSSMAC,
            ibDot11WIDSAPID
        }
    ::= { ibDot11WIDSRogueAPExtTable 1 }

IbDot11WIDSRogueAPExtEntry ::= SEQUENCE
    {
        ibDot11WIDSAPID                  IbDot11ObjectIDType,
        ibDot11DetectCurAPSigStrength    Integer32,
        ibDot11DetectAPByChannel         IbDot11ChannelScopeType,
        ibDot11DetectAPByRadioID         IbDot11RadioScopeType,
        ibDot11AttackAPStatus            TruthValue,
        ibDot11DetectAPFirstTm           TimeTicks,
        ibDot11DetectAPLastTm            TimeTicks
    }

ibDot11WIDSAPID OBJECT-TYPE
    SYNTAX      IbDot11ObjectIDType
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "To uniquely identify each AP, and relation-ship between
        ibDot11WIDSAPID and AP device will be static."
    ::= { ibDot11WIDSRogueAPExtEntry 1 }

ibDot11DetectCurAPSigStrength OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "dBm"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the current value of signal strength that WIDS monitor
        AP received from the rogue AP."
    ::= { ibDot11WIDSRogueAPExtEntry 2 }

ibDot11DetectAPByChannel OBJECT-TYPE
    SYNTAX      IbDot11ChannelScopeType
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents on which radio channel that WIDS monitor AP detected
        the rogue AP."
    ::= { ibDot11WIDSRogueAPExtEntry 3 }

ibDot11DetectAPByRadioID OBJECT-TYPE
    SYNTAX      IbDot11RadioScopeType
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents on which radio the monitor AP has detected the rogue
        AP."
    ::= { ibDot11WIDSRogueAPExtEntry 4 }

ibDot11AttackAPStatus OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents whether monitor AP have taken countermeasure on the
        rogue AP."
    ::= { ibDot11WIDSRogueAPExtEntry 5 }

ibDot11DetectAPFirstTm OBJECT-TYPE
    SYNTAX      TimeTicks
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the time that monitor AP detected the rogue AP for
        the first time."
    ::= { ibDot11WIDSRogueAPExtEntry 6 }

ibDot11DetectAPLastTm OBJECT-TYPE
    SYNTAX      TimeTicks
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the time that monitor AP detected the rogue AP for
        the last time."
    ::= { ibDot11WIDSRogueAPExtEntry 7 }
-- *****************************************************************************
-- * end of ibDot11WIDSRogueAPExtTable Definition
-- *****************************************************************************

