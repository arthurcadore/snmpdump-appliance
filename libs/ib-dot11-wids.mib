-- *****************************************************************************
-- *  ibDot11WIDSRogueHistoryTable Definition
-- *****************************************************************************
ibDot11WIDSRogueHistoryTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbDot11WIDSRogueHistoryEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table contains information of all expired rogue devices which
         have been deleted from the list of detected rogue devices because
         they could not be detected within the device aging duration."
    ::= { ibDot11WIDSDetectGroup 8 }

ibDot11WIDSRogueHistoryEntry OBJECT-TYPE
    SYNTAX      IbDot11WIDSRogueHistoryEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry contains information of an expired rogue device which
         has been deleted from the list of detected rogue devices because
         they could not be detected within the device aging duration."
    INDEX
        {
            ibDot11WIDSRogueHisIndex
        }
    ::= { ibDot11WIDSRogueHistoryTable 1 }

IbDot11WIDSRogueHistoryEntry ::= SEQUENCE
    {
        ibDot11WIDSRogueHisIndex        Integer32,
        ibDot11WIDSRogueHisMAC          MacAddress,
        ibDot11WIDSRogueHisVendor       OCTET STRING,
        ibDot11WIDSRogueHisType         IbDot11WIDSDevType,
        ibDot11WIDSRogueHisChl          IbDot11ChannelScopeType,
        ibDot11WIDSRogueHisSSID         OCTET STRING,
        ibDot11WIDSRogueHisLastDctTime  DateAndTime
    }

ibDot11WIDSRogueHisIndex OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Represents index of this entry."
    ::= { ibDot11WIDSRogueHistoryEntry 1 }

ibDot11WIDSRogueHisMAC OBJECT-TYPE
    SYNTAX      MacAddress
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the MAC address of the device."
    ::= { ibDot11WIDSRogueHistoryEntry 2 }

ibDot11WIDSRogueHisVendor OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the vendor for the device."
    ::= { ibDot11WIDSRogueHistoryEntry 3 }

ibDot11WIDSRogueHisType OBJECT-TYPE
    SYNTAX      IbDot11WIDSDevType
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the type of the device."
    ::= { ibDot11WIDSRogueHistoryEntry 4 }

ibDot11WIDSRogueHisChl OBJECT-TYPE
    SYNTAX      IbDot11ChannelScopeType
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the channel in which the device was last detected."
    ::= { ibDot11WIDSRogueHistoryEntry 5 }

ibDot11WIDSRogueHisSSID OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the service set identifier for the ESS of the device."
    ::= { ibDot11WIDSRogueHistoryEntry 6 }

ibDot11WIDSRogueHisLastDctTime OBJECT-TYPE
    SYNTAX      DateAndTime
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the time at which the device was last detected."
    ::= { ibDot11WIDSRogueHistoryEntry 7 }
-- *****************************************************************************
-- * end of ibDot11WIDSRogueHistoryTable Definition
-- *****************************************************************************

-- *****************************************************************************
-- *  ibDot11WIDSAtkHistroyTable Definition
-- *****************************************************************************
ibDot11WIDSAtkHistroyTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbDot11WIDSAtkHistroyEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table contains information of the history of attacks detected in
         the WLAN system."
    ::= { ibDot11WIDSDetectGroup 9 }

ibDot11WIDSAtkHistroyEntry OBJECT-TYPE
    SYNTAX      IbDot11WIDSAtkHistroyEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry contains information of the history of attacks detected in
         the WLAN system."
    INDEX
        {
            ibDot11WIDSAtkHisIndex
        }
    ::= { ibDot11WIDSAtkHistroyTable 1 }

IbDot11WIDSAtkHistroyEntry ::= SEQUENCE
    {
        ibDot11WIDSAtkHisIndex       Integer32,
        ibDot11WIDSAtkHisMAC         MacAddress,
        ibDot11WIDSAtkHisType        IbDot11WIDSAtkType,
        ibDot11WIDSAtkHisChl         IbDot11ChannelScopeType,
        ibDot11WIDSAtkHisRSSI        Integer32,
        ibDot11WIDSAtkHisDctTime     DateAndTime,
        ibDot11WIDSAtkHisAPName      OCTET STRING
    }

ibDot11WIDSAtkHisIndex OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Represents index of this entry."
    ::= { ibDot11WIDSAtkHistroyEntry 1 }

ibDot11WIDSAtkHisMAC OBJECT-TYPE
    SYNTAX      MacAddress
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the Mac address.  In case of spoof attacks, this field
         provides the BSSID which was spoofed.  In case of other attacks,
         this field provides the MAC address of the device which initiated
         the attack."
    ::= { ibDot11WIDSAtkHistroyEntry 2 }

ibDot11WIDSAtkHisType OBJECT-TYPE
    SYNTAX      IbDot11WIDSAtkType
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the type of attack."
    ::= { ibDot11WIDSAtkHistroyEntry 3 }

ibDot11WIDSAtkHisChl OBJECT-TYPE
    SYNTAX      IbDot11ChannelScopeType
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the channel in which the attack was detected."
    ::= { ibDot11WIDSAtkHistroyEntry 4 }

ibDot11WIDSAtkHisRSSI OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the average RSSI of the designated attack."
    ::= { ibDot11WIDSAtkHistroyEntry 5 }

ibDot11WIDSAtkHisDctTime OBJECT-TYPE
    SYNTAX      DateAndTime
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the time at which this attack was detected."
    ::= { ibDot11WIDSAtkHistroyEntry 6 }

ibDot11WIDSAtkHisAPName OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the name of the AP which detected this attack."
    ::= { ibDot11WIDSAtkHistroyEntry 7 }
-- *****************************************************************************
-- * end of ibDot11WIDSAtkHistroyTable Definition
-- *****************************************************************************

-- *****************************************************************************
-- *  ibDot11WIDSAtkStatis Definition
-- *****************************************************************************
ibDot11WIDSAtkStatis OBJECT IDENTIFIER       ::= { ibDot11WIDSDetectGroup 10 }

ibDot11WIDSAtkStasStartTime OBJECT-TYPE
    SYNTAX      DateAndTime
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents current attack tracking time.  It is started at the system
         startup and is refreshed each hour subsequently."
    ::= { ibDot11WIDSAtkStatis 1 }

-- *****************************************************************************
-- *  ibDot11WIDSAtkStasTable Definition
-- *****************************************************************************
ibDot11WIDSAtkStasTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbDot11WIDSAtkStasEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table contains information of the counts of attacks detected."
    ::= { ibDot11WIDSAtkStatis 2 }

ibDot11WIDSAtkStasEntry OBJECT-TYPE
    SYNTAX      IbDot11WIDSAtkStasEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry contains information of the counts of attacks detected."
    INDEX
        {
            ibDot11WIDSAtkStasType
        }
    ::= { ibDot11WIDSAtkStasTable 1 }

IbDot11WIDSAtkStasEntry ::= SEQUENCE
    {
        ibDot11WIDSAtkStasType       IbDot11WIDSAtkType,
        ibDot11WIDSAtkStasCurCnt     Unsigned32,
        ibDot11WIDSAtkStasTotalCnt   Unsigned32
    }

ibDot11WIDSAtkStasType OBJECT-TYPE
    SYNTAX      IbDot11WIDSAtkType
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Represents the type of attack."
    ::= { ibDot11WIDSAtkStasEntry 1 }

ibDot11WIDSAtkStasCurCnt OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the count of attacks detected since the time
         specified by the current attack tracking time.  The current
         attack tracking time is started at the system startup and
         is refreshed each hour subsequently."
    ::= { ibDot11WIDSAtkStasEntry 2 }

ibDot11WIDSAtkStasTotalCnt OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the total count of the attacks detected since
         the system startup."
    ::= { ibDot11WIDSAtkStasEntry 3 }
-- *****************************************************************************
-- *  end of ibDot11WIDSAtkStasTable Definition
-- *****************************************************************************

-- *****************************************************************************
-- *  ibDot11BlackListTable Definition
-- *****************************************************************************
ibDot11BlackListTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbDot11BlackListEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table contains information of blacklist entries, including
         dynamic and static."
    ::= { ibDot11WIDSDetectGroup 11 }

ibDot11BlackListEntry OBJECT-TYPE
    SYNTAX      IbDot11BlackListEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry contains information of blacklist."
    INDEX
        {
            ibDot11BlackListMAC
        }
    ::= { ibDot11BlackListTable 1 }

IbDot11BlackListEntry ::= SEQUENCE
    {
        ibDot11BlackListMAC        MacAddress,
        ibDot11BlackListTime       Unsigned32,
        ibDot11BlackListReason     OCTET STRING,
        ibDot11BlackListRowStatus  RowStatus,
        ibDot11BlackListTimeTicks  TimeTicks
    }

ibDot11BlackListMAC OBJECT-TYPE
    SYNTAX      MacAddress
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This object represents the MAC address of the device inserted into
         the table."
    ::= { ibDot11BlackListEntry 1 }

ibDot11BlackListTime OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "minutes"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the time elapsed since the entry was last updated.
         If it is static blacklist, the value is always 0."
    ::= { ibDot11BlackListEntry 2 }

ibDot11BlackListReason OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the reason why the entry was added into the blacklist."
    ::= { ibDot11BlackListEntry 3 }

ibDot11BlackListRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object represents the status of this table entry."
    ::= { ibDot11BlackListEntry 4 }

ibDot11BlackListTimeTicks OBJECT-TYPE
    SYNTAX      TimeTicks
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the time elapsed since the entry was last updated in timetick.
         If it is static blacklist, the value is always 0."
    ::= { ibDot11BlackListEntry 5 }
-- *****************************************************************************
-- * end of ibDot11BlackListTable Definition
-- *****************************************************************************


-- *****************************************************************************
-- * end of ibDot11WIDSAtkStatis Definition
-- *****************************************************************************

-- *****************************************************************************
-- *     Notifications OF ibDot11WIDSNotifyGroup
-- *****************************************************************************
-- WIDS Notification
        ibDot11WIDSTraps OBJECT IDENTIFIER
    ::= { ibDot11WIDSNotifyGroup 1 }

ibDot11WIDSDetectRogueTrap NOTIFICATION-TYPE
    OBJECTS
        {
            ibDot11WIDSRogueMAC,
            ibDot11WIDSRogueType,
            ibDot11WIDSMonitorMAC,
            ibDot11MonitorAPID,
            ibDot11MonitorApRadioID
        }
    STATUS      current
    DESCRIPTION
        "The notification represents that a rogue AP or a station was
        detected by WIDS.
        The NMS would refer to MIB table under ibDot11WIDSDetectGroup
        group to get more detailed information."
    ::= { ibDot11WIDSTraps 1 }

ibDot11WIDSAdHocTrap NOTIFICATION-TYPE
    OBJECTS
        {
            ibDot11WIDSAdHocMAC,
            ibDot11WIDSMonitorMAC
        }
    STATUS      current
    DESCRIPTION
        "The notification represents a rogue Ad hoc station was detected."
    ::= { ibDot11WIDSTraps 2 }

ibDot11WIDSUnauthorSSIDTrap NOTIFICATION-TYPE
    OBJECTS
        {
            ibDot11UnauthorSSIDName,
            ibDot11WIDSMonitorMAC,
            ibDot11MonitorAPID,
            ibDot11MonitorApRadioID
        }
    STATUS      current
    DESCRIPTION
        "The notification represents which unauthorized SSID are
        accessed in the network.
        The notification will be sent to NMS when an
        unauthorized SSID is detected on the network for the
        first time."
    ::= { ibDot11WIDSTraps 3 }

ibDot11WIDSDisappearRogueTrap NOTIFICATION-TYPE
    OBJECTS
        {
            ibDot11WIDSRogueMAC
        }
    STATUS      current
    DESCRIPTION
        "The notification represents that a rogue device has aged out
        and moved to history table or the device type has been changed
        to friendly.
        The notification will be sent to NMS whenever a rogue disappears."
    ::= { ibDot11WIDSTraps 4 }

ibDot11WIDSDetectAttack NOTIFICATION-TYPE
    OBJECTS
    {
        ibDot11WIDSAtkHisType,
        ibDot11WIDSAtkHisChl,
        ibDot11WIDSAtkHisDctTime,
        ibDot11WIDSAtkHisAPName
    }
    STATUS current
    DESCRIPTION
        "This notification occurs when some type of attack is detected.
        "
    ::= { ibDot11WIDSTraps 5 }

ibDot11WIDSDetectWBridge NOTIFICATION-TYPE
    OBJECTS
    {
        ibDot11WIDSRptAPName,
        ibDot11WIDSRptAPRadioID,
        ibDot11WIDSRptAPLstDctTime
    }
    STATUS current
    DESCRIPTION
        "This notification occurs whenever a detected device is classified
        as rogue wireless-bridge.
        "
    ::= { ibDot11WIDSTraps 6 }

ibDot11WIDSFloodTrap NOTIFICATION-TYPE
    OBJECTS
    {
        ibDot11WIDSAtkMac,
        ibDot11WIDSAtkFrameType,
        ibDot11WIDSFirstTrapTime
    }
    STATUS current
    DESCRIPTION
        "This notification occurs when flood attack is detected.
        "
    ::= { ibDot11WIDSTraps 7 }

ibDot11WIDSSpoofTrap NOTIFICATION-TYPE
    OBJECTS
    {
        ibDot11WIDSAtkMac,
        ibDot11WIDSAtkFrameType,
        ibDot11WIDSAtkChannel,
        ibDot11WIDSAtkTime,
        ibDot11WIDSAtkDestMac,
        ibDot11WIDSFirstTrapTime
    }
    STATUS current
    DESCRIPTION
        "This notification occurs when spoof attack is detected.
        "
    ::= { ibDot11WIDSTraps 8 }

ibDot11WIDSWeakIVTrap NOTIFICATION-TYPE
    OBJECTS
    {
        ibDot11WIDSAtkMac,
        ibDot11WIDSAtkChannel,
        ibDot11WIDSAtkTime,
        ibDot11WIDSAtkDestMac,
        ibDot11WIDSFirstTrapTime
    }
    STATUS current
    DESCRIPTION
        "This notification occurs when weak IV attack is detected.
        "
