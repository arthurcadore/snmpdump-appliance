-- The RPR defect report table

    ibRprDefect OBJECT IDENTIFIER ::= { ibRprObjects 5 }

    ibRprDefectReportTable OBJECT-TYPE
        SYNTAX SEQUENCE OF IbRprDefectReportEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "The RPR Defect Report Table that contains the trap information on
             the ring."
        ::= { ibRprDefect 1 }

    ibRprDefectReportEntry OBJECT-TYPE
        SYNTAX IbRprDefectReportEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "An entry in the RPR Defect Report Table."
        INDEX { ibRprDefectIfIndex }
        ::= { ibRprDefectReportTable 1 }

    IbRprDefectReportEntry ::=
        SEQUENCE {
            ibRprDefectIfIndex
                InterfaceIndex,
            ibRprDefectCurrentStatus
                BITS
        }

    ibRprDefectIfIndex OBJECT-TYPE
        SYNTAX InterfaceIndex
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "The ifIndex of RPR interface."
        REFERENCE
            "RFC 2863, ifIndex"
        ::= { ibRprDefectReportEntry 1 }

    ibRprDefectCurrentStatus OBJECT-TYPE
        SYNTAX BITS {
            topologyOpenRing(0),
            topoInstability(1),
            topoInconsistent(2),
            dulpMacAddress(3),
            dulpIPAddress(4),
            lrttDefect(5),
            protCfgDefect(6),
            jumboCfgDefect(7),
            excessReservedRateDefect(8),
            excessMaxStationNum(9),
            miscabling(10),
            backPressure(11)
        }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Indicates the current status of RPR ring.
             This attribute is used to notify the management system that
             alarms generate or clear about some RPR ring."
        REFERENCE
            "IEEE 802.17 V3.3 Subclause 11.2.9."
        ::= { ibRprDefectReportEntry 2 }

-- The priority to RPR service class map table

