IB-RPR-MIB DEFINITIONS ::= BEGIN

    IMPORTS
        ibCommon
            FROM IB-OID-MIB
        rprTopoImageEntry, rprTopoImageMacAddress, rprTopoImageInetAddress
            FROM IEEE-802DOT17-RPR-MIB
        InterfaceIndex
            FROM IF-MIB
        SnmpAdminString
            FROM SNMP-FRAMEWORK-MIB
        RowStatus, TruthValue, MacAddress, TEXTUAL-CONVENTION
            FROM SNMPv2-TC
        MODULE-IDENTITY, OBJECT-TYPE, NOTIFICATION-TYPE,
        Gauge32, Counter64, Integer32
            FROM SNMPv2-SMI
        InetAddress
            FROM INET-ADDRESS-MIB;

        ibRpr MODULE-IDENTITY
            LAST-UPDATED "200503161000Z"  -- March 16, 2005 at 10:18:41 GMT
            ORGANIZATION
                "Intelbras S.A. Industria de Telecomunicacao Eletronica Brasileria."
            CONTACT-INFO
                ""
            DESCRIPTION
                "This MIB manages RPR(Resilient Packet Ring)interfaces by
                 providing an operational table which controls parameters
                 of each RPR interface and reports alarm conditions.

                 A RPR ring is composed of two fiber rings: Outer and Inner."
            REVISION "200503161000Z"      -- March 16, 2005 at 10:18:41 GMT
            DESCRIPTION
                "First published version."
            ::= { ibCommon 60 }

--
-- Textual conventions
--

    IbRprRingletID ::= TEXTUAL-CONVENTION
        STATUS current
        DESCRIPTION
            "Indicates the path on which data flow is selected.
             The valid value is ringlet0 and ringlet1. They indicate
             two RPR fiber rings. Ringlet0 refers to outer ring and
             ringlet1 refers to inner ring."
        REFERENCE
            "IEEE 802.17 V3.3 Subclause 11.2.1.1 Span and
             ringlet parameterization."
        SYNTAX INTEGER
            {
             ringlet0(1),
             ringlet1(2)
        }

    IbRprServiceClass ::= TEXTUAL-CONVENTION
        STATUS current
        DESCRIPTION
            "Indicates the service class of RPR.
             The valid value is ClassC, ClassB, ClassA1 and ClassA0."
        REFERENCE
                "IEEE 802.17 V3.3 Subclause 7.3 Service class."
        SYNTAX INTEGER
            {
             classC(1),
             classB(2),
             classA1(3),
             classA0(4)
        }

    ibRprObjects OBJECT IDENTIFIER ::= { ibRpr 1 }

-- RPR maximum define table

    ibRprMaxmumDefine OBJECT IDENTIFIER ::= { ibRprObjects 1 }

    ibRprMaxmumDefineTable OBJECT-TYPE
        SYNTAX SEQUENCE OF IbRprMaxmumDefineEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "This table contains the specs of RPR ring."
      ::= { ibRprMaxmumDefine 1 }

    ibRprMaxmumDefineEntry OBJECT-TYPE
        SYNTAX IbRprMaxmumDefineEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "The entry of RPR Maximum Table."
        INDEX { ibRprMaxMumIfIndex }
        ::= { ibRprMaxmumDefineTable 1 }

    IbRprMaxmumDefineEntry ::=
        SEQUENCE {
            ibRprMaxMumIfIndex
                InterfaceIndex,
            ibRprMaxStationNumDefine
                INTEGER,
            ibRprMaxReservedRateDefine
                Gauge32
        }

    ibRprMaxMumIfIndex OBJECT-TYPE
        SYNTAX InterfaceIndex
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "The ifIndex of this RPR interface."
        ::= { ibRprMaxmumDefineEntry 1 }

    ibRprMaxStationNumDefine OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Max station number supported by RPR ring."
        ::= { ibRprMaxmumDefineEntry 2 }


    ibRprMaxReservedRateDefine OBJECT-TYPE
        SYNTAX Gauge32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Max reserved rate supported by each RPR ringlet.
             An estimate of the interface's current bandwidth in
             units of 1,000,000 bits per second.  If this object
             reports a value of 'n' then the speed of the interface
             is somewhere in the range of 'n-500,000' to
             'n+499,999'.  For interfaces which do not vary in
             bandwidth or for those where no accurate estimation
             can be made, this object should contain the nominal
             bandwidth.  For a sub-layer which has no concept of
             bandwidth, this object should be zero."
        ::= { ibRprMaxmumDefineEntry 3 }

    ibRprTopoImage OBJECT IDENTIFIER ::= { ibRprObjects 2 }

-- extension to the RPR topology table

    ibRprTopoImageXTable OBJECT-TYPE
        SYNTAX SEQUENCE OF IbRprTopoImageXEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "This table contains additional objects
             for the rprTopoImageTable in IEEE-802DOT17-RPR-MIB."
        REFERENCE
            "rprTopoImageTable in IEEE-802DOT17-RPR-MIB."
        ::= { ibRprTopoImage 1 }

    ibRprTopoImageXEntry OBJECT-TYPE
        SYNTAX IbRprTopoImageXEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Each entry contains information specific to a particular
             station on the ring. The table has at least one entry for
             the station itself, this entry indicates zero hops on each
             ringlet."
         AUGMENTS { rprTopoImageEntry }
         ::= { ibRprTopoImageXTable 1 }

    IbRprTopoImageXEntry ::=
        SEQUENCE {
            ibRprTopoImageXWestEdgeStatus
                TruthValue,
            ibRprTopoImageXEastEdgeStatus
                TruthValue,
            ibRprTopoImageXStationName
                SnmpAdminString
        }

    ibRprTopoImageXWestEdgeStatus OBJECT-TYPE
        SYNTAX TruthValue
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The current edge status of the west span.
             If an edge exists,the ibRprTopoImageXWestEdgeStatus is true.
             Otherwise, the ibRprTopoImageXWestEdgeStatus is false."
        REFERENCE
            "IEEE 802.17  V3.3 Subclause 11.1.1.5, Edges"
        ::= { ibRprTopoImageXEntry 3 }

    ibRprTopoImageXEastEdgeStatus OBJECT-TYPE
        SYNTAX TruthValue
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The current edge status of the east span.
             If an edge exists, the ibRprTopoImageXEastEdgeStatus is true.
             Otherwise, the ibRprTopoImageXEastEdgeStatus is false."
        REFERENCE
             "IEEE 802.17  V3.3 Subclause 11.1.1.5, Edges"
        ::= { ibRprTopoImageXEntry 4 }

    ibRprTopoImageXStationName OBJECT-TYPE
        SYNTAX SnmpAdminString
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "The operator assigned station name. The operator can assigned it
             by ibRprTopoImageXStationName."
        REFERENCE
            "IEEE 802.17 V3.3 Subclause 11.2.6, stationName."
        ::= { ibRprTopoImageXEntry 5 }

    ibRprSpanCounters OBJECT IDENTIFIER ::= { ibRprObjects 3 }

-- RPR source mac counters table

    ibRprSrcMacCountTable OBJECT-TYPE
        SYNTAX SEQUENCE OF IbRprSrcMacCountEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "The RPR Source Mac Count Table contains the cumulative sum
             of frames and octects from the particular source station."
        ::= { ibRprSpanCounters 1 }

    ibRprSrcMacCountEntry OBJECT-TYPE
        SYNTAX IbRprSrcMacCountEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "An entry in the RPR Source Mac Count Table."
        INDEX { ibRprSrcMacCountIfIndex, ibRprSrcMacCountBySrcAddress }
        ::= { ibRprSrcMacCountTable 1 }

    IbRprSrcMacCountEntry ::=
        SEQUENCE {
            ibRprSrcMacCountIfIndex
                InterfaceIndex,
            ibRprSrcMacCountBySrcAddress
                MacAddress,
          ibRprSrcMacCountReceivedFrames
                Counter64,
            ibRprSrcMacCountReceivedOctets
                Counter64
        }

    ibRprSrcMacCountIfIndex OBJECT-TYPE
        SYNTAX InterfaceIndex
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "The ifIndex of this RPR interface."
        REFERENCE
            "RFC 2863, ifIndex."
        ::= { ibRprSrcMacCountEntry 1 }

    ibRprSrcMacCountBySrcAddress OBJECT-TYPE
        SYNTAX MacAddress
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "MAC address of the source station where the frames come from."
        ::= { ibRprSrcMacCountEntry 2 }

    ibRprSrcMacCountReceivedFrames OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Frames received from a particular MAC address of source station."
        ::= { ibRprSrcMacCountEntry 3 }

    ibRprSrcMacCountReceivedOctets OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Octets received from a particular MAC address of source station."
        ::= { ibRprSrcMacCountEntry 4 }

-- RPR destination mac counters table

    ibRprDestMacCountTable OBJECT-TYPE
        SYNTAX SEQUENCE OF IbRprDestMacCountEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "The RPR Destination MAC Count Table contains the cumulative sum
             of  frames and octects to the particular destination station."
        ::= { ibRprSpanCounters 2 }

    ibRprDestMacCountEntry OBJECT-TYPE
        SYNTAX IbRprDestMacCountEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "An entry in the RPR MAC Destination Count Table."
        INDEX { ibRprDestMacCountIfIndex, ibRprDestMacCountByDestAddress }
        ::= { ibRprDestMacCountTable 1 }

    IbRprDestMacCountEntry ::=
        SEQUENCE {
            ibRprDestMacCountIfIndex
                InterfaceIndex,
            ibRprDestMacCountByDestAddress
                MacAddress,
            ibRprDestMacCountReceivedFrames
                Counter64,
            ibRprDestMacCountReceivedOctets
                Counter64
        }

    ibRprDestMacCountIfIndex OBJECT-TYPE
        SYNTAX InterfaceIndex
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "The ifIndex of RPR interface."
        REFERENCE
            "RFC 2863, ifIndex"
        ::= { ibRprDestMacCountEntry 1 }

    ibRprDestMacCountByDestAddress OBJECT-TYPE
        SYNTAX MacAddress
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "MAC address of the destination station where the frames are sent
             to."
        ::= { ibRprDestMacCountEntry 2 }

    ibRprDestMacCountReceivedFrames OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Frames sent to a particular MAC address of destination station."
        ::= { ibRprDestMacCountEntry 3 }

    ibRprDestMacCountReceivedOctets OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Octets sent to a particular MAC address of destination station."
        ::= { ibRprDestMacCountEntry 4 }

-- RPR packet drop counters table

     ibRprPktDropCountTable OBJECT-TYPE
        SYNTAX SEQUENCE OF IbRprPktDropCountEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "The RPR Packet Drop Count Table contains the cumulative sum
             of packets dropped for congestion."
        ::= { ibRprSpanCounters 3 }

     ibRprPktDropCountEntry OBJECT-TYPE
        SYNTAX IbRprPktDropCountEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "An entry in the RPR Packet Drop Count Table."
        INDEX { ibRprPktDropCntIfIndex, ibRprPktDropCntRingletID }
        ::= { ibRprPktDropCountTable 1 }

     IbRprPktDropCountEntry ::=
        SEQUENCE {
                ibRprPktDropCntIfIndex
                    InterfaceIndex,
                ibRprPktDropCntRingletID
                    IbRprRingletID,
                ibRprDownFlowClassAPktDrops
                    Counter64,
                ibRprUpFlowClassAPktDrops
                    Counter64,
                ibRprDownFlowClassBPktDrops
                    Counter64,
                ibRprUpFlowClassBPktDrops
                    Counter64,
                ibRprDownFlowClassCPktDrops
                    Counter64,
                ibRprUpFlowClassCPktDrops
                    Counter64
        }

     ibRprPktDropCntIfIndex OBJECT-TYPE
        SYNTAX InterfaceIndex
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "The ifIndex of this RPR interface."
        REFERENCE
             "RFC 2863, ifIndex."
        ::= { ibRprPktDropCountEntry 1 }

     ibRprPktDropCntRingletID OBJECT-TYPE
        SYNTAX IbRprRingletID
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "The RPR Ringlet of the station through which the packets transmit."
        ::= { ibRprPktDropCountEntry 2 }

     ibRprDownFlowClassAPktDrops OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total dropped number of Class A packets which transmit
             down from the host."
        ::= { ibRprPktDropCountEntry 3 }

     ibRprUpFlowClassAPktDrops OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total dropped number of Class A packets which transmit
             up to the host."
        ::= { ibRprPktDropCountEntry 4 }

     ibRprDownFlowClassBPktDrops OBJECT-TYPE
        SYNTAX Counter64
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The total dropped number of Class B packets which transmit
             down from the host."
        ::= { ibRprPktDropCountEntry 5 }

     ibRprUpFlowClassBPktDrops OBJECT-TYPE
         SYNTAX Counter64
         MAX-ACCESS read-only
         STATUS current
         DESCRIPTION
            "The total dropped number of Class B packets which transmit
             up to the host."
         ::= { ibRprPktDropCountEntry 6 }

     ibRprDownFlowClassCPktDrops OBJECT-TYPE
         SYNTAX Counter64
         MAX-ACCESS read-only
         STATUS current
         DESCRIPTION
             "The total dropped number of Class C packets which transmit
              down from the host."
         ::= { ibRprPktDropCountEntry 7 }

     ibRprUpFlowClassCPktDrops OBJECT-TYPE
         SYNTAX Counter64
         MAX-ACCESS read-only
         STATUS current
         DESCRIPTION
             "The total dropped number of Class C packets which transmit
              up to the host."
         ::= { ibRprPktDropCountEntry 8 }

    ibRprRS OBJECT IDENTIFIER ::= { ibRprObjects 4 }

-- The RPR static ring selection table

    ibRprStaticRSTable OBJECT-TYPE
        SYNTAX SEQUENCE OF IbRprStaticRSEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "The RPR Static Ring Selection table contains the static
             ring selection information by the way of configuration."
        ::= { ibRprRS 1 }

    ibRprStaticRSEntry OBJECT-TYPE
        SYNTAX IbRprStaticRSEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "An entry in the RPR Static Ring Selection table."
        INDEX { ibRprStaticRSIfIndex, ibRprStaticRSMacAddress }
        ::= { ibRprStaticRSTable 1 }

    IbRprStaticRSEntry ::=
        SEQUENCE {
            ibRprStaticRSIfIndex
                InterfaceIndex,
            ibRprStaticRSMacAddress
                MacAddress,
            ibRprStaticRSRingletID
                IbRprRingletID,
            ibRprStaticRSTtl
                INTEGER,
            ibRprStaticRSValid
                TruthValue,
            ibRprStaticRSRowStatus
                RowStatus
        }

    ibRprStaticRSIfIndex OBJECT-TYPE
        SYNTAX InterfaceIndex
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "The ifIndex of RPR interface."
        REFERENCE
            "RFC 2863, ifIndex"
        ::= { ibRprStaticRSEntry 1 }

    ibRprStaticRSMacAddress OBJECT-TYPE
        SYNTAX MacAddress
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "The MAC address of the station on the ring."
        ::= { ibRprStaticRSEntry 2 }

    ibRprStaticRSRingletID OBJECT-TYPE
        SYNTAX IbRprRingletID
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
            "The identifier of the ring which frames are sent
             from one station to another. Ringlet0 refers to outer ring,
             while ringlet1 does inner."
        ::= { ibRprStaticRSEntry 3 }

    ibRprStaticRSTtl  OBJECT-TYPE
        SYNTAX INTEGER
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The hops from source station to destination station by the
             selected ringlet."
        ::= { ibRprStaticRSEntry 4 }

    ibRprStaticRSValid OBJECT-TYPE
        SYNTAX TruthValue
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The flag identify whether the configed ringlet is valid.
             If it is true, the configed ringlet is valid, otherwise
             it's invalid."
        ::= { ibRprStaticRSEntry 5 }

    ibRprStaticRSRowStatus OBJECT-TYPE
        SYNTAX RowStatus
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
            "The status of this table entry.
             The status includes createAndGo, active and destroy."
        ::= { ibRprStaticRSEntry 6 }

