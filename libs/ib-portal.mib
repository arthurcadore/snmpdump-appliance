IB-PORTAL-MIB DEFINITIONS ::= BEGIN
IMPORTS
    ibCommon
        FROM IB-OID-MIB
    OBJECT-TYPE, NOTIFICATION-TYPE, MODULE-IDENTITY, Integer32, Counter64, Unsigned32,
    TimeTicks
        FROM SNMPv2-SMI
    RowStatus, MacAddress, TEXTUAL-CONVENTION
        FROM SNMPv2-TC
    ifIndex, InterfaceIndex
        FROM IF-MIB
    InetAddressPrefixLength, InetAddressType, InetAddress, InetAddressIPv4
        FROM INET-ADDRESS-MIB;

ibPortal MODULE-IDENTITY
    LAST-UPDATED "201607141020Z"        -- Jul 14, 2016 at 10:20 GMT
    ORGANIZATION
        "Intelbras S.A. Industria de Telecomunicacao Eletronica Brasileria."
    CONTACT-INFO
        ""
    DESCRIPTION
        "The MIB module is used for managing portal."
    REVISION "201607141020Z"        -- Jul 14, 2016 at 10:20 GMT
    DESCRIPTION
        "Modified to add new node in table ibPortalForbiddenRuleTable."
    REVISION "201510081020Z"        -- Oct 8, 2015 at 10:20 GMT
    DESCRIPTION
        "Modified to add new table."
    ::= { ibCommon 99 }

IbPortalAuthMethod ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
    "Specify the portal authentication method."
    SYNTAX      INTEGER
    {
        invalid(1),
        direct(2),
        layer3(3),
        redhcp(4)
    }

ibPortalConfig OBJECT IDENTIFIER ::= { ibPortal 1 }

--
-- PORTAL MAX USER NUMBER
--

ibPortalMaxUserNumber OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Specify the maximum number of online users,
        the value to be set should meet the following two conditions:
        1. Less than or equal to ibPortalUserNumberUpperLimit.
        2. Greater than or equal to one.

        The default value is equal to ibPortalUserNumberUpperLimit."
    ::= { ibPortalConfig 1 }

--
-- PORTAL CURRENT USER NUMBER
--

ibPortalCurrentUserNumber OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The current number of users connecting to the portal."
    ::= { ibPortalConfig 2 }

--
-- PORTAL STATUS
--

ibPortalStatus OBJECT-TYPE
    SYNTAX      INTEGER
    {
         enabled(1),
         disabled(2)
    }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "It indicates the portal status.
         enabled:
            Portal is enabled on one or more interfaces.
         disabled:
            Portal is not enabled on any interface."
    ::= { ibPortalConfig 3 }

--
-- PORTAL USER NUMBER_UPPER_LIMIT
--

ibPortalUserNumberUpperLimit OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The upper limit of ibPortalMaxUserNumber."
    ::= { ibPortalConfig 4 }

--
-- PORTAL NAS-ID Config
--
ibPortalNasId OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The nas id of this device."
    ::= { ibPortalConfig 5 }

--
-- PORTAL TABLES
--

ibPortalTables OBJECT IDENTIFIER ::= { ibPortal 2 }


--
-- PORTAL server table
--
ibPortalServerTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbPortalServerEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table defines the name and URL of portal server.
        It can not be created and destroyed by MIB."
    ::= { ibPortalTables 1 }

ibPortalServerEntry OBJECT-TYPE
    SYNTAX      IbPortalServerEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "There is a row in this table for each name and URL of portal server.
        It is indexed using the object ibPortalServerName."
    INDEX
        {
            ibPortalServerName
        }
    ::= { ibPortalServerTable 1 }


IbPortalServerEntry ::= SEQUENCE
    {
        ibPortalServerName               OCTET STRING,
        ibPortalServerUrl                OCTET STRING,
        ibPortalServerPort               Integer32
    }

ibPortalServerName OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE (1..32))
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "It is the index of the table of ibPortalServerTable, it shows
        the name of the portal server."
    ::= { ibPortalServerEntry 1 }

ibPortalServerUrl OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE (1..127))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "It shows the uniform resource locator to the portal server.
        Users which have not logon to portal will be redirected to the
        portal server according to ibPortalServerUrl.

        The value is consisted of protocol, IP address and relative path.
        For example: HTTP://10.1.1.1/portal

        It is not configurable when the portal server has been enabled."
    ::= { ibPortalServerEntry 2 }

ibPortalServerPort OBJECT-TYPE
    SYNTAX      Integer32 (1..65534)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "It shows the UDP port of the portal server.

        The port will be used when client sends packets to portal server.
        Default value is 50100.

        It is not configurable when the portal server has been enabled."
    ::= { ibPortalServerEntry 3 }

--
-- PORTAL If Info Table
--
ibPortalIfInfoTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbPortalIfInfoEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table describes the portal information on some interface."
    ::= { ibPortalTables 2 }

ibPortalIfInfoEntry OBJECT-TYPE
    SYNTAX      IbPortalIfInfoEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "There is a row in this table for a interface on which
        portal has been enabled.
        It is indexed using the object ifIndex."
    INDEX
        {
            ifIndex
        }
    ::= { ibPortalIfInfoTable 1 }


IbPortalIfInfoEntry ::= SEQUENCE
    {
        ibPortalAuthReqNumber            Integer32,
        ibPortalAuthSuccNumber           Integer32,
        ibPortalAuthFailNumber           Integer32
    }

ibPortalAuthReqNumber OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "It shows the number of authentication request messages
        which are sent by portal module to AAA module."
    ::= { ibPortalIfInfoEntry 1 }


ibPortalAuthSuccNumber OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "It shows the number of authentication success response messages
        which are sent by AAA module to portal module."
    ::= { ibPortalIfInfoEntry 2 }

ibPortalAuthFailNumber OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "It shows the number of authentication failure response messages
        which are sent by AAA module to portal module."
    ::= { ibPortalIfInfoEntry 3 }

--
-- PORTAL If Config Table
--
ibPortalIfServerTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbPortalIfServerEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table describes the portal information on some interface."
    ::= { ibPortalTables 3 }

ibPortalIfServerEntry OBJECT-TYPE
    SYNTAX      IbPortalIfServerEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "There is a row in this table for a interface on which
        portal has been enabled.
        It is indexed using the object ifIndex."
    INDEX
        {
            ibPortalIfServerIndex
        }
    ::= { ibPortalIfServerTable 1 }

IbPortalIfServerEntry ::= SEQUENCE
    {
        ibPortalIfServerIndex                   Integer32,
        ibPortalIfServerUrl                     OCTET STRING,
        ibPortalIfServerRowStatus               RowStatus
    }

ibPortalIfServerIndex  OBJECT-TYPE
    SYNTAX      Integer32 (0..2147483647)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "It is the index of the table of ibIfPtServerTable, it shows
        the index of the portal server."
    ::={ ibPortalIfServerEntry 1 }

ibPortalIfServerUrl OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "It shows the uniform resource locator to the portal server.
        Users which have not logon to portal will be redirected to the
        portal server according to ibPortalServerIfUrl.
        The value is consisted of protocol, IP address and relative path.
        For example: HTTP://10.1.1.1/portal
        It is not configurable when the portal server has been enabled."
    ::= { ibPortalIfServerEntry 2 }

ibPortalIfServerRowStatus OBJECT-TYPE
    SYNTAX   RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object is responsible for managing the creation, deletion and
        modification of rows, which support active status and CreateAndGo,
        Destroy operation.  To create a new row, portal server must be specified.
        To destroy an existent row, the portal server MUST NOT be referred by
        interface."
    ::= { ibPortalIfServerEntry 3 }

-- PORTAL Vlan Not binding NAS ID Table
ibPortalIfVlanNasIDTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbPortalIfVlanNasIDEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table describes the interface on which portal has been enabled,
        the Vlan that the interface belongs to and the NAS ID to which the Vlan
        is bound."
    ::= { ibPortalTables 4 }

ibPortalIfVlanNasIDEntry OBJECT-TYPE
    SYNTAX      IbPortalIfVlanNasIDEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "There is a row in this table for a interface on which portal has been
        enabled, the Vlan that the interface belongs to and the NAS ID to which
        the Vlan is bound.
        It is indexed using the object ifIndex and Vlan that the interface
        belongs to."
    INDEX
        {
            ibPortalIfVlanNasIDIfIndex,
            ibPortalIfVlanNasIDVlanID
        }
    ::= { ibPortalIfVlanNasIDTable 1 }

IbPortalIfVlanNasIDEntry ::= SEQUENCE
    {
        ibPortalIfVlanNasIDIfIndex         InterfaceIndex,
        ibPortalIfVlanNasIDVlanID          Integer32,
        ibPortalIfVlanNasIDNasID           OCTET STRING
    }

ibPortalIfVlanNasIDIfIndex  OBJECT-TYPE
    SYNTAX      InterfaceIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "It is the index of the table of ibPortalIfVlanNasIDTable, it shows
        the interface on which portal has been enabled."
    ::={ ibPortalIfVlanNasIDEntry 1 }

ibPortalIfVlanNasIDVlanID OBJECT-TYPE
    SYNTAX      Integer32 (1..2147483647)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "It is the index of the table of ibPortalIfVlanNasIDTable, it shows
        the Vlan that the interface belongs to."
    ::= { ibPortalIfVlanNasIDEntry 2 }

ibPortalIfVlanNasIDNasID OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE(0..64))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "It is the NAS ID to which the Vlan is bound to."
    ::= { ibPortalIfVlanNasIDEntry 3 }

--
-- PORTAL SSID Free-rule Config Table
--

ibPortalSSIDFreeRuleTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbPortalSSIDFreeRuleEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table describes SSID portal-free rule(s) allows specified users
        to access specified external websites without portal authentication."
    ::= { ibPortalTables 5 }

ibPortalSSIDFreeRuleEntry OBJECT-TYPE
    SYNTAX      IbPortalSSIDFreeRuleEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "There is a row in this table for SSID portal-free rule.
        It is indexed using the object Index."
    INDEX
        {
            ibPortalSSIDFreeRuleIndex
        }
    ::= { ibPortalSSIDFreeRuleTable 1 }

IbPortalSSIDFreeRuleEntry ::= SEQUENCE
    {
        ibPortalSSIDFreeRuleIndex                   Integer32,
        ibPortalSSIDFreeRuleSrcSSID                 OCTET STRING,
        ibPortalSSIDFreeRuleRowStatus               RowStatus,
        ibPortalSSIDFreeRuleSrcSpot                 OCTET STRING
    }

ibPortalSSIDFreeRuleIndex  OBJECT-TYPE
    SYNTAX      Integer32 (0..2147483647)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "It is the index of the table of ibPortalSSIDFreeRuleTable, it shows
        the index of the SSID portal-free rule."
    ::={ ibPortalSSIDFreeRuleEntry 1 }

ibPortalSSIDFreeRuleSrcSSID  OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE(1..128))
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The SSID match against the packet's source SSID."
    ::={ ibPortalSSIDFreeRuleEntry 2 }

ibPortalSSIDFreeRuleRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object is responsible for managing the creation, deletion and
        modification of rows, which support active status and CreateAndGo,
        Destroy operation."
    ::= { ibPortalSSIDFreeRuleEntry 3 }

ibPortalSSIDFreeRuleSrcSpot  OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE(0..63))
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The spot match against the packet's source spot."
    ::={ ibPortalSSIDFreeRuleEntry 4 }

--
-- PORTAL MAC Trigger Server
--

ibPortalMacTriggerSrvTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbPortalMacTriggerSrvEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table describes the configration of MAC Trigger Portal server."
    ::= { ibPortalTables 6 }

ibPortalMacTriggerSrvEntry OBJECT-TYPE
    SYNTAX      IbPortalMacTriggerSrvEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "There is a row in the table for MAC Trigger Portal server.
        It is indexed using the object Index."
    INDEX
        {
            ibPortalMacTriggerSrvIndex
        }
    ::= { ibPortalMacTriggerSrvTable 1 }

IbPortalMacTriggerSrvEntry ::= SEQUENCE
    {
        ibPortalMacTriggerSrvIndex              Integer32,
        ibPortalMacTriggerSrvIPAddrType         InetAddressType,
        ibPortalMacTriggerSrvIP                 InetAddress,
        ibPortalMacTriggerSrvPort               Integer32,
        ibPortalMacTriggerSrvRowStatus          RowStatus
    }

ibPortalMacTriggerSrvIndex  OBJECT-TYPE
    SYNTAX      Integer32 (0..2147483647)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "It is the index of the table of ibPortalMacTriggerSrvTable, it shows
        the index of the MAC Trigger Portal server."
    ::={ ibPortalMacTriggerSrvEntry 1 }

ibPortalMacTriggerSrvIPAddrType  OBJECT-TYPE
    SYNTAX      InetAddressType
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The type of MAC Trigger Portal server IP address.  While
       other types of addresses are defined in the InetAddressType
       textual convention, and DNS names, a classifier can only look at
       packets on the wire."
    ::={ ibPortalMacTriggerSrvEntry 2 }

ibPortalMacTriggerSrvIP OBJECT-TYPE
    SYNTAX      InetAddress
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "It shows the IP address of MAC Trigger Portal server."
    ::= { ibPortalMacTriggerSrvEntry 3 }

ibPortalMacTriggerSrvPort OBJECT-TYPE
    SYNTAX      Integer32 (1..65534)
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "It shows the UDP port of MAC Trigger Portal server.
        The port will be used when client sends packets to MAC Trigger Portal server.
        Default value is 50100."
    ::= { ibPortalMacTriggerSrvEntry 4 }

ibPortalMacTriggerSrvRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object is responsible for managing the creation, deletion and
        modification of rows, which support active status and CreateAndGo,
        Destroy operation."
    ::= { ibPortalMacTriggerSrvEntry 5 }

--
-- PORTAL Eanble MAC Trigger on interface
--

ibPortalMacTriggerOnIfTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbPortalMacTriggerOnIfEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table describes the configration of Portal MAC Trigger on
        interface."
    ::= { ibPortalTables 7 }

ibPortalMacTriggerOnIfEntry OBJECT-TYPE
    SYNTAX      IbPortalMacTriggerOnIfEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "There is a row in the table for Portal MAC Trigger configuration on
        interface.  It is indexed using the object Index."
    INDEX
        {
            ibPortalMacTriggerOnIfIfIndex
        }
    ::= { ibPortalMacTriggerOnIfTable 1 }

IbPortalMacTriggerOnIfEntry ::= SEQUENCE
    {
        ibPortalMacTriggerOnIfIfIndex               InterfaceIndex,
        ibPortalMacTriggerOnIfDetctFlowPeriod       Integer32,
        ibPortalMacTriggerOnIfThresholdFlow         Unsigned32,
        ibPortalMacTriggerOnIfRowStatus             RowStatus
    }

ibPortalMacTriggerOnIfIfIndex  OBJECT-TYPE
    SYNTAX      InterfaceIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "It is the interface index and the index of
        ibPortalMacTriggerOnIfTable."
    ::={ ibPortalMacTriggerOnIfEntry 1 }

ibPortalMacTriggerOnIfDetctFlowPeriod  OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "seconds"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "It shows the user flow detection interval (in seconds) when
        MAC Trigger and Portal are enabled.
        Default value is 300."
    DEFVAL { 300 }
    ::={ ibPortalMacTriggerOnIfEntry 2 }

ibPortalMacTriggerOnIfThresholdFlow  OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "bytes"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "It shows the user traffic threshold (in bytes) that triggers authentication
        when MAC Trigger and Portal are enabled.
        Default value is 0."
    DEFVAL { 0 }
    ::={ ibPortalMacTriggerOnIfEntry 3 }

ibPortalMacTriggerOnIfRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object is responsible for managing the creation, deletion and
        modification of rows, which support active status and CreateAndGo,
        Destroy operation."
    ::= { ibPortalMacTriggerOnIfEntry 4 }

