ibPortalExtTables OBJECT IDENTIFIER ::= { ibPortal 7 }

--
-- PORTAL server table
--
ibPortalExtSrvTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbPortalExtSrvEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table defines the name of the portal server.  It can be created
        and destroyed by MIB."
    ::= { ibPortalExtTables 1 }

ibPortalExtSrvEntry OBJECT-TYPE
    SYNTAX      IbPortalExtSrvEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "There is a row in this table for each name and URL of the portal server.
        It is indexed by using the object ibPortalExtSrvName."
    INDEX
        {
            ibPortalExtSrvName
        }
    ::= { ibPortalExtSrvTable 1 }


IbPortalExtSrvEntry ::= SEQUENCE
    {
        ibPortalExtSrvName          OCTET STRING,
        ibPortalExtSrvIPAddrType    InetAddressType,
        ibPortalExtSrvIP            InetAddress,
        ibPortalExtSrvPort          Integer32,
        ibPortalExtSrvRowStatus     RowStatus,
        ibPortalExtSrvVpnName       OCTET STRING,
        ibPortalExtSrvKey           OCTET STRING
    }

ibPortalExtSrvName OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE (1..32))
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "It is the index of the table ibPortalExtSrvTable.  It shows
        the name of the portal server."
    ::= { ibPortalExtSrvEntry 1 }

ibPortalExtSrvIPAddrType OBJECT-TYPE
    SYNTAX      InetAddressType
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The type of the portal server IP address."
    ::={ ibPortalExtSrvEntry 2 }

ibPortalExtSrvIP OBJECT-TYPE
    SYNTAX      InetAddress
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "It shows the IP address of the portal server."
    ::= { ibPortalExtSrvEntry 3 }

ibPortalExtSrvPort OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "It shows the UDP port of the portal server.
        The port will be used when clients send packets to the portal server.
        Default value is 50100.
        It is not configurable when the portal server has been enabled."
    ::= { ibPortalExtSrvEntry 4 }

ibPortalExtSrvRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object is responsible for managing the creation, deletion and
        modification of rows, which support active status and CreateAndGo and
        Destroy operations.  To create a new row, the portal server must be
        specified.
        To destroy an existing row, the portal server MUST NOT be referenced by
        an interface."
    ::= { ibPortalExtSrvEntry 5 }

ibPortalExtSrvVpnName OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE (0..31))
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Specifies the VPN instance to which the portal authentication
        server belongs."
    ::= { ibPortalExtSrvEntry 6 }

ibPortalExtSrvKey OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE (0..64))
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Specifies a shared key for securing communication between the device
        and the portal authentication server.
        When read, ibPortalExtSrvKey always returns a zero-length string."
    ::= { ibPortalExtSrvEntry 7 }

--
-- PORTAL web server table
--
ibPortalExtWebSrvTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbPortalExtWebSrvEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table defines the name of the portal Web server.  It can be created
         and destroyed by MIB."
    ::= { ibPortalExtTables 2 }

ibPortalExtWebSrvEntry OBJECT-TYPE
    SYNTAX      IbPortalExtWebSrvEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "There is a row in this table for each name and portal Web server.
        It is indexed by using the object ibPortalExtWebSrvName."
    INDEX
        {
            ibPortalExtWebSrvName
        }
    ::= { ibPortalExtWebSrvTable 1 }


IbPortalExtWebSrvEntry ::= SEQUENCE
    {
        ibPortalExtWebSrvName         OCTET STRING,
        ibPortalExtWebSrvUrl          OCTET STRING,
        ibPortalExtWebSrvRowStatus    RowStatus
    }

ibPortalExtWebSrvName OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE (1..32))
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "It is the index of the table ibPortalExtWebSrvTable.  It shows
        the name of the portal Web server."
    ::= { ibPortalExtWebSrvEntry 1 }

ibPortalExtWebSrvUrl OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "It shows the URL to the portal Web server.
        Users which have not logged on to portal will be redirected to the
        portal server according to ibPortalServerIfUrl.
        The value is consisted of protocol, IP address and relative path.
        For example: http://10.1.1.1/portal."
    ::= { ibPortalExtWebSrvEntry 2 }

ibPortalExtWebSrvRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object is responsible for managing the creation, deletion and
        modification of rows, which support active status and CreateAndGo and
        Destroy operations.  To create a new row, the portal server must be specified.
        To destroy an existing row, the portal Web server MUST NOT be referenced by an
        interface."
    ::= { ibPortalExtWebSrvEntry 3 }

ibPortalExtMTSrvTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbPortalExtMTSrvEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table describes the configuration of the MAC Trigger server."
    ::= { ibPortalExtTables 3 }

ibPortalExtMTSrvEntry OBJECT-TYPE
    SYNTAX      IbPortalExtMTSrvEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "There is a row in the table for the MAC Trigger server.  It is indexed by using the object Index."
    INDEX
        {
            ibPortalExtMTSrvName
        }
    ::= { ibPortalExtMTSrvTable 1 }

IbPortalExtMTSrvEntry ::= SEQUENCE
    {
        ibPortalExtMTSrvName          OCTET STRING,
        ibPortalExtMTSrvIPAddrType    InetAddressType,
        ibPortalExtMTSrvIP            InetAddress,
        ibPortalExtMTSrvPort          Integer32,
        ibPortalExtMTDetctFlowPeriod  Integer32,
        ibPortalExtMTThresholdFlow    Unsigned32,
        ibPortalExtMTSrvRowStatus     RowStatus
    }

ibPortalExtMTSrvName OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE (1..32))
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "It is the index of the table ibPortalExtMTSrvTable.  It shows
         the name of the MAC Trigger server."
    ::= { ibPortalExtMTSrvEntry 1 }

ibPortalExtMTSrvIPAddrType OBJECT-TYPE
    SYNTAX      InetAddressType
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The type of the MAC Trigger server IP address."
    ::={ ibPortalExtMTSrvEntry 2 }

ibPortalExtMTSrvIP OBJECT-TYPE
    SYNTAX      InetAddress
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "It shows the IP address of the MAC Trigger server."
    ::= { ibPortalExtMTSrvEntry 3 }

ibPortalExtMTSrvPort OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "It shows the UDP port of the MAC Trigger server.
        The port will be used when clients send packets to the MAC Trigger server.
        Default value is 50100."
    DEFVAL { 50100 }
    ::= { ibPortalExtMTSrvEntry 4 }

ibPortalExtMTDetctFlowPeriod  OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "seconds"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "It shows the user flow detection interval (in seconds) when
        MAC Trigger and Portal are enabled.
        Default value is 300."
    DEFVAL { 300 }
    ::= { ibPortalExtMTSrvEntry 5 }

ibPortalExtMTThresholdFlow  OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "bytes"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "It shows the user traffic threshold (in bytes) that triggers authentication
        when MAC Trigger and Portal are enabled.
        Default value is 0."
    DEFVAL { 0 }
    ::= { ibPortalExtMTSrvEntry 6 }

ibPortalExtMTSrvRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object is responsible for managing the creation, deletion and
        modification of rows, which support active status and CreateAndGo and
        Destroy operations."
    ::= { ibPortalExtMTSrvEntry 7 }
--
-- PORTAL extend if table
