--

ibPortalFreeRuleTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbPortalFreeRuleEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table describes portal-free rule(s) allows specified users to
         access specified external websites without portal authentication."
    ::= { ibPortalTables 8 }

ibPortalFreeRuleEntry OBJECT-TYPE
    SYNTAX      IbPortalFreeRuleEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "There is a row in this table for portal-free rule.
        It is indexed using the object Index."
    INDEX
        {
            ibPortalFreeRuleIndex
        }
    ::= { ibPortalFreeRuleTable 1 }

IbPortalFreeRuleEntry ::= SEQUENCE
    {
        ibPortalFreeRuleIndex                   Integer32,
        ibPortalFreeRuleSrcIfIndex              InterfaceIndex,
        ibPortalFreeRuleSrcVlanID               Integer32,
        ibPortalFreeRuleSrcMac                  MacAddress,
        ibPortalFreeRuleAddrType                InetAddressType,
        ibPortalFreeRuleSrcAddr                 InetAddress,
        ibPortalFreeRuleSrcPrefix               InetAddressPrefixLength,
        ibPortalFreeRuleDstAddr                 InetAddress,
        ibPortalFreeRuleDstPrefix               InetAddressPrefixLength,
        ibPortalFreeRuleProtocol                INTEGER,
        ibPortalFreeRuleSrcPort                 Integer32,
        ibPortalFreeRuleDstPort                 Integer32,
        ibPortalFreeRuleRowStatus               RowStatus
    }

ibPortalFreeRuleIndex  OBJECT-TYPE
    SYNTAX      Integer32 (0..2147483647)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "It is the index of the table of ibPortalFreeRuleTable, it shows
        the index of the portal-free rule."
    ::={ ibPortalFreeRuleEntry 1 }

ibPortalFreeRuleSrcIfIndex  OBJECT-TYPE
    SYNTAX      InterfaceIndex
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The index of source interface."
    ::={ ibPortalFreeRuleEntry 2 }

ibPortalFreeRuleSrcVlanID  OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The identification of source vlan."
    ::={ ibPortalFreeRuleEntry 3 }

ibPortalFreeRuleSrcMac  OBJECT-TYPE
    SYNTAX      MacAddress
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The MAC address to match against the packet's source MAC address."
    ::={ ibPortalFreeRuleEntry 4 }

ibPortalFreeRuleAddrType  OBJECT-TYPE
    SYNTAX      InetAddressType
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The type of IP address used by this classifier entry.  While
       other types of addresses are defined in the InetAddressType
       textual convention, and DNS names, a classifier can only look at
       packets on the wire."
    ::={ ibPortalFreeRuleEntry 5 }

ibPortalFreeRuleSrcAddr  OBJECT-TYPE
    SYNTAX      InetAddress
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The IP address to match against the packet's source IP
       address.  This may be a DNS name or an IPv4 or IPv6
       prefix.  ibPortalFreeRuleSrcPrefix indicates the
       number of bits that are relevant."
    ::={ ibPortalFreeRuleEntry 6 }

ibPortalFreeRuleSrcPrefix  OBJECT-TYPE
    SYNTAX      InetAddressPrefixLength
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The length of the CIDR Prefix carried in
       ibPortalFreeRuleSrcAddr.  In IPv4 addresses, a length of 0
       indicates a match of any address; a length of 32 indicates a
       match of a single host address, and a length between 0 and 32
       indicates the use of a CIDR Prefix.  IPv6 is similar, except that
       prefix lengths range from 0..128."
    ::={ ibPortalFreeRuleEntry 7 }

ibPortalFreeRuleDstAddr  OBJECT-TYPE
    SYNTAX      InetAddress
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The IP address to match against the packet's destination IP
       address.  This may be a DNS name or an IPv4 or IPv6
       prefix.  ibPortalFreeRuleDstPrefix indicates the
       number of bits that are relevant."
    ::={ ibPortalFreeRuleEntry 8 }

ibPortalFreeRuleDstPrefix  OBJECT-TYPE
    SYNTAX      InetAddressPrefixLength
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The length of the CIDR Prefix carried in
       ibPortalFreeRuleDstAddr.  In IPv4 addresses, a length of 0
       indicates a match of any address; a length of 32 indicates a
       match of a single host address, and a length between 0 and 32
       indicates the use of a CIDR Prefix.  IPv6 is similar, except that
       prefix lengths range from 0..128."
    ::={ ibPortalFreeRuleEntry 9 }

ibPortalFreeRuleProtocol  OBJECT-TYPE
    SYNTAX      INTEGER
    {
        invalid(0),
        tcp(6),
        udp(17)
    }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The protocol-type of port."
    ::={ ibPortalFreeRuleEntry 10 }

ibPortalFreeRuleSrcPort  OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The source port allows users to access."
    ::={ ibPortalFreeRuleEntry 11 }

ibPortalFreeRuleDstPort  OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The destination port allows users to access."
    ::={ ibPortalFreeRuleEntry 12 }

ibPortalFreeRuleRowStatus OBJECT-TYPE
    SYNTAX   RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object is responsible for managing the creation, deletion and
        modification of rows, which support active status and CreateAndGo,
        Destroy operation."
    ::= { ibPortalFreeRuleEntry 13 }

