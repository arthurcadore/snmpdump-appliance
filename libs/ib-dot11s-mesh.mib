-- *  ibDot11sMeshLinkStatusTable Definition
-- *****************************************************************************
ibDot11sMeshLinkStatusTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbDot11sMeshLinkStatusEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table is used to represent the status of mesh link."
    ::= { ibDot11sMeshStatusGroup 1 }

ibDot11sMeshLinkStatusEntry OBJECT-TYPE
    SYNTAX      IbDot11sMeshLinkStatusEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry contains status information of mesh link."
    INDEX
        {
            ibDot11sMeshLinkIfIndex
        }
    ::= { ibDot11sMeshLinkStatusTable 1 }

IbDot11sMeshLinkStatusEntry ::= SEQUENCE
    {
        ibDot11sMeshLinkIfIndex        Unsigned32,
        ibDot11sMeshLinkName           OCTET STRING,
        ibDot11sMeshLinkBSSID          MacAddress,
        ibDot11sMeshLinkPeerMac        MacAddress,
        ibDot11sMeshLinkExistDuration  Integer32,
        ibDot11sMeshLinkType           INTEGER,
        ibDot11sMeshLinkSNR            Integer32,
        ibDot11sMeshLinkNoise          Integer32,
        ibDot11sMeshLinkPeerSNR        Integer32,
        ibDot11sMeshLinkPeerNoise      Integer32,
        ibDot11sMeshLinkPeerIPAddress  IpAddress,
        ibDot11sMeshLinkPeerSystemName OCTET STRING
    }

ibDot11sMeshLinkIfIndex OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This object represents the interface index of the mesh link."
    ::= { ibDot11sMeshLinkStatusEntry 1 }

ibDot11sMeshLinkName OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object represents the name of the mesh link."
    ::= { ibDot11sMeshLinkStatusEntry 2 }

ibDot11sMeshLinkBSSID OBJECT-TYPE
    SYNTAX      MacAddress
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object represents the BSS ID of the mesh link."
    ::= { ibDot11sMeshLinkStatusEntry 3 }

ibDot11sMeshLinkPeerMac OBJECT-TYPE
    SYNTAX      MacAddress
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object represents the peer MAC address of the mesh link."
    ::= { ibDot11sMeshLinkStatusEntry 4 }

ibDot11sMeshLinkExistDuration OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "second"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object represents the time for which the link has been up."
    ::= { ibDot11sMeshLinkStatusEntry 5 }

ibDot11sMeshLinkType OBJECT-TYPE
    SYNTAX      INTEGER
        {
            active(1),
            dormant(2)
        }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This object represents the type for the link."
    ::= { ibDot11sMeshLinkStatusEntry 6 }

ibDot11sMeshLinkSNR OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "percent"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the SNR for the link."
    ::= { ibDot11sMeshLinkStatusEntry 7 }

ibDot11sMeshLinkNoise OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the noise for the link."
    ::= { ibDot11sMeshLinkStatusEntry 8 }

ibDot11sMeshLinkPeerSNR OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "percent"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the SNR transferred from the neighbor of this mesh link."
    ::= { ibDot11sMeshLinkStatusEntry 9 }

ibDot11sMeshLinkPeerNoise OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the noise transferred from the neighbor of this mesh link."
    ::= { ibDot11sMeshLinkStatusEntry 10 }

ibDot11sMeshLinkPeerIPAddress OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the IP address of the neighbor of this mesh link."
    ::= { ibDot11sMeshLinkStatusEntry 11 }

ibDot11sMeshLinkPeerSystemName OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the system name of the neighbor of this mesh link."
    ::= { ibDot11sMeshLinkStatusEntry 12 }

