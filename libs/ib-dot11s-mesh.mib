-- * End of ibDot11sMeshLinkStatusTable Definition
-- *****************************************************************************

-- *****************************************************************************
-- *  ibDot11sMeshLinkStatisTable Definition
-- *****************************************************************************
ibDot11sMeshLinkStatisTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbDot11sMeshLinkStatisEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table is used to represent the statistical information of
         mesh link."
    ::= { ibDot11sMeshStatusGroup 2 }

ibDot11sMeshLinkStatisEntry OBJECT-TYPE
    SYNTAX      IbDot11sMeshLinkStatisEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry contains statistical information of mesh link."
    INDEX
        {
            ibDot11APElementIndex,
            ibDot11sMeshLinkStatIfIndex
        }
    ::= { ibDot11sMeshLinkStatisTable 1 }

IbDot11sMeshLinkStatisEntry ::= SEQUENCE
    {
        ibDot11sMeshLinkStatIfIndex    Unsigned32,
        ibDot11sMeshLinkNbrIndex       Unsigned32,
        ibDot11sMeshLinkRxTotByte      Counter32,
        ibDot11sMeshLinkRxTotPkt       Counter32,
        ibDot11sMeshLinkRxUniPkt       Counter32,
        ibDot11sMeshLinkRxBrocPkt      Counter32,
        ibDot11sMeshLinkRxMuticPkt     Counter32,
        ibDot11sMeshLinkRxDiscPkt      Counter32,
        ibDot11sMeshLinkTxTotByte      Counter32,
        ibDot11sMeshLinkTxTotPkt       Counter32,
        ibDot11sMeshLinkTxUniPkt       Counter32,
        ibDot11sMeshLinkTxBrocPkt      Counter32,
        ibDot11sMeshLinkTxMuticPkt     Counter32,
        ibDot11sMeshLinkTxDiscPkt      Counter32,
        ibDot11sMeshLinkIFName         OCTET STRING
    }

ibDot11sMeshLinkStatIfIndex OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This object represents the interface index of the mesh link."
    ::= { ibDot11sMeshLinkStatisEntry 1 }

ibDot11sMeshLinkNbrIndex OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the neighbor index of the mesh link."
    ::= { ibDot11sMeshLinkStatisEntry 2 }

ibDot11sMeshLinkRxTotByte OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the amount of bytes that the mesh link received."
    ::= { ibDot11sMeshLinkStatisEntry 3 }

ibDot11sMeshLinkRxTotPkt OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the amount of packets that the mesh link received."
    ::= { ibDot11sMeshLinkStatisEntry 4 }

ibDot11sMeshLinkRxUniPkt OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the amount of unicast packets that the mesh link received."
    ::= { ibDot11sMeshLinkStatisEntry 5 }

ibDot11sMeshLinkRxBrocPkt OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the amount of broadcast packets that the mesh link
         received."
    ::= { ibDot11sMeshLinkStatisEntry 6 }

ibDot11sMeshLinkRxMuticPkt OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the amount of multicast packets that the mesh link
         received."
    ::= { ibDot11sMeshLinkStatisEntry 7 }

ibDot11sMeshLinkRxDiscPkt OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the amount of packets that the mesh link discarded."
    ::= { ibDot11sMeshLinkStatisEntry 8 }

ibDot11sMeshLinkTxTotByte OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the total amount of bytes that the mesh link transmitted."
    ::= { ibDot11sMeshLinkStatisEntry 9 }

ibDot11sMeshLinkTxTotPkt OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the total amount of packets that the mesh link transmitted."
    ::= { ibDot11sMeshLinkStatisEntry 10 }

ibDot11sMeshLinkTxUniPkt OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the amount of unicast packets that the mesh link
         transmitted."
    ::= { ibDot11sMeshLinkStatisEntry 11 }

ibDot11sMeshLinkTxBrocPkt OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the amount of broadcast packets that the mesh link
         transmitted."
    ::= { ibDot11sMeshLinkStatisEntry 12 }

ibDot11sMeshLinkTxMuticPkt OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the amount of multicast packets that the mesh link
         transmitted."
    ::= { ibDot11sMeshLinkStatisEntry 13 }

ibDot11sMeshLinkTxDiscPkt OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the amount of discarded packets that the mesh link
         transmitted."
    ::= { ibDot11sMeshLinkStatisEntry 14 }

ibDot11sMeshLinkIFName    OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the name of mesh link interface."
    ::= { ibDot11sMeshLinkStatisEntry 15 }

-- *****************************************************************************
-- * End of ibDot11sMeshLinkStatisTable Definition
-- *****************************************************************************

-- *****************************************************************************
-- *  ibDot11sMeshNbrStatusTable Definition
-- *****************************************************************************
ibDot11sMeshNbrStatusTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbDot11sMeshNbrStatusEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table represents the status information for the neighbors of MP."
    ::= { ibDot11sMeshStatusGroup 3 }

ibDot11sMeshNbrStatusEntry OBJECT-TYPE
    SYNTAX      IbDot11sMeshNbrStatusEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry of the table will provide status information for the
         neighbors of MP."
    INDEX
        {
            ibDot11APElementIndex,
            ibDot11sMeshNbrIndex
        }
    ::= { ibDot11sMeshNbrStatusTable 1 }

IbDot11sMeshNbrStatusEntry ::= SEQUENCE
    {
        ibDot11sMeshNbrIndex          Unsigned32,
        ibDot11sMeshNbrRadioID        Unsigned32,
        ibDot11sMeshLocalMeshID       OCTET STRING,
        ibDot11sMeshNbrMeshID         OCTET STRING,
        ibDot11sMeshNbrBSSID          MacAddress,
        ibDot11sMeshNbrPeerMac        MacAddress,
        ibDot11sMeshLinkInMp          Unsigned32,
        ibDot11sMeshMPLinkStatus      INTEGER,
        ibDot11sMeshNbrChannel        Unsigned32,
        ibDot11sMeshNbrLinkDuration   Integer32,
        ibDot11sMeshNbrRSSI           Integer32,
        ibDot11sMeshNbrSNR            Integer32
    }

ibDot11sMeshNbrIndex OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Represents the index for the neighbor."
    ::= { ibDot11sMeshNbrStatusEntry 1 }

ibDot11sMeshNbrRadioID OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the radio ID of the neighbor."
    ::= { ibDot11sMeshNbrStatusEntry 2 }

ibDot11sMeshLocalMeshID OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(0..32))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the local mesh ID."
    ::= { ibDot11sMeshNbrStatusEntry 3 }

ibDot11sMeshNbrMeshID OBJECT-TYPE
    SYNTAX      OCTET STRING(SIZE(0..32))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the mesh ID of the neighbor."
    ::= { ibDot11sMeshNbrStatusEntry 4 }

ibDot11sMeshNbrBSSID OBJECT-TYPE
    SYNTAX      MacAddress
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the BSS ID of the neighbor."
    ::= { ibDot11sMeshNbrStatusEntry 5 }

ibDot11sMeshNbrPeerMac OBJECT-TYPE
    SYNTAX      MacAddress
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the MAC address of the peer neighbor."
    ::= { ibDot11sMeshNbrStatusEntry 6 }

ibDot11sMeshLinkInMp OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the interface index of mesh link that used to connect with
         this neighbor."
    ::= { ibDot11sMeshNbrStatusEntry 7 }

ibDot11sMeshMPLinkStatus OBJECT-TYPE
    SYNTAX      INTEGER
        {
            processing(1),
            up(2),
            down(3)
        }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the status of mesh link that used to connect with this
         neighbor."
    ::= { ibDot11sMeshNbrStatusEntry 8 }

ibDot11sMeshNbrChannel OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the channel number used by this neighbor."
    ::= { ibDot11sMeshNbrStatusEntry 9 }

ibDot11sMeshNbrLinkDuration OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "second"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the duration of mesh link that used to connect with this
         neighbor."
    ::= { ibDot11sMeshNbrStatusEntry 10 }

ibDot11sMeshNbrRSSI OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the RSSI of mesh link that used to connect with this
         neighbor."
    ::= { ibDot11sMeshNbrStatusEntry 11 }

ibDot11sMeshNbrSNR OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "percent"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Represents the SNR of mesh link that used to connect with this
         neighbor."
    ::= { ibDot11sMeshNbrStatusEntry 12 }

-- *****************************************************************************
-- *  End of ibDot11sMeshNbrStatusTable Definition
