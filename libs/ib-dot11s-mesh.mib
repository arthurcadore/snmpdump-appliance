-- *****************************************************************************

ibDot11sConfigGroup OBJECT IDENTIFIER  ::= { ibDot11sMesh 1 }
--  Dot11s Configuration Group has the following children:
--  ibDot11sMeshGlobalPara             ::= { ibDot11sConfigGroup 1 }
--  ibDot11sMeshPflTable               ::= { ibDot11sConfigGroup 2 }
--  ibDot11sMpPlcyTable                ::= { ibDot11sConfigGroup 3 }
--  ibDot11sMlspCfgTable               ::= { ibDot11sConfigGroup 4 }
--  ibDot11sRadioCfgTable              ::= { ibDot11sConfigGroup 5 }
--  ibDot11sAPCfgTable                 ::= { ibDot11sConfigGroup 6 }
--  ibDot11sMlspExtCfgTable            ::= { ibDot11sConfigGroup 7 }

ibDot11sWDSConfigGroup OBJECT IDENTIFIER ::= { ibDot11sMesh 2 }
--  Dot11s WDS Configuration Group has the following children:
--  ibDot11sWDSPeerMacTable         ::= { ibDot11sWDSConfigGroup 1 }

ibDot11sMeshStatusGroup OBJECT IDENTIFIER ::= { ibDot11sMesh 3 }
--  Dot11s Mesh Status Group has the following children:
--  ibDot11sMeshLinkStatusTable    ::= { ibDot11sMeshStatusGroup 1 }
