--  ibDot11sMeshNbrStatusTable     ::= { ibDot11sMeshStatusGroup 3 }

ibDot11sMeshLinkNotifyGroup OBJECT IDENTIFIER ::= { ibDot11sMesh 4 }
-- *****************************************************************************
-- *  ibDot11sMeshGlobalPara Definition
-- *****************************************************************************
ibDot11sMeshGlobalPara OBJECT IDENTIFIER  ::= { ibDot11sConfigGroup 1 }

ibDot11sMeshMkdID OBJECT-TYPE
    SYNTAX      MacAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object represents the mesh key distributor identifier (MKD-ID).
        If the MKD-ID is not configured, the value is '00:00:00:00:00:00'."
    ::= { ibDot11sMeshGlobalPara 1 }

-- *****************************************************************************
-- * End of ibDot11sMeshGlobalPara Definition
-- *****************************************************************************
-- *****************************************************************************
-- *  ibDot11sMeshPflTable Definition
-- *****************************************************************************
ibDot11sMeshPflTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbDot11sMeshPflEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table is used to configure mesh profile.
        A mesh profile is created and mapped to a MP so that
        it can provide mesh services to other MPs which have the same
        mesh ID.
        When the mesh profile is enabled, all of the object in this table
        cannot be modified except ibDot11sMeshPflEnable.
        The mesh profile cannot be deleted when it is bound with the radio."
    ::= { ibDot11sConfigGroup 2 }

ibDot11sMeshPflEntry OBJECT-TYPE
    SYNTAX      IbDot11sMeshPflEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry contains information of a mesh profile."
    INDEX
        {
            ibDot11sMeshPflIndex
        }
    ::= { ibDot11sMeshPflTable 1 }

IbDot11sMeshPflEntry ::= SEQUENCE
    {
        ibDot11sMeshPflIndex         Integer32,
        ibDot11sMeshPflMeshID        OCTET STRING,
        ibDot11sMeshPflBindIntNum    Integer32,
        ibDot11sMeshPflKeepAlive     Integer32,
        ibDot11sMeshPflBackhaulRate  Integer32,
        ibDot11sMeshMkdServEnable    TruthValue,
        ibDot11sMeshPflEnable        TruthValue,
        ibDot11sMeshPflRowStatus     RowStatus,
        ibDot11sMeshAKMMode          IbDot11AKMType,
        ibDot11sMeshSimpleKey        OCTET STRING
    }

ibDot11sMeshPflIndex OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This object represents the index of the mesh profile."
    ::= { ibDot11sMeshPflEntry 1 }

ibDot11sMeshPflMeshID OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE (0..32))
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object represents the mesh ID of the mesh profile.
        The string length of this object is zero when the mesh ID is not
        configured.  If the mesh ID is not configured, ibDot11sMeshPflEnable
        cannot be set to 'true'."
    ::= { ibDot11sMeshPflEntry 2 }

ibDot11sMeshPflBindIntNum OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object represents the specified mesh interface number
        bound to the mesh profile.
        If the value is set to -1, the binding will be removed."
    DEFVAL      { -1 }
    ::= { ibDot11sMeshPflEntry 3 }

ibDot11sMeshPflKeepAlive OBJECT-TYPE
    SYNTAX      Integer32(1..1800)
    UNITS       "second"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object represents the mesh link keep-alive interval."
    ::= { ibDot11sMeshPflEntry 4 }

ibDot11sMeshPflBackhaulRate OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "Kbps"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object represents the link backhaul rate."
    ::= { ibDot11sMeshPflEntry 5 }

ibDot11sMeshMkdServEnable OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object indicates whether the mesh key distributor (MKD)
        service for the mesh profile is enabled.
        'true': The MKD service for the mesh profile is enabled.
        'false': The MKD service for the mesh profile is disabled."
    DEFVAL      { false }
    ::= { ibDot11sMeshPflEntry 6 }

ibDot11sMeshPflEnable OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object indicates whether the mesh profile is enabled.
        'true': The mesh profile is enabled.
        'false': The mesh profile is disabled."
    DEFVAL      { false }
    ::= { ibDot11sMeshPflEntry 7 }

ibDot11sMeshPflRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The status of this table entry."
    ::= { ibDot11sMeshPflEntry 8 }

ibDot11sMeshAKMMode OBJECT-TYPE
    SYNTAX      IbDot11AKMType
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the authentication and key management mode defined by 802.11s.
        The following values are supported:
        'none': No key management mode configured.
        'psk': pre-shared key authentication."
    DEFVAL      { none }
    ::= { ibDot11sMeshPflEntry 9 }

ibDot11sMeshSimpleKey OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Represents the simple key for SAE."
    ::= { ibDot11sMeshPflEntry 10 }

-- *****************************************************************************
-- * End of ibDot11sMeshPflTable Definition
