-- *****************************************************************************
ibDot11sAPCfgTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbDot11sAPCfgEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table is used to configure mesh to the specified AP."
    ::= { ibDot11sConfigGroup 6 }

ibDot11sAPCfgEntry OBJECT-TYPE
    SYNTAX      IbDot11sAPCfgEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry contains mesh configure information
        of the specified AP."
    INDEX
        {
            ibDot11APElementIndex
        }
    ::= { ibDot11sAPCfgTable 1 }

IbDot11sAPCfgEntry ::= SEQUENCE
    {
        ibDot11sPortalEnable         TruthValue
    }

ibDot11sPortalEnable OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object indicates whether the portal service is enabled.
        'true': The portal service is enabled.
        'false': The portal service is disabled."
    DEFVAL      { false }
    ::= { ibDot11sAPCfgEntry 1 }

-- *****************************************************************************
