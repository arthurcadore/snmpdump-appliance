ibDot11sMlspCfgTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbDot11sMlspCfgEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table is used to configure MLSP proxy MAC address.
        Only if the specified MP policy is modifiable and the MLSP
        is enabled, the row of this table can be created and deleted.
        The row of this table cannot be modified."
    ::= { ibDot11sConfigGroup 4 }

ibDot11sMlspCfgEntry OBJECT-TYPE
    SYNTAX      IbDot11sMlspCfgEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry contains information of MLSP proxy MAC address."
    INDEX
        {
            ibDot11sMpPlcyIndex,
            ibDot11sMlspProxyIndex
        }
    ::= { ibDot11sMlspCfgTable 1 }

IbDot11sMlspCfgEntry ::= SEQUENCE
    {
        ibDot11sMlspProxyIndex       Integer32,
        ibDot11sMlspProxyMac         MacAddress,
        ibDot11sMlspRowStatus        RowStatus
    }

ibDot11sMlspProxyIndex OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This object represents the MLSP proxy MAC address index."
    ::= { ibDot11sMlspCfgEntry 1 }

ibDot11sMlspProxyMac OBJECT-TYPE
    SYNTAX      MacAddress
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object represents the MLSP proxy MAC address."
    ::= { ibDot11sMlspCfgEntry 2 }

ibDot11sMlspRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The status of this table entry."
    ::= { ibDot11sMlspCfgEntry 3 }

-- *****************************************************************************
-- * End of ibDot11sMlspCfgTable Definition
