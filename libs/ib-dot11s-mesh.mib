ibDot11sMeshLinkTraps OBJECT IDENTIFIER ::= { ibDot11sMeshLinkNotifyGroup 0 }

ibDot11sMeshLinkSwitchTrap NOTIFICATION-TYPE
    OBJECTS
        {
            ibDot11sMeshLinkBSSIDMAC,
            ibDot11sMeshLinkPeerMAC
        }
    STATUS        current
    DESCRIPTION
        "This notification is to indicate the occurrence of a mesh link switchover."
    ::= { ibDot11sMeshLinkTraps 1 }

ibDot11sMeshLinkTrapObjects OBJECT IDENTIFIER ::= { ibDot11sMeshLinkNotifyGroup 1 }

ibDot11sMeshLinkBSSIDMAC OBJECT-TYPE
    SYNTAX      MacAddress
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "Represents the BSSID of the MP."
    ::= { ibDot11sMeshLinkTrapObjects 1 }

ibDot11sMeshLinkPeerMAC OBJECT-TYPE
    SYNTAX      MacAddress
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "Represents the MAC address of the peer MP after mesh link switchover."
    ::= { ibDot11sMeshLinkTrapObjects 2 }
END

