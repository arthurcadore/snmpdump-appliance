-- *****************************************************************************
-- *  ibDot11sMpPlcyTable Definition
-- *****************************************************************************
ibDot11sMpPlcyTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IbDot11sMpPlcyEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table is used to configure MP policy.
        There is a default policy on system and its index is 1.
        The default policy cannot be deleted or modified.
        The MP policy cannot be deleted when it is applied to a radio."
    ::= { ibDot11sConfigGroup 3 }

ibDot11sMpPlcyEntry OBJECT-TYPE
    SYNTAX      IbDot11sMpPlcyEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry contains information of a MP policy."
    INDEX
        {
            ibDot11sMpPlcyIndex
        }
    ::= { ibDot11sMpPlcyTable 1 }

IbDot11sMpPlcyEntry ::= SEQUENCE
    {
        ibDot11sMpPlcyIndex          Integer32,
        ibDot11sMpPlcyName           OCTET STRING,
        ibDot11sMpPlcyInitEnable     TruthValue,
        ibDot11sMlspEnable           TruthValue,
        ibDot11sProbReqInterval      Integer32,
        ibDot11sRoleAuthEnable       TruthValue,
        ibDot11sLinkHoldRSSI         Integer32,
        ibDot11sLinkHoldTime         Integer32,
        ibDot11sSwitchMargin         Integer32,
        ibDot11sLinkSaturationRSSI   Integer32,
        ibDot11sLinkRateMode         INTEGER,
        ibDot11sMaxLinkNum           Integer32,
        ibDot11sMpPlcyRowStatus      RowStatus,
        ibDot11sMlspMode             INTEGER,
        ibDot11sWaveGuideHoldRSSI    Integer32,
        ibDot11sWaveGuideSwitch      TruthValue,
        ibDot11sOptimizeEnable       TruthValue,
        ibDot11sOptimizeThreshold    Integer32,
        ibDot11sMlspLogEnable        TruthValue,
        ibDot11sMlspLogInterval      Integer32,
        ibDot11sActiveKeepaliveCount Integer32,
        ibDot11sLinkEstaThreshold    Integer32,
        ibDot11sTransmissionMethod   INTEGER,
        ibDot11sTransmitLevel        Integer32,
        ibDot11sTempLinkEnable       TruthValue
    }

ibDot11sMpPlcyIndex OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This object represents the MP policy index."
    ::= { ibDot11sMpPlcyEntry 1 }

ibDot11sMpPlcyName OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE (1..31))
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object represents the MP policy name.
        MP policy cannot be created with the name of the
        default policy.
        Modification is not supported."
    ::= { ibDot11sMpPlcyEntry 2 }

ibDot11sMpPlcyInitEnable OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object indicates whether the link initiation for
        the MP policy is enabled.
        'true': The link initiation for the MP policy is enabled.
        'false': The link initiation for the MP policy is disabled."
    DEFVAL      { true }
    ::= { ibDot11sMpPlcyEntry 3 }

ibDot11sMlspEnable OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object indicates whether the mesh link switch protocol (MLSP)
        is enabled.
        'true': MLSP is enabled.
        'false': MLSP is disabled."
    DEFVAL      { false }
    ::= { ibDot11sMpPlcyEntry 4 }

ibDot11sProbReqInterval OBJECT-TYPE
    SYNTAX      Integer32
    UNITS       "millisecond"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object represents the probe request interval."
    ::= { ibDot11sMpPlcyEntry 5 }

ibDot11sRoleAuthEnable OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object indicates whether the device can play as the role
        of an authenticator."
    DEFVAL      { true }
    ::= { ibDot11sMpPlcyEntry 6 }

ibDot11sLinkHoldRSSI OBJECT-TYPE
    SYNTAX      Integer32(5..100)
    UNITS       "dBm"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object represents the link-hold
        receive signal strength indicator (RSSI)."
    ::= { ibDot11sMpPlcyEntry 7 }

ibDot11sLinkHoldTime OBJECT-TYPE
    SYNTAX      Integer32(1000..20000)
    UNITS       "millisecond"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object represents the link-hold time."
    ::= { ibDot11sMpPlcyEntry 8 }

ibDot11sSwitchMargin OBJECT-TYPE
    SYNTAX      Integer32(1..100)
    UNITS       "dBm"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object represents the link-switch margin."
    ::= { ibDot11sMpPlcyEntry 9 }

ibDot11sLinkSaturationRSSI OBJECT-TYPE
    SYNTAX      Integer32(30..150)
    UNITS       "dBm"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object represents the link saturation
        receive signal strength indicator (RSSI)."
    ::= { ibDot11sMpPlcyEntry 10 }

ibDot11sLinkRateMode OBJECT-TYPE
    SYNTAX      INTEGER
        {
            fixed(1),
            realtime(2)
        }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object represents the rate mode of the mesh link."
    DEFVAL      { fixed }
    ::= { ibDot11sMpPlcyEntry 11 }

ibDot11sMaxLinkNum OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object represents the maximum number of the mesh link."
    ::= { ibDot11sMpPlcyEntry 12 }

ibDot11sMpPlcyRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "The status of this table entry."
    ::= { ibDot11sMpPlcyEntry 13 }

ibDot11sMlspMode OBJECT-TYPE
    SYNTAX      INTEGER
        {
            normal(0),
            satble(1)
        }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object represents the MLSP mode."
    DEFVAL      { normal }
    ::= { ibDot11sMpPlcyEntry 14 }

ibDot11sWaveGuideHoldRSSI OBJECT-TYPE
    SYNTAX      Integer32(5..100)
    UNITS       "dBm"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object represents the link-hold
        receive signal strength indicator (RSSI) of wave guides."
    DEFVAL      { 15 }
    ::= { ibDot11sMpPlcyEntry 15 }

ibDot11sWaveGuideSwitch OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object represents the wave-guide switch state."
    DEFVAL      { false }
    ::= { ibDot11sMpPlcyEntry 16 }

ibDot11sOptimizeEnable OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object indicates whether the mesh link switch protocol (MLSP)
        optimization function is enabled."
    DEFVAL      { false }
    ::= { ibDot11sMpPlcyEntry 17 }

ibDot11sOptimizeThreshold OBJECT-TYPE
    SYNTAX      Integer32(15..80)
    UNITS       "dBm"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object represents the mesh link switch protocol (MLSP)
        optimization threshold."
    DEFVAL      { 30 }
    ::= { ibDot11sMpPlcyEntry 18 }

ibDot11sMlspLogEnable OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "This object indicates whether the mesh link switch protocol (MLSP)
