        STATUS obsolete
        DESCRIPTION "Active port on combo interface."
        ::= { ibifXXEntry 22 }

    ibifBMbpsMulSuppressionMax OBJECT-TYPE
        SYNTAX  Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
                "The maximum value of the multicast suppression with bandwidth-based(Mbps)
                that a port can be configured."
        ::= { ibifXXEntry 23 }

    ibifBMbpsMulSuppression OBJECT-TYPE
        SYNTAX  Integer32
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
                "With bandwidth-based multicast suppression, the bandwidth is measured
                in Mbps.
                The upper limit of the multicast suppession with bandwidth-based(Mbps)
                is the value of ibifBMbpsMulSuppressionMax in the entry.
                The default value of ibifBMbpsMulSuppression is the value of
                ibifBMbpsMulSuppressionMax."
        ::= { ibifXXEntry 24 }

    ibifBKbpsMulSuppressionMax OBJECT-TYPE
        SYNTAX  Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
