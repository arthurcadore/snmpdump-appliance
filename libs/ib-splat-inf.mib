        ibifFlowControl     TruthValue  ,
        ibifSrcMacControl   TruthValue  ,
        ibifClearStat       INTEGER,
        ibifXXBasePortIndex INTEGER,
        ibifXXDevPortIndex  INTEGER,
        ibifPpsMcastControl  Integer32,
        ibifPpsBcastDisValControl INTEGER,
        ibifUniSuppressionStep Integer32,
        ibifPpsUniSuppressionMax Integer32,
        ibifMulSuppressionStep Integer32,
        ibifPpsMulSuppressionMax Integer32,
        ibifUniSuppression   Integer32,
        ibifPpsUniSuppression Integer32,
        ibifMulSuppression    Integer32,
        ibifPpsMulSuppression Integer32,
        ibifComboActivePort   INTEGER,
        ibifBMbpsMulSuppressionMax Integer32,
        ibifBMbpsMulSuppression Integer32,
        ibifBKbpsMulSuppressionMax Integer32,
        ibifBKbpsMulSuppressionStep Integer32,
        ibifBKbpsMulSuppression Integer32,
        ibifUnknownPacketDropMul DropDirection,
        ibifUnknownPacketDropUni DropDirection,
        ibifBMbpsUniSuppressionMax Integer32,
        ibifBMbpsUniSuppression Integer32,
        ibifBKbpsUniSuppressionMax Integer32,
        ibifBKbpsUniSuppressionStep Integer32,
        ibifBKbpsUniSuppression Integer32,
        ibifOutPayloadOctets Counter64,
        ibifInPayloadOctets Counter64,
        ibifInErrorPktsRate Integer32,
        ibifInPkts Counter64,
        ibifInNormalPkts  Counter64,
        ibifOutPkts Counter64,
        ibifMulSuppressionFlag  INTEGER
        }


   ibifUnBoundPort  OBJECT-TYPE
        SYNTAX  TruthValue
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Whether it is the unbound port. (true indicates that the port is
            the main port of the aggregation or the port does not participate
            in the aggregation.)"
        ::= { ibifXXEntry 1 }


    ibifISPhyPort  OBJECT-TYPE
        SYNTAX  TruthValue
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "Whether it is a physical interface."
        ::= { ibifXXEntry 2 }


    ibifAggregatePort OBJECT-TYPE
        SYNTAX  TruthValue
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "Whether it is the aggregated port. (if the port participates
                    in the aggregation, this value is true.)"
        ::= { ibifXXEntry 3 }


    ibifMirrorPort  OBJECT-TYPE
        SYNTAX  TruthValue
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "Whether it is a mirror port."
        ::= { ibifXXEntry 4 }


    ibifVLANType  OBJECT-TYPE
        SYNTAX  INTEGER
        {
        vLANTrunk  (1),
        access   (2),
        hybrid    (3),
        fabric   (4)
        }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "port vlan types.
            hybrid (3) port can carry multiple VLANs.
            If fabric function is supported, fabric(4) means the port is
            a fabric port."
        ::= { ibifXXEntry 5 }


    ibifMcastControl OBJECT-TYPE
        SYNTAX  INTEGER (0..100)
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "Broadcast storm suppression with the step length of 1,
            ranging from 0 to 100 percent.
            In some products the step is 5, ranging from 0 to 100."
        ::= { ibifXXEntry 6 }

    ibifFlowControl  OBJECT-TYPE
        SYNTAX  TruthValue
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "Flow control status."
        ::= { ibifXXEntry 7 }


    ibifSrcMacControl OBJECT-TYPE
        SYNTAX  TruthValue
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "Whether to filter by source MAC address."
        ::= { ibifXXEntry 8 }


    ibifClearStat  OBJECT-TYPE
        SYNTAX  INTEGER
        {
            clear(1)
        }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION "Clear specify port statistics.  Operation read is not supported."
        ::= { ibifXXEntry 9 }

    ibifXXBasePortIndex OBJECT-TYPE
        SYNTAX  INTEGER
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "Index number of the port and the first port index of the
                    device is 1."
        ::= { ibifXXEntry 10 }

    ibifXXDevPortIndex OBJECT-TYPE
        SYNTAX  INTEGER
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "Device index of the port."
        ::= { ibifXXEntry 11 }

   ibifPpsMcastControl OBJECT-TYPE
        SYNTAX  Integer32
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "The broadcast suppression with pps(packet per second) type.
            The max value is determined by the port type and product."
        ::= { ibifXXEntry 12 }

   ibifPpsBcastDisValControl OBJECT-TYPE
        SYNTAX  INTEGER
        {
          enable(1),
          disable(2)
        }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "Control the port's pps(packet per second) broadcast suppression.
            When the port is enabled, its pps broadcast suppression value is the
            global disperse value, and when disabled, it doesn't suppress broadcast."
        ::= { ibifXXEntry 13 }

   ibifUniSuppressionStep OBJECT-TYPE
        SYNTAX  Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "The step of unicast suppression in ratio mode."
        ::= { ibifXXEntry 14 }

   ibifPpsUniSuppressionMax OBJECT-TYPE
        SYNTAX  Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "The max pps(packet per second) value of unicast suppression
                    in pps mode."
        ::= { ibifXXEntry 15 }

   ibifMulSuppressionStep OBJECT-TYPE
        SYNTAX  Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "The step of multicast suppression in ratio mode."
        ::= { ibifXXEntry 16 }

   ibifPpsMulSuppressionMax OBJECT-TYPE
        SYNTAX  Integer32
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION "The max pps(packet per second) value of multicast suppression
                    in pps mode."
        ::= { ibifXXEntry 17 }

   ibifUniSuppression OBJECT-TYPE
        SYNTAX  Integer32
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "The unicast suppression with the ranging from 0 to 100 percent
            in ratio mode.
            The step is determined by ibifUniSuppressionStep."
        ::= { ibifXXEntry 18 }

   ibifPpsUniSuppression OBJECT-TYPE
        SYNTAX  Integer32
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "The unicast suppression in pps(packet per second) mode.
            The max value is determined by ibifPpsUniSuppressionMax."
        ::= { ibifXXEntry 19 }

   ibifMulSuppression OBJECT-TYPE
        SYNTAX  Integer32
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "The multicast suppression with ranging from 0 to 100 percent
            in ratio mode.
            The step is determined by ibifMulSuppressionStep.
            If ibifMulSuppressionFlag is 2 it means only unknown
            traffic suppression."
        ::= { ibifXXEntry 20 }

   ibifPpsMulSuppression OBJECT-TYPE
        SYNTAX  Integer32
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "The multicast suppression in pps(packet per second) mode.
            The max pps value is determined by ibifPpsMulSuppressionMax.
            If ibifMulSuppressionFlag is 2 it means only unknown
            traffic suppression."
        ::= { ibifXXEntry 21 }

