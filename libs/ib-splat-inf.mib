        SYNTAX IbPortIsolateGroupEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION "The entry of ibPortIsolateGroupTable."
        INDEX    { ibPortIsolateGroupIndex }
        ::= { ibPortIsolateGroupTable 1 }

    IbPortIsolateGroupEntry ::=
    SEQUENCE {
        ibPortIsolateGroupIndex              Integer32,
        ibPortIsolateUplinkIfIndex           InterfaceIndex,
        ibPortIsolateGroupRowStatus          RowStatus,
        ibPortIsolateGroupDescription        DisplayString
        }

   ibPortIsolateGroupIndex OBJECT-TYPE
        SYNTAX  Integer32
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "Port isolate group identifier.
            The index of the ibPortIsolateGroupTable.
            The value ranges from 1 to the limit of isolate group quantity."
        ::= { ibPortIsolateGroupEntry 1 }

   ibPortIsolateUplinkIfIndex  OBJECT-TYPE
