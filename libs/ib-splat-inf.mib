    ibLswExtInterface OBJECT IDENTIFIER ::= { iblswCommon 1 }

    ibifXXTable  OBJECT-TYPE
        SYNTAX SEQUENCE OF IbIfXXEntry
        MAX-ACCESS not-accessible
        STATUS   current
        DESCRIPTION "Extended interface information table."
        ::= { ibLswExtInterface  1 }


    ibifXXEntry  OBJECT-TYPE
        SYNTAX IbIfXXEntry
        MAX-ACCESS not-accessible
        STATUS   current
        DESCRIPTION "Entries of extended interface information table."
        INDEX { ifIndex }
        ::= { ibifXXTable 1}

    IbIfXXEntry ::=
    SEQUENCE {
        ibifUnBoundPort     TruthValue  ,
        ibifISPhyPort       TruthValue  ,
        ibifAggregatePort   TruthValue  ,
        ibifMirrorPort      TruthValue  ,
